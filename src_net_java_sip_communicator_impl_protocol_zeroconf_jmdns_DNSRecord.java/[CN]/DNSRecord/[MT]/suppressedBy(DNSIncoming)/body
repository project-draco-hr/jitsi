{
  try {
    for (int i=msg.numAnswers; i-- > 0; ) {
      if (suppressedBy((DNSRecord)msg.answers.get(i))) {
        return true;
      }
    }
    return false;
  }
 catch (  ArrayIndexOutOfBoundsException e) {
    logger.log(Level.WARNING,"suppressedBy() message " + msg + " exception ",e);
    return false;
  }
}
