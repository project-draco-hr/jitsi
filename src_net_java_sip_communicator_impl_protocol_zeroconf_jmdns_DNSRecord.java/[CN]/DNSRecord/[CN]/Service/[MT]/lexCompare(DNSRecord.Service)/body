{
  byte[] thisBytes=this.toByteArray();
  byte[] thatBytes=that.toByteArray();
  for (int i=0, n=Math.min(thisBytes.length,thatBytes.length); i < n; i++) {
    if (thisBytes[i] > thatBytes[i]) {
      return 1;
    }
 else {
      if (thisBytes[i] < thatBytes[i]) {
        return -1;
      }
    }
  }
  return thisBytes.length - thatBytes.length;
}
