{
  if (callPeer == null) {
    throw new NullPointerException("Argument is null");
  }
  if (!(callPeer instanceof CallPeerSipImpl)) {
    throw new IllegalArgumentException();
  }
  CallPeerSipImpl cp=(CallPeerSipImpl)(callPeer);
  if (this.dtmfMethod == null || this.dtmfMethod.equals("RFC4733 / SIP-INFO")) {
    if (isRFC4733Active(cp)) {
      dtmfModeRFC4733.stopSendingDTMF(((AudioMediaStream)cp.getMediaHandler().getStream(MediaType.AUDIO)));
    }
 else {
      dtmfModeInfo.stopSendingDTMF(cp);
    }
  }
}
