{
  String result="";
  result+="startDate: " + new SimpleDateFormat("yyyy-MM-dd HH:mm:ss z").format(startDate) + "\n";
  result+="endDate: " + new SimpleDateFormat("yyyy-MM-dd HH:mm:ss z").format(endDate) + "\n";
  result+="originalStartDate: " + new SimpleDateFormat("yyyy-MM-dd HH:mm:ss z").format(originalStartDate) + "\n";
  return result;
}
