{
  super.paintComponent(g);
  Image logoImage=ImageLoader.getImage(ImageLoader.WINDOW_TITLE_BAR);
  g.drawImage(logoImage,0,0,null);
  g.setColor(new Color(GuiActivator.getResources().getColor("service.gui.LOGO_BAR_BACKGROUND")));
  Graphics2D g2=(Graphics2D)g;
  g2.setPaint(texture);
  g2.fillRect(logoImage.getWidth(null),0,this.getWidth(),this.getHeight());
}
