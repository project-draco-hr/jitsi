{
  Format format;
  if (this.format == null) {
    format=getFdFormat();
    if (format == null)     format=super.doGetFormat();
 else {
      VideoFormat videoFormat=(VideoFormat)format;
      if (videoFormat.getSize() != null)       this.format=format;
    }
  }
 else   format=this.format;
  return format;
}
