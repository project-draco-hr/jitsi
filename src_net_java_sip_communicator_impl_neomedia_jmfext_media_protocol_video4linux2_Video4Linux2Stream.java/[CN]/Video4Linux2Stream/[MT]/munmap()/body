{
  try {
    if (mmaps != null) {
      for (int i=0; i < mmaps.length; i++) {
        long mmap=mmaps[i];
        if (mmap != 0) {
          Video4Linux2.munmap(mmap,mmapLengths[i]);
          mmaps[i]=0;
          mmapLengths[i]=0;
        }
      }
    }
  }
  finally {
    mmaps=null;
    mmapLengths=null;
  }
}
