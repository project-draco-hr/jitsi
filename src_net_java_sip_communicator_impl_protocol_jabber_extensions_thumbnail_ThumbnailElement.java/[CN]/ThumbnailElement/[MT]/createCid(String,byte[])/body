{
  try {
    return "sha1+" + Sha1Crypto.encode(thumbnailData) + "@"+ serverAddress;
  }
 catch (  NoSuchAlgorithmException e) {
    logger.debug("Failed to encode the thumbnail in SHA-1.",e);
  }
catch (  UnsupportedEncodingException e) {
    logger.debug("Failed to encode the thumbnail in SHA-1.",e);
  }
  return null;
}
