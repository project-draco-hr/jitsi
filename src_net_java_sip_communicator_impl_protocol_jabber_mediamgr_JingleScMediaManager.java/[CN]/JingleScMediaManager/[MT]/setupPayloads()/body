{
  String[] audioEnc;
  try {
    audioEnc=JabberActivator.getMediaService().getSupportedAudioEncodings();
  }
 catch (  Exception e) {
    audioEnc=null;
    logger.warn("Cannot get Audio encodings!",e);
  }
  if (audioEnc != null) {
    for (int i=0; i < audioEnc.length; i++) {
      int payloadType=MediaUtils.getPayloadType(Integer.parseInt(audioEnc[i]));
      String payloadName=MediaUtils.getPayloadName(payloadType);
      if (payloadName != null) {
        payloads.add(new PayloadType.Audio(payloadType,payloadName));
      }
 else       if (payloadType >= 0) {
        payloads.add(new PayloadType.Audio(payloadType,audioEnc[i]));
      }
    }
  }
  if (payloads.isEmpty()) {
    logger.warn("The list of payloads supported" + " by JmfMediaManager is empty ");
  }
}
