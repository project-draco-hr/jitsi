{
  String[] audioEnc=null;
  if (audioEnc != null) {
    for (    String audioEncI : audioEnc) {
      int payloadType=MediaUtils.getPayloadType(Integer.parseInt(audioEncI));
      String payloadName=MediaUtils.getPayloadName(payloadType);
      if (payloadName != null) {
        payloads.add(new PayloadType.Audio(payloadType,payloadName));
      }
 else       if (payloadType >= 0) {
        payloads.add(new PayloadType.Audio(payloadType,audioEncI));
      }
    }
  }
  if (payloads.isEmpty()) {
    logger.warn("The list of payloads supported" + " by JmfMediaManager is empty ");
  }
}
