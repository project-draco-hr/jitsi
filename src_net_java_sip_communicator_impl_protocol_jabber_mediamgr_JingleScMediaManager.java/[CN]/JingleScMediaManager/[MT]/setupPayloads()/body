{
  String[] audioEnc=JabberActivator.getMediaService().getSupportedAudioEncodings();
  for (int i=0; i < audioEnc.length; i++) {
    int payloadType=Integer.parseInt(audioEnc[i]);
    if (MediaUtils.getPayloadName(payloadType) != null) {
      payloads.add(new PayloadType.Audio(payloadType,MediaUtils.getPayloadName(payloadType)));
    }
 else {
      payloads.add(new PayloadType.Audio(payloadType,audioEnc[i]));
    }
  }
  if (payloads.isEmpty()) {
    logger.warn("The list of payloads supported" + " by JmfMediaManager is empty ");
  }
}
