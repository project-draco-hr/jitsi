{
  float FLT_MAX_G729=Ld8k.FLT_MAX_G729;
  int M=Ld8k.M;
  int NC0=Ld8k.NC0;
  int i, j;
  float dmin, dist, temp;
  int cand=0;
  dmin=FLT_MAX_G729;
  for (i=0; i < NC0; i++) {
    dist=0.f;
    for (j=0; j < M; j++) {
      temp=rbuf[j] - lspcb1[i][j];
      dist+=temp * temp;
    }
    if (dist < dmin) {
      dmin=dist;
      cand=i;
    }
  }
  return cand;
}
