{
  AdHocChatRoom chatRoom=null;
  try {
    YahooConference conference=yahooProvider.getYahooSession().createConference(new String[]{},"",yahooProvider.getYahooSession().getLoginIdentity());
    chatRoom=findRoom(conference);
  }
 catch (  Exception e) {
    logger.debug("Failed to create the chat Room" + e);
  }
  return chatRoom;
}
