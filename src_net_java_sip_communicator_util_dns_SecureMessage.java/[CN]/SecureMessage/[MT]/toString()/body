{
  StringBuilder s=new StringBuilder(super.toString());
  s.append('\n');
  s.append(";; Secure: ");
  s.append(secure);
  s.append('\n');
  s.append(";; Bogus:  ");
  s.append(bogus);
  s.append('\n');
  if (bogus) {
    s.append(";;  Reason: ");
    s.append(bogusReason);
    s.append('\n');
  }
  return s.toString();
}
