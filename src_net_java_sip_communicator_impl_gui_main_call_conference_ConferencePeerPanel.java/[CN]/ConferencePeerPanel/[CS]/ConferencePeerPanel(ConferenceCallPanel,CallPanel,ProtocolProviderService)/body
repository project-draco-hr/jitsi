{
  super(callRenderer,true);
  this.callRenderer=callRenderer;
  this.callPanel=callPanel;
  this.callPeer=null;
  byte[] globalAccountImage=AccountStatusPanel.getGlobalAccountImage();
  if (globalAccountImage != null && globalAccountImage.length > 0)   this.setPeerImage(globalAccountImage);
  this.setPeerName(protocolProvider.getAccountID().getUserID() + " (" + GuiActivator.getResources().getI18NString("service.gui.ACCOUNT_ME").toLowerCase()+ ")");
  this.setTitleBackground(new Color(GuiActivator.getResources().getColor("service.gui.CALL_LOCAL_USER_BACKGROUND")));
}
