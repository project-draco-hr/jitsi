{
  String localUserAddress=callPeer.getProtocolProvider().getAccountID().getAccountAddress();
  boolean isLocalMember=addressesAreEqual(member.getAddress(),localUserAddress);
  if (isLocalMember)   return;
  if (!isFocusUI())   setFocusUI(true);
  if (callPeer.getCall().getCallPeerCount() > 1)   setSingleFocusUI(false);
 else   setSingleFocusUI(true);
  if (conferenceMembersPanels.containsKey(member))   return;
  ConferenceMemberPanel memberPanel=new ConferenceMemberPanel(member);
  conferenceMembersPanels.put(member,memberPanel);
  GridBagConstraints constraints=new GridBagConstraints();
  constraints.fill=GridBagConstraints.BOTH;
  constraints.gridx=0;
  constraints.gridy=getComponentCount();
  constraints.weightx=1;
  constraints.weighty=0;
  constraints.insets=new Insets(10,0,0,0);
  this.add(memberPanel,constraints);
  member.addPropertyChangeListener(memberPanel);
}
