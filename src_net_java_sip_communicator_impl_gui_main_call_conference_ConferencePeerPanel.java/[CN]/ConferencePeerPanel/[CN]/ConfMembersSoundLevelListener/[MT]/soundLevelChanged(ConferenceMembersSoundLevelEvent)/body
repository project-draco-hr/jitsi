{
  Map<ConferenceMember,Integer> levels=event.getLevels();
  Iterator<Entry<ConferenceMember,ConferenceMemberPanel>> memberPanelsIter=conferenceMembersPanels.entrySet().iterator();
  while (memberPanelsIter.hasNext()) {
    Map.Entry<ConferenceMember,ConferenceMemberPanel> entry=memberPanelsIter.next();
    ConferenceMember member=entry.getKey();
    ConferenceMemberPanel memberPanel=entry.getValue();
    if (levels.containsKey(member))     memberPanel.updateSoundBar(levels.get(member));
 else     memberPanel.updateSoundBar(0);
  }
}
