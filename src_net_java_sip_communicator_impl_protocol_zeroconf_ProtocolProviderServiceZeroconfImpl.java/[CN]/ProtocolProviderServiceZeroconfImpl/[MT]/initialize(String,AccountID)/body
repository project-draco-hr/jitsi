    supportedOperationSets.put(OperationSetBasicInstantMessaging.class.getName(),basicInstantMessaging);
    supportedOperationSets.put(OperationSetTypingNotifications.class.getName(),new OperationSetTypingNotificationsZeroconfImpl(this));
    isInitialized=true;
  }
}
