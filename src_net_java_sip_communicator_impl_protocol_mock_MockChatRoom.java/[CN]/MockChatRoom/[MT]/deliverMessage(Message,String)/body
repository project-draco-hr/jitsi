{
  ChatRoomMember fromMember=null;
  Iterator iter=members.iterator();
  while (iter.hasNext()) {
    ChatRoomMember elem=(ChatRoomMember)iter.next();
    if (elem.getName().equals(from)) {
      fromMember=elem;
      break;
    }
  }
  if (fromMember == null)   return;
  ChatRoomMessageReceivedEvent evt=new ChatRoomMessageReceivedEvent(this,fromMember,System.currentTimeMillis(),msg,ChatRoomMessageReceivedEvent.CONVERSATION_MESSAGE_RECEIVED);
  iter=messageListeners.iterator();
  while (iter.hasNext()) {
    ChatRoomMessageListener elem=(ChatRoomMessageListener)iter.next();
    elem.messageReceived(evt);
  }
}
