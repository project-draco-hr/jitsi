{
  AccountID accountID=protocolProvider.getAccountID();
  this.userIDField.setText(accountID.getUserID());
  this.firstField.setText((String)accountID.getAccountProperties().get("first"));
  this.lastField.setText((String)accountID.getAccountProperties().get("last"));
  this.mailField.setText((String)accountID.getAccountProperties().get("mail"));
  Boolean remember=(Boolean)accountID.getAccountProperties().get("rememberContacts");
  if (remember.booleanValue())   this.rememberContacts.setSelected(true);
}
