{
  Iterator<ContentPacketExtension> contentIter=contents.iterator();
  while (contentIter.hasNext()) {
    ContentPacketExtension content=contentIter.next();
    if (name.equals(content.getName())) {
      contentIter.remove();
      RtpDescriptionPacketExtension rtpDescription=content.getFirstChildOfType(RtpDescriptionPacketExtension.class);
      if (rtpDescription != null) {
        closeStreamConnector(MediaType.parseString(rtpDescription.getMedia()));
      }
      return content;
    }
  }
  return null;
}
