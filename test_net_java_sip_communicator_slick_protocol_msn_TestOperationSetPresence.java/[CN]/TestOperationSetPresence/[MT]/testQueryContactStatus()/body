{
  logger.debug("Will Query an AWAY contact.");
  subtestQueryContactStatus(MsnStatusEnum.AWAY,MsnStatusEnum.AWAY);
  logger.debug("Will Query an BRB contact.");
  subtestQueryContactStatus(MsnStatusEnum.BE_RIGHT_BACK,MsnStatusEnum.BE_RIGHT_BACK);
  logger.debug("Will Query a Busy contact.");
  subtestQueryContactStatus(MsnStatusEnum.BUSY,MsnStatusEnum.BUSY);
  logger.debug("Will Query a Idle contact.");
  subtestQueryContactStatus(MsnStatusEnum.IDLE,MsnStatusEnum.IDLE);
  logger.debug("Will Query an Invisible contact.");
  subtestQueryContactStatus(MsnStatusEnum.HIDE,MsnStatusEnum.OFFLINE);
  logger.debug("Will Query an Online contact.");
  subtestQueryContactStatus(MsnStatusEnum.ONLINE,MsnStatusEnum.ONLINE);
}
