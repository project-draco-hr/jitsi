{
  int n;
  float mu, ga, temp;
  int ptrs;
  if (parcor0 > 0.f) {
    mu=parcor0 * GAMMA3_PLUS;
  }
 else {
    mu=parcor0 * GAMMA3_MINUS;
  }
  ga=1.f / (1.f - Math.abs(mu));
  ptrs=0;
  for (n=0; n < L_SUBFR; n++) {
    temp=mu * sig_in[ptrs];
    ptrs++;
    temp+=sig_in[ptrs];
    sig_out[sig_out_offset + n]=ga * temp;
  }
}
