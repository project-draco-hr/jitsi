{
  if (parentProvider == null)   throw new IllegalArgumentException("parent protocol provider cannot be null");
  this.parentProvider=parentProvider;
  if (chatRoom == null)   throw new IllegalArgumentException("chat room instance cannot be null");
  this.chatRoom=chatRoom;
  if (contactID == null)   throw new IllegalArgumentException("contact ID cannot be null");
  this.contactID=contactID;
  if (chatRoomMemberRole == null)   throw new IllegalArgumentException("member role cannot be null");
  this.chatRoomMemberRole=chatRoomMemberRole;
}
