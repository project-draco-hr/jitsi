{
  GoogleContactsSourceService found=null;
  for (  Map.Entry<GoogleContactsSourceService,ServiceRegistration> entry : cssList.entrySet()) {
    GoogleContactsConnectionImpl cnx=entry.getKey().getConnection();
    if (cnx == null) {
      return;
    }
    String cssName=cnx.getLogin();
    if (cssName.equals(login)) {
      try {
        entry.getValue().unregister();
      }
  finally {
        entry.getKey().stop();
      }
      found=entry.getKey();
      break;
    }
  }
  if (found != null) {
    cssList.remove(found);
  }
}
