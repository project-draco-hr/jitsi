{
  Iterator iter=itemsMap.entrySet().iterator();
  while (iter.hasNext()) {
    Map.Entry entry=(Map.Entry)iter.next();
    JMenuItem item=new JMenuItem(((IcqStatusEnum)entry.getKey()).getStatusName(),new ImageIcon((Image)entry.getValue()));
    item.addActionListener(this);
    this.popup.add(item);
  }
}
