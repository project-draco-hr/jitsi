{
  logger.debug("Conference Logon Received: " + ev.toString());
  try {
    ChatRoomYahooImpl chatRoom=(ChatRoomYahooImpl)findRoom(ev.getRoom().getName());
    if (chatRoom != null) {
      ChatRoomMemberYahooImpl newMember=new ChatRoomMemberYahooImpl(chatRoom,ev.getFrom(),ev.getFrom(),ChatRoomMemberRole.MEMBER);
      chatRoom.addChatRoomMember(newMember);
    }
  }
 catch (  Exception e) {
    logger.debug("Failed to add a user to the chat room. " + e);
  }
}
