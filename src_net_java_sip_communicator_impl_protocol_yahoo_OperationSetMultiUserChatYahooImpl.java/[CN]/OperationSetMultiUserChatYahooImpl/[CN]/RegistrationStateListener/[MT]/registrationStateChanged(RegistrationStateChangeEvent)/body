{
  if (evt.getNewState() == RegistrationState.REGISTERED) {
    opSetPersPresence=(OperationSetPersistentPresenceYahooImpl)yahooProvider.getSupportedOperationSets().get(OperationSetPersistentPresence.class.getName());
    yahooProvider.getYahooSession().addSessionListener(new YahooMessageListener());
  }
}
