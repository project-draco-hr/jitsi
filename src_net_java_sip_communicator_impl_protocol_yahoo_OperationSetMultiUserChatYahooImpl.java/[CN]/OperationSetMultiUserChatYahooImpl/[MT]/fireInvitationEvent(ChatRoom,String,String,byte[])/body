{
  ChatRoomInvitationYahooImpl invitation=new ChatRoomInvitationYahooImpl(targetChatRoom,inviter,reason,password);
  ChatRoomInvitationReceivedEvent evt=new ChatRoomInvitationReceivedEvent(this,invitation,new Date(System.currentTimeMillis()));
  Iterator listeners=null;
synchronized (invitationListeners) {
    listeners=new ArrayList(invitationListeners).iterator();
  }
  while (listeners.hasNext()) {
    ChatRoomInvitationListener listener=(ChatRoomInvitationListener)listeners.next();
    listener.invitationReceived(evt);
  }
}
