{
  this.yahooProvider=yahooProvider;
  yahooProvider.addRegistrationStateChangeListener(new RegistrationStateListener());
  opSetBasic=(OperationSetBasicInstantMessagingYahooImpl)yahooProvider.getOperationSet(OperationSetBasicInstantMessaging.class);
  OperationSetPersistentPresence presenceOpSet=(OperationSetPersistentPresence)yahooProvider.getOperationSet(OperationSetPersistentPresence.class);
  presenceOpSet.addSubscriptionListener(this);
}
