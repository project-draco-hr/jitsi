{
  JButton button=(JButton)e.getSource();
  String buttonName=button.getName();
  if (buttonName.equals("login")) {
    this.dispose();
    AccountID accountID=this.loginManager.installAccount(providerFactory,uinComboBox.getSelectedItem().toString(),new String(passwdField.getPassword()));
    this.loginManager.login(providerFactory,accountID);
  }
 else {
    this.dispose();
  }
}
