{
  String attribute;
  if (ON_HOLD_LOCALLY == (onHold & ON_HOLD_LOCALLY))   attribute=(ON_HOLD_REMOTELY == (onHold & ON_HOLD_REMOTELY)) ? "inactive" : "sendonly";
 else   attribute=(ON_HOLD_REMOTELY == (onHold & ON_HOLD_REMOTELY)) ? "recvonly" : null;
  if (attribute != null)   mediaDescription.setAttribute(attribute,null);
}
