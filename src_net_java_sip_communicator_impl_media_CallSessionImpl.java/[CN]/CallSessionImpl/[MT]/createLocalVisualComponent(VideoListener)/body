{
  DataSource dataSource=mediaServCallback.getMediaControl(getCall()).createLocalVideoDataSource();
  if (dataSource != null) {
    Player player;
    try {
      player=Manager.createPlayer(dataSource);
    }
 catch (    IOException ex) {
      throw new MediaException("Failed to create Player for local video DataSource.",MediaException.IO_ERROR,ex);
    }
catch (    NoPlayerException ex) {
      throw new MediaException("Failed to create Player for local video DataSource.",MediaException.INTERNAL_ERROR,ex);
    }
    player.addControllerListener(new ControllerListener(){
      public void controllerUpdate(      ControllerEvent event){
        controllerUpdateForCreateLocalVisualComponent(event,listener);
      }
    }
);
    player.start();
  }
  return null;
}
