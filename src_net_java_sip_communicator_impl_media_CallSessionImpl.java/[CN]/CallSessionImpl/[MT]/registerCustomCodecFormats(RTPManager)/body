{
  if (!FMJConditionals.REGISTER_FORMATS_WITH_EVERY_RTP_MANAGER && formatsRegisteredOnce) {
    return;
  }
  for (int i=0; i < CUSTOM_CODEC_FORMATS.length; i++) {
    javax.media.Format format=CUSTOM_CODEC_FORMATS[i];
    if (logger.isDebugEnabled())     logger.debug("registering format " + format + " with RTP manager");
    rtpManager.addFormat(format,MediaUtils.jmfToSdpEncoding(format.getEncoding()));
  }
  formatsRegisteredOnce=true;
}
