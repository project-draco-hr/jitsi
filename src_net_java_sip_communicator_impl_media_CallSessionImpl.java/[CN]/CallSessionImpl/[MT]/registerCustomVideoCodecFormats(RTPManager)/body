{
  if (!FMJConditionals.REGISTER_FORMATS_WITH_EVERY_RTP_MANAGER && formatsRegisteredOnceVideo) {
    return;
  }
  javax.media.Format format=new VideoFormat(Constants.H264_RTP);
  if (logger.isDebugEnabled())   logger.debug("registering format " + format + " with RTP manager");
  rtpManager.addFormat(format,MediaUtils.jmfToSdpEncoding(format.getEncoding()));
  formatsRegisteredOnceVideo=true;
}
