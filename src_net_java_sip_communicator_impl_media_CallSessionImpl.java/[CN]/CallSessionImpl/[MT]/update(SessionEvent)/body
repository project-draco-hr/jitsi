{
  if (logger.isDebugEnabled()) {
    if (event instanceof NewParticipantEvent) {
      Participant participant=((NewParticipantEvent)event).getParticipant();
      if (logger.isDebugEnabled())       logger.debug("A new participant had just joined: " + participant.getCNAME());
    }
 else {
      if (logger.isDebugEnabled())       logger.debug("Received the following JMF Session event - " + event.getClass().getName() + "="+ event);
    }
  }
}
