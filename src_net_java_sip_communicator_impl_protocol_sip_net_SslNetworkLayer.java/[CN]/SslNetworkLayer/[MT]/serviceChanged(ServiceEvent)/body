{
  Object sService=SipActivator.getBundleContext().getService(event.getServiceReference());
  if (!(sService instanceof CertificateVerificationService)) {
    return;
  }
  if (event.getType() == ServiceEvent.REGISTERED)   certificateVerification=(CertificateVerificationService)sService;
 else   if (event.getType() == ServiceEvent.UNREGISTERING)   certificateVerification=null;
}
