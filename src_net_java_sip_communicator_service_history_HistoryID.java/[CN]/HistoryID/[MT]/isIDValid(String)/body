{
  boolean isValid=true;
  int pos=id.indexOf('%');
  if (pos < 0) {
    isValid=!hasSpecialChar(id);
  }
 else {
    if (pos + 1 < id.length()) {
      String start=id.substring(0,pos);
      String end=id.substring(pos + 1);
      isValid=!hasSpecialChar(start);
      if (isValid) {
        try {
          Integer.parseInt(end,16);
          isValid=true;
        }
 catch (        Exception e) {
          isValid=false;
        }
      }
    }
 else {
      isValid=false;
    }
  }
  return isValid;
}
