{
  Format format=getFormat();
  Dimension size=((VideoFormat)format).getSize();
  if ((size == null) || ((size.width == 0) && (size.height == 0)))   format=format.intersects(new VideoFormat(null,new Dimension(videoFrame.getWidth(),videoFrame.getHeight()),Format.NOT_SPECIFIED,Format.byteArray,Format.NOT_SPECIFIED));
  return format;
}
