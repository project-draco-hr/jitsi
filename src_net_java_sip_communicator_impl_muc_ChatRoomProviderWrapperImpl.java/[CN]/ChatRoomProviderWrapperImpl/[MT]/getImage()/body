{
  byte[] logoImage=null;
  ProtocolIcon protocolIcon=protocolProvider.getProtocolIcon();
  if (protocolIcon.isSizeSupported(ProtocolIcon.ICON_SIZE_64x64))   logoImage=protocolIcon.getIcon(ProtocolIcon.ICON_SIZE_64x64);
 else   if (protocolIcon.isSizeSupported(ProtocolIcon.ICON_SIZE_48x48))   logoImage=protocolIcon.getIcon(ProtocolIcon.ICON_SIZE_48x48);
  return logoImage;
}
