{
  int listSize=listModel.size();
  boolean dateAdded=false;
  if (listSize > 0) {
    for (int i=0; i < listSize; i++) {
      Date dateFromList=(Date)listModel.get(i);
      if (dateFromList.after(date)) {
        listModel.add(i,date);
        dateAdded=true;
        break;
      }
    }
    if (!dateAdded) {
      listModel.addElement(date);
    }
  }
 else {
    listModel.addElement(date);
  }
}
