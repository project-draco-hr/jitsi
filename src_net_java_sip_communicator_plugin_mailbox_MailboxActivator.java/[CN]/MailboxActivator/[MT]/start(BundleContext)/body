{
  if (logger.isInfoEnabled())   logger.info("Mailbox plug-in...[STARTING]");
  MailboxActivator.bundleContext=bundleContext;
  mailbox=new Mailbox();
  mailbox.start(bundleContext);
  bundleContext.registerService(ConfigurationForm.class.getName(),new LazyConfigurationForm("net.java.sip.communicator.plugin.mailbox.MailboxConfigurationPanel",getClass().getClassLoader(),"plugin.mailbox.PLUGIN_ICON","plugin.mailbox.MAILBOX",500),null);
  if (logger.isInfoEnabled())   logger.info("Mailbox plug-in...[STARTED]");
}
