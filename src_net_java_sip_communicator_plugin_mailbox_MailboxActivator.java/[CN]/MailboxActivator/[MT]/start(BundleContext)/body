{
  logger.info("Mailbox plug-in...[STARTING]");
  this.bundleContext=bundleContext;
  mailbox=new Mailbox();
  mailbox.start(bundleContext);
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  uiService=(UIService)bundleContext.getService(uiServiceRef);
  ConfigurationWindow configWindow=uiService.getConfigurationWindow();
  if (configWindow != null) {
    MailboxConfigurationForm mailboxForm=new MailboxConfigurationForm();
    configWindow.addConfigurationForm(mailboxForm);
  }
  logger.info("Mailbox plug-in...[STARTED]");
}
