{
  for (  String ac : availableAudioEncodings) {
    Integer pref1=encodingPreferences.get(ac);
    int pref1IntValue=(pref1 == null) ? 0 : pref1;
    if (pref1IntValue > 0)     supportedAudioEncodings.add(ac);
 else     supportedAudioEncodings.remove(ac);
  }
  for (  String ac : availableVideoEncodings) {
    Integer pref1=encodingPreferences.get(ac);
    int pref1IntValue=(pref1 == null) ? 0 : pref1;
    if (pref1IntValue > 0)     supportedVideoEncodings.add(ac);
 else     supportedVideoEncodings.remove(ac);
  }
}
