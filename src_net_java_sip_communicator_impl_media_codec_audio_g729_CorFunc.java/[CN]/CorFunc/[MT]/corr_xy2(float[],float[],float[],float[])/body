{
  int L_SUBFR=Ld8k.L_SUBFR;
  float y2y2, xny2, y1y2;
  int i;
  y2y2=0.01f;
  for (i=0; i < L_SUBFR; i++)   y2y2+=y2[i] * y2[i];
  g_coeff[2]=y2y2;
  xny2=0.01f;
  for (i=0; i < L_SUBFR; i++)   xny2+=xn[i] * y2[i];
  g_coeff[3]=-2.0f * xny2;
  y1y2=0.01f;
  for (i=0; i < L_SUBFR; i++)   y1y2+=y1[i] * y2[i];
  g_coeff[4]=2.0f * y1y2;
}
