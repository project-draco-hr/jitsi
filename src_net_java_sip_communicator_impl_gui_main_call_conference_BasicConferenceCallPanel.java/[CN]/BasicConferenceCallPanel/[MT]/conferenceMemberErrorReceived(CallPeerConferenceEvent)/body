{
  CallPeer callPeer=ev.getSourceCallPeer();
  callPeerPanels.get(callPeer).setErrorReason(GuiActivator.getResources().getI18NString("service.gui.PROBLEMS_ENCOUNTERED"));
  GuiActivator.getAlertUIService().showAlertPopup(GuiActivator.getResources().getI18NString("service.gui.ERROR_RECEIVED_FROM") + callPeer.getDisplayName(),ev.getErrorString());
}
