{
  if (contacts.contains(dictContact))   return this;
  Iterator<ContactGroup> subGroupsIter=subgroups();
  while (subGroupsIter.hasNext()) {
    ContactGroupDictImpl subgroup=(ContactGroupDictImpl)subGroupsIter.next();
    ContactGroupDictImpl parent=subgroup.findContactParent(dictContact);
    if (parent != null)     return parent;
  }
  return null;
}
