{
  ContactSourceService contactSource=sourceContact.getContactSource();
  UIContactSource sourceUI=sourceContactList.getContactSource(contactSource);
  if (sourceUI != null && (contactSource instanceof ExtendedContactSourceService) || isMatching(sourceContact)) {
    sourceContactList.addContact(sourceUI.createUIContact(sourceContact),sourceUI.getUIGroup(),false,true);
  }
 else   sourceUI.removeUIContact(sourceContact);
}
