{
  ContactPropertyChangeEvent evt=new ContactPropertyChangeEvent(source,eventID,oldValue,newValue);
  List<SubscriptionListener> listeners;
synchronized (subscriptionListeners) {
    listeners=new ArrayList<SubscriptionListener>(subscriptionListeners);
  }
  logger.debug("Dispatching a Contact Property Change Event to" + listeners.size() + " listeners. Evt="+ evt);
  for (Iterator<SubscriptionListener> listenerIt=listeners.iterator(); listenerIt.hasNext(); )   listenerIt.next().contactModified(evt);
}
