{
  callable=new Callable<Object>(){
    public Object call(){
      Object value=null;
      try {
        value=construct();
      }
 catch (      final Throwable t) {
        if (t instanceof ThreadDeath)         throw (ThreadDeath)t;
 else {
          SwingUtilities.invokeLater(new Runnable(){
            public void run(){
              catchException(t);
            }
          }
);
        }
      }
      SwingUtilities.invokeLater(new Runnable(){
        public void run(){
          finished();
        }
      }
);
      return value;
    }
  }
;
}
