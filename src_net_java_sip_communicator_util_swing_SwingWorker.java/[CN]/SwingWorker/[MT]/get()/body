{
  Future<?> future;
synchronized (this) {
    future=this.future;
  }
  Object value=null;
  if (future != null) {
    boolean interrupted=false;
    while (!future.isDone()) {
      value=null;
      try {
        value=future.get();
      }
 catch (      CancellationException ce) {
        break;
      }
catch (      ExecutionException ee) {
        break;
      }
catch (      InterruptedException ie) {
        interrupted=true;
      }
    }
    if (interrupted)     Thread.currentThread().interrupt();
  }
  return value;
}
