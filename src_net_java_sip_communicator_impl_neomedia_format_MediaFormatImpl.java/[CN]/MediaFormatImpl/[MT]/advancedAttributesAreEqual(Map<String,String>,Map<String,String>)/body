{
  if (adv == null && adv2 != null || adv != null && adv2 == null)   return false;
  if (adv == null && adv2 == null)   return true;
  if (adv.size() != adv2.size())   return false;
  for (  Map.Entry<String,String> a : adv.entrySet()) {
    String value=adv2.get(a.getKey());
    if (value == null)     return false;
 else     if (!value.equals(a.getValue()))     return false;
  }
  return true;
}
