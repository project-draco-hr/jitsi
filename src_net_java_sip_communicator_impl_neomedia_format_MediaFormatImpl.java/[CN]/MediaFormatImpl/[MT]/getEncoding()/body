{
  String encoding=getJMFEncoding();
  if (encoding != null) {
    int encodingLength=encoding.length();
    if (encodingLength > 3) {
      int rtpPos=encodingLength - 4;
      if (encoding.substring(rtpPos).equalsIgnoreCase("/rtp"))       encoding=encoding.substring(0,rtpPos);
    }
  }
  return encoding;
}
