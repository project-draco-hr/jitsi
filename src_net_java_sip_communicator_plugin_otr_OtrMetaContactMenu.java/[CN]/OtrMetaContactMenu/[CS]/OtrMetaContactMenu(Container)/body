{
  this.container=container;
  if (Container.CONTAINER_CONTACT_RIGHT_BUTTON_MENU.equals(container)) {
    Icon icon=OtrActivator.resourceService.getImage("plugin.otr.MENU_ITEM_ICON_16x16");
    if (icon != null)     this.setIcon(icon);
  }
  this.setText(OtrActivator.resourceService.getI18NString("plugin.otr.menu.TITLE"));
}
