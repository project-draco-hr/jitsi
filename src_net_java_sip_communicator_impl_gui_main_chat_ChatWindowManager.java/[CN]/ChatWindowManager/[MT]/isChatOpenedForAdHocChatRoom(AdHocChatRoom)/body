{
synchronized (syncChat) {
    for (    ChatPanel chatPanel : chatPanels) {
      ChatSession chatSession=chatPanel.getChatSession();
      Object descriptor=chatSession.getDescriptor();
      if (descriptor instanceof AdHocChatRoomWrapper) {
        AdHocChatRoomWrapper chatRoomWrapper=(AdHocChatRoomWrapper)descriptor;
        if (chatRoomWrapper.getAdHocChatRoomID().equals(adHocChatRoom.getIdentifier()) && getChat(chatSession).isShown()) {
          return true;
        }
      }
    }
    return false;
  }
}
