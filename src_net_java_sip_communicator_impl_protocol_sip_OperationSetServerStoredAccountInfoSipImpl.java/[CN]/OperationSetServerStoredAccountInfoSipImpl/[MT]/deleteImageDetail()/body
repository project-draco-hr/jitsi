{
  XCapClient xCapClient=provider.getXCapClient();
  try {
    xCapClient.deletePresContent(ProtocolProviderServiceSipImpl.PRES_CONTENT_IMAGE_NAME);
  }
 catch (  XCapException e) {
    throw new OperationFailedException("Cannot delete image detail",OperationFailedException.NETWORK_FAILURE);
  }
}
