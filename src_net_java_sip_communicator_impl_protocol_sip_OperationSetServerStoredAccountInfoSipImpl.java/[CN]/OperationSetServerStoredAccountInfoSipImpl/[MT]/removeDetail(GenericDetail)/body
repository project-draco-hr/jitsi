{
  boolean isFound=false;
  Iterator<?> iter=getAllAvailableDetails();
  while (iter.hasNext()) {
    GenericDetail item=(GenericDetail)iter.next();
    if (item.equals(detail)) {
      isFound=true;
    }
  }
  if (!isFound) {
    return false;
  }
  if (ImageDetail.class.isAssignableFrom(detail.getClass()) && isImageDetailSupported()) {
    deleteImageDetail();
    accountImage=null;
  }
  return true;
}
