{
  if (intArray == null)   return;
synchronized (intArraysSyncRoot) {
    List<int[]> intArrays;
    if ((this.intArrays == null) || ((intArrays=this.intArrays.get()) == null)) {
      intArrays=new LinkedList<int[]>();
      this.intArrays=new SoftReference<List<int[]>>(intArrays);
    }
    if (intArrays.size() != 0)     for (    int[] element : intArrays)     if (element == intArray)     return;
    intArrays.add(intArray);
  }
}
