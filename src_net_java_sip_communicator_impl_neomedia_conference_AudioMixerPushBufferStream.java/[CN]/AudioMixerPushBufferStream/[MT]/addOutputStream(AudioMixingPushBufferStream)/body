{
  if (outputStream == null)   throw new IllegalArgumentException("outputStream");
synchronized (outputStreams) {
    if (!outputStreams.contains(outputStream) && outputStreams.add(outputStream) && (outputStreams.size() == 1)) {
      boolean started=false;
      try {
        audioMixer.start(this);
        started=true;
      }
  finally {
        if (!started)         outputStreams.remove(outputStream);
      }
    }
  }
}
