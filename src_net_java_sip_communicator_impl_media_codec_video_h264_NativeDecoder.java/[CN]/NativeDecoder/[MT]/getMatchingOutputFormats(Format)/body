{
  VideoFormat ivf=(VideoFormat)in;
  if (ivf.getSize() == null)   return new Format[]{outputFormats[defaultSizeIx]};
  for (int i=0; i < outputFormats.length; i++) {
    RGBFormat f=(RGBFormat)outputFormats[i];
    if (f.getSize().equals(ivf.getSize()))     return new Format[]{f};
  }
  return null;
}
