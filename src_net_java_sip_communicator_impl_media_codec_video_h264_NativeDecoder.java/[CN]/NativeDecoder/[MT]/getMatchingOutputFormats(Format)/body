{
  VideoFormat ivf=(VideoFormat)in;
  supportedOutputFormats=new VideoFormat[supportedSizes.length];
  for (int i=0; i < supportedSizes.length; i++) {
    Dimension size=supportedSizes[i];
    supportedOutputFormats[i]=new RGBFormat(size,-1,Format.intArray,ivf.getFrameRate(),32,0xFF0000,0xFF00,0xFF,1,size.width,Format.FALSE,Format.NOT_SPECIFIED);
  }
  return supportedOutputFormats;
}
