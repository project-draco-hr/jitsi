{
  VideoFormat ivf=(VideoFormat)in;
  if (ivf.getSize() == null)   return new Format[]{supportedOutputFormats[defaultSizeIx]};
  for (int i=0; i < supportedOutputFormats.length; i++) {
    RGBFormat f=(RGBFormat)supportedOutputFormats[i];
    if (f.getSize().equals(ivf.getSize()))     return new Format[]{f};
  }
  return null;
}
