{
  callButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.CALL_CONTACT"));
  callVideoButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.VIDEO_CALL"));
  desktopSharingButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.SHARE_DESKTOP"));
  chatButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.SEND_MESSAGE"));
  addContactButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.ADD_CONTACT"));
  webButton.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.WEBPAGE"));
  ToolTipManager ttManager=ToolTipManager.sharedInstance();
  ttManager.unregisterComponent(callButton);
  ttManager.unregisterComponent(callVideoButton);
  ttManager.unregisterComponent(desktopSharingButton);
  ttManager.unregisterComponent(chatButton);
  ttManager.unregisterComponent(addContactButton);
  ttManager.unregisterComponent(webButton);
}
