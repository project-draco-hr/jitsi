{
  if (treeNode instanceof ContactNode && !(((ContactNode)treeNode).getContactDescriptor() instanceof ShowMoreContact)) {
    setBorder(BorderFactory.createEmptyBorder(TOP_BORDER,LEFT_BORDER,BOTTOM_BORDER,RIGHT_BORDER));
  }
 else {
    setBorder(BorderFactory.createEmptyBorder(0,LEFT_BORDER,0,RIGHT_BORDER));
  }
}
