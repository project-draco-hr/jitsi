{
  JingleIQ ringing=new JingleIQ();
  ringing.setTo(from);
  ringing.setFrom(to);
  ringing.setType(IQ.Type.SET);
  ringing.setSID(sid);
  ringing.setAction(JingleAction.SESSION_INFO);
  SessionInfoPacketExtension sessionInfoType=new SessionInfoPacketExtension(type);
  ringing.setSessionInfo(sessionInfoType);
  return ringing;
}
