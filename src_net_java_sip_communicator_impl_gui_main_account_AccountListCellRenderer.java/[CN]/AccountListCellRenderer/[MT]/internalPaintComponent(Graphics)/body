{
  AntialiasingManager.activateAntialiasing(g);
  Graphics2D g2=(Graphics2D)g;
  if (index % 2 > 0) {
    g2.setColor(rowColor);
    g2.fillRect(0,0,this.getWidth(),this.getHeight());
  }
  if (this.isSelected) {
    g2.setColor(Constants.SELECTED_COLOR);
    g2.fillRect(0,0,this.getWidth(),this.getHeight());
  }
}
