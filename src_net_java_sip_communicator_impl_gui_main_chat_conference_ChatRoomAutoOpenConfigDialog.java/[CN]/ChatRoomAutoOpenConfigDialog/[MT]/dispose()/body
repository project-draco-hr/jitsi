{
  GuiActivator.getConfigurationService().removePropertyChangeListener(MessageHistoryService.PNAME_IS_MESSAGE_HISTORY_ENABLED,propertyListener);
  GuiActivator.getConfigurationService().removePropertyChangeListener(MessageHistoryService.PNAME_IS_MESSAGE_HISTORY_PER_CONTACT_ENABLED_PREFIX + "." + chatRoomId,propertyListener);
  super.dispose();
}
