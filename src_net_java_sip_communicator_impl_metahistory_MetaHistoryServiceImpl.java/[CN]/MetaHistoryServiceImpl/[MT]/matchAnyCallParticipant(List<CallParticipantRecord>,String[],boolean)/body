{
  Iterator<CallParticipantRecord> iter=cps.iterator();
  while (iter.hasNext()) {
    CallParticipantRecord callParticipant=iter.next();
    for (int i=0; i < keywords.length; i++) {
      String k=keywords[i];
      if (caseSensitive && callParticipant.getParticipantAddress().contains(k))       return true;
 else       if (callParticipant.getParticipantAddress().toLowerCase().contains(k.toLowerCase()))       return true;
    }
  }
  return false;
}
