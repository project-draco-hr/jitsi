{
  this.irc.deleteListener(this);
  IrcConnection.this.joined.remove(this.chatroom.getIdentifier());
  LOGGER.debug("Leaving chat room " + this.chatroom.getIdentifier() + ". Chat room listener removed.");
  IrcConnection.this.provider.getMUC().fireLocalUserPresenceEvent(this.chatroom,LocalUserChatRoomPresenceChangeEvent.LOCAL_USER_LEFT,null);
}
