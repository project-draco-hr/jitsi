{
  int L_WINDOW=Ld8k.L_WINDOW;
  float[] hamwindow=TabLd8k.hamwindow;
  float[] y=new float[L_WINDOW];
  float sum;
  int i, j;
  for (i=0; i < L_WINDOW; i++)   y[i]=x[x_offset + i] * hamwindow[i];
  for (i=0; i <= m; i++) {
    sum=0.0f;
    for (j=0; j < L_WINDOW - i; j++)     sum+=y[j] * y[j + i];
    r[i]=sum;
  }
  if (r[0] < 1.0f)   r[0]=1.0f;
}
