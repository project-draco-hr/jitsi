{
  while (started) {
synchronized (this) {
      while (transferHandler == null && started) {
        try {
          wait(1000);
        }
 catch (        InterruptedException ie) {
        }
      }
    }
    if (started && transferHandler != null) {
      transferHandler.transferData(this);
      try {
        Thread.currentThread().sleep(1);
      }
 catch (      InterruptedException ise) {
      }
    }
  }
}
