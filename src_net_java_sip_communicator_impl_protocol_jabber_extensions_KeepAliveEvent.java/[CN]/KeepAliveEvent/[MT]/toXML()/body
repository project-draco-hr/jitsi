{
  StringBuffer buf=new StringBuffer();
  buf.append("<").append(getElementName()).append(" xmlns=\"").append(getNamespace()).append("\">");
  buf.append("<").append(SOURCE_PROVIDER_HASH).append(">").append(getSrcProviderHash()).append("</").append(SOURCE_PROVIDER_HASH).append(">");
  buf.append("<").append(SOURCE_OPSET_HASH).append(">").append(getSrcOpSetHash()).append("</").append(SOURCE_OPSET_HASH).append(">");
  buf.append("</").append(getElementName()).append(">");
  return buf.toString();
}
