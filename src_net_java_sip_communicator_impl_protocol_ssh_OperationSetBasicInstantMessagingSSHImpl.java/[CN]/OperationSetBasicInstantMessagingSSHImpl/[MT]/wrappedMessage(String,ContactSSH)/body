{
  if (message.startsWith("/upload")) {
    int firstSpace=message.indexOf(' ');
    sshContact.getFileTransferOperationSet().sendFile(sshContact,null,message.substring(message.indexOf(' ',firstSpace + 1) + 1),message.substring(firstSpace + 1,message.indexOf(' ',firstSpace + 1)));
    return true;
  }
 else   if (message.startsWith("/download")) {
    int firstSpace=message.indexOf(' ');
    sshContact.getFileTransferOperationSet().sendFile(null,sshContact,message.substring(firstSpace + 1,message.indexOf(' ',firstSpace + 1)),message.substring(message.indexOf(' ',firstSpace + 1) + 1));
    return true;
  }
  return false;
}
