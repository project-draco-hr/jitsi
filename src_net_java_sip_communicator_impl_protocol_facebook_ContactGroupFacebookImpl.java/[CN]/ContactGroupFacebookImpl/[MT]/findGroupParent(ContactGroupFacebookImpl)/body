{
  if (subGroups.contains(facebookGroup))   return this;
  Iterator<ContactGroup> subGroupsIter=subgroups();
  while (subGroupsIter.hasNext()) {
    ContactGroupFacebookImpl subgroup=(ContactGroupFacebookImpl)subGroupsIter.next();
    ContactGroupFacebookImpl parent=subgroup.findGroupParent(facebookGroup);
    if (parent != null)     return parent;
  }
  return null;
}
