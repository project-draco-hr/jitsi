{
  int i, dataSize4;
  double result;
  result=0.0f;
  dataSize4=dataSize & 0xFFFC;
  for (i=0; i < dataSize4; i+=4) {
    result+=data1[data1_offset + i + 0] * data2[data2_offset + i + 0] + data1[data1_offset + i + 1] * data2[data2_offset + i + 1] + data1[data1_offset + i + 2] * data2[data2_offset + i + 2] + data1[data1_offset + i + 3] * data2[data2_offset + i + 3];
  }
  for (; i < dataSize; i++) {
    result+=data1[data1_offset + i] * data2[data2_offset + i];
  }
  return result;
}
