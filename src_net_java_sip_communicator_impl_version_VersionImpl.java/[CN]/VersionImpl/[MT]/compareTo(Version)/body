{
  if (version == null)   return -1;
  if (getVersionMajor() != version.getVersionMajor())   return getVersionMajor() - version.getVersionMajor();
  if (getVersionMinor() != version.getVersionMinor())   return getVersionMinor() - version.getVersionMinor();
  return getNightlyBuildID().compareTo(version.getNightlyBuildID());
}
