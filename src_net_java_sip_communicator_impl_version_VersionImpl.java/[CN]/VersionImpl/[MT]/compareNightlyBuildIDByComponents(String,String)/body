{
  String[] s1=v1.split("\\.");
  String[] s2=v2.split("\\.");
  int len=Math.max(s1.length,s2.length);
  for (int i=0; i < len; i++) {
    int n1=0;
    int n2=0;
    if (i < s1.length)     n1=Integer.valueOf(s1[i]);
    if (i < s2.length)     n2=Integer.valueOf(s2[i]);
    if (n1 == n2)     continue;
 else     return n1 - n2;
  }
  return 0;
}
