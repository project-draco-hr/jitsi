{
  try {
    Vector<Attribute> atts=description.getAttributes(false);
    for (    Attribute a : atts)     if (a.getName().equals(attributeName))     return true;
    return false;
  }
 catch (  SdpException exc) {
    if (logger.isDebugEnabled())     logger.debug("Invalid media type in a= line: " + description,exc);
    throw new IllegalArgumentException("Invalid media type in a= line: " + description,exc);
  }
}
