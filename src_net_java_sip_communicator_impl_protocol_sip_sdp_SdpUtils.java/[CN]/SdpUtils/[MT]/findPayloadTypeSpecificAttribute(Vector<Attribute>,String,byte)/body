{
  if (mediaAttributes == null || mediaAttributes.size() == 0)   return null;
  Iterator<Attribute> attIter=mediaAttributes.iterator();
  String ptStr=Byte.toString(payloadType);
  while (attIter.hasNext()) {
    Attribute attr=attIter.next();
    if (!attributeName.equals(attr.getName()))     continue;
    String attrValue=attr.getValue();
    if (attrValue == null)     continue;
    attrValue=attrValue.trim();
    if (!attrValue.startsWith(ptStr + " "))     continue;
    return attr;
  }
  return null;
}
