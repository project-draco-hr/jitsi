{
  if (info.getType() == SessionInfoType.ringing)   setState(CallPeerState.ALERTING_REMOTE_SIDE);
  if (info.getType() == SessionInfoType.hold) {
    getMediaHandler().setRemotelyOnHold(true);
  }
 else   if (info.getType() == SessionInfoType.unhold || info.getType() == SessionInfoType.active) {
    getMediaHandler().setRemotelyOnHold(true);
  }
}
