{
  List<ContentPacketExtension> offer=getMediaHandler().createContentList(true);
  if (logger.isTraceEnabled())   logger.trace("will send ringing response: ");
  this.sessionInitIQ=JinglePacketFactory.createSessionInitiate(getProtocolProvider().getOurJID(),this.peerJID,JingleIQ.generateSID(),offer);
  getProtocolProvider().getConnection().sendPacket(sessionInitIQ);
}
