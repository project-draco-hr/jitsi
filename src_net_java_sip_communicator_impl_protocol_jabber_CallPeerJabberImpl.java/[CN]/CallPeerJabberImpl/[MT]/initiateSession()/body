{
  isInitiator=false;
  List<ContentPacketExtension> offer=getMediaHandler().createContentList();
  if (logger.isTraceEnabled())   logger.trace("will send ringing response: ");
  ProtocolProviderServiceJabberImpl protocolProvider=getProtocolProvider();
  this.sessionInitIQ=JinglePacketFactory.createSessionInitiate(protocolProvider.getOurJID(),this.peerJID,JingleIQ.generateSID(),offer);
  protocolProvider.getConnection().sendPacket(sessionInitIQ);
}
