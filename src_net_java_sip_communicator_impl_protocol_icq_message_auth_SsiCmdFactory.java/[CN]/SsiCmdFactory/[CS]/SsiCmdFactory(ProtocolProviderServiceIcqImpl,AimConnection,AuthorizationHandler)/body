{
  this.icqProvider=icqProvider;
  this.authorizationHandler=authorizationHandler;
  this.aimConnection=aimConnection;
  List types=super.getSupportedTypes();
  ArrayList tempTypes=new ArrayList(types);
  tempTypes.add(new CmdType(SsiCommand.FAMILY_SSI,AbstractAuthCommand.CMD_AUTH_REPLY_RECV));
  tempTypes.add(new CmdType(SsiCommand.FAMILY_SSI,AbstractAuthCommand.CMD_AUTH_REQUEST_RECV));
  this.SUPPORTED_TYPES=DefensiveTools.getUnmodifiable(tempTypes);
  this.operationSetPresence=(OperationSetPersistentPresenceIcqImpl)icqProvider.getSupportedOperationSets().get(OperationSetPresence.class.getName());
}
