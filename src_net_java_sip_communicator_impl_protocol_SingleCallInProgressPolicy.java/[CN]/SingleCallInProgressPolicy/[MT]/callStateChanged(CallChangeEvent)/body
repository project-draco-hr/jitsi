{
  Call call=callChangeEvent.getSourceCall();
  if (CallState.CALL_IN_PROGRESS.equals(call.getCallState()) && CallState.CALL_INITIALIZATION.equals(callChangeEvent.getOldValue())) {
synchronized (calls) {
      for (Iterator<Call> callIter=calls.iterator(); callIter.hasNext(); ) {
        Call otherCall=callIter.next();
        if (!call.equals(otherCall) && CallState.CALL_IN_PROGRESS.equals(otherCall.getCallState())) {
          putOnHold(otherCall);
        }
      }
    }
  }
}
