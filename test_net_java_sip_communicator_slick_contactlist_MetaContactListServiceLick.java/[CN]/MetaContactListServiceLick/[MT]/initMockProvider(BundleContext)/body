{
  MockProvider provider=new MockProvider("SlickMockUser");
  fillMockContactList(provider);
  System.setProperty(MetaContactListService.PROVIDER_MASK_PROPERTY,"1");
  Hashtable mockProvProperties=new Hashtable();
  mockProvProperties.put(MetaContactListService.PROVIDER_MASK_PROPERTY,"1");
  mockProviderRegistration=context.registerService(ProtocolProviderService.class.getName(),provider,mockProvProperties);
  logger.debug("Registerd a mock protocol provider!");
  MclSlickFixture.mockProvider=provider;
}
