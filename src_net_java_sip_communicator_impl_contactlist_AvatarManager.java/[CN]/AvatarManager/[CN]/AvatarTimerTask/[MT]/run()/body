{
  Iterator<MetaContact> unresolvedIter=unresolvedAvatarList.iterator();
  if (unresolvedIter.hasNext()) {
    MetaContact metaContact=unresolvedIter.next();
    metaContact.getAvatar();
    logger.info("Avatar resolved for contact: " + metaContact.getDisplayName());
    unresolvedAvatarList.remove(metaContact);
  }
 else {
    this.cancel();
  }
}
