{
  LinkedList result=new LinkedList();
  Iterator iter=contact.getContacts();
  while (iter.hasNext()) {
    Contact item=(Contact)iter.next();
    try {
      History history=this.getHistory(null,item);
      Iterator recs=history.getReader().findByKeywords(keywords,SEARCH_FIELD);
      while (recs.hasNext()) {
        result.add(convertHistoryRecordToMessageEvent((HistoryRecord)recs.next(),item));
      }
    }
 catch (    IOException e) {
      logger.error("Could not read history",e);
    }
  }
  return result;
}
