{
  if (!this.historyService.isHistoryCreated(historyID))   return false;
  History history;
  if (this.historyService.isHistoryExisting(historyID)) {
    history=this.historyService.getHistory(historyID);
  }
 else {
    history=this.historyService.createHistory(historyID,recordStructure);
  }
  return history.getReader().findLast(1,keywords,field,caseSensitive).hasNext();
}
