{
  Map operationSets=protocolProvider.getSupportedOperationSets();
  String key=OperationSetBasicInstantMessaging.class.getName();
  if (operationSets.containsKey(key)) {
    OperationSetBasicInstantMessaging basicIntantMessaging=(OperationSetBasicInstantMessaging)operationSets.get(key);
    basicIntantMessaging.addMessageListener(this);
    logger.debug("New protocol provider service implementing the " + "OperationSetBasicInstantMessaging registered: " + protocolProvider.getProtocolName() + ". Listening for messages.");
  }
}
