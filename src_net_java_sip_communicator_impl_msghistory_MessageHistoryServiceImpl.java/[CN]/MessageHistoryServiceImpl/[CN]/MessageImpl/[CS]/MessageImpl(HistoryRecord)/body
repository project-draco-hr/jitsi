{
  for (int i=0; i < hr.getPropertyNames().length; i++) {
    String propName=hr.getPropertyNames()[i];
    if (propName.equals("msg") || propName.equals("msg_CDATA"))     textContent=hr.getPropertyValues()[i];
 else     if (propName.equals("msgTyp"))     contentType=hr.getPropertyValues()[i];
 else     if (propName.equals("enc"))     contentEncoding=hr.getPropertyValues()[i];
 else     if (propName.equals("uid"))     messageUID=hr.getPropertyValues()[i];
 else     if (propName.equals("sub"))     subject=hr.getPropertyValues()[i];
 else     if (propName.equals("dir"))     if (hr.getPropertyValues()[i].equals("in"))     isOutgoing=false;
 else     if (hr.getPropertyValues()[i].equals("out"))     isOutgoing=true;
 else     if (propName.equals("receivedTimestamp"))     messageReceivedDate=new Date(Long.parseLong(hr.getPropertyValues()[i]));
  }
}
