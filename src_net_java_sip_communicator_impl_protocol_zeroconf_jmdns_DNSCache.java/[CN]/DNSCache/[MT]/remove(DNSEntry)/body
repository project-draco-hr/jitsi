{
  CacheNode node=hashtable.get(entry.getName());
  if (node != null) {
    if (node.value == entry) {
      if (node.next == null) {
        hashtable.remove(entry.getName());
      }
 else {
        hashtable.put(entry.getName(),node.next);
      }
      size--;
      return true;
    }
    CacheNode previous=node;
    node=node.next;
    while (node != null) {
      if (node.value == entry) {
        previous.next=node.next;
        size--;
        return true;
      }
      previous=node;
      node=node.next;
    }
    ;
  }
  return false;
}
