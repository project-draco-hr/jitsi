{
  bundleContext=bc;
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  UIService uiService=(UIService)bundleContext.getService(uiServiceRef);
  AccountRegistrationWizardContainer wizardContainer=uiService.getAccountRegWizardContainer();
  YahooAccountRegistrationWizard yahooWizard=new YahooAccountRegistrationWizard(wizardContainer);
  wizardContainer.addAccountRegistrationWizard(yahooWizard);
}
