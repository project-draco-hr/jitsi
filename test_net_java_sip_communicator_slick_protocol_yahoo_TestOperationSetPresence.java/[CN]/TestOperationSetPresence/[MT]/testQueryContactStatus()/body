{
  logger.debug("Will Query an BRB contact.");
  subtestQueryContactStatus(YahooStatusEnum.BE_RIGHT_BACK,YahooStatusEnum.BE_RIGHT_BACK);
  logger.debug("Will Query a Busy contact.");
  subtestQueryContactStatus(YahooStatusEnum.BUSY,YahooStatusEnum.BUSY);
  logger.debug("Will Query a Idle contact.");
  subtestQueryContactStatus(YahooStatusEnum.IDLE,YahooStatusEnum.IDLE);
  logger.debug("Will Query an Invisible contact.");
  subtestQueryContactStatus(YahooStatusEnum.INVISIBLE,YahooStatusEnum.OFFLINE);
  logger.debug("Will Query an Online contact.");
  subtestQueryContactStatus(YahooStatusEnum.AVAILABLE,YahooStatusEnum.AVAILABLE);
}
