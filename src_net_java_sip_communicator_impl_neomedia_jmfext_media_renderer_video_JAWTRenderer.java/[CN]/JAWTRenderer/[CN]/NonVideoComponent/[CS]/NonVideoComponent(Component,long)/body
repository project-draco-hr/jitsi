{
  this.component=component;
  try {
    handle=JAWTRenderer.open(this.component);
    if (handle != 0) {
      JAWTRenderer.addNotifyLightweightComponent(handle,this.component,parentHandle);
    }
  }
 catch (  ResourceUnavailableException rue) {
    logger.error("Failed to JAWTRenderer.open for NonVideoComponent",rue);
  }
}
