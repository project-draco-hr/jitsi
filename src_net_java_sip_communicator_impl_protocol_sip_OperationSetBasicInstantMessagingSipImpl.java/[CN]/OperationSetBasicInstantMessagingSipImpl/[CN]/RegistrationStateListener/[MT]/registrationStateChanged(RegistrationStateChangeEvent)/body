{
  logger.debug("The provider changed state from: " + evt.getOldState() + " to: "+ evt.getNewState());
  if (evt.getNewState() == RegistrationState.REGISTERED) {
    opSetPersPresence=(OperationSetPersistentPresenceSipImpl)sipProvider.getSupportedOperationSets().get(OperationSetPersistentPresence.class.getName());
  }
}
