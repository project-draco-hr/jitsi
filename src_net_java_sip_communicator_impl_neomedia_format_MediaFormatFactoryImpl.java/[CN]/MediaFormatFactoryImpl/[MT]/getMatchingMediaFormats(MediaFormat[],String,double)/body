{
  List<MediaFormat> supportedMediaFormats=new ArrayList<MediaFormat>();
  for (  MediaFormat mediaFormat : mediaFormats)   if (mediaFormat.getEncoding().equalsIgnoreCase(encoding) && ((CLOCK_RATE_NOT_SPECIFIED == clockRate) || (mediaFormat.getClockRate() == clockRate)))   supportedMediaFormats.add(mediaFormat);
  return supportedMediaFormats;
}
