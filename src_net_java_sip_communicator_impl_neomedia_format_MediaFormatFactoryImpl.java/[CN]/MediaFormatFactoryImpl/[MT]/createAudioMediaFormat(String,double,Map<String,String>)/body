{
  for (  MediaFormat format : MediaUtils.encodingToMediaFormats(encoding))   if ((format instanceof AudioMediaFormat) && (format.getClockRate() == clockRate) && MediaFormatImpl.formatParametersAreEqual(format.getFormatParameters(),formatParams))   return (AudioMediaFormat)format;
  return new AudioMediaFormatImpl(encodingToJmfEncoding(encoding),clockRate,formatParams);
}
