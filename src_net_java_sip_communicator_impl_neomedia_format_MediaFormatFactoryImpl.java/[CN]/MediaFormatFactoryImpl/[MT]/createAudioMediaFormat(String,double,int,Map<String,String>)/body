{
  for (  MediaFormat format : MediaUtils.getMediaFormats(encoding))   if ((format instanceof AudioMediaFormat) && (format.getClockRate() == clockRate)) {
    AudioMediaFormat audioFormat=(AudioMediaFormat)format;
    if ((audioFormat.getChannels() == channels) && MediaFormatImpl.formatParametersAreEqual(format.getFormatParameters(),formatParams))     return audioFormat;
  }
  return new AudioMediaFormatImpl(encodingToJmfEncoding(encoding),clockRate,channels,formatParams);
}
