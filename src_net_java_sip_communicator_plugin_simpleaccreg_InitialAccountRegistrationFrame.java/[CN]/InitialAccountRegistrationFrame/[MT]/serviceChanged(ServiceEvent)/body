{
  Object sService=SimpleAccountRegistrationActivator.bundleContext.getService(event.getServiceReference());
  if (!(sService instanceof AccountRegistrationWizard))   return;
  AccountRegistrationWizard wizard=(AccountRegistrationWizard)sService;
  if (event.getType() == ServiceEvent.REGISTERED) {
    this.addAccountRegistrationForm(wizard);
  }
}
