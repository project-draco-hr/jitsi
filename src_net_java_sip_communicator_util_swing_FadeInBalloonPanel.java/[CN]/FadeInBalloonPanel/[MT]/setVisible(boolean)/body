{
  FadeTracker fadeTracker=FadeTracker.getInstance();
  if (isVisible) {
    fadeTracker.trackFadeIn(FadeKind.ROLLOVER,FadeInBalloonPanel.this,true,new PanelRepaintCallback());
  }
 else {
    fadeTracker.trackFadeOut(FadeKind.ROLLOVER,FadeInBalloonPanel.this,true,new PanelRepaintCallback());
  }
}
