{
  LocalUserSoundLevelEvent soundLevelEvent=new LocalUserSoundLevelEvent(this,level);
  List<LocalUserSoundLevelListener> listeners;
synchronized (soundLevelListeners) {
    listeners=new ArrayList<LocalUserSoundLevelListener>(soundLevelListeners);
  }
  for (Iterator<LocalUserSoundLevelListener> listenerIter=listeners.iterator(); listenerIter.hasNext(); ) {
    LocalUserSoundLevelListener listener=listenerIter.next();
    listener.localUserSoundLevelChanged(soundLevelEvent);
  }
}
