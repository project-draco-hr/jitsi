{
  Iterable<LocalUserSoundLevelListener> listeners;
synchronized (soundLevelListeners) {
    if (soundLevelListeners.isEmpty())     return;
    listeners=new ArrayList<LocalUserSoundLevelListener>(soundLevelListeners);
  }
  LocalUserSoundLevelEvent soundLevelEvent=new LocalUserSoundLevelEvent(this,level);
  for (  LocalUserSoundLevelListener listener : listeners)   listener.localUserSoundLevelChanged(soundLevelEvent);
}
