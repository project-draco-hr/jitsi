{
  this(call);
  if (isFullScreenMode) {
    this.setBgImage(ImageLoader.getImage(ImageLoader.HOLD_BUTTON_FULL_SCREEN));
    this.setBgRolloverImage(ImageLoader.getImage(ImageLoader.HOLD_BUTTON_FULL_SCREEN));
    this.setPressedImage(ImageLoader.getImage(ImageLoader.HOLD_BUTTON_FULL_SCREEN));
    this.setIconImage(null);
  }
}
