{
  Map<String,Boolean> encryptionProtocolStatus=this.getEncryptionProtocolStatus();
  Iterator<String> encryptionProtocolStatusIterator=encryptionProtocolStatus.keySet().iterator();
  String encryptionProtocol;
  while (encryptionProtocolStatusIterator.hasNext()) {
    encryptionProtocol=encryptionProtocolStatusIterator.next();
    properties.put(ProtocolProviderFactory.ENCRYPTION_PROTOCOL_STATUS + "." + encryptionProtocol,encryptionProtocolStatus.get(encryptionProtocol).toString());
  }
}
