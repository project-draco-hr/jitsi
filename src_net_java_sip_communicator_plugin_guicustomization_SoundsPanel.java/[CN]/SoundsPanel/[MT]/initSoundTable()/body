{
  Iterator soundKeys=GuiCustomizationActivator.getResources().getCurrentSounds();
  int rowHeight=40;
  while (soundKeys.hasNext()) {
    String key=(String)soundKeys.next();
    URL soundURL=GuiCustomizationActivator.getResources().getSoundURL(key);
    PlaySoundButton playSoundButton=new PlaySoundButton(soundURL);
    playSoundButton.setAction(new PlaySoundAction());
    JButton fileChooserButton=new JButton();
    fileChooserButton.setAction(new ChangeSoundAction(playSoundButton));
    soundsTableModel.addRow(new Object[]{key,playSoundButton,fileChooserButton});
    playSoundButton.setText("Play!");
    fileChooserButton.setText("Choose sound");
    soundsTable.setRowHeight(soundsTableModel.getRowCount() - 1,rowHeight);
  }
}
