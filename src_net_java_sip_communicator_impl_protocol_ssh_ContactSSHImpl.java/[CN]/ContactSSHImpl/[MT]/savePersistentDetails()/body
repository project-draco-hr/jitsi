{
  persistentData=this.sshConfigurationForm.getHostName() + detailsSeperator + this.sshConfigurationForm.getUserName()+ detailsSeperator+ new String(Base64.encode(this.sshConfigurationForm.getPassword().getBytes()))+ detailsSeperator+ sshConfigurationForm.getPort()+ detailsSeperator+ sshConfigurationForm.getTerminalType()+ detailsSeperator+ sshConfigurationForm.getUpdateInterval();
}
