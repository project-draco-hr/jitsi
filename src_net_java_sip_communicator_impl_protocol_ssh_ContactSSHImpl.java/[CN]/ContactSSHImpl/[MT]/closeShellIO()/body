{
  try {
    shellReader.close();
    shellInputStream.close();
  }
 catch (  IOException ex) {
  }
  try {
    shellWriter.close();
    shellOutputStream.close();
  }
 catch (  IOException ex) {
  }
  shellInputStream=null;
  shellReader=null;
  shellOutputStream=null;
  shellWriter=null;
  try {
    sshShellChannel.disconnect();
  }
 catch (  Exception e) {
  }
  sshShellChannel=null;
  if (!sshSession.isConnected()) {
    sshSession=null;
    jsch=null;
  }
  ((OperationSetPersistentPresenceSSHImpl)getParentPresenceOperationSet()).changeContactPresenceStatus(this,SSHStatusEnum.ONLINE);
}
