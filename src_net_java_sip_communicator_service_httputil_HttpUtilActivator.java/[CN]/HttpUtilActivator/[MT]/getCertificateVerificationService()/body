{
  if (guiCertificateVerification == null) {
    ServiceReference guiVerifyReference=bundleContext.getServiceReference(CertificateVerificationService.class.getName());
    if (guiVerifyReference != null)     guiCertificateVerification=(CertificateVerificationService)bundleContext.getService(guiVerifyReference);
  }
  return guiCertificateVerification;
}
