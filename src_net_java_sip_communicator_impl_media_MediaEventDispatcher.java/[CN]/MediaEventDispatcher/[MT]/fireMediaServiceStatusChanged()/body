{
  MediaListener[] targets=null;
synchronized (this) {
    if (mediaListeners != null) {
      targets=mediaListeners.toArray(new MediaListener[mediaListeners.size()]);
    }
  }
  if (targets != null) {
    for (int i=0; i < targets.length; i++) {
      targets[i].mediaServiceStatusChanged();
    }
  }
}
