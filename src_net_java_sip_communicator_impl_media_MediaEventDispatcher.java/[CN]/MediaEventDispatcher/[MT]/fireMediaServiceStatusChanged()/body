{
  Vector targets=null;
synchronized (this) {
    if (mediaListeners != null) {
      targets=(Vector)mediaListeners.clone();
    }
  }
  if (targets != null) {
    for (int i=0; i < targets.size(); i++) {
      MediaListener target=(MediaListener)targets.elementAt(i);
      target.mediaServiceStatusChanged();
    }
  }
}
