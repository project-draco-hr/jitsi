{
  if (subGroups.contains(rssGroup))   return this;
  Iterator subGroupsIter=subgroups();
  while (subGroupsIter.hasNext()) {
    ContactGroupRssImpl subgroup=(ContactGroupRssImpl)subGroupsIter.next();
    ContactGroupRssImpl parent=subgroup.findGroupParent(rssGroup);
    if (parent != null)     return parent;
  }
  return null;
}
