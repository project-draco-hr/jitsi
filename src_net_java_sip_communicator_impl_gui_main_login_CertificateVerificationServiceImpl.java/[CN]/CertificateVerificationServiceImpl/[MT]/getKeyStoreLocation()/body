{
  String keyStoreFile=GuiActivator.getConfigurationService().getString(KEYSTORE_FILE_PROP);
  if (keyStoreFile == null || keyStoreFile.length() == 0)   return GuiActivator.getFileAccessService().getPrivatePersistentFile(KEY_STORE_FILE_NAME);
  File f=new File(keyStoreFile);
  if (f.exists())   return f;
 else {
    GuiActivator.getConfigurationService().removeProperty(KEYSTORE_FILE_PROP);
    return GuiActivator.getFileAccessService().getPrivatePersistentFile(KEY_STORE_FILE_NAME);
  }
}
