{
  super(GuiActivator.getUIService().getMainFrame(),false);
  setTitle(GuiActivator.getResources().getI18NString("service.gui.CERT_DIALOG_TITLE"));
  setModal(true);
  this.cert=certs[0];
  this.host=host;
