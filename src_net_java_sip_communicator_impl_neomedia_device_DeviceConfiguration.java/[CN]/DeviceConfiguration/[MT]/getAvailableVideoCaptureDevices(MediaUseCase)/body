{
  Format[] formats=new Format[]{new AVFrameFormat(),new VideoFormat(VideoFormat.RGB),new VideoFormat(VideoFormat.YUV)};
  Set<CaptureDeviceInfo> videoCaptureDevices=new HashSet<CaptureDeviceInfo>();
  for (  Format format : formats) {
    Vector<CaptureDeviceInfo> captureDeviceInfos=CaptureDeviceManager.getDeviceList(format);
    videoCaptureDevices.addAll(captureDeviceInfos);
  }
  return videoCaptureDevices.toArray(NO_CAPTURE_DEVICES);
}
