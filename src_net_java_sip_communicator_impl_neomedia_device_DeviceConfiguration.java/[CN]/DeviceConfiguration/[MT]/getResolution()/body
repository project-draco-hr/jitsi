{
  if (resolution == null) {
    int height=NeomediaActivator.getConfigurationService().getInt(PROP_VIDEO_RESOLUTION_HEIGHT,DEFAULT_VIDEO_HEIGHT);
    int width=NeomediaActivator.getConfigurationService().getInt(PROP_VIDEO_RESOLUTION_WIDTH,DEFAULT_VIDEO_WIDTH);
    resolution=new Dimension(width,height);
  }
  return resolution;
}
