{
  this.videoMaxBandwidth=videoMaxBandwidth;
  if (videoMaxBandwidth != DEFAULT_VIDEO_MAX_BANDWIDTH) {
    NeomediaActivator.getConfigurationService().setProperty(PROP_VIDEO_MAX_BANDWIDTH,videoMaxBandwidth);
  }
 else {
    NeomediaActivator.getConfigurationService().removeProperty(PROP_VIDEO_MAX_BANDWIDTH);
  }
}
