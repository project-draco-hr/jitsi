{
  if (evt.getPropertyName().equals(PROP_VIDEO_HEIGHT) || evt.getPropertyName().equals(PROP_VIDEO_WIDTH)) {
    videoSize=null;
  }
 else   if (evt.getPropertyName().equals(PROP_VIDEO_FRAMERATE)) {
    frameRate=-1;
  }
 else   if (evt.getPropertyName().equals(PROP_VIDEO_MAX_BANDWIDTH)) {
    videoMaxBandwidth=-1;
  }
}
