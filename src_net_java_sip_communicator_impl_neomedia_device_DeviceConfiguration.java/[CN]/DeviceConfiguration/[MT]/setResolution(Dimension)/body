{
  if (resolution.getWidth() != DEFAULT_VIDEO_WIDTH && resolution.getHeight() != DEFAULT_VIDEO_HEIGHT) {
    NeomediaActivator.getConfigurationService().setProperty(PROP_VIDEO_RESOLUTION_WIDTH,resolution.width);
    NeomediaActivator.getConfigurationService().setProperty(PROP_VIDEO_RESOLUTION_HEIGHT,resolution.height);
  }
 else {
    NeomediaActivator.getConfigurationService().removeProperty(PROP_VIDEO_RESOLUTION_HEIGHT);
    NeomediaActivator.getConfigurationService().removeProperty(PROP_VIDEO_RESOLUTION_WIDTH);
  }
  this.resolution=resolution;
  firePropertyChange(VIDEO_CAPTURE_DEVICE,videoCaptureDevice,videoCaptureDevice);
}
