{
  if (evt.isTyping()) {
    String typingUserID=evt.getFrom();
    if (typingUserID != null) {
      Contact sourceContact=opSetPersPresence.findContactByID(typingUserID);
      if (sourceContact == null)       return;
      if (evt.getMode() == 1)       fireTypingNotificationsEvent(sourceContact,STATE_TYPING);
 else       fireTypingNotificationsEvent(sourceContact,STATE_STOPPED);
    }
  }
}
