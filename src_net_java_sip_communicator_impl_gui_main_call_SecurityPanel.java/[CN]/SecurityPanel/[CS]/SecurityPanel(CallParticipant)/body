{
  this.participant=participant;
  this.setLayout(new GridLayout(1,0,5,5));
  this.setBorder(BorderFactory.createTitledBorder("Compare with partner"));
  iconEncrVerified=ImageLoader.getImage(ImageLoader.ENCR_VERIFIED);
  iconEncr=ImageLoader.getImage(ImageLoader.ENCR);
  notificationService=GuiActivator.getNotificationService();
  if (notificationService != null) {
    notificationService.registerDefaultNotificationForEvent(ZRTP_SECURE_NOTIFICATION,NotificationService.ACTION_SOUND,SoundProperties.ZRTP_SECURE,null);
    notificationService.registerDefaultNotificationForEvent(ZRTP_ALERT_NOTIFICATION,NotificationService.ACTION_SOUND,SoundProperties.ZRTP_ALERT,null);
  }
  this.addComponentsToPane();
}
