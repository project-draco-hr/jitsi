{
  this.messageID=messageID;
  this.chatString=chatString;
  this.contentType=contentType;
  ConfigurationService cfg=GuiActivator.getConfigurationService();
  isEnabled=cfg.getBoolean(ReplacementProperty.REPLACEMENT_ENABLE,true);
  isProposalEnabled=cfg.getBoolean(ReplacementProperty.REPLACEMENT_PROPOSAL,true);
}
