{
  String osgiFilter="(" + ConfigurationForm.FORM_TYPE + "="+ ConfigurationForm.GENERAL_TYPE+ ")";
  ServiceReference[] confFormsRefs=null;
  try {
    confFormsRefs=GuiActivator.bundleContext.getServiceReferences(ConfigurationForm.class.getName(),osgiFilter);
  }
 catch (  InvalidSyntaxException ex) {
  }
  String chatConfigFormClassName="net.java.sip.communicator.plugin.chatconfig.ChatConfigPanel";
  if (confFormsRefs != null) {
    for (int i=0; i < confFormsRefs.length; i++) {
      ConfigurationForm form=(ConfigurationForm)GuiActivator.bundleContext.getService(confFormsRefs[i]);
      if (form instanceof LazyConfigurationForm) {
        LazyConfigurationForm lazyConfigForm=(LazyConfigurationForm)form;
        if (lazyConfigForm.getFormClassName().equals(chatConfigFormClassName))         return form;
      }
 else       if (form.getClass().getName().equals(chatConfigFormClassName)) {
        return form;
      }
    }
  }
  return null;
}
