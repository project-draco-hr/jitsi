{
  StringBuilder res=new StringBuilder();
  res.append("<cite id='");
  res.append(messageUID);
  res.append("-editedAt'> ");
  if (date > 0) {
    res.append("&nbsp;");
    String contents=GuiActivator.getResources().getI18NString("service.gui.EDITED_AT",new String[]{GuiUtils.formatTime(date)});
    res.append(contents);
  }
  res.append("</cite>");
  return res.toString();
}
