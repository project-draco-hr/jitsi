{
  List<String> list=GuiActivator.getUIService().getConferenceChatManager().getExistingChatRooms(chatRoomProvider);
  if (list != null) {
    if (list.size() == 0) {
      list.add(GuiActivator.getResources().getI18NString("service.gui.NO_AVAILABLE_ROOMS"));
    }
    chatRoomsList.setListData(new Vector<String>(list));
    chatRoomsList.setBorder(BorderFactory.createLineBorder(Color.LIGHT_GRAY));
    chatRoomsScrollPane.setPreferredSize(new Dimension(500,120));
    chatRoomsScrollPane.setOpaque(false);
    chatRoomsScrollPane.getViewport().add(chatRoomsList);
    this.mainPanel.add(chatRoomsScrollPane);
    if (wizardContainer != null)     wizardContainer.refresh();
    this.setCursor(Cursor.getDefaultCursor());
  }
}
