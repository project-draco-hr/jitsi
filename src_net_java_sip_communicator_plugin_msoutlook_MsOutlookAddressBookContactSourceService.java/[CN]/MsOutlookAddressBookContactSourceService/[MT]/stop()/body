{
  boolean interrupted=false;
synchronized (queries) {
    while (!queries.isEmpty()) {
      queries.get(0).cancel();
      try {
        queries.wait();
      }
 catch (      InterruptedException iex) {
        interrupted=true;
      }
    }
  }
  if (interrupted)   Thread.currentThread().interrupt();
  MAPIUninitialize();
}
