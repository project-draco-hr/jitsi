{
  dialPadPanel.removeAll();
  Image bgImage=ImageLoader.getImage(ImageLoader.DIAL_BUTTON_BG);
  for (  DTMFHandler.DTMFToneInfo info : DTMFHandler.AVAILABLE_TONES) {
    if (info.imageID == null)     continue;
    JComponent c=OSUtils.IS_MAC ? createMacOSXDialButton(info.macImageID,info.macImageRolloverID,info.tone.getValue()) : createDialButton(bgImage,info.imageID,info.tone.getValue());
    dialPadPanel.add(c);
  }
}
