{
  this.width=width;
  this.height=height;
  this.setPreferredSize(new Dimension(width,height));
  this.frameImage=ImageUtils.scaleImageWithinBounds(UtilActivator.getResources().getImage("service.gui.USER_PHOTO_FRAME").getImage(),width,height);
  if (imageIcon != null)   this.setImageIcon(imageIcon);
}
