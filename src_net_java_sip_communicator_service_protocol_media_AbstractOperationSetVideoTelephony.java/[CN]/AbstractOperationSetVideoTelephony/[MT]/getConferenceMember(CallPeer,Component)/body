{
  VideoMediaStream peerVideoStream=(VideoMediaStream)((W)peer).getMediaHandler().getStream(MediaType.VIDEO);
  if (peerVideoStream == null)   return null;
  for (  ConferenceMember member : peer.getConferenceMembers()) {
    Component memberComponent=peerVideoStream.getVisualComponent(member.getVideoSsrc());
    if (memberComponent != null && memberComponent.equals(visualComponent)) {
      return member;
    }
  }
  return null;
}
