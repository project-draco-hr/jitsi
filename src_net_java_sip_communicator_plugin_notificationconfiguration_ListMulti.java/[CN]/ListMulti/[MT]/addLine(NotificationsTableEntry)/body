{
  Object row[]=new Object[5];
  row[0]=dataNTE.getEnabled() ? new String("enable") : new String("disable");
  row[1]=(dataNTE.getProgram() && (dataNTE.getProgramFile().trim().length() > 0)) ? new String("Yes") : new String("No");
  row[2]=dataNTE.getPopup() ? new String("Yes") : new String("No");
  row[3]=(dataNTE.getSound() && (dataNTE.getSoundFile().trim().length() > 0)) ? new String("Yes") : new String("No");
  row[4]=dataNTE.getEvent();
  this.addLine(row);
}
