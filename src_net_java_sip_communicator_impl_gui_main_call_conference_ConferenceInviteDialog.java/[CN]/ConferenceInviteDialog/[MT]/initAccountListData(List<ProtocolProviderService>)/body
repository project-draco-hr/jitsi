{
  Iterator<ProtocolProviderService> providersIter=protocolProviders.iterator();
  while (providersIter.hasNext()) {
    ProtocolProviderService protocolProvider=providersIter.next();
    accountSelectorBox.addItem(protocolProvider);
  }
  if (accountSelectorBox.getItemCount() > 0)   accountSelectorBox.setSelectedIndex(0);
}
