{
  this.removeAllMetaContacts();
  MetaContactListService metaContactListService=GuiActivator.getContactListService();
  Iterator<ProtocolProviderService> protocolProviders=GuiActivator.getUIService().getMainFrame().getProtocolProviders();
  while (protocolProviders.hasNext()) {
    ProtocolProviderService protocolProvider=protocolProviders.next();
    Iterator<MetaContact> contactListIter=metaContactListService.findAllMetaContactsForProvider(protocolProvider);
    while (contactListIter.hasNext()) {
      MetaContact metaContact=contactListIter.next();
      if (!containsContact(metaContact)) {
        if (metaContact.getDefaultContact(OperationSetBasicTelephony.class) != null)         addMetaContact(metaContact);
      }
    }
  }
}
