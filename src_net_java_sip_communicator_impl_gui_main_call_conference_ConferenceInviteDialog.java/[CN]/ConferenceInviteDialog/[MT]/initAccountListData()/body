{
  Iterator<ProtocolProviderService> protocolProviders=GuiActivator.getUIService().getMainFrame().getProtocolProviders();
  while (protocolProviders.hasNext()) {
    ProtocolProviderService protocolProvider=protocolProviders.next();
    OperationSet opSet=protocolProvider.getOperationSet(OperationSetTelephonyConferencing.class);
    if (opSet != null && protocolProvider.isRegistered()) {
      accountSelectorBox.addItem(protocolProvider);
    }
  }
  ProtocolProviderService lastConfProvider=ConfigurationManager.getLastCallConferenceProvider();
  if (call != null)   accountSelectorBox.setSelectedItem(call.getProtocolProvider());
 else   if (lastConfProvider != null)   accountSelectorBox.setSelectedItem(lastConfProvider);
}
