{
  CallParticipantState oldState=getState();
  if (oldState == newState)   return;
  this.state=newState;
  if (CallParticipantState.CONNECTED.equals(newState) && !CallParticipantState.isOnHold(oldState)) {
    callDurationStartTime=System.currentTimeMillis();
  }
  fireCallParticipantChangeEvent(CallParticipantChangeEvent.CALL_PARTICIPANT_STATE_CHANGE,oldState,newState);
}
