{
  button.setBorder(null);
  if (isOver) {
switch (status) {
case OVER:
      button.setBorder(OVERBORDER);
    break;
case PRESSED:
  button.setBorder(PRESSEDBORDER);
break;
}
}
button.setBackground(tabScroller.tabPanel.getBackground());
button.paint(image.getGraphics());
g.drawImage(image,dx,dy,null);
}
