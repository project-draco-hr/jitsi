{
  int index=1;
  if (parentGroup != null) {
    if (this.listModel.contains(parentGroup)) {
      int i=this.listModel.indexOf(parentGroup);
      index=i + 1;
      this.listModel.add(index,contact);
    }
  }
 else {
    this.listModel.add(index,contact);
  }
  this.scrollRectToVisible(this.getCellBounds(index,index + 1));
}
