{
  ProtocolProviderService selectedProvider=((Account)getAccountsCombo().getSelectedItem()).getProtocolProvider();
  if (opSetClass.equals(OperationSetBasicTelephony.class)) {
    CallManager.createCall(selectedProvider,contactAddress);
  }
 else   if (opSetClass.equals(OperationSetVideoTelephony.class)) {
    CallManager.createVideoCall(selectedProvider,contactAddress);
  }
 else   if (opSetClass.equals(OperationSetDesktopSharingServer.class)) {
    CallManager.createDesktopSharing(selectedProvider,contactAddress);
  }
}
