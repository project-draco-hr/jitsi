{
  String extension=SoundFileUtils.getExtension(f);
  if (extension != null) {
    return extension.equals(SoundFileUtils.wav) || extension.equals(SoundFileUtils.mid) || extension.equals(SoundFileUtils.mp2)|| extension.equals(SoundFileUtils.mp3)|| extension.equals(SoundFileUtils.mod)|| extension.equals(SoundFileUtils.ogg)|| extension.equals(SoundFileUtils.wma)|| extension.equals(SoundFileUtils.gsm)|| extension.equals(SoundFileUtils.au)|| extension.equals(SoundFileUtils.ram);
  }
  return false;
}
