{
  String extension=SoundFileUtils.getExtension(f);
  if (extension != null) {
    return extension.equals(SoundFileUtils.mp2) || extension.equals(SoundFileUtils.wav) || extension.equals(SoundFileUtils.au)|| extension.equals(SoundFileUtils.aif)|| extension.equals(SoundFileUtils.gsm);
  }
  return false;
}
