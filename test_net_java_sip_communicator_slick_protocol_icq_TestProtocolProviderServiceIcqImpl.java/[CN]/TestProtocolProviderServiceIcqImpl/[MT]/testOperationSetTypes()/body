{
  Map supportedOperationSets=fixture.provider.getSupportedOperationSets();
  Iterator setNames=supportedOperationSets.keySet().iterator();
  while (setNames.hasNext()) {
    String setName=(String)setNames.next();
    Object opSet=supportedOperationSets.get(setName);
    assertTrue(opSet + " was not an instance of " + setName+ " as declared",Class.forName(setName).isInstance(opSet));
  }
}
