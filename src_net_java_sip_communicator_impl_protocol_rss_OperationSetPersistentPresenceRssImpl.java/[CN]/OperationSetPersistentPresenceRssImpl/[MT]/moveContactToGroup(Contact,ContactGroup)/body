{
  ContactRssImpl rssContact=(ContactRssImpl)contactToMove;
  ContactGroupRssImpl parentRssGroup=findContactParent(rssContact);
  parentRssGroup.removeContact(rssContact);
  if (!rssContact.isPersistent()) {
    fireSubscriptionEvent(rssContact,parentRssGroup,SubscriptionEvent.SUBSCRIPTION_REMOVED);
    try {
      this.subscribe(newParent,contactToMove.getAddress());
      fireSubscriptionEvent(rssContact,newParent,SubscriptionEvent.SUBSCRIPTION_CREATED);
    }
 catch (    Exception ex) {
      logger.error("Failed to move contact " + rssContact.getAddress(),ex);
    }
  }
 else {
    ((ContactGroupRssImpl)newParent).addContact(rssContact);
    fireSubscriptionMovedEvent(contactToMove,parentRssGroup,newParent);
  }
}
