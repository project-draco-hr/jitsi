{
  SubscriptionMovedEvent evt=new SubscriptionMovedEvent(source,this.parentProvider,oldParent,newParent);
  Iterator listeners=null;
synchronized (subscriptionListeners) {
    listeners=new ArrayList(subscriptionListeners).iterator();
  }
  while (listeners.hasNext()) {
    SubscriptionListener listener=(SubscriptionListener)listeners.next();
    listener.subscriptionMoved(evt);
  }
}
