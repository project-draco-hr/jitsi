{
  ContactGroupRssImpl newGroup=new ContactGroupRssImpl(ContactGroupRssImpl.createNameFromUID(groupUID),parentProvider);
  newGroup.setResolved(true);
  if (parentGroup == null)   parentGroup=getServerStoredContactListRoot();
  ((ContactGroupRssImpl)parentGroup).addSubgroup(newGroup);
  this.fireServerStoredGroupEvent(newGroup,ServerStoredGroupEvent.GROUP_CREATED_EVENT);
  return newGroup;
}
