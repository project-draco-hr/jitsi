{
  ContactGroupRssImpl rssGroup=(ContactGroupRssImpl)group;
  ContactGroupRssImpl parent=findGroupParent(rssGroup);
  if (parent == null) {
    throw new IllegalArgumentException("group " + group + " does not seem to belong to this protocol's contact list.");
  }
  parent.removeSubGroup(rssGroup);
  this.fireServerStoredGroupEvent(rssGroup,ServerStoredGroupEvent.GROUP_REMOVED_EVENT);
}
