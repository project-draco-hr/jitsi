{
  out.finish();
  if (!out.isEmpty()) {
    DatagramPacket packet=new DatagramPacket(out.data,out.off,group,DNSConstants.MDNS_PORT);
    try {
      DNSIncoming msg=new DNSIncoming(packet);
      if (logger.isTraceEnabled())       logger.trace("send() JmDNS out:" + msg.print(true));
    }
 catch (    IOException exc) {
      logger.error("send(DNSOutgoing) - JmDNS can not parse what it sends!!!",exc);
    }
    socket.send(packet);
  }
}
