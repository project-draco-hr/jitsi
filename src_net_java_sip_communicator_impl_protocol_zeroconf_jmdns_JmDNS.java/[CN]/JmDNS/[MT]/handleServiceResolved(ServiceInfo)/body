{
  List<ServiceListener> list=serviceListeners.get(info.type.toLowerCase());
  if (list != null) {
    ServiceEvent event=new ServiceEvent(this,info.type,info.getName(),info);
    List<ServiceListener> listCopy=new ArrayList<ServiceListener>(list);
    for (    ServiceListener serviceListener : listCopy)     serviceListener.serviceResolved(event);
  }
}
