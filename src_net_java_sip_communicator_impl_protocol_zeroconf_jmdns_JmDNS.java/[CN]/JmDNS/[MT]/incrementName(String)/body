{
  try {
    int l=name.lastIndexOf('(');
    int r=name.lastIndexOf(')');
    if ((l >= 0) && (l < r)) {
      name=name.substring(0,l) + "(" + (Integer.parseInt(name.substring(l + 1,r)) + 1)+ ")";
    }
 else {
      name+=" (2)";
    }
  }
 catch (  NumberFormatException e) {
    name+=" (2)";
  }
  return name;
}
