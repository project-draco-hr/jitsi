{
  if (state == DNSState.PROBING_1) {
    task=this;
  }
synchronized (JmDNS.this) {
    for (Iterator<ServiceInfo> iterator=services.values().iterator(); iterator.hasNext(); ) {
      ServiceInfo info=iterator.next();
      if (info.getState() == DNSState.PROBING_1) {
        info.task=this;
      }
    }
  }
}
