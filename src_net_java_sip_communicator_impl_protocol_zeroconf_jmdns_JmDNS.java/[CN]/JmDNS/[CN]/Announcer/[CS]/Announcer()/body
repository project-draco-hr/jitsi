{
  if (state == DNSState.ANNOUNCING_1) {
    task=this;
  }
synchronized (JmDNS.this) {
    for (Iterator s=services.values().iterator(); s.hasNext(); ) {
      ServiceInfo info=(ServiceInfo)s.next();
      if (info.getState() == DNSState.ANNOUNCING_1) {
        info.task=this;
      }
    }
  }
}
