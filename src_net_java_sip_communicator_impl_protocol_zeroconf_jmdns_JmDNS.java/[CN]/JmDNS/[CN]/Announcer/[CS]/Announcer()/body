{
  if (state == DNSState.ANNOUNCING_1) {
    task=this;
  }
synchronized (JmDNS.this) {
    for (Iterator<ServiceInfo> s=services.values().iterator(); s.hasNext(); ) {
      ServiceInfo info=s.next();
      if (info.getState() == DNSState.ANNOUNCING_1) {
        info.task=this;
      }
    }
  }
}
