{
  try {
    byte[] image=getFavIconFromSiteRoot(contact);
    if (image == null) {
      image=getDefaultRssIcon();
    }
    return image;
  }
 catch (  Exception exc) {
    if (logger.isTraceEnabled())     logger.trace("Cannot load image for contact " + this + " : "+ exc.getMessage(),exc);
    return null;
  }
}
