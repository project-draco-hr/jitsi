{
  String securityString=zrtpControl.getSecurityString();
  boolean isSecurityVerified=zrtpControl.isSecurityVerified();
  if (securityString != null) {
    securityStringLabel.setText(GuiActivator.getResources().getI18NString("service.gui.COMPARE_WITH_PARTNER_SHORT",new String[]{securityString}));
  }
 else {
    securityStringLabel.setText(null);
  }
  sasVerificationButton.setImage(isSecurityVerified ? iconEncrVerified : iconEncr);
  revalidate();
  repaint();
}
