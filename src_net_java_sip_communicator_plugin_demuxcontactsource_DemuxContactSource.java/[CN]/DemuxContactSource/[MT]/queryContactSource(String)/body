{
  if (contactSource instanceof ExtendedContactSourceService)   return new DemuxContactQuery(((ExtendedContactSourceService)contactSource).queryContactSource(Pattern.compile(Pattern.quote(queryString),Pattern.MULTILINE | Pattern.CASE_INSENSITIVE | Pattern.UNICODE_CASE)));
 else   return new DemuxContactQuery(contactSource.queryContactSource(queryString));
}
