{
  if (!iconChanged) {
    ImageIcon avatarIcon=new ImageIcon(ImageLoader.getImage(ImageLoader.DEFAULT_USER_PHOTO).getScaledInstance(AVATAR_WIDTH,AVATAR_HEIGHT,Image.SCALE_SMOOTH));
    imageLabel.setIcon(avatarIcon);
  }
  securityStatusLabel.setIcon(new ImageIcon(ImageLoader.getImage(securityImageID)));
}
