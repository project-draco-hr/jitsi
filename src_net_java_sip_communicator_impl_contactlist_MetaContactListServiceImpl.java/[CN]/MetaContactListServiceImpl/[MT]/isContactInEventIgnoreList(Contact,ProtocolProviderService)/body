{
  Enumeration<String> ignoredAddressList=contactEventIgnoreList.keys();
  while (ignoredAddressList.hasMoreElements()) {
    String contactAddress=ignoredAddressList.nextElement();
    if (contact.getAddress().equals(contactAddress) || contact.equals(contactAddress)) {
      List<ProtocolProviderService> existingProvList=this.contactEventIgnoreList.get(contactAddress);
      return existingProvList != null && existingProvList.contains(ownerProvider);
    }
  }
  return false;
}
