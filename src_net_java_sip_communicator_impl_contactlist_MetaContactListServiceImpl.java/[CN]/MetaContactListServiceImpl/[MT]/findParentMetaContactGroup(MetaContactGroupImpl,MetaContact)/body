{
  if (root.contains(child)) {
    return root;
  }
  Iterator<MetaContactGroupImpl> subgroups=root.getSubgroups();
  while (subgroups.hasNext()) {
    MetaContactGroup contactGroup=findParentMetaContactGroup(subgroups.next(),child);
    if (contactGroup != null) {
      return contactGroup;
    }
  }
  return null;
}
