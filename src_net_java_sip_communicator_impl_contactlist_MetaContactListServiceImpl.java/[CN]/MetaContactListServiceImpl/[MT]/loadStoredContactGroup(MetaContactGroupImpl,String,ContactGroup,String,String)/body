{
  ProtocolProviderService sourceProvider=(ProtocolProviderService)currentlyInstalledProviders.get(accountID);
  OperationSetPersistentPresence presenceOpSet=(OperationSetPersistentPresence)sourceProvider.getSupportedOperationSets().get(OperationSetPersistentPresence.class.getName());
  ContactGroup newProtoGroup=presenceOpSet.createUnresolvedContactGroup(contactGroupUID,persistentData,parentProtoGroup == null ? presenceOpSet.getServerStoredContactListRoot() : parentProtoGroup);
  containingMetaGroup.addProtoGroup(newProtoGroup);
  return newProtoGroup;
}
