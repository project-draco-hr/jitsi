{
  logger.trace("ContactGroup removed: " + evt);
  MetaContactGroupImpl metaContactGroup=(MetaContactGroupImpl)findMetaContactGroupByContactGroup(evt.getSrouceGroup());
  if (metaContactGroup == null) {
    logger.error("Received a RemovedGroup event for an orphan grp: " + evt.getSrouceGroup());
    return;
  }
  removeContactGroupFromMetaContactGroup(metaContactGroup,evt.getSrouceGroup(),evt.getSourceProvider());
}
