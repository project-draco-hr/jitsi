{
  logger.trace("ContactGroup created: " + evt);
  if (isGroupInEventIgnoreList(evt.getSrouceGroup().getGroupName(),evt.getSourceProvider())) {
    return;
  }
  MetaContactGroupImpl parentMetaGroup=(MetaContactGroupImpl)findMetaContactGroupByContactGroup(evt.getParentGroup());
  if (parentMetaGroup == null) {
    logger.error("Failed to identify a parent where group " + evt.getSrouceGroup().getGroupName() + "should be placed.");
  }
  MetaContactGroup newMetaGroup=handleGroupCreatedEvent(parentMetaGroup,evt.getSrouceGroup());
  fireMetaContactGroupEvent(newMetaGroup,evt.getSourceProvider(),MetaContactGroupEvent.META_CONTACT_GROUP_ADDED);
}
