{
  MetaContactImpl mc=(MetaContactImpl)findMetaContactByContact(evt.getSourceContact());
  if (evt.getPropertyName().equals(ContactPropertyChangeEvent.PROPERTY_DISPLAY_NAME) && evt.getOldValue() != null && ((String)evt.getOldValue()).equals(mc.getDisplayName())) {
    renameMetaContact(mc,(String)evt.getNewValue());
  }
}
