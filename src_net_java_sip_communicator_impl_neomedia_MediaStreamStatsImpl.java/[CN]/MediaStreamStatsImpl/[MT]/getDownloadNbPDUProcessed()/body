{
  int nbReceived=0;
  java.util.List<ReceiveStream> listReceiveStream=this.mediaStreamImpl.getDeviceSession().getReceiveStreams();
  for (int i=0; i < listReceiveStream.size(); ++i) {
    ReceiveStream receiveStream=listReceiveStream.get(i);
    nbReceived+=receiveStream.getSourceReceptionStats().getPDUProcessed();
  }
  return nbReceived;
}
