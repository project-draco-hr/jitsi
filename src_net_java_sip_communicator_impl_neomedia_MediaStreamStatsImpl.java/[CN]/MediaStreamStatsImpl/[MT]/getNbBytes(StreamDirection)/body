{
  long nbBytes=0;
  StreamRTPManager rtpManager=this.mediaStreamImpl.getRTPManager();
  if (rtpManager != null) {
switch (streamDirection) {
case DOWNLOAD:
      nbBytes=rtpManager.getGlobalReceptionStats().getBytesRecd();
    break;
case UPLOAD:
  nbBytes=rtpManager.getGlobalTransmissionStats().getBytesSent();
break;
}
}
return nbBytes;
}
