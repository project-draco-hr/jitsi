{
  this.contact=contact;
  Iterator i=contact.getContacts();
  while (i.hasNext()) {
    Contact protocolContact=(Contact)i.next();
    String protocolName=protocolContact.getProtocolProvider().getProtocolName();
    this.protocolIcons.put(protocolName,Constants.getProtocolStatusIcons(protocolName).get(Constants.OFFLINE_STATUS));
  }
}
