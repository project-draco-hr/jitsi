{
  @SuppressWarnings("unchecked") Vector<CaptureDeviceInfo> captureDeviceInfos=(Vector<CaptureDeviceInfo>)CaptureDeviceManager.getDeviceList(null);
  MediaLocator locator=captureDevice.getLocator();
  for (  CaptureDeviceInfo captureDeviceInfo : captureDeviceInfos)   if (captureDeviceInfo.getLocator().equals(locator))   return captureDeviceInfo;
  return null;
}
