{
  if (!started) {
    if (!connected) {
      throw new IOException(getClass().getSimpleName() + " not connected");
    }
    try {
      doStart();
    }
 catch (    IOException ioex) {
      logger.error("Failed to start " + getClass().getSimpleName() + " "+ getLocator(),ioex);
      throw ioex;
    }
    started=true;
  }
}
