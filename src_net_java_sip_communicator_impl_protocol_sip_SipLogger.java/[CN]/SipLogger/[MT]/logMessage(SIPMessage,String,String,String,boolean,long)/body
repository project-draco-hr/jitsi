{
  if (sender) {
    logger.trace("JAIN-SIP sent message from \"" + from + "\" to \""+ to+ "\" at "+ time+ " (status: "+ status+ "):\n"+ message);
  }
 else {
    logger.trace("JAIN-SIP received message from \"" + from + "\" to \""+ to+ "\" at "+ time+ " (status: "+ status+ "):\n"+ message);
  }
}
