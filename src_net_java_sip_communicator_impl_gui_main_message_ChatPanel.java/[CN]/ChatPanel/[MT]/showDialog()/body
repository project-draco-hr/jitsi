{
  Hashtable contactChats=chatWindow.getContactChatsTable();
  if (Constants.TABBED_CHAT_WINDOW) {
    if (!contactChats.containsValue(this))     this.chatWindow.addChatTab(this);
 else     this.chatWindow.setSelectedContactTab(getDefaultContact());
  }
 else {
    if (!contactChats.containsValue(this))     this.chatWindow.addChat(this);
  }
  this.chatWindow.setVisible(true);
}
