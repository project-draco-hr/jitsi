{
  if (Constants.TABBED_CHAT_WINDOW) {
    if (!chatWindow.containsContactChat(this))     this.chatWindow.addChatTab(this);
 else     this.chatWindow.setSelectedContactTab(getMetaContact());
  }
 else {
    if (!chatWindow.containsContactChat(this))     this.chatWindow.addChat(this);
  }
  this.chatWindow.setVisible(true);
}
