{
  JTabbedPane tabbedPane=new SIPCommTabbedPane();
  this.setBorder(BorderFactory.createEmptyBorder(10,10,10,10));
  tabbedPane.addTab(Resources.getString("service.gui.ACCOUNT"),accountPanel);
  tabbedPane.addTab(Resources.getString("service.gui.CONNECTION"),connectionPanel);
