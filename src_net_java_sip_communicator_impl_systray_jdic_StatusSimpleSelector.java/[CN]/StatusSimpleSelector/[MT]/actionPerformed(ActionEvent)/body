{
  JMenuItem menuItem=(JMenuItem)evt.getSource();
  String itemName=menuItem.getName();
  if (itemName.equals("online")) {
    if (!this.provider.isRegistered()) {
      new ProviderRegistration(parentSystray.getUiService(),provider).start();
    }
  }
 else {
    if (!this.provider.getRegistrationState().equals(RegistrationState.UNREGISTERED) && !this.provider.getRegistrationState().equals(RegistrationState.UNREGISTERING)) {
      new ProviderUnRegistration(this.provider).start();
    }
  }
}
