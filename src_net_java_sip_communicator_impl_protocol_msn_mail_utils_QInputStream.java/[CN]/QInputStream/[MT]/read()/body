{
  int c=in.read();
  if (c == UNDERSCORE)   return SPACE;
  if (c == EQ) {
    buf[0]=(byte)in.read();
    buf[1]=(byte)in.read();
    try {
      return Integer.parseInt(new String(buf,0,2),16);
    }
 catch (    NumberFormatException e) {
      throw new IOException("Quoted-Printable encoding error: " + e.getMessage());
    }
  }
  return c;
}
