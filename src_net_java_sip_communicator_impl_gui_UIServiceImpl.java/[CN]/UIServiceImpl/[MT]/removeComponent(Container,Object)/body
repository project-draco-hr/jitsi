{
  if (!supportedContainers.contains(containerID)) {
    throw new IllegalArgumentException("The constraint that you specified is not" + " supported by this UIService implementation.");
  }
 else {
    if (registeredPlugins.containsKey(containerID)) {
      ((Vector)registeredPlugins.get(containerID)).remove(component);
    }
    DefaultPluginComponent pluginComponent=new DefaultPluginComponent((Component)component,containerID);
    this.firePluginEvent(pluginComponent,PluginComponentEvent.PLUGIN_COMPONENT_REMOVED);
  }
}
