{
  if (resourcesService == null) {
    ServiceReference serviceReference=ExtendedCallHistorySearchActivator.context.getServiceReference(ResourceManagementService.class.getName());
    if (serviceReference == null)     return null;
    resourcesService=(ResourceManagementService)ExtendedCallHistorySearchActivator.context.getService(serviceReference);
  }
  return resourcesService;
}
