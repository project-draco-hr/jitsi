{
  String propertyName=event.getPropertyName();
  if (CallPeerMediaHandlerSipImpl.AUDIO_LOCAL_SSRC.equals(propertyName) || CallPeerMediaHandlerSipImpl.AUDIO_REMOTE_SSRC.equals(propertyName) || CallPeerMediaHandlerSipImpl.VIDEO_LOCAL_SSRC.equals(propertyName)|| CallPeerMediaHandlerSipImpl.VIDEO_REMOTE_SSRC.equals(propertyName)) {
    Call call=((CallPeerMediaHandlerSipImpl)event.getSource()).getPeer().getCall();
    if (call != null)     notifyAll(call);
  }
}
