{
  int calleeCount=callees.length;
  Address[] calleeAddresses=new Address[calleeCount];
  for (int i=0; i < calleeCount; i++)   calleeAddresses[i]=parseAddressString(callees[i]);
  CallSipImpl call=basicTelephony.createOutgoingCall();
  call.setConferenceFocus(true);
  for (  Address calleeAddress : calleeAddresses)   inviteCalleeToCall(calleeAddress,call);
  return call;
}
