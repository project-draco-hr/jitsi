{
  CallPeerSipImpl callPeer=getCallPeer();
  if (callPeer == null) {
    logger.error("Failed to find the CallPeer of the conference " + "subscription " + this);
    return null;
  }
  String conferenceInfoXML=getConferenceInfoXML(callPeer,version);
  byte[] notifyContent;
  if (conferenceInfoXML == null)   notifyContent=null;
 else {
    try {
      notifyContent=conferenceInfoXML.getBytes("UTF-8");
    }
 catch (    UnsupportedEncodingException uee) {
      logger.warn("Failed to gets bytes from String for the UTF-8 " + "charset",uee);
      notifyContent=conferenceInfoXML.getBytes();
    }
    ++version;
  }
  return notifyContent;
}
