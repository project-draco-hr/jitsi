{
  if (logger.isInfoEnabled())   logger.info("Try to open port on NAT if any");
  try {
    StreamConnector connector=getStreamConnector(type);
synchronized (connector) {
      if (connector.getProtocol() == StreamConnector.Protocol.TCP) {
        return;
      }
      if (connector.getDataSocket() != null)       connector.getDataSocket().send(new DatagramPacket(new byte[0],0,target.getDataAddress().getAddress(),target.getDataAddress().getPort()));
      if (connector.getControlSocket() != null)       connector.getControlSocket().send(new DatagramPacket(new byte[0],0,target.getControlAddress().getAddress(),target.getControlAddress().getPort()));
    }
  }
 catch (  Exception e) {
    logger.error("Error cannot send to remote peer",e);
  }
}
