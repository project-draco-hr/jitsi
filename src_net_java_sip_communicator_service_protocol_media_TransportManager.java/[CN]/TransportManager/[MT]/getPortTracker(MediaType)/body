{
  initializePortNumbers();
  if (mediaType != null) {
switch (mediaType) {
case AUDIO:
      if (audioPortTracker != null)       return audioPortTracker;
 else       break;
case VIDEO:
    if (videoPortTracker != null)     return videoPortTracker;
 else     break;
case DATA:
  if (dataPortTracker != null)   return dataPortTracker;
 else   break;
}
}
return defaultPortTracker;
}
