{
  ConfigurationService configService=ProtocolMediaActivator.getConfigurationService();
  String trafficClass=null;
  if (type == MediaType.AUDIO)   trafficClass=(String)configService.getProperty(RTP_AUDIO_DSCP_PROPERTY);
 else   if (type == MediaType.VIDEO)   trafficClass=(String)configService.getProperty(RTP_VIDEO_DSCP_PROPERTY);
 else   return 0;
  if (trafficClass != null) {
    return Integer.parseInt(trafficClass);
  }
  return 0;
}
