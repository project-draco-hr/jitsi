{
  List<Object> controls=new ArrayList<Object>();
  Collections.addAll(controls,super.getControls());
synchronized (this) {
    if (frameRateControls == null) {
      FrameRateControl frameRateControl=createFrameRateControl();
      if (frameRateControl == null)       frameRateControls=new FrameRateControl[0];
 else       frameRateControls=new FrameRateControl[]{frameRateControl};
    }
    if (frameRateControls != null)     Collections.addAll(controls,frameRateControls);
  }
  return controls.toArray();
}
