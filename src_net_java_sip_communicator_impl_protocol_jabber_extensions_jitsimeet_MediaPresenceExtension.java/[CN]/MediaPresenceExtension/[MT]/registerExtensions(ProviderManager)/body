{
  providerManager.addExtensionProvider(MediaPresenceExtension.ELEMENT_NAME,MediaPresenceExtension.NAMESPACE,new DefaultPacketExtensionProvider<MediaPresenceExtension>(MediaPresenceExtension.class));
  providerManager.addExtensionProvider(MediaPresenceExtension.Source.ELEMENT_NAME,MediaPresenceExtension.Source.NAMESPACE,new DefaultPacketExtensionProvider<Source>(Source.class));
}
