{
  String initialLevel=NeomediaActivator.getConfigurationService().getString(getStoreLevelPropertyName());
  try {
    if (initialLevel != null) {
      currentVolumeLevel=Float.valueOf(initialLevel);
      initialVolumeLevel=currentVolumeLevel;
      if (logger.isDebugEnabled())       logger.debug("Restore volume: " + currentVolumeLevel);
    }
  }
 catch (  Throwable t) {
    logger.warn("Error restoring volume",t);
  }
}
