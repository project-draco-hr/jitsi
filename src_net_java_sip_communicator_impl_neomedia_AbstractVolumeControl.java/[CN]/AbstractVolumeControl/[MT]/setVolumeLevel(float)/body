{
  if (value < MIN_VOLUME_LEVEL)   value=MIN_VOLUME_LEVEL;
 else   if (value > MAX_VOLUME_LEVEL)   value=MAX_VOLUME_LEVEL;
  if (volumeLevel == value)   return value;
  volumeLevel=value;
  fireVolumeChange();
  ConfigurationService cfg=NeomediaActivator.getConfigurationService();
  if (cfg != null) {
    cfg.setProperty(this.volumeLevelConfigurationPropertyName,String.valueOf(volumeLevel));
  }
  float f1=value / initialVolumeLevel;
  db=(float)((Math.log(((double)f1 != 0.0D) ? f1 : 0.0001D) / Math.log(10D)) * 20D);
  fireGainEvents();
  return volumeLevel;
}
