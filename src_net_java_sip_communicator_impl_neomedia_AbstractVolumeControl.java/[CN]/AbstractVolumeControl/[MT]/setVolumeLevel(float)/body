{
  if (volumeLevel == value)   return value;
  if (value < MIN_VOLUME_LEVEL)   volumeLevel=MIN_VOLUME_LEVEL;
 else   if (value > MAX_VOLUME_LEVEL)   volumeLevel=MAX_VOLUME_LEVEL;
 else   volumeLevel=value;
  fireVolumeChange();
  NeomediaActivator.getConfigurationService().setProperty(this.volumeLevelConfigurationPropertyName,String.valueOf(volumeLevel));
  float f1=value / initialVolumeLevel;
  db=(float)((Math.log((double)f1 != 0.0D ? f1 : 0.0001D) / Math.log(10D)) * 20D);
  fireGainEvents();
  return volumeLevel;
}
