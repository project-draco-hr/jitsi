{
  VolumeChangeListener[] ls;
synchronized (volumeChangeListeners) {
    ls=volumeChangeListeners.toArray(new VolumeChangeListener[volumeChangeListeners.size()]);
  }
  VolumeChangeEvent changeEvent=new VolumeChangeEvent(this,volumeLevel,mute);
  for (  VolumeChangeListener l : ls)   l.volumeChange(changeEvent);
}
