{
  if (this.db != gain) {
    this.db=gain;
    float f1=(float)Math.pow(10D,(double)this.db / 20D);
    float volumeLevel=f1 * this.initialVolumeLevel;
    if (volumeLevel < 0.0F)     volumeLevel=0.0F;
 else     if (volumeLevel > 1.0F)     volumeLevel=1.0F;
    setVolumeLevel(volumeLevel);
  }
  return this.db;
}
