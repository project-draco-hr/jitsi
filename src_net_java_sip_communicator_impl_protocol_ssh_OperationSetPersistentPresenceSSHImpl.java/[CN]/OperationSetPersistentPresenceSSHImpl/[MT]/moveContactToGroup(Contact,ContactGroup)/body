{
  ContactSSHImpl sshContact=(ContactSSHImpl)contactToMove;
  ContactGroupSSHImpl parentSSHGroup=findContactParent(sshContact);
  parentSSHGroup.removeContact(sshContact);
  if (!sshContact.isPersistent()) {
    fireSubscriptionEvent(sshContact,parentSSHGroup,SubscriptionEvent.SUBSCRIPTION_REMOVED);
    try {
      this.subscribe(newParent,contactToMove.getAddress());
      fireSubscriptionEvent(sshContact,newParent,SubscriptionEvent.SUBSCRIPTION_CREATED);
    }
 catch (    Exception ex) {
      logger.error("Failed to move contact " + sshContact.getAddress(),ex);
    }
  }
 else {
    ((ContactGroupSSHImpl)newParent).addContact(sshContact);
    fireSubscriptionMovedEvent(contactToMove,parentSSHGroup,newParent);
  }
}
