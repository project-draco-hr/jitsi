{
  ContactGroupSSHImpl sshGroup=(ContactGroupSSHImpl)group;
  ContactGroupSSHImpl parent=findGroupParent(sshGroup);
  if (parent == null) {
    throw new IllegalArgumentException("group " + group + " does not seem to belong to this protocol's contact "+ "list.");
  }
  parent.removeSubGroup(sshGroup);
  this.fireServerStoredGroupEvent(sshGroup,ServerStoredGroupEvent.GROUP_REMOVED_EVENT);
}
