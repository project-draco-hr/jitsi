{
  bundleContext=bc;
  Dictionary<String,String> properties=new Hashtable<String,String>();
  properties.put(ConfigurationForm.FORM_TYPE,ConfigurationForm.GENERAL_TYPE);
  bundleContext.registerService(ConfigurationForm.class.getName(),new LazyConfigurationForm("net.java.sip.communicator.plugin.advancedconfig.AdvancedConfigurationPanel",getClass().getClassLoader(),"plugin.advancedconfig.PLUGIN_ICON","service.gui.ADVANCED",300),properties);
  if (logger.isInfoEnabled())   logger.info("ADVANCED CONFIG PLUGIN... [REGISTERED]");
}
