{
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  if (uiServiceRef == null || uiServiceRef.getBundle().getState() != Bundle.ACTIVE) {
    return;
  }
  UIService uiService=(UIService)bundleContext.getService(uiServiceRef);
  boolean isContainerSupported=uiService.isContainerSupported(UIService.CONTAINER_HELP_MENU);
  if (isContainerSupported) {
    uiService.removeComponent(UIService.CONTAINER_HELP_MENU,aboutEntry);
  }
}
