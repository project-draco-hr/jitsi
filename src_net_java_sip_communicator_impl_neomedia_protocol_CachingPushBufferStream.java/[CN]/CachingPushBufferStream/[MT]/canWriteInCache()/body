{
synchronized (cache) {
    int cacheSize=cache.size();
    if (cacheSize < 1)     return true;
    if (cacheSize >= MAX_CACHE_SIZE)     return false;
    long bufferLength=getBufferLength();
    if (bufferLength < 1)     return false;
    if (cacheLengthInMillis < 1)     return false;
    return (cacheLengthInMillis < bufferLength);
  }
}
