{
  logger.debug("Loading imageID=" + imageID);
  try {
    InputStream in=MailboxActivator.getResources().getImageInputStream(imageID);
    byte[] image=new byte[in.available()];
    in.read(image);
    return image;
  }
 catch (  IOException e) {
    logger.error("Failed to load image:" + imageID,e);
  }
  return null;
}
