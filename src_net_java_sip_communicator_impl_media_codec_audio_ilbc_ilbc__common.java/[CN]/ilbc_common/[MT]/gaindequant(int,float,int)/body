{
  float scale;
  scale=Math.abs(maxIn);
  if (scale < 0.1) {
    scale=(float)0.1;
  }
  if (cblen == 8) {
    return scale * ilbc_constants.gain_sq3Tbl[index];
  }
 else   if (cblen == 16) {
    return scale * ilbc_constants.gain_sq4Tbl[index];
  }
 else   if (cblen == 32) {
    return scale * ilbc_constants.gain_sq5Tbl[index];
  }
  return 0.0f;
}
