{
  if (logger.isDebugEnabled()) {
    logger.debug("Add empty contact: " + id);
  }
  if (id != null) {
    final MsOutlookAddrBookSourceContact sourceContact=new MsOutlookAddrBookSourceContact(getContactSource(),id,null,null,new LinkedList<ContactDetail>());
    addQueryResult(sourceContact);
  }
}
