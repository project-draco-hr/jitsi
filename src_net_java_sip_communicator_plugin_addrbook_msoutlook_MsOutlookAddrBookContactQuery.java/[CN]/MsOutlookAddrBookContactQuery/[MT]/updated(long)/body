{
  Object[] props=null;
  try {
    props=IMAPIProp_GetProps(person,MAPI_MAILUSER_PROP_IDS,MAPI_UNICODE);
  }
 catch (  MsOutlookMAPIHResultException e) {
    if (logger.isDebugEnabled()) {
      logger.debug(MsOutlookAddrBookContactQuery.class.getSimpleName() + "#IMAPIProp_GetProps(long, long[], long)",e);
    }
  }
  if (props[PR_ORIGINAL_ENTRYID] != null) {
    SourceContact sourceContact=findSourceContactByID((String)props[PR_ORIGINAL_ENTRYID]);
    if (sourceContact != null && sourceContact instanceof MsOutlookAddrBookSourceContact) {
      MsOutlookAddrBookSourceContact editableSourceContact=(MsOutlookAddrBookSourceContact)sourceContact;
      List<ContactDetail> contactDetails=getContactDetails(props);
      editableSourceContact.setDetails(contactDetails);
      fireContactChanged(sourceContact);
    }
  }
}
