{
  if (!wasConferenceFocus && call.isConferenceFocus()) {
    Iterator<CallPeerJabberImpl> callPeerIter=call.getCallPeers();
    while (callPeerIter.hasNext()) {
      CallPeerJabberImpl callPeer=callPeerIter.next();
      if (callPeer.getState() == CallPeerState.CONNECTED)       callPeer.sendCoinSessionInfo(true);
    }
  }
  return getBasicTelephony().createOutgoingCall(call,calleeAddress,Arrays.asList(new PacketExtension[]{new CoinPacketExtension(true)}));
}
