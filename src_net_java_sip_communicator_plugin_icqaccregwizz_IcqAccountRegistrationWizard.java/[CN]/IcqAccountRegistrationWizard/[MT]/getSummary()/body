{
  LinkedHashMap summaryTable=new LinkedHashMap();
  summaryTable.put("UIN",registration.getUin());
  summaryTable.put("Remember password",new Boolean(registration.isRememberPassword()));
  if (registration.isAdvancedSettingsEnabled()) {
    if (registration.getProxy() != null)     summaryTable.put(Resources.getString("proxy"),registration.getProxy());
    if (registration.getProxyPort() != null)     summaryTable.put(Resources.getString("proxyPort"),registration.getProxyPort());
    if (registration.getProxyType() != null)     summaryTable.put(Resources.getString("proxyType"),registration.getProxyType());
    if (registration.getProxyPort() != null)     summaryTable.put(Resources.getString("proxyUsername"),registration.getProxyUsername());
    if (registration.getProxyType() != null)     summaryTable.put(Resources.getString("proxyPassword"),registration.getProxyPassword());
  }
  return summaryTable.entrySet().iterator();
}
