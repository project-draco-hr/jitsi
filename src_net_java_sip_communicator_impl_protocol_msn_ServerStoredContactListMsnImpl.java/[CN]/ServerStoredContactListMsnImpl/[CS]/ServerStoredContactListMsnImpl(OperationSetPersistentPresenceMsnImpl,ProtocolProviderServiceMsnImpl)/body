{
  this.parentOperationSet=parentOperationSet;
  this.msnProvider=provider;
  this.rootGroup=new RootContactGroupMsnImpl(this.msnProvider);
  provider.addRegistrationStateChangeListener(new RegistrationStateChangeListener(){
    public void registrationStateChanged(    RegistrationStateChangeEvent evt){
      if (evt.getNewState() == RegistrationState.UNREGISTERED || evt.getNewState() == RegistrationState.AUTHENTICATION_FAILED || evt.getNewState() == RegistrationState.CONNECTION_FAILED) {
        isInitialized=false;
      }
    }
  }
);
}
