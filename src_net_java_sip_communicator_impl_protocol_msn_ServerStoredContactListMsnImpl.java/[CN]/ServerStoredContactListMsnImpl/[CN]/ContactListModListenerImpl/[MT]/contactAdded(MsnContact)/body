{
  ContactMsnImpl contact=findContactById(newContact.getEmail().getEmailAddress());
  if (contact != null && contact.isPersistent()) {
    contact.setResolved(newContact);
    fireContactResolved(contact.getParentContactGroup(),contact);
    return;
  }
 else {
    Integer isWaitingForAddInGroup=(Integer)waitAddInGroup.get(newContact.getEmail().getEmailAddress());
    if (isWaitingForAddInGroup != null) {
      if (isWaitingForAddInGroup.intValue() == 0)       waitAddInGroup.put(newContact.getEmail().getEmailAddress(),new Integer(1));
 else       waitAddInGroup.remove(newContact.getEmail().getEmailAddress());
    }
 else {
      contact=new ContactMsnImpl(newContact,ServerStoredContactListMsnImpl.this,true,true);
      rootGroup.addContact(contact);
      fireContactAdded(rootGroup,contact);
    }
  }
}
