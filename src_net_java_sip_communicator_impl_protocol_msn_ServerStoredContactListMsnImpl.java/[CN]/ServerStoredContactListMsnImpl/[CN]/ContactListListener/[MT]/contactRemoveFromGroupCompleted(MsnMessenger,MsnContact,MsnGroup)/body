{
  String contactID=c.getEmail().getEmailAddress();
  ContactMsnImpl contactToRemove=findContactById(contactID);
  if (g == null) {
    if (logger.isTraceEnabled())     logger.trace("Group is null! ");
    return;
  }
  ContactGroupMsnImpl dstGroup=findContactGroupByMsnId(g.getGroupId());
  if (contactToRemove == null) {
    if (logger.isTraceEnabled())     logger.trace("Contact not found " + c);
    return;
  }
  if (dstGroup == null) {
    if (logger.isTraceEnabled())     logger.trace("Group not found " + g);
    return;
  }
  if (dstGroup.removeContact(contactToRemove))   fireContactRemoved(dstGroup,contactToRemove);
}
