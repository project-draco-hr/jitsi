{
  VolatileContact volatileBuddy=new VolatileContact(id);
  ContactMsnImpl newUnresolvedContact=new ContactMsnImpl(volatileBuddy,this,false,false);
  if (parentGroup instanceof ContactGroupMsnImpl)   ((ContactGroupMsnImpl)parentGroup).addContact(newUnresolvedContact);
 else   if (parentGroup instanceof RootContactGroupMsnImpl)   ((RootContactGroupMsnImpl)parentGroup).addContact(newUnresolvedContact);
  fireContactAdded(parentGroup,newUnresolvedContact);
  return newUnresolvedContact;
}
