{
  SubscriptionEvent evt=new SubscriptionEvent(sourceContact,yahooProvider,parentGroup,eventID);
  logger.debug("Dispatching a Subscription Event to" + subscriptionListeners.size() + " listeners. Evt="+ evt);
  Iterator listeners=null;
synchronized (subscriptionListeners) {
    listeners=new ArrayList(subscriptionListeners).iterator();
  }
  while (listeners.hasNext()) {
    SubscriptionListener listener=(SubscriptionListener)listeners.next();
    if (evt.getEventID() == SubscriptionEvent.SUBSCRIPTION_CREATED)     listener.subscriptionCreated(evt);
 else     if (evt.getEventID() == SubscriptionEvent.SUBSCRIPTION_REMOVED)     listener.subscriptionRemoved(evt);
 else     if (evt.getEventID() == SubscriptionEvent.SUBSCRIPTION_FAILED)     listener.subscriptionFailed(evt);
  }
}
