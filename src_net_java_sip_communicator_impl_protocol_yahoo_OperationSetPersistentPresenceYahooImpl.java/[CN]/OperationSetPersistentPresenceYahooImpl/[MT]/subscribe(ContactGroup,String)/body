{
  assertConnected();
  if (!(parent instanceof ContactGroupYahooImpl))   throw new IllegalArgumentException("Argument is not an yahoo contact group (group=" + parent + ")");
  if (contactIdentifier.indexOf("@") > -1)   contactIdentifier=contactIdentifier.substring(0,contactIdentifier.indexOf("@"));
  ssContactList.addContact((ContactGroupYahooImpl)parent,contactIdentifier);
}
