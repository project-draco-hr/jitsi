{
  if (!(to instanceof ContactRssImpl))   throw new IllegalArgumentException("The specified contact is not a Rss contact." + to);
  MessageDeliveredEvent msgDeliveredEvt=new MessageDeliveredEvent(message,to,new Date());
  fireMessageDelivered(message,to);
  RssThread rssThr=new RssThread(this,(ContactRssImpl)to,false,true);
}
