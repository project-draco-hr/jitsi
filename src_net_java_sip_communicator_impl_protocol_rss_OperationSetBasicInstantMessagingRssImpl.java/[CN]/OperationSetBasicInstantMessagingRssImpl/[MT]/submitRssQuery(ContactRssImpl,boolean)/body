{
  String newDisplayName;
  String oldDisplayName;
  String news;
  RssFeedReader rssFeed=rssContact.getRssFeedReader();
  news=rssFeed.getNewFeeds();
  if (rssContact.getPresenceStatus() == RssStatusEnum.OFFLINE) {
    getParentProvider().getOperationSetPresence().changePresenceStatusForContact(rssContact,RssStatusEnum.ONLINE);
  }
  if (news != null) {
    oldDisplayName=rssContact.getDisplayName();
    newDisplayName=rssFeed.getTitle();
    rssContact.setDisplayName(newDisplayName);
    this.opSetPersPresence.fireContactPropertyChangeEvent(ContactPropertyChangeEvent.PROPERTY_DISPLAY_NAME,rssContact,oldDisplayName,newDisplayName);
    fireMessageEvent(new MessageReceivedEvent(createMessage(news),rssContact,new Date()));
  }
 else   if (userRequestedUpdate) {
    news=rssFeed.getNoNewFeedString();
    fireMessageEvent(new MessageReceivedEvent(createMessage(news),rssContact,new Date()));
  }
}
