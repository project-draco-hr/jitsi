{
  FormatControl[] formatControls=internalGetFormatControls();
  if ((formatControls == null) || (formatControls.length == 0))   return ControlsAdapter.EMPTY_CONTROLS;
 else {
    Object[] controls=new Object[formatControls.length];
    System.arraycopy(formatControls,0,controls,0,formatControls.length);
    return controls;
  }
}
