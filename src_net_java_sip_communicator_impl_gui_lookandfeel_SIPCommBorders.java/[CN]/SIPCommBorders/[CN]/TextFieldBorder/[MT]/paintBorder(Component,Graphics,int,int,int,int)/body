{
  if (!(c instanceof JTextComponent)) {
    if (c.isEnabled()) {
      SIPCommLFUtils.drawRoundBorder(g,x,y,w,h,5,5);
    }
 else {
      SIPCommLFUtils.drawRoundDisabledBorder(g,x,y,w,h,5,5);
    }
    return;
  }
  if (c.isEnabled() && ((JTextComponent)c).isEditable()) {
    SIPCommLFUtils.drawRoundBorder(g,x,y,w,h,5,5);
  }
 else {
    SIPCommLFUtils.drawRoundDisabledBorder(g,x,y,w,h,5,5);
  }
}
