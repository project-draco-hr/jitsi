{
  ContactPropertyChangeEvent evt=new ContactPropertyChangeEvent(sourceContact,eventID,oldValue,newValue);
  logger.debug("Dispatching a Contact Property Change Event to" + subscriptionListeners.size() + " listeners. Evt="+ evt);
synchronized (subscriptionListeners) {
    Iterator listeners=subscriptionListeners.iterator();
    while (listeners.hasNext()) {
      SubscriptionListener listener=(SubscriptionListener)listeners.next();
      listener.contactModified(evt);
    }
  }
}
