{
  String initialPlaybackLevel=NeomediaActivator.getConfigurationService().getString(PLAYBACK_VOLUME_LEVEL_PROPERTY_NAME);
  try {
    if (initialPlaybackLevel != null) {
      currentVolumeLevel=Float.valueOf(initialPlaybackLevel);
      initialVolumeLevel=currentVolumeLevel;
      if (logger.isDebugEnabled())       logger.debug("Restore playback volume: " + currentVolumeLevel);
    }
  }
 catch (  Throwable t) {
    logger.warn("Error restoring playback volume",t);
  }
}
