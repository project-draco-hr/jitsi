{
  if (this.currentVolumeLevel == value)   return value;
  if (value < MIN_VOLUME_LEVEL)   this.currentVolumeLevel=MIN_VOLUME_LEVEL;
 else   if (value > MAX_VOLUME_LEVEL)   this.currentVolumeLevel=MAX_VOLUME_LEVEL;
 else   this.currentVolumeLevel=value;
  fireVolumeChange();
  NeomediaActivator.getConfigurationService().setProperty(PLAYBACK_VOLUME_LEVEL_PROPERTY_NAME,String.valueOf(currentVolumeLevel));
  float f1=value / initialVolumeLevel;
  db=(float)((Math.log((double)f1 != 0.0D ? f1 : 0.0001D) / Math.log(10D)) * 20D);
  fireGainEvents();
  return this.currentVolumeLevel;
}
