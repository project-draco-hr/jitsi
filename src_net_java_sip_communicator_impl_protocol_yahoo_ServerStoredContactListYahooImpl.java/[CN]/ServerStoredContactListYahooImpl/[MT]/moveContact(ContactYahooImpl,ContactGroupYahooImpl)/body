{
  try {
    contactListModListenerImpl.waitForMove(contact.getSourceContact().getId(),contact.getParentContactGroup().getGroupName());
    yahooSession.addFriend(contact.getSourceContact().getId(),newParent.getGroupName());
  }
 catch (  IOException ex) {
    contactListModListenerImpl.removeWaitForMove(contact.getSourceContact().getId());
    logger.error("Contact cannot be added " + ex.getMessage());
  }
}
