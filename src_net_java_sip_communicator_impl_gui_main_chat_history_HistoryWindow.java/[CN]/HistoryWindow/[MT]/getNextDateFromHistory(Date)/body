{
  Iterator<Date> iterator=datesDisplayed.iterator();
  while (iterator.hasNext()) {
    Date curr=iterator.next();
    if (curr.equals(date)) {
      if (iterator.hasNext())       return iterator.next();
 else       break;
    }
  }
  long ts=System.currentTimeMillis();
  return new Date(ts - ts % (24 * 60 * 60* 1000));
}
