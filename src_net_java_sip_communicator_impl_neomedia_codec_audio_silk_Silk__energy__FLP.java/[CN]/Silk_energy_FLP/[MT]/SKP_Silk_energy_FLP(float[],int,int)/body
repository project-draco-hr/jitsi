{
  int i, dataSize4;
  double result;
  result=0.0f;
  dataSize4=dataSize & 0xFFFC;
  for (i=0; i < dataSize4; i+=4) {
    result+=data[data_offset + i + 0] * data[data_offset + i + 0] + data[data_offset + i + 1] * data[data_offset + i + 1] + data[data_offset + i + 2] * data[data_offset + i + 2] + data[data_offset + i + 3] * data[data_offset + i + 3];
  }
  for (; i < dataSize; i++) {
    result+=data[data_offset + i] * data[data_offset + i];
  }
  assert(result >= 0.0);
  return result;
}
