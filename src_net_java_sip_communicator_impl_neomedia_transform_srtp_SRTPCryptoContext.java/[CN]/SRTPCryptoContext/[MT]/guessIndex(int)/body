{
  if (this.seqNum < 32768) {
    if (seqNo - this.seqNum > 32768) {
      guessedROC=roc - 1;
    }
 else {
      guessedROC=roc;
    }
  }
 else {
    if (seqNum - 32768 > seqNo) {
      guessedROC=roc + 1;
    }
 else {
      guessedROC=roc;
    }
  }
  return ((long)guessedROC) << 16 | seqNo;
}
