{
  Vector<MediaDescription> mediaDescs=createMediaDescriptions();
  String userName=getPeer().getProtocolProvider().getAccountID().getUserID();
  SessionDescription sDes=SdpUtils.createSessionDescription(getTransportManager().getLastUsedLocalHost(),userName,mediaDescs);
  new IceTransportManagerSipImpl(getPeer()).startCandidateHarvest(sDes,null,false,false,false,false,false);
  this.localSess=sDes;
  return localSess;
}
