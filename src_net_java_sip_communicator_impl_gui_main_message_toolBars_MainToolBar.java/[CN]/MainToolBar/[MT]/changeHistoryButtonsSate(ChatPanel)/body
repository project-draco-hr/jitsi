{
  ChatConversationPanel convPanel=chatPanel.getChatConversationPanel();
  if (chatPanel.getFirstHistoryMsgTimestamp() == null)   return;
  if (chatPanel.getFirstHistoryMsgTimestamp().compareTo(convPanel.getPageFirstMsgTimestamp()) < 0) {
    previousButton.setEnabled(true);
  }
 else {
    previousButton.setEnabled(false);
  }
  if (chatPanel.getLastHistoryMsgTimestamp().compareTo(convPanel.getPageLastMsgTimestamp()) > 0) {
    nextButton.setEnabled(true);
  }
 else {
    nextButton.setEnabled(false);
  }
}
