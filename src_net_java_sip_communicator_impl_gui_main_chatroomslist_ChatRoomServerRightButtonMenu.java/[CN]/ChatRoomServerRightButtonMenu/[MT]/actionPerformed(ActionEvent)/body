{
  JMenuItem menuItem=(JMenuItem)e.getSource();
  String itemName=menuItem.getName();
  if (itemName.equals("createChatRoom")) {
    CreateChatRoomDialog createChatRoomDialog=new CreateChatRoomDialog(mainFrame,protocolProvider);
    createChatRoomDialog.setVisible(true);
  }
 else   if (itemName.equals("joinChatRoom")) {
    JoinChannelDialog joinChannelDialog=new JoinChannelDialog(mainFrame,protocolProvider);
    joinChannelDialog.pack();
    joinChannelDialog.setVisible(true);
  }
}
