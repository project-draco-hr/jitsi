{
  ServiceRegistration osgiRegistration=null;
  Hashtable mockProvProperties=new Hashtable();
  mockProvProperties.put(ProtocolProviderFactory.PROTOCOL_PROPERTY_NAME,provider.getProtocolName());
  osgiRegistration=MsgHistoryServiceLick.bc.registerService(ProtocolProviderService.class.getName(),provider,mockProvProperties);
  logger.debug("Registered a mock protocol provider!");
  return osgiRegistration;
}
