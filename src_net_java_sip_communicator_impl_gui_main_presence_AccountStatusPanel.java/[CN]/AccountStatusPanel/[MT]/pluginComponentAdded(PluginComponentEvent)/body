{
  PluginComponent pluginComponent=event.getPluginComponent();
  Container containerID=pluginComponent.getContainer();
  Object component=pluginComponent.getComponent();
  if (!(component instanceof Component))   return;
  if (containerID.equals(Container.CONTAINER_MAIN_TOOL_BAR) || containerID.equals(Container.CONTAINER_ACCOUNT_SOUTH)) {
    this.revalidate();
    this.repaint();
  }
}
