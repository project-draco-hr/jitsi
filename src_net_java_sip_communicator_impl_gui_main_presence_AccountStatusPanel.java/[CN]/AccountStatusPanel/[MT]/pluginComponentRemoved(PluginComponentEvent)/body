{
  PluginComponent pluginComponent=event.getPluginComponent();
  Container pluginContainer=pluginComponent.getContainer();
  Object component=pluginComponent.getComponent();
  if (!(component instanceof Component))   return;
  if (pluginContainer.equals(Container.CONTAINER_MAIN_TOOL_BAR))   toolbarPluginPanel.remove((Component)component);
 else   if (pluginContainer.equals(Container.CONTAINER_ACCOUNT_SOUTH))   southPluginPanel.remove((Component)component);
  this.revalidate();
  this.repaint();
}
