{
  super.removeConferenceMembersSoundLevelListener(listener);
  AudioMediaStream audioStream=(AudioMediaStream)mediaHandler.getStream(MediaType.AUDIO);
  if (audioStream != null) {
    SoundLevelListenerWrapperForConferences sli=confmembersSoundLevelListeners.get(listener);
    if (sli != null)     audioStream.removeConferenceMemberSoundLevelListener(sli);
  }
  confmembersSoundLevelListeners.remove(listener);
}
