{
  CallPeerSoundLevelEvent event=new CallPeerSoundLevelEvent(this,level);
  CallPeerSoundLevelListener[] listeners;
synchronized (soundLevelListeners) {
    listeners=soundLevelListeners.toArray(new CallPeerSoundLevelListener[soundLevelListeners.size()]);
  }
  for (  CallPeerSoundLevelListener listener : listeners) {
    listener.peerSoundLevelChanged(event);
  }
}
