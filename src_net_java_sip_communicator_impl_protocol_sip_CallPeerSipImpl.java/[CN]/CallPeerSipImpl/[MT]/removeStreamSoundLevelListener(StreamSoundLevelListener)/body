{
  super.removeStreamSoundLevelListener(listener);
  AudioMediaStream audioStream=(AudioMediaStream)mediaHandler.getStream(MediaType.AUDIO);
  if (audioStream != null) {
    SoundLevelListenerWrapperForStream sli=streamSoundLevelListeners.get(listener);
    if (sli != null)     audioStream.removeSoundLevelListener(sli);
  }
  streamSoundLevelListeners.remove(listener);
}
