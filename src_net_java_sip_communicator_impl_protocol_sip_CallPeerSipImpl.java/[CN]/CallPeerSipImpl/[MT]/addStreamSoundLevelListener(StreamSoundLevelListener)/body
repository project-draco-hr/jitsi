{
  AudioMediaStream audioStream=(AudioMediaStream)mediaHandler.getStream(MediaType.AUDIO);
  if (audioStream != null) {
    SoundLevelListenerWrapperForStream sli=new SoundLevelListenerWrapperForStream(listener);
    audioStream.addSoundLevelListener(sli);
    streamSoundLevelListeners.put(listener,sli);
    super.addStreamSoundLevelListener(listener);
  }
}
