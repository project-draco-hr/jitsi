{
  Map<String,Integer> levels=evt.getLevels();
  if (levels.size() == 0)   return;
  Map<ConferenceMember,Integer> res=new HashMap<ConferenceMember,Integer>();
  ConferenceMember[] cms=getConferenceMembers();
  for (int i=0; i < cms.length; i++) {
    ConferenceMemberSipImpl cm=(ConferenceMemberSipImpl)cms[i];
    String ssrc=cm.getSSRC();
    if (ssrc != null) {
      Integer level=levels.get(ssrc);
      if (level != null)       res.put(cm,level);
    }
  }
  if (res.size() > 0)   listener.membersSoundLevelChanged(new ConferenceMembersSoundLevelEvent(CallPeerSipImpl.this,res));
}
