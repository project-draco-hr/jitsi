{
  Map<String,Integer> levels=evt.getLevels();
  if (levels.size() == 1) {
    listener.streamSoundLevelChanged(new StreamSoundLevelEvent(CallPeerSipImpl.this,levels.values().iterator().next()));
  }
 else   logger.warn("suspicious behavior of the SoundLevelListener " + "for peer " + CallPeerSipImpl.this);
}
