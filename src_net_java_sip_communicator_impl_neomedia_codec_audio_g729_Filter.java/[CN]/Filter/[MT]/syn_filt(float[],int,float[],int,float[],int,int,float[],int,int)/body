{
  int L_SUBFR=Ld8k.L_SUBFR;
  int M=Ld8k.M;
  int i, j;
  float[] yy_b=new float[L_SUBFR + M];
  float s;
  int yy, py, pa;
  yy=0;
  for (i=0; i < M; i++)   yy_b[yy++]=mem[mem_offset++];
  for (i=0; i < l; i++) {
    py=yy;
    pa=0;
    s=x[x_offset++];
    for (j=0; j < M; j++)     s-=(a[a_offset + ++pa]) * (yy_b[--py]);
    yy_b[yy++]=s;
    y[y_offset++]=s;
  }
  if (update != 0)   for (i=0; i < M; i++)   mem[--mem_offset]=yy_b[--yy];
}
