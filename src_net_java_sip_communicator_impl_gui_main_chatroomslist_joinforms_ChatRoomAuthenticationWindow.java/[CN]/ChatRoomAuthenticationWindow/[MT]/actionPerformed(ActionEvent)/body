{
  JButton button=(JButton)evt.getSource();
  String buttonName=button.getName();
  if (buttonName.equals("ok")) {
    new Thread(){
      public void run(){
        mainFrame.getMultiUserChatManager().joinChatRoom(chatRoom,idValue.getText(),new String(passwdField.getPassword()).getBytes());
      }
    }
.start();
  }
  this.dispose();
}
