{
  Hashtable hashtable=new Hashtable();
  hashtable.put(ProtocolProviderFactory.PROTOCOL_PROPERTY_NAME,ProtocolNames.ICQ);
  ProtocolProviderFactoryIcqImpl icqProviderFactory=new ProtocolProviderFactoryIcqImpl();
  ServiceReference confReference=context.getServiceReference(ConfigurationService.class.getName());
  ConfigurationService configurationService=(ConfigurationService)context.getService(confReference);
  icqProviderFactory.loadStoredAccounts(context,configurationService);
  icqAccManRegistration=context.registerService(ProtocolProviderFactory.class.getName(),icqProviderFactory,hashtable);
}
