{
  try {
    InetSocketAddress addr=null;
    if (ProtocolProviderServiceSipImpl.checkPreferIPv6Addresses())     addr=NetworkUtils.getAAAARecord(hostname,80);
 else     addr=NetworkUtils.getARecord(hostname,80);
    if (addr != null)     return addr.getAddress();
 else     return null;
  }
 catch (  java.text.ParseException e) {
    throw new IOException(e.getMessage());
  }
}
