{
  if (image == null || image.length <= 0) {
    TreeContactList.setSourceContactImage(peerName,photoLabel,100,100);
  }
 else {
    this.peerImage=ImageUtils.getScaledRoundedIcon(image,100,100);
    this.peerImage=getPhotoLabelIcon();
synchronized (videoContainers) {
      for (      JLabel photoLabel : photoLabels)       photoLabel.setIcon(this.peerImage);
    }
  }
}
