{
  CallChangeEvent event=new CallChangeEvent(this,type,oldValue,newValue,cause);
  if (logger.isDebugEnabled()) {
    logger.debug("Dispatching a CallChange event to " + callListeners.size() + " listeners. The CallChange event is: "+ event);
  }
  CallChangeListener[] listeners;
synchronized (callListeners) {
    listeners=callListeners.toArray(new CallChangeListener[callListeners.size()]);
  }
  for (  CallChangeListener listener : listeners)   if (type.equals(CallChangeEvent.CALL_STATE_CHANGE))   listener.callStateChanged(event);
}
