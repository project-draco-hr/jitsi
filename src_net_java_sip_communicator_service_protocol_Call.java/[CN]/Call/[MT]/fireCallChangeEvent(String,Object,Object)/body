{
  CallChangeEvent ccEvent=new CallChangeEvent(this,type,oldValue,newValue);
  logger.debug("Dispatching a CallParticipant event to " + callListeners.size() + " listeners. event is: "+ ccEvent.toString());
  Iterator listeners=callListeners.iterator();
  while (listeners.hasNext()) {
    CallChangeListener listener=(CallChangeListener)listeners.next();
    if (type.equals(CallChangeEvent.CALL_STATE_CHANGE))     listener.callStateChanged(ccEvent);
  }
}
