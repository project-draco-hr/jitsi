{
  AudioNotifierService audioNotifService=NotificationActivator.getAudioNotifier();
  if (audioNotifService == null)   return;
  for (  Map.Entry<SCAudioClip,NotificationData> entry : playedClips.entrySet()) {
    if (entry.getValue() == data) {
      SCAudioClip audio=entry.getKey();
      audio.stop();
      audioNotifService.destroyAudio(audio);
      return;
    }
  }
}
