{
  ContactGroupSipImpl newGroup=new ContactGroupSipImpl(ContactGroupSipImpl.createNameFromUID(groupUID),parentProvider);
  newGroup.setResolved(false);
  if (parentGroup == null)   parentGroup=getServerStoredContactListRoot();
  ((ContactGroupSipImpl)parentGroup).addSubgroup(newGroup);
  this.fireServerStoredGroupEvent(newGroup,ServerStoredGroupEvent.GROUP_CREATED_EVENT);
  return newGroup;
}
