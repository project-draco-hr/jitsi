{
  bundleContext=bc;
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  uiService=(UIService)bundleContext.getService(uiServiceRef);
  ConfigurationManager.loadGuiConfigurations();
  GeneralConfigurationForm preferencesForm=new GeneralConfigurationForm();
  bundleContext.registerService(ConfigurationForm.class.getName(),preferencesForm,null);
  logger.info("PREFERENCES PLUGIN... [REGISTERED]");
}
