{
  NotificationService notificationService=NotificationWiringActivator.getNotificationService();
  if (notificationService == null)   return;
  Iterable<NotificationHandler> soundHandlers=notificationService.getActionHandlers(NotificationAction.ACTION_SOUND);
  if (soundHandlers != null)   for (  NotificationHandler handler : soundHandlers)   ((SoundNotificationHandler)handler).stop(data);
}
