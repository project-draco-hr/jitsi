{
  BufferedImage image=images.get(imageID);
  if (image == null) {
    URL path=NotificationWiringActivator.getResources().getImageURL(imageID.getId());
    if (path != null) {
      try {
        image=ImageIO.read(path);
        images.put(imageID,image);
      }
 catch (      Exception ex) {
        logger.error("Failed to load image: " + path,ex);
      }
    }
  }
  return image;
}
