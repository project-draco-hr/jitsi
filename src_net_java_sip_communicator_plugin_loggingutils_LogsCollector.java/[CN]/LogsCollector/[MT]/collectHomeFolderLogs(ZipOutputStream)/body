{
  try {
    File[] fs=LoggingUtilsActivator.getFileAccessService().getPrivatePersistentDirectory(LOGGING_DIR_NAME).listFiles();
    for (    File f : fs) {
      if (f.getName().endsWith(".lck"))       continue;
      addFileToZip(f,out);
    }
  }
 catch (  Exception e) {
    logger.error("Error obtaining logs folder",e);
  }
}
