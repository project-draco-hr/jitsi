{
  if (usernameField.getText() == null || usernameField.getText().equals("") || usernameField.getText().indexOf("@") != -1 || usernameField.getText().indexOf("-") != -1) {
    wizard.getWizardContainer().setNextFinishButtonEnabled(false);
  }
 else {
    wizard.getWizardContainer().setNextFinishButtonEnabled(true);
  }
}
