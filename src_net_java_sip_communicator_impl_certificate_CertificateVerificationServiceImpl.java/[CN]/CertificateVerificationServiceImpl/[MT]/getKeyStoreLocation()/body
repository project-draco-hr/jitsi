{
  String keyStoreFile=CertificateVerificationActivator.getConfigurationService().getString(KEYSTORE_FILE_PROP);
  if (keyStoreFile == null || keyStoreFile.length() == 0)   return CertificateVerificationActivator.getFileAccessService().getPrivatePersistentFile(KEY_STORE_FILE_NAME);
  File f=new File(keyStoreFile);
  if (f.exists())   return f;
 else {
    CertificateVerificationActivator.getConfigurationService().removeProperty(KEYSTORE_FILE_PROP);
    return CertificateVerificationActivator.getFileAccessService().getPrivatePersistentFile(KEY_STORE_FILE_NAME);
  }
}
