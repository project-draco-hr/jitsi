{
  String defaultAlwaysTrustMode=CertificateVerificationActivator.getResources().getSettingsString(CertificateVerificationService.ALWAYS_TRUST_MODE_ENABLED_PROP_NAME);
  if (CertificateVerificationActivator.getConfigurationService().getBoolean(CertificateVerificationService.ALWAYS_TRUST_MODE_ENABLED_PROP_NAME,Boolean.parseBoolean(defaultAlwaysTrustMode))) {
    return;
  }
  try {
    tm.checkServerTrusted(chain,authType);
  }
 catch (  CertificateException certificateException) {
    checkLocalTrust(chain,authType,certificateException);
  }
}
