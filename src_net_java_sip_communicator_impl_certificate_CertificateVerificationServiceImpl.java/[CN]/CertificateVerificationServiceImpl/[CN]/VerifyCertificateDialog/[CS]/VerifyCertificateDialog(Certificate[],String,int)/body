{
  super(false);
  setTitle(CertificateVerificationActivator.getResources().getI18NString("service.gui.CERT_DIALOG_TITLE"));
  setModal(true);
  this.cert=certs[0];
  this.host=host;
  this.port=port;
  setDefaultCloseOperation(JDialog.DISPOSE_ON_CLOSE);
  init();
  setLocationRelativeTo(getParent());
}
