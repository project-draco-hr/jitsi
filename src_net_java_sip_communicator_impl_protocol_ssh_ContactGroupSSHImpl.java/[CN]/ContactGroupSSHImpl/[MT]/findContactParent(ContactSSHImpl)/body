{
  if (contacts.contains(sshContact))   return this;
  Iterator subGroupsIter=subgroups();
  while (subGroupsIter.hasNext()) {
    ContactGroupSSHImpl subgroup=(ContactGroupSSHImpl)subGroupsIter.next();
    ContactGroupSSHImpl parent=subgroup.findContactParent(sshContact);
    if (parent != null)     return parent;
  }
  return null;
}
