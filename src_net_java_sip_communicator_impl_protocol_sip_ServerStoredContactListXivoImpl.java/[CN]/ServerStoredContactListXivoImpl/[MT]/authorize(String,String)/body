{
  if (connection == null || sessionId == null || password == null)   return false;
  JSONObject obj=new JSONObject();
  try {
    obj.accumulate("class","login_pass");
    obj.accumulate("hashedpassword",Sha1Crypto.encode(sessionId + ":" + password));
    return send(obj);
  }
 catch (  Exception e) {
    logger.error("Error login with password",e);
    return false;
  }
}
