{
  setDevice(device);
  this.rtpConnector=new RTPTransformConnector(connector);
  if (zrtpControl != null) {
    this.zrtpControl=zrtpControl;
  }
 else   this.zrtpControl=new ZrtpControlImpl();
  this.zrtpControl.setConnector(rtpConnector);
  csrcEngine=new CsrcTransformEngine(this);
  rtpConnector.setEngine(new TransformEngineChain(csrcEngine,this.zrtpControl.getZrtpEngine()));
}
