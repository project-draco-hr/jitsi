{
  srtpControl.setConnector(newValue);
  if (newValue != null) {
    if (newValue instanceof RTPTransformUDPConnector)     ((RTPTransformUDPConnector)newValue).setEngine(createTransformEngineChain());
 else     if (newValue instanceof RTPTransformTCPConnector)     ((RTPTransformTCPConnector)newValue).setEngine(createTransformEngineChain());
    if (rtpConnectorTarget != null)     doSetTarget(rtpConnectorTarget);
  }
}
