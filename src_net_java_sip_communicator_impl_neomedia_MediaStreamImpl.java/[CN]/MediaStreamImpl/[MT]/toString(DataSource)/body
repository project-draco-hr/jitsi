{
  StringBuffer str=new StringBuffer();
  str.append(dataSource.getClass().getSimpleName());
  str.append(" with hashCode ");
  str.append(dataSource.hashCode());
  MediaLocator locator=dataSource.getLocator();
  if (locator != null) {
    str.append(" and locator ");
    str.append(locator);
  }
  return str.toString();
}
