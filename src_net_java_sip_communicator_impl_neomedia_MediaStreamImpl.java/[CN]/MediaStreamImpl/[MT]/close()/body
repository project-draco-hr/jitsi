{
  stop();
  rtpConnector.removeTargets();
  if (rtpManager != null) {
    rtpManager.removeReceiveStreamListener(this);
    rtpManager.removeSendStreamListener(this);
    rtpManager.removeSessionListener(this);
    rtpManager.dispose();
    rtpManager=null;
  }
  disposePlayers();
  getDevice().close();
}
