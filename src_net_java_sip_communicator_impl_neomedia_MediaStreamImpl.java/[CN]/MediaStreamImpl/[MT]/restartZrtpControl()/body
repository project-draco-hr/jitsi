{
  if (!srtpControl.getSecureCommunicationStatus())   return;
  srtpControl.cleanup();
  AbstractRTPConnector rtpConnector=getRTPConnector();
  srtpControl.setConnector(rtpConnector);
  if (rtpConnector instanceof RTPTransformUDPConnector)   ((RTPTransformUDPConnector)rtpConnector).setEngine(createTransformEngineChain());
 else   if (rtpConnector instanceof RTPTransformTCPConnector)   ((RTPTransformTCPConnector)rtpConnector).setEngine(createTransformEngineChain());
  zrtpRestarted=true;
}
