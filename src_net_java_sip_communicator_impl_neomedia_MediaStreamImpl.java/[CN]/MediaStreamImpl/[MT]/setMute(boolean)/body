{
  if (this.mute != mute) {
    if (logger.isTraceEnabled())     logger.trace((mute ? "Muting" : "Unmuting") + " stream with hashcode " + hashCode());
    this.mute=mute;
    MediaDeviceSession deviceSession=getDeviceSession();
    if (deviceSession != null)     deviceSession.setMute(this.mute);
  }
}
