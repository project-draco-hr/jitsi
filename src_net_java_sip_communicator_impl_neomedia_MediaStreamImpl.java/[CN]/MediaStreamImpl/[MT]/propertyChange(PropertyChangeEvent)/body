{
  if (this instanceof VideoMediaStreamImpl) {
    return;
  }
  String prop=evt.getPropertyName();
  if (prop.equals(DeviceConfiguration.AUDIO_CAPTURE_DEVICE)) {
    if (evt.getOldValue() == evt.getNewValue())     return;
    if (deviceSessionReinit == false)     firePropertyChange("CHANGE_CAPTURE_DEV",evt.getOldValue(),evt.getNewValue());
    deviceSessionReinit=false;
  }
}
