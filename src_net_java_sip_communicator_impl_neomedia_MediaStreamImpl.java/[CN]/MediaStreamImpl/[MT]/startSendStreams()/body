{
  RTPManager rtpManager=getRTPManager();
  @SuppressWarnings("unchecked") Iterable<SendStream> sendStreams=rtpManager.getSendStreams();
  if (sendStreams != null)   for (  SendStream sendStream : sendStreams)   try {
    sendStream.getDataSource().connect();
    sendStream.start();
    if (logger.isTraceEnabled())     logger.trace("Started SendStream" + " with hashCode " + sendStream.hashCode());
  }
 catch (  IOException ioe) {
    logger.warn("Failed to start stream " + sendStream,ioe);
  }
}
