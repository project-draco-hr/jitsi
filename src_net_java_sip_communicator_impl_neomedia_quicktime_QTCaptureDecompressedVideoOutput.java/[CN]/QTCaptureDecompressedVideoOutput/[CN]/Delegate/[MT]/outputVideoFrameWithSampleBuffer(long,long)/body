{
  if (videoFrame == null)   videoFrame=new MutableCVPixelBuffer(videoFramePtr);
 else   videoFrame.setPtr(videoFramePtr);
  if (sampleBuffer == null)   sampleBuffer=new MutableQTSampleBuffer(sampleBufferPtr);
 else   sampleBuffer.setPtr(sampleBufferPtr);
  outputVideoFrameWithSampleBuffer(videoFrame,sampleBuffer);
}
