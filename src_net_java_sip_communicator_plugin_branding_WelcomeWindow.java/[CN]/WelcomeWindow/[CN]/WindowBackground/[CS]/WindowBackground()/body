{
  this.setOpaque(true);
  try {
    bgImage=ImageIO.read(WindowBackground.class.getClassLoader().getResource(BrandingResources.getResourceString("splashScreenBg")));
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  this.setPreferredSize(new Dimension(bgImage.getWidth(this),bgImage.getHeight(this)));
}
