{
  setOpaque(true);
  Image image=null;
  try {
    image=ImageIO.read(BrandingActivator.getResources().getImageURL("splashScreenBg"));
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  this.image=image;
  if (image != null) {
    setPreferredSize(new Dimension(image.getWidth(this),image.getHeight(this)));
  }
}
