{
  bundleContext=bc;
  StatusConfigForm statusCF=new StatusConfigForm();
  bundleContext.registerService(ConfigurationForm.class.getName(),statusCF,null);
  new Thread(new Runnable(){
    public void run(){
      try {
        Thread.sleep(5000);
      }
 catch (      InterruptedException e) {
      }
      startThread();
    }
  }
).start();
}
