{
  logger.debug("Will Query an AWAY contact.");
  subtestQueryContactStatus(JabberStatusEnum.AWAY,JabberStatusEnum.AWAY);
  logger.debug("Will Query a DND contact.");
  subtestQueryContactStatus(JabberStatusEnum.DO_NOT_DISTURB,JabberStatusEnum.DO_NOT_DISTURB);
  logger.debug("Will Query a Free For Chat contact.");
  subtestQueryContactStatus(JabberStatusEnum.FREE_FOR_CHAT,JabberStatusEnum.FREE_FOR_CHAT);
  logger.debug("Will Query an Online contact.");
  subtestQueryContactStatus(JabberStatusEnum.AVAILABLE,JabberStatusEnum.AVAILABLE);
}
