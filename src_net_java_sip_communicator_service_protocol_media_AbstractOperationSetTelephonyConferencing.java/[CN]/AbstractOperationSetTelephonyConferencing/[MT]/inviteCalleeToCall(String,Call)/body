{
  CalleeAddressT calleeAddress=parseAddressString(uri);
  @SuppressWarnings("unchecked") MediaAwareCallT mediaAwareCall=(MediaAwareCallT)call;
  boolean wasConferenceFocus;
  if (mediaAwareCall.isConferenceFocus())   wasConferenceFocus=true;
 else {
    wasConferenceFocus=false;
    mediaAwareCall.setConferenceFocus(true);
  }
  return inviteCalleeToCall(calleeAddress,mediaAwareCall,wasConferenceFocus);
}
