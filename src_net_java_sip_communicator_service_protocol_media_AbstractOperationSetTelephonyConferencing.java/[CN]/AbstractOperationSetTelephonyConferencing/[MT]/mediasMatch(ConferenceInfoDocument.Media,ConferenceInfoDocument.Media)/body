{
  if (a == null && b == null)   return true;
 else   if (a == null || b == null)   return false;
 else   if (!stringsMatch(a.getId(),b.getId()))   return false;
 else   if (!stringsMatch(a.getSrcId(),b.getSrcId()))   return false;
 else   if (!stringsMatch(a.getType(),b.getType()))   return false;
 else   if (!stringsMatch(a.getStatus(),b.getStatus()))   return false;
  return true;
}
