{
  if (call.getCallGroup() != null) {
    CallGroup group=call.getCallGroup();
    for (    Call c : group.getCalls()) {
      if (c == call)       continue;
      AbstractOperationSetTelephonyConferencing<?,?,?,?,?> opSet=(AbstractOperationSetTelephonyConferencing<?,?,?,?,?>)c.getProtocolProvider().getOperationSet(OperationSetTelephonyConferencing.class);
      if (opSet != null) {
        opSet.notifyAll(c);
      }
    }
  }
}
