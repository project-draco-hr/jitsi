{
  PredictableTransformLayer transformLayer=new PredictableTransformLayer();
  opSetTransform1.addTransformLayer(transformLayer);
  if (!opSetTransform1.containsLayer(transformLayer))   fail("Transform layer did not install.");
  opSetTransform2.addTransformLayer(transformLayer);
  if (!opSetTransform2.containsLayer(transformLayer))   fail("Transform layer did not install.");
}
