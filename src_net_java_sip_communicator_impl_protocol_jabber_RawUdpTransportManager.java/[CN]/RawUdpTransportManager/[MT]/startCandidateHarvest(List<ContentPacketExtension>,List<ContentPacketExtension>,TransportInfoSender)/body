{
  for (  ContentPacketExtension content : theirOffer) {
    RtpDescriptionPacketExtension rtpDesc=content.getFirstChildOfType(RtpDescriptionPacketExtension.class);
    StreamConnector connector=getStreamConnector(MediaType.parseString(rtpDesc.getMedia()));
    RawUdpTransportPacketExtension ourTransport=createTransport(connector);
    ContentPacketExtension cpExt=findContentByName(ourAnswer,content.getName());
    if (cpExt != null)     cpExt.addChildExtension(ourTransport);
  }
  this.local=ourAnswer;
}
