{
  CobriConferenceIQ.Channel channel=null;
  if (cobri != null) {
    CobriConferenceIQ.Content content=cobri.getContent(mediaType.toString());
    if (content != null) {
      List<CobriConferenceIQ.Channel> channels=content.getChannels();
      if (channels.size() == 2)       channel=channels.get(local ? 0 : 1);
    }
  }
  return channel;
}
