{
  NewAccount newAccount=null;
  try {
    JSONObject jsonObject=new JSONObject(response);
    boolean isSuccess=jsonObject.getBoolean("success");
    if (isSuccess) {
      newAccount=new NewAccount(usernameField.getText() + "@ippi.fr",passField.getPassword(),null,"ippi.fr");
    }
 else {
      String errorMessage=jsonObject.getString("error_message");
      errorPane.setText(errorMessage);
      add(errorPane,BorderLayout.NORTH);
      SwingUtilities.getWindowAncestor(CreateIppiAccountForm.this).pack();
    }
  }
 catch (  JSONException e1) {
    if (logger.isInfoEnabled())     logger.info("Failed Json parsing.",e1);
  }
  return newAccount;
}
