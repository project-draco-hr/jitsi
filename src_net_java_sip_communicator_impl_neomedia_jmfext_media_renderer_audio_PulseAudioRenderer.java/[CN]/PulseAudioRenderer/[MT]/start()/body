{
  pulseAudioSystem.lockMainloop();
  try {
    if (stream == 0) {
      try {
        openWithMainloopLock();
      }
 catch (      ResourceUnavailableException rue) {
        throw new UndeclaredThrowableException(rue);
      }
    }
    cork(false);
  }
  finally {
    pulseAudioSystem.unlockMainloop();
  }
}
