{
  if (event.getSource() instanceof CallConference) {
    String propertyName=event.getPropertyName();
    if (CONFERENCE_FOCUS.equals(propertyName)) {
      conferenceFocusChanged((Boolean)event.getOldValue(),(Boolean)event.getNewValue());
    }
 else     if (DEFAULT_DEVICE.equals(propertyName)) {
      firePropertyChange(DEFAULT_DEVICE,event.getOldValue(),event.getNewValue());
    }
  }
}
