{
  boolean result=false;
  if (getAddress() != null) {
    InetAddress from=packet.getAddress();
    if (from != null) {
      if (from.isLinkLocalAddress() && (!getAddress().isLinkLocalAddress())) {
        result=true;
      }
      if (from.isLoopbackAddress() && (!getAddress().isLoopbackAddress())) {
        result=true;
      }
    }
  }
  return result;
}
