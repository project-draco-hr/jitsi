{
  String accountPrefix=findAccountPrefix(bundleContext,accountID,getFactoryImplPackageName());
  if (accountPrefix == null)   return null;
  ServiceReference credentialsReference=bundleContext.getServiceReference(CredentialsStorageService.class.getName());
  CredentialsStorageService credentialsService=(CredentialsStorageService)bundleContext.getService(credentialsReference);
  return credentialsService.loadPassword(accountPrefix);
}
