{
  Component[] buttons=new Component[]{new HoldButton(call,true,CallPeerState.isOnHold(callPeer.getState())),new InputVolumeControlButton(call,true,callPeer.isMute()),new OutputVolumeControlButton(true),new RecordButton(call,true,callContainer.isRecordingStarted()),CallPeerRendererUtils.createExitFullScreenButton(this)};
  return CallPeerRendererUtils.createButtonBar(true,buttons);
}
