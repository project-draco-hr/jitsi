{
  ShowHideVideoButton showHideButton=new ShowHideVideoButton(null,true,callContainer.isShowHideVideoButtonSelected());
  boolean isVideoButtonSelected=false;
  showHideButton.setEnabled(isVideoButtonSelected);
  Component[] buttons=new Component[]{new OutputVolumeControlButton(true).getComponent(),new InputVolumeControlButton(call,true,callPeer.isMute()),new HoldButton(call,true,CallPeerState.isOnHold(callPeer.getState())),new RecordButton(call,true,callContainer.isRecordingStarted()),new FullScreenButton(callContainer,true),new LocalVideoButton(call,true,isVideoButtonSelected),showHideButton,new HangupButton(callContainer)};
  return CallPeerRendererUtils.createButtonBar(true,buttons);
}
