{
  String fromServer;
  boolean quit=false;
  if (!this.isConnected()) {
    return;
  }
  try {
    this.out.println("QUIT");
    while (quit == false && (fromServer=this.in.readLine()) != null) {
      if (fromServer.startsWith("221")) {
        quit=true;
      }
    }
    this.out.close();
    this.in.close();
    this.socket.close();
    this.connected=false;
  }
 catch (  IOException ioe) {
    throw new DictException(ioe);
  }
}
