{
  for (int i=0; i < chatProviderWrapper.countChatRooms(); i++)   addChatRoom(chatProviderWrapper.getChatRoom(i),false);
  OperationSetPresence presence=chatProviderWrapper.getProtocolProvider().getOperationSet(OperationSetPresence.class);
  if (presence != null)   presence.addProviderPresenceStatusListener(this);
}
