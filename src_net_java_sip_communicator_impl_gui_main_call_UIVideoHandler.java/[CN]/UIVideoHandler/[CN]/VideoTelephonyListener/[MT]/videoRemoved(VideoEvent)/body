{
  CallPanel callContainer=callRenderer.getCallContainer();
  if (callContainer.isConference() && callPeer.getCall() != null && !CallManager.isVideoStreaming(callPeer.getCall()) && callContainer.isVideoConferenceInterfaceEnabled()) {
    callContainer.enableVideoConferenceInterface(false);
  }
  handleVideoEvent(callPeer,event);
}
