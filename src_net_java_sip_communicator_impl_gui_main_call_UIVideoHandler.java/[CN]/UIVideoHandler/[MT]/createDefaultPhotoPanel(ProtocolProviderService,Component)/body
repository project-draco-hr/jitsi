{
  OperationSetServerStoredAccountInfo accountInfoOpSet=pps.getOperationSet(OperationSetServerStoredAccountInfo.class);
  ImageIcon photoLabelIcon=null;
  if (accountInfoOpSet != null) {
    byte[] accountImage=AccountInfoUtils.getImage(accountInfoOpSet);
    if ((accountImage != null) && (accountImage.length > 0))     photoLabelIcon=new ImageIcon(accountImage);
  }
  if (photoLabelIcon == null) {
    photoLabelIcon=new ImageIcon(ImageLoader.getImage(ImageLoader.DEFAULT_USER_PHOTO));
  }
  return createDefaultPhotoPanel(photoLabelIcon,videoToolbar);
}
