{
  ProtocolProviderFactory factory=SIPAccRegWizzActivator.getSIPProtocolProviderFactory();
  java.util.List<AccountID> registeredAccounts=factory.getRegisteredAccounts();
  for (  AccountID accountID : registeredAccounts) {
    if (accountName.equalsIgnoreCase(accountID.getUserID()) && serverName.equalsIgnoreCase(accountID.getService()))     return true;
  }
  return false;
}
