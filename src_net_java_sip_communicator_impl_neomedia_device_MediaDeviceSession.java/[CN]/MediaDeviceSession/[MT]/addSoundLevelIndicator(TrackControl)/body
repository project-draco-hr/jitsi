{
  SoundLevelIndicatorEffect slie=new SoundLevelIndicatorEffect(LocalUserSoundLevelEvent.MIN_LEVEL,LocalUserSoundLevelEvent.MAX_LEVEL,new SoundLevelIndicatorEffect.SoundLevelIndicatorListener(){
    public void soundLevelChanged(    int level){
      NeomediaActivator.getMediaServiceImpl().fireLocalUserSoundLevelEvent(level);
    }
  }
);
  tc.setCodecChain(new Codec[]{slie});
}
