{
  Processor processor=getProcessor();
  DataSource outputDataSource;
  if ((processor == null) || ((processor.getState() < Processor.Realized) && !waitForState(processor,Processor.Realized)))   outputDataSource=null;
 else {
    outputDataSource=processor.getDataOutput();
    if (logger.isTraceEnabled() && (outputDataSource != null))     logger.trace("Processor with hashCode " + processor.hashCode() + " provided "+ MediaStreamImpl.toString(outputDataSource));
    startProcessorInAccordWithDirection(processor);
  }
  return outputDataSource;
}
