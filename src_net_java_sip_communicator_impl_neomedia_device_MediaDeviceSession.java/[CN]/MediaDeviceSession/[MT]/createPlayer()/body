{
  Throwable exception;
  try {
    this.playbackDataSource.connect();
    exception=null;
  }
 catch (  IOException ioex) {
    exception=ioex;
  }
  if (exception == null) {
    Player player=createPlayer(this.playbackDataSource);
    if (player == null)     this.playbackDataSource.disconnect();
 else     return player;
  }
 else   logger.error("Failed to connect to " + MediaStreamImpl.toString(this.playbackDataSource),exception);
  return null;
}
