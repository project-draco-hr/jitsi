{
  if (receiveStreamDataSource == null)   return;
  receiveStreams.put(receiveStream,receiveStreamDataSource);
  if (logger.isTraceEnabled())   logger.trace("Added ReceiveStream with ssrc " + receiveStream.getSSRC());
  addSSRC(receiveStream.getSSRC());
synchronized (players) {
    Processor player=players.get(receiveStreamDataSource);
    if (player == null) {
      Throwable exception;
      try {
        receiveStreamDataSource.connect();
        exception=null;
      }
 catch (      IOException ioex) {
        exception=ioex;
      }
      if (exception == null) {
        player=createPlayer(receiveStream,receiveStreamDataSource);
        if (player == null)         receiveStreamDataSource.disconnect();
 else         players.put(receiveStreamDataSource,player);
      }
 else       logger.error("Failed to connect to DataSource" + " of ReceiveStream with ssrc " + receiveStream.getSSRC(),exception);
    }
  }
}
