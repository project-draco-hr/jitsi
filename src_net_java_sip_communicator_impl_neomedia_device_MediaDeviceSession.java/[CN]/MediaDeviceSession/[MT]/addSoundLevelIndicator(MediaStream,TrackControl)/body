{
  if (mediaStream == null || !(mediaStream instanceof AudioMediaStreamImpl))   return;
  final AudioMediaStreamImpl aStream=(AudioMediaStreamImpl)mediaStream;
  if (streamSoundLevelListeners.size() == 0)   return;
  SoundLevelIndicatorEffect slie=new SoundLevelIndicatorEffect(SoundLevelChangeEvent.MIN_LEVEL,SoundLevelChangeEvent.MAX_LEVEL,new SoundLevelIndicatorEffect.SoundLevelIndicatorListener(){
    public void soundLevelChanged(    int level){
      fireStreamSoundLevelEvent(level);
    }
  }
);
  tc.setCodecChain(new Codec[]{slie});
}
