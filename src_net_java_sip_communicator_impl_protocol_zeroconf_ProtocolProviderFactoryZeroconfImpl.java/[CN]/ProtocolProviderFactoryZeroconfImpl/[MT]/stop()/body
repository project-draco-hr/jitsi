{
  Enumeration registrations=this.registeredAccounts.elements();
  while (registrations.hasMoreElements()) {
    ServiceRegistration reg=((ServiceRegistration)registrations.nextElement());
    ProtocolProviderServiceZeroconfImpl provider=(ProtocolProviderServiceZeroconfImpl)ZeroconfActivator.getBundleContext().getService(reg.getReference());
    provider.shutdown();
    reg.unregister();
  }
  registeredAccounts.clear();
}
