{
  for (byte i=0; i < 10; i++) {
synchronized (waitedCallIds) {
      if (waitedCallIds.size() == 0) {
        break;
      }
    }
synchronized (this) {
      try {
        wait(500);
      }
 catch (      InterruptedException e) {
        logger.debug("abnormal behavior, may cause " + "unnecessary CPU use",e);
      }
    }
  }
}
