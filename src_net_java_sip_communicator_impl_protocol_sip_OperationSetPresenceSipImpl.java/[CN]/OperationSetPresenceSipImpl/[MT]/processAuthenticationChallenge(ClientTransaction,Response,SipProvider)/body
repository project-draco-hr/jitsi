{
  try {
    logger.debug("Authenticating a message request.");
    ClientTransaction retryTran=this.parentProvider.getSipSecurityManager().handleChallenge(response,clientTransaction,jainSipProvider);
    retryTran.sendRequest();
    return;
  }
 catch (  Exception exc) {
    logger.error("We failed to authenticate a message request.",exc);
    throw new OperationFailedException("Failed to authenticate" + "a message request",OperationFailedException.INTERNAL_ERROR,exc);
  }
}
