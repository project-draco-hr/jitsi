{
  String res=uriStr.trim();
  if (res.indexOf('@') == -1) {
    res=res + '@' + ((SipURI)this.parentProvider.getOurSipAddress().getURI()).getHost();
  }
  if (!res.toLowerCase().startsWith("sip:") && !res.toLowerCase().startsWith("pres:")) {
    res="sip:" + res;
  }
  Address uri=this.parentProvider.getAddressFactory().createAddress(res);
  return uri;
}
