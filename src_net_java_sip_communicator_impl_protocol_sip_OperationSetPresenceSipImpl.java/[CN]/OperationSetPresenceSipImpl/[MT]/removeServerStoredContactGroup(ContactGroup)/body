{
  ContactGroupSipImpl sipGroup=(ContactGroupSipImpl)group;
  ContactGroupSipImpl parent=this.contactListRoot.findGroupParent(sipGroup);
  if (parent == null) {
    throw new IllegalArgumentException("group " + group + " does not seem to belong to this protocol's contact list.");
  }
  parent.removeSubGroup(sipGroup);
  this.fireServerStoredGroupEvent(sipGroup,ServerStoredGroupEvent.GROUP_REMOVED_EVENT);
}
