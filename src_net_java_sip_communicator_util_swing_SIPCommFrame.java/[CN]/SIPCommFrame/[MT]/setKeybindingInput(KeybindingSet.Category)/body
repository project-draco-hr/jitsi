{
  if (this.bindings != null) {
    this.bindings.deleteObserver(this);
    resetInputMap();
  }
  this.bindings=UtilActivator.getKeybindingsService().getBindings(category);
  for (  Map.Entry<KeyStroke,String> key2action : this.bindings.getBindings().entrySet()) {
    imap.put(key2action.getKey(),key2action.getValue());
  }
  this.bindings.addObserver(this);
}
