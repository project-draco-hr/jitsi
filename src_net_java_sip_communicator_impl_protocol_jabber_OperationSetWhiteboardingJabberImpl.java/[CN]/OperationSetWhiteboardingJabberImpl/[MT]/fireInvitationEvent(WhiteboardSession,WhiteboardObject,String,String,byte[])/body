{
  WhiteboardInvitationJabberImpl invitation=new WhiteboardInvitationJabberImpl(targetWhiteboard,whiteboardObject,inviter,reason,password);
  WhiteboardInvitationReceivedEvent evt=new WhiteboardInvitationReceivedEvent(this,invitation,new Date(System.currentTimeMillis()));
  logger.debug("Dispatching a WhiteboardInvitation event to " + invitationListeners.size() + " listeners. event is: "+ evt.toString());
  Iterator listeners=null;
synchronized (invitationListeners) {
    listeners=new ArrayList(invitationListeners).iterator();
  }
  while (listeners.hasNext()) {
    WhiteboardInvitationListener listener=(WhiteboardInvitationListener)listeners.next();
    listener.invitationReceived(evt);
  }
}
