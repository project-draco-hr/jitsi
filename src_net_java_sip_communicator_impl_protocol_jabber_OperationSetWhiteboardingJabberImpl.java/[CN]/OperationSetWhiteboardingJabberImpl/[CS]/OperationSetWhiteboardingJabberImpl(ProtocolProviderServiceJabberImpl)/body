{
  this.jabberProvider=provider;
  provider.addRegistrationStateChangeListener(new RegistrationStateListener());
  ProviderManager pManager=ProviderManager.getInstance();
  pManager.addExtensionProvider(WhiteboardObjectPacketExtensionImpl.ELEMENT_NAME,WhiteboardObjectPacketExtensionImpl.NAMESPACE,new WhiteboardObjectJabberProvider());
}
