{
  if (!opiVidCap.setCompress("RGB"))   return;
  Dimension size=new Dimension(opiVidCap.getWidth(),opiVidCap.getHeight());
  addVideoFormat(new RGBFormat(size,Format.NOT_SPECIFIED,Format.byteArray,Format.NOT_SPECIFIED,16,0xF800,0x7E0,0x1F,2,Format.NOT_SPECIFIED,Format.FALSE,Format.NOT_SPECIFIED));
}
