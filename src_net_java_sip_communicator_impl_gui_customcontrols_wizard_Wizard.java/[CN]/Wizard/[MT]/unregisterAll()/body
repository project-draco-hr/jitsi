{
  Iterator i=wizardModel.getAllPages();
  while (i.hasNext()) {
    Map.Entry entry=(Map.Entry)i.next();
    Object id=entry.getKey();
    if (!id.equals(DEFAULT_PAGE_IDENTIFIER) && !id.equals(SUMMARY_PAGE_IDENTIFIER)) {
      cardPanel.remove((Component)((WizardPage)entry.getValue()).getWizardForm());
      wizardModel.unregisterPage(id);
    }
  }
}
