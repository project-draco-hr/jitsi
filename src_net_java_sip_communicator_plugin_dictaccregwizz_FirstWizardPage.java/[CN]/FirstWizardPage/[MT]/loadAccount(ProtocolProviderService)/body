{
  AccountID accountID=protocolProvider.getAccountID();
  String host=(String)accountID.getAccountProperties().get(ProtocolProviderFactory.SERVER_ADDRESS);
  String port=(String)accountID.getAccountProperties().get(ProtocolProviderFactory.SERVER_PORT);
  String strategy=(String)accountID.getAccountProperties().get(ProtocolProviderFactory.STRATEGY);
  this.initAccountID=accountID;
  this.hostField.setText(host);
  this.portField.setText(port);
  this.initstrategy=strategy;
  this.populateStrategies();
}
