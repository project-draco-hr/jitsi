{
  AccountID accountID=protocolProvider.getAccountID();
  String host=accountID.getAccountPropertyString(ProtocolProviderFactory.SERVER_ADDRESS);
  String port=accountID.getAccountPropertyString(ProtocolProviderFactory.SERVER_PORT);
  String strategy=accountID.getAccountPropertyString(ProtocolProviderFactory.STRATEGY);
  this.initAccountID=accountID;
  this.hostField.setText(host);
  this.portField.setText(port);
  this.initstrategy=strategy;
  this.populateStrategies();
}
