{
  ProtocolProviderFactory factory=DictAccRegWizzActivator.getDictProtocolProviderFactory();
  String accHost;
  int accPort;
  for (  AccountID accountID : factory.getRegisteredAccounts()) {
    accHost=accountID.getAccountPropertyString(ProtocolProviderFactory.SERVER_ADDRESS);
    if (host.equalsIgnoreCase(accHost)) {
      accPort=Integer.parseInt(accountID.getAccountPropertyString(ProtocolProviderFactory.SERVER_PORT));
      if (port == accPort) {
        return true;
      }
    }
  }
  return false;
}
