{
  ProtocolProviderFactory factory=DictAccRegWizzActivator.getDictProtocolProviderFactory();
  ArrayList registeredAccounts=factory.getRegisteredAccounts();
  String accHost;
  int accPort;
  for (int i=0; i < registeredAccounts.size(); i++) {
    AccountID accountID=(AccountID)registeredAccounts.get(i);
    accHost=(String)accountID.getAccountProperties().get(ProtocolProviderFactory.SERVER_ADDRESS);
    if (host.equalsIgnoreCase(accHost)) {
      accPort=Integer.parseInt((String)accountID.getAccountProperties().get(ProtocolProviderFactory.SERVER_PORT));
      if (port == accPort) {
        return true;
      }
    }
  }
  return false;
}
