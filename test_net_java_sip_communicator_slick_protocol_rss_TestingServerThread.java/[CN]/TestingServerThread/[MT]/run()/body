{
  if (launcher.isActive()) {
    try {
      socket=serverSocket.accept();
      BufferedReader in=new BufferedReader(new InputStreamReader(socket.getInputStream()));
      processRequest(in);
      launcher.stop();
    }
 catch (    IOException e) {
      e.printStackTrace();
      socket=null;
    }
catch (    InterruptedException ie) {
      ie.printStackTrace();
    }
  }
}
