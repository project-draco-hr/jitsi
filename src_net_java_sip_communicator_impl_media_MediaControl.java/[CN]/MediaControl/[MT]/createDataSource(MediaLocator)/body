{
  try {
    if (logger.isInfoEnabled())     logger.info("Creating datasource for:" + ((locator != null) ? locator.toExternalForm() : "null"));
    return Manager.createDataSource(locator);
  }
 catch (  NoDataSourceException ex) {
    logger.error("Could not create data source for " + ((locator != null) ? locator.toExternalForm() : "null"),ex);
    return null;
  }
catch (  IOException ex) {
    logger.error("Could not create data source for " + ((locator != null) ? locator.toExternalForm() : "null"),ex);
    return null;
  }
}
