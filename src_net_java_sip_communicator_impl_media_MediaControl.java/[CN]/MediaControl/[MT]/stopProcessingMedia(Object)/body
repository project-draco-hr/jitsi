{
  if (sourceProcessor == null)   return;
  if (sourceProcessor.getState() == Processor.Started) {
    avDataSource.disconnect();
    sourceProcessor.stop();
    try {
      initCaptureDevices();
    }
 catch (    Exception e) {
      logger.error("Error initing media processor.",e);
    }
  }
  processorReaders.remove(reader);
}
