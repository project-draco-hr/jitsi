{
  PhoneNumberUtil numberUtil=PhoneNumberUtil.getInstance();
  try {
    if (numberUtil.isPossibleNumber(possibleNumber,null))     return true;
 else     return numberUtil.isPossibleNumber(numberUtil.parse(possibleNumber,Locale.getDefault().getCountry()));
  }
 catch (  NumberParseException e) {
    return false;
  }
}
