{
  String plusSign="+";
  boolean plusSignFound=false;
  if (phoneNumber.startsWith(plusSign))   plusSignFound=true;
  String normalizedNumber=PhoneNumberUtil.normalizeDigitsOnly(PhoneNumberUtil.convertAlphaCharactersInNumber(phoneNumber));
  if (plusSignFound && !normalizedNumber.startsWith(plusSign))   return plusSign + normalizedNumber;
  return normalizedNumber;
}
