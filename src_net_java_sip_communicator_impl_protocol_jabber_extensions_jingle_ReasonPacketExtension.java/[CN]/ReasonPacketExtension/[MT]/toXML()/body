{
  StringBuilder bldr=new StringBuilder("<" + getElementName() + ">");
  bldr.append("<" + getReason() + "/>");
  if (getText() != null) {
    bldr.append("<text>");
    bldr.append(getText());
    bldr.append("</text>");
  }
  if (getOtherExtension() != null) {
    bldr.append(getOtherExtension().toXML());
  }
  bldr.append("</" + getElementName() + ">");
  return bldr.toString();
}
