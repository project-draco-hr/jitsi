{
  Image protocolImage=ImageLoader.getBytesInImage(protocolProvider.getProtocolIcon().getIcon(ProtocolIcon.ICON_SIZE_32x32));
  if (protocolImage != null) {
    return new ImageIcon(protocolImage);
  }
 else {
    protocolImage=ImageLoader.getBytesInImage(protocolProvider.getProtocolIcon().getIcon(ProtocolIcon.ICON_SIZE_48x48));
    if (protocolImage != null) {
      return ImageUtils.scaleIconWithinBounds(protocolImage,32,32);
    }
 else {
      protocolImage=ImageLoader.getBytesInImage(protocolProvider.getProtocolIcon().getIcon(ProtocolIcon.ICON_SIZE_64x64));
      if (protocolImage != null) {
        return ImageUtils.scaleIconWithinBounds(protocolImage,32,32);
      }
    }
  }
  return null;
}
