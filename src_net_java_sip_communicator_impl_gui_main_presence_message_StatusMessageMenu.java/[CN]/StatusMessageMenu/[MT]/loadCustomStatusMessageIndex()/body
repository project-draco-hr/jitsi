{
  java.util.List<String> customMessagesProps=GuiActivator.getConfigurationService().getPropertyNamesByPrefix(CUSTOM_MESSAGES_PREFIX,false);
  if (customMessagesProps.size() > 0) {
    this.addSeparator();
  }
  for (  String p : customMessagesProps) {
    String message=GuiActivator.getConfigurationService().getString(p);
    CustomMessageItems newMenuItem=new CustomMessageItems(message);
    newMenuItem.setName(message);
    newMenuItem.addActionListener(this);
    this.add(newMenuItem);
  }
}
