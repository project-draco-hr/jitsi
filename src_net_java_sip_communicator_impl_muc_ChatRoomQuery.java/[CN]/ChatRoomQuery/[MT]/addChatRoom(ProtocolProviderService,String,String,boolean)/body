{
  if (queryString == null || ((chatRoomName.startsWith(queryString) || chatRoomID.startsWith(queryString)))) {
    ChatRoomSourceContact contact=new ChatRoomSourceContact(chatRoomName,chatRoomID,this,pps);
synchronized (contactResults) {
      contactResults.add(contact);
    }
    if (addQueryResult) {
      addQueryResult(contact);
    }
 else {
      fireContactReceived(contact);
    }
  }
}
