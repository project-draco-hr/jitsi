{
  if (queryString == null || ((room.getName().startsWith(queryString) || room.getIdentifier().startsWith(queryString)))) {
    ChatRoomSourceContact contact=new ChatRoomSourceContact(room,this);
synchronized (contactResults) {
      contactResults.add(contact);
    }
    if (addQueryResult) {
      addQueryResult(contact);
    }
 else {
      fireContactReceived(contact);
    }
  }
}
