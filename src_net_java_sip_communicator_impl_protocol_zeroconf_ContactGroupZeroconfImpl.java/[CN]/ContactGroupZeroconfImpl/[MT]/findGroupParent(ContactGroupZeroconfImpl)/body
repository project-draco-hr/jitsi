{
  if (subGroups.contains(zeroconfGroup))   return this;
  Iterator subGroupsIter=subgroups();
  while (subGroupsIter.hasNext()) {
    ContactGroupZeroconfImpl subgroup=(ContactGroupZeroconfImpl)subGroupsIter.next();
    ContactGroupZeroconfImpl parent=subgroup.findGroupParent(zeroconfGroup);
    if (parent != null)     return parent;
  }
  return null;
}
