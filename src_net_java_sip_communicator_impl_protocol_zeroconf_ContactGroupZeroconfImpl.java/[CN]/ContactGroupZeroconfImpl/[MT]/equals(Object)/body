{
  if (obj == null || !(obj instanceof ContactGroupZeroconfImpl))   return false;
  ContactGroupZeroconfImpl zeroconfGroup=(ContactGroupZeroconfImpl)obj;
  if (!zeroconfGroup.getGroupName().equals(getGroupName()) || !zeroconfGroup.getUID().equals(getUID()) || zeroconfGroup.countContacts() != countContacts() || zeroconfGroup.countSubgroups() != countSubgroups())   return false;
  Iterator theirContacts=zeroconfGroup.contacts();
  while (theirContacts.hasNext()) {
    ContactZeroconfImpl theirContact=(ContactZeroconfImpl)theirContacts.next();
    ContactZeroconfImpl ourContact=(ContactZeroconfImpl)getContact(theirContact.getAddress());
    if (ourContact == null || !ourContact.equals(theirContact))     return false;
  }
  Iterator theirSubgroups=zeroconfGroup.subgroups();
  while (theirSubgroups.hasNext()) {
    ContactGroupZeroconfImpl theirSubgroup=(ContactGroupZeroconfImpl)theirSubgroups.next();
    ContactGroupZeroconfImpl ourSubgroup=(ContactGroupZeroconfImpl)getGroup(theirSubgroup.getGroupName());
    if (ourSubgroup == null || !ourSubgroup.equals(theirSubgroup))     return false;
  }
  return true;
}
