{
  super(new BorderLayout());
  this.wizard=wizard;
  this.setPreferredSize(new Dimension(600,500));
  mainPanel.setLayout(new BoxLayout(mainPanel,BoxLayout.Y_AXIS));
  this.setBorder(BorderFactory.createEmptyBorder(10,10,10,10));
