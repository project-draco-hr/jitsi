{
  if (isThisChatRoom(msg.getChannelName())) {
    String user=msg.getSource().getNick();
    if (IrcStack.this.getNick().equals(msg.getSource().getNick())) {
    }
 else {
      ChatRoomMemberIrcImpl member=new ChatRoomMemberIrcImpl(IrcStack.this.provider,this.chatroom,user,ChatRoomMemberRole.SILENT_MEMBER);
      this.chatroom.fireMemberPresenceEvent(member,null,ChatRoomMemberPresenceChangeEvent.MEMBER_JOINED,null);
    }
  }
}
