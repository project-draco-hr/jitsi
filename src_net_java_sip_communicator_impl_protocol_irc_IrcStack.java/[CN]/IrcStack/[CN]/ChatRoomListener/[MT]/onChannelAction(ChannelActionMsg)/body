{
  if (!isThisChatRoom(msg.getChannelName())) {
    return;
  }
  String userNick=msg.getSource().getNick();
  ChatRoomMemberIrcImpl member=new ChatRoomMemberIrcImpl(IrcStack.this.provider,this.chatroom,userNick,ChatRoomMemberRole.MEMBER);
  MessageIrcImpl message=MessageIrcImpl.newActionFromIRC(member,msg.getText());
  this.chatroom.fireMessageReceivedEvent(message,member,new Date(),ChatRoomMessageReceivedEvent.CONVERSATION_MESSAGE_RECEIVED);
}
