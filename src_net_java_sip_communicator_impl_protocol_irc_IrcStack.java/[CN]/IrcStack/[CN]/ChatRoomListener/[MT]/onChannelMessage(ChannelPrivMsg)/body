{
  if (isThisChatRoom(msg.getChannelName()) == false)   return;
  MessageIrcImpl message=new MessageIrcImpl(msg.getText(),"text/plain","UTF-8",null);
  ChatRoomMemberIrcImpl member=new ChatRoomMemberIrcImpl(IrcStack.this.provider,this.chatroom,msg.getSource().getNick(),ChatRoomMemberRole.MEMBER);
  this.chatroom.fireMessageReceivedEvent(message,member,new Date(),ChatRoomMessageReceivedEvent.CONVERSATION_MESSAGE_RECEIVED);
}
