{
  final IRCApi irc=IrcStack.this.session.get();
  if (irc != null) {
    irc.deleteListener(this);
  }
  IrcStack.this.joined.remove(this.chatroom.getIdentifier());
  IrcStack.this.provider.getMUC().fireLocalUserPresenceEvent(this.chatroom,LocalUserChatRoomPresenceChangeEvent.LOCAL_USER_LEFT,null);
}
