{
  System.out.printf("Received private message from: %s",msg.getSource().getNick());
  String sourceNick=msg.getSource().getNick();
  ChatRoomMember source=this.chatroom.getChatRoomMember(sourceNick);
  if (source != null) {
    MessageIrcImpl message=new MessageIrcImpl("PRIVATE: " + msg.getText(),"text/plain","UTF-8",null);
    this.chatroom.fireMessageReceivedEvent(message,source,new Date(),ChatRoomMessageReceivedEvent.ACTION_MESSAGE_RECEIVED);
  }
}
