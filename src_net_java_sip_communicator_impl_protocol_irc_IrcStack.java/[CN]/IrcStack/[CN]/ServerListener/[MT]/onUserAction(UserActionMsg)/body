{
  final String user=msg.getSource().getNick();
  final String text=Utils.parse(msg.getText());
  MessageIrcImpl message=new MessageIrcImpl(text,MessageIrcImpl.DEFAULT_MIME_TYPE,MessageIrcImpl.DEFAULT_MIME_ENCODING,"");
  Contact from=IrcStack.this.provider.getPersistentPresence().findContactByID(user);
  IrcStack.this.provider.getBasicInstantMessaging().fireMessageReceived(message,from);
}
