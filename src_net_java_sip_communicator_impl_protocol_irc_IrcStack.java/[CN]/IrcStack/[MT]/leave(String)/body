{
  if (!isConnected()) {
    throw new IllegalStateException("Not connected to an IRC server.");
  }
  final IRCApi irc=this.session.get();
  if (irc == null) {
    return;
  }
  try {
    irc.leaveChannel(chatRoomName);
  }
 catch (  ApiException e) {
    LOGGER.warn("exception occurred while leaving channel",e);
  }
}
