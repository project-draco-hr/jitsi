{
  logger.trace("KICK on " + channel + ": Received from "+ kickerNick+ " "+ kickerLogin+ "@"+ kickerHostname);
  ChatRoomIrcImpl chatRoom=(ChatRoomIrcImpl)ircMUCOpSet.getChatRoom(channel);
  if (chatRoom == null || !chatRoom.isJoined())   return;
  if (chatRoom.getUserNickname().equals(kickerNick)) {
    notifyChatRoomOperation(0);
  }
  if (chatRoom.getUserNickname().equals(recipientNick))   ircMUCOpSet.fireLocalUserPresenceEvent(chatRoom,LocalUserChatRoomPresenceChangeEvent.LOCAL_USER_KICKED,reason);
 else {
    ChatRoomMember member=chatRoom.getChatRoomMember(recipientNick);
    ChatRoomMember actorMember=chatRoom.getChatRoomMember(kickerNick);
    chatRoom.removeChatRoomMember(recipientNick);
    chatRoom.fireMemberPresenceEvent(member,actorMember,ChatRoomMemberPresenceChangeEvent.MEMBER_KICKED,reason);
  }
}
