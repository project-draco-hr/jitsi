{
  if (logger.isDebugEnabled())   logger.debug("DEVOICE on " + channel + ": Received from "+ sourceNick+ " "+ sourceLogin+ "@"+ sourceHostname+ "on "+ recipient);
  ChatRoomIrcImpl chatRoom=ircMUCOpSet.getChatRoom(channel);
  if (chatRoom == null || !chatRoom.isJoined())   return;
  ChatRoomMember sourceMember=chatRoom.getChatRoomMember(sourceNick);
  if (sourceMember == null)   return;
  chatRoom.fireMemberRoleEvent(sourceMember,ChatRoomMemberRole.SILENT_MEMBER);
}
