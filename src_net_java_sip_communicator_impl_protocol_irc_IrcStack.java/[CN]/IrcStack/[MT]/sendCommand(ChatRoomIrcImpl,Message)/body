{
  if (message.getContent().startsWith("/me")) {
    this.sendAction(chatRoom.getName(),message.getContent().substring(3));
  }
 else   if (message.getContent().startsWith("/notice")) {
    this.sendNotice(chatRoom.getName(),message.getContent().substring(7));
  }
 else {
    String command=message.getContent();
    if (command.startsWith("/msg"))     command=command.replaceFirst("/msg","/PRIVMSG");
    this.sendRawLine(command.substring(1));
  }
}
