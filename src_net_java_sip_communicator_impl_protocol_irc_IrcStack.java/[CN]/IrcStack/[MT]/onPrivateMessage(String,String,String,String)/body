{
  logger.trace("PRIVATE MESSAGE received from " + sender + " "+ login+ "@"+ hostname+ " the message: "+ messageContent);
  String senderContactAddress=sender + "." + login+ "@"+ hostname;
  MessageIrcImpl message=new MessageIrcImpl(messageContent,MessageIrcImpl.DEFAULT_MIME_TYPE,MessageIrcImpl.DEFAULT_MIME_ENCODING,null);
  ChatRoomIrcImpl chatRoom=ircMUCOpSet.findPrivateChatRoom(senderContactAddress);
  if (chatRoom == null || !chatRoom.isJoined())   return;
  ChatRoomMember sourceMember=chatRoom.getChatRoomMember(sender);
  if (sourceMember == null)   sourceMember=new ChatRoomMemberIrcImpl(parentProvider,chatRoom,sender,login,hostname,ChatRoomMemberRole.GUEST);
  chatRoom.fireMessageReceivedEvent(message,sourceMember,new Date(System.currentTimeMillis()),ChatRoomMessageReceivedEvent.CONVERSATION_MESSAGE_RECEIVED);
}
