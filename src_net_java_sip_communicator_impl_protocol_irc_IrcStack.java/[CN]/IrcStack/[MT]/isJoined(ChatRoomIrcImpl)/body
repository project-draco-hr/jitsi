{
  if (ircMUCOpSet.getSystemRoom() != null && ircMUCOpSet.getSystemRoom().equals(chatRoom))   return isConnected();
  if (this.isConnected()) {
    String[] channels=this.getChannels();
    for (int i=0; i < channels.length; i++) {
      if (chatRoom.getName().equals(channels[i])) {
        return true;
      }
    }
    return false;
  }
 else {
    return false;
  }
}
