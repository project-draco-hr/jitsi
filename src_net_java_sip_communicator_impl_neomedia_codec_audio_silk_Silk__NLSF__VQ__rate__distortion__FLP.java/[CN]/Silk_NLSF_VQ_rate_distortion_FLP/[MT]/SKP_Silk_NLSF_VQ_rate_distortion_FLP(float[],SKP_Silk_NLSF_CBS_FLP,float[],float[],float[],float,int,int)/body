{
  float[] pRD_vec;
  int pRD_vec_offset;
  int i, n;
  Silk_NLSF_VQ_sum_error_FLP.SKP_Silk_NLSF_VQ_sum_error_FLP(pRD,in,w,psNLSF_CBS_FLP.CB,N,psNLSF_CBS_FLP.nVectors,LPC_order);
  pRD_vec=pRD;
  pRD_vec_offset=0;
  for (n=0; n < N; n++) {
    for (i=0; i < psNLSF_CBS_FLP.nVectors; i++) {
      pRD_vec[pRD_vec_offset + i]+=mu * (rate_acc[n] + psNLSF_CBS_FLP.Rates[i]);
    }
    pRD_vec_offset+=psNLSF_CBS_FLP.nVectors;
  }
}
