{
  Vector<?> managerSendStreams=manager.getSendStreams();
  Vector<SendStream> sendStreams=null;
  if (managerSendStreams != null) {
    sendStreams=new Vector<SendStream>(managerSendStreams.size());
    for (    SendStreamDesc sendStreamDesc : this.sendStreams)     if (managerSendStreams.contains(sendStreamDesc.sendStream)) {
      SendStream sendStream=sendStreamDesc.getSendStream(streamRTPManager,false);
      if (sendStream != null)       sendStreams.add(sendStream);
    }
  }
  return sendStreams;
}
