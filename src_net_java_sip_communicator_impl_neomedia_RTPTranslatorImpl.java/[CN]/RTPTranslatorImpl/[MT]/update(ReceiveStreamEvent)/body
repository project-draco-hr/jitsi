{
  if (event != null) {
    ReceiveStream receiveStream=event.getReceiveStream();
    if (receiveStream != null) {
      StreamRTPManagerDesc streamRTPManagerDesc=findStreamRTPManagerDescByReceiveSSRC(receiveStream.getSSRC(),null);
      if (streamRTPManagerDesc != null)       for (      ReceiveStreamListener listener : streamRTPManagerDesc.getReceiveStreamListeners())       listener.update(event);
    }
  }
}
