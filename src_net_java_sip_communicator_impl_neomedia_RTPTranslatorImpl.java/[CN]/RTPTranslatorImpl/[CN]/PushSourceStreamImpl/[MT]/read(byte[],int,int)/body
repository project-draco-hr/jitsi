{
  PushSourceStreamDesc streamDesc;
  int read;
synchronized (this) {
    streamDesc=streamToReadFrom;
    read=(streamDesc == null) ? 0 : streamDesc.stream.read(buffer,offset,length);
  }
  if (read > 0) {
    read=RTPTranslatorImpl.this.read(streamDesc,buffer,offset,length,read);
  }
  return read;
}
