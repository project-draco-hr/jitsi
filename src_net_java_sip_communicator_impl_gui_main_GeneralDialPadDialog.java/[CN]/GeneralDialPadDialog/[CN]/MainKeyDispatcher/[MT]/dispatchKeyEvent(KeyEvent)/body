{
  if (!dialPadDialog.isFocused() || (e.getID() != KeyEvent.KEY_TYPED))   return false;
  if (e.getKeyChar() == KeyEvent.CHAR_UNDEFINED || e.getKeyCode() == KeyEvent.VK_ENTER || e.getKeyCode() == KeyEvent.VK_DELETE || e.getKeyCode() == KeyEvent.VK_BACK_SPACE || e.getKeyCode() == KeyEvent.VK_TAB || e.getKeyCode() == KeyEvent.VK_SPACE) {
    return false;
  }
  if (!callField.isFocusOwner() && keyManager.getFocusOwner() != null) {
    callField.requestFocusInWindow();
    keyManager.redispatchEvent(callField,e);
    return true;
  }
  return false;
}
