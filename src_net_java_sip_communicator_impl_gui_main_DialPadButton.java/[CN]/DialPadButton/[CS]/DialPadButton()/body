{
  super("");
  loadSkin();
  setBorder(BorderFactory.createEmptyBorder(0,0,0,0));
  addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
      if (dialPad == null)       dialPad=new GeneralDialPadDialog();
      dialPad.clear();
      dialPad.setVisible(true);
    }
  }
);
}
