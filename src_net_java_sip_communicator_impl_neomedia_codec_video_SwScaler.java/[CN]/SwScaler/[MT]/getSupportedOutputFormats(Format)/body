{
  if (input == null) {
    return supportedOutputFormats;
  }
  if (((VideoFormat)supportedOutputFormats[0]).getSize() != null) {
    return supportedOutputFormats;
  }
  return new Format[]{new YUVFormat(((VideoFormat)input).getSize(),-1,Format.byteArray,-1.0f,YUVFormat.YUV_420,-1,-1,0,-1,-1)};
}
