{
  PluginComponent c=event.getPluginComponent();
  if (!c.getContainer().equals(Container.CONTAINER_CONTACT_RIGHT_BUTTON_MENU))   return;
  Object constraints=UIServiceImpl.getBorderLayoutConstraintsFromContainer(c.getConstraints());
  this.add((Component)c.getComponent(),constraints);
  c.setCurrentContact(contactItem);
  this.repaint();
}
