{
  OutputPortAudioStream out=new OutputPortAudioStream(deviceIndex,sampleRate,channels);
  outputStreams.add(out);
  boolean echoCancelIsEnabled=isEnabledEchoCancel();
  if (echoCancelIsEnabled && (inputStreams.size() > 0)) {
    MasterPortAudioStream st=inputStreams.values().iterator().next();
    st.setParams(out,echoCancelIsEnabled,isEnabledDeNoise(),getFramesPerBuffer(),getFilterLength());
  }
  return out;
}
