{
  Iterator<Map.Entry<CallParticipant,CallParticipantPanel>> participants=participantsPanels.entrySet().iterator();
  while (participants.hasNext()) {
    Map.Entry<CallParticipant,CallParticipantPanel> participantEntry=participants.next();
    CallParticipant entryParticipant=participantEntry.getKey();
    if ((entryParticipant != null) && entryParticipant.equals(participant)) {
      return participantEntry.getValue();
    }
  }
  return null;
}
