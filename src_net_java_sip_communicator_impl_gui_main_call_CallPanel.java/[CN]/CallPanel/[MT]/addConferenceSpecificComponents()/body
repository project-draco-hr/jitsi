{
  settingsPanel.add(localLevel);
  settingsPanel.add(remoteLevel);
  Iterator<? extends CallPeer> callPeers=call.getCallPeers();
  while (callPeers.hasNext()) {
    if (callPeers.next().getState() == CallPeerState.CONNECTED) {
      enableButtons(true);
      return;
    }
  }
  enableButtons(false);
}
