{
  Vector<Contact> contactVector=new Vector<Contact>(call.getCallPeerCount());
  Iterator<? extends CallPeer> callPeers=call.getCallPeers();
  CallPeer callPeer;
  Contact contact;
  if (call.getProtocolProvider().getOperationSet(OperationSetBasicInstantMessaging.class) != null) {
    while (callPeers.hasNext()) {
      callPeer=callPeers.next();
      contact=callPeer.getContact();
      if (callPeer.getProtocolProvider().getOperationSet(OperationSetBasicInstantMessaging.class) != null && contact != null) {
        contactVector.add(contact);
      }
    }
  }
  return contactVector;
}
