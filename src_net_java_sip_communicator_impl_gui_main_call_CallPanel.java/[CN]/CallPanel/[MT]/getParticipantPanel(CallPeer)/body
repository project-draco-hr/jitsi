{
  for (  Map.Entry<CallPeer,CallParticipantPanel> participantEntry : participantsPanels.entrySet()) {
    CallPeer entryParticipant=participantEntry.getKey();
    if ((entryParticipant != null) && entryParticipant.equals(participant)) {
      return participantEntry.getValue();
    }
  }
  return null;
}
