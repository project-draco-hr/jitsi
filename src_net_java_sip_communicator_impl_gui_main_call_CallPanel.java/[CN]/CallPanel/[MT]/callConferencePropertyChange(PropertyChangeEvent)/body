{
  if (CallConference.CALLS.equals(ev.getPropertyName())) {
    Object oldValue=ev.getOldValue();
    if (oldValue instanceof Call) {
      Iterator<? extends CallPeer> oldPeerIter=((Call)oldValue).getCallPeers();
      while (oldPeerIter.hasNext())       oldPeerIter.next().removeCallPeerConferenceListener(this);
    }
    Object newValue=ev.getNewValue();
    if (newValue instanceof Call) {
      Iterator<? extends CallPeer> newPeerIter=((Call)newValue).getCallPeers();
      while (newPeerIter.hasNext())       newPeerIter.next().addCallPeerConferenceListener(this);
    }
  }
}
