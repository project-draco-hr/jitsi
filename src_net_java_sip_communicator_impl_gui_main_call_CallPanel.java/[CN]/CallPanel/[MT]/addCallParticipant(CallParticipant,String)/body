{
  CallParticipantPanel participantPanel=getParticipantPanel(participant);
  if (participantPanel == null) {
    participantPanel=new CallParticipantPanel(callManager,participant);
    this.mainPanel.add(participantPanel);
    this.mainPanel.addContainerListener(participantPanel);
    participantPanel.setCallType(callType);
    this.participantsPanels.put(new CallParticipantWrapper(participant),participantPanel);
  }
  return participantPanel;
}
