{
  CallParticipantPanel participantPanel=getParticipantPanel(participantName);
  if (participantPanel == null) {
    participantPanel=new CallParticipantPanel(callManager,participantName);
    this.mainPanel.add(participantPanel);
    this.mainPanel.addContainerListener(participantPanel);
    participantPanel.setCallType(callType);
    this.participantsPanels.put(new CallParticipantWrapper(participantName),participantPanel);
  }
  return participantPanel;
}
