{
  CallParticipant participant=(CallParticipant)securityEvent.getSource();
  CallParticipantPanel participantPanel=getParticipantPanel(participant);
  participantPanel.setStateIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.SECURE_BUTTON_OFF)));
  if (securityEvent.getSessionType().equals(CallParticipantSecurityOnEvent.AUDIO_SESSION)) {
    participantPanel.setAudioSecurityOn(false);
  }
 else   if (securityEvent.getSessionType().equals(CallParticipantSecurityOnEvent.VIDEO_SESSION)) {
    participantPanel.setVideoSecurityOn(false);
  }
}
