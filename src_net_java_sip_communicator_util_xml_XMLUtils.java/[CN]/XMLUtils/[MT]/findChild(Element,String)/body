{
  if (parent == null || tagName == null)   throw new NullPointerException("Parent or tagname were null! " + "parent = " + parent + "; tagName = "+ tagName);
  NodeList nodes=parent.getChildNodes();
  Node node;
  int len=nodes.getLength();
  for (int i=0; i < len; i++) {
    node=nodes.item(i);
    if (node.getNodeType() == Node.ELEMENT_NODE && ((Element)node).getNodeName().equals(tagName))     return (Element)node;
  }
  return null;
}
