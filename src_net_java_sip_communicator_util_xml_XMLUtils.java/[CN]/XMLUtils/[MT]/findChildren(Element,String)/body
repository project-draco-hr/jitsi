{
  if (parent == null || tagName == null)   throw new NullPointerException("Parent or tagname were null! " + "parent = " + parent + "; tagName = "+ tagName);
  List<Element> result=new ArrayList<Element>();
  NodeList nodes=parent.getChildNodes();
  Node node;
  int len=nodes.getLength();
  for (int i=0; i < len; i++) {
    node=nodes.item(i);
    if (node.getNodeType() == Node.ELEMENT_NODE) {
      Element element=(Element)node;
      if (element.getNodeName().equals(tagName))       result.add(element);
    }
  }
  return result;
}
