{
  Enumeration<ChatRoomMsnImpl> chatRooms=chatRoomCache.elements();
  while (chatRooms.hasMoreElements()) {
    ChatRoomMsnImpl chatRoom=chatRooms.nextElement();
    ChatRoomMemberMsnImpl member=chatRoom.findMemberForAddress(contact.getAddress());
    if (member != null) {
      member.setContact(contact);
      if (contact != null)       member.setAvatar(contact.getImage());
    }
  }
}
