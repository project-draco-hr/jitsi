{
  this.msnProvider=msnProvider;
  msnProvider.addRegistrationStateChangeListener(new RegistrationStateListener());
  OperationSetPersistentPresence presenceOpSet=(OperationSetPersistentPresence)msnProvider.getOperationSet(OperationSetPersistentPresence.class);
  presenceOpSet.addSubscriptionListener(this);
}
