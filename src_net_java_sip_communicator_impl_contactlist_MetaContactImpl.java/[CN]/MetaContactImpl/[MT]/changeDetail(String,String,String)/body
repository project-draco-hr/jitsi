{
  ArrayList values=(ArrayList)details.get(name);
  if (values == null)   return;
  int changedIx=-1;
  for (int i=0; i < values.size(); i++) {
    if (values.get(i).equals(oldValue)) {
      changedIx=i;
      break;
    }
  }
  if (changedIx == -1)   return;
  values.set(changedIx,newValue);
  mclServiceImpl.fireMetaContactEvent(new MetaContactModifiedEvent(this,name,oldValue,newValue));
}
