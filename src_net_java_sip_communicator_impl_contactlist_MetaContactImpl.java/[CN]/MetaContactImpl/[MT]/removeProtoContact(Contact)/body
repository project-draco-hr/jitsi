{
synchronized (parentGroupModLock) {
    if (parentGroup != null) {
      parentGroup.lightRemoveMetaContact(this);
    }
    totalStatus-=contact.getPresenceStatus().getStatus();
    this.protoContacts.remove(contact);
    if (parentGroup != null) {
      parentGroup.lightAddMetaContact(this);
    }
  }
}
