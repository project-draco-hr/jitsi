{
  if (!isLazy)   return getAvatar();
  Iterator iter=this.getContacts();
  while (iter.hasNext()) {
    Contact protoContact=(Contact)iter.next();
    String avatarPath=AVATAR_DIR + "/" + protoContact.getProtocolProvider().getProtocolName()+ "/"+ escapeSpecialCharacters(protoContact.getAddress());
    return getLocalAvatar(avatarPath);
  }
  return null;
}
