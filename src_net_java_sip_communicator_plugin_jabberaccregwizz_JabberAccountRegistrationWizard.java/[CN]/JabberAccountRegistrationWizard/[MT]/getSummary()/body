{
  Hashtable summaryTable=new Hashtable();
  summaryTable.put("User ID",registration.getUserID());
  summaryTable.put("Remember password",new Boolean(registration.isRememberPassword()));
  summaryTable.put("Server address",registration.getServerAddress());
  summaryTable.put("Server port",String.valueOf(registration.getPort()));
  summaryTable.put("Keep alive",String.valueOf(registration.isSendKeepAlive()));
  summaryTable.put("Resource",registration.getResource());
  summaryTable.put("Priority",String.valueOf(registration.getPriority()));
  return summaryTable.entrySet().iterator();
}
