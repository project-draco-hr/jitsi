{
  MediaLocator locator=null;
  try {
    if (stream == null) {
      locator=getLocator();
      stream=new DSAudioStream(locator);
    }
  }
 catch (  Exception e) {
    stream=null;
    logger.error("Failed to create DSAudioStream from locator " + locator,e);
  }
  return (stream == null) ? EMPTY_STREAMS : new PullBufferStream[]{stream};
}
