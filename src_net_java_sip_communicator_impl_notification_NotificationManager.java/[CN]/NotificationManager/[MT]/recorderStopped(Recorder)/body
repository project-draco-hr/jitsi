{
  try {
    NotificationManager.fireNotification(NotificationManager.CALL_SAVED,NotificationActivator.getResources().getI18NString("plugin.callrecordingconfig.CALL_SAVED"),NotificationActivator.getResources().getI18NString("plugin.callrecordingconfig.CALL_SAVED_TO",new String[]{recorder.getFilename()}));
  }
 catch (  Throwable t) {
    logger.error("Error notifying for recorder stopped",t);
  }
}
