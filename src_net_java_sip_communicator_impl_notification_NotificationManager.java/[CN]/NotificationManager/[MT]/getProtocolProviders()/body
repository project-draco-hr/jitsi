{
  ServiceReference[] serRefs=null;
  try {
    serRefs=NotificationActivator.bundleContext.getServiceReferences(ProtocolProviderService.class.getName(),null);
  }
 catch (  InvalidSyntaxException e) {
    logger.error("NotificationManager : " + e);
  }
  List<ProtocolProviderService> providersList=new ArrayList<ProtocolProviderService>();
  if (serRefs != null) {
    for (    ServiceReference serRef : serRefs) {
      ProtocolProviderService pp=(ProtocolProviderService)NotificationActivator.bundleContext.getService(serRef);
      providersList.add(pp);
    }
  }
  return providersList;
}
