{
  try {
    String title=NotificationActivator.getResources().getI18NString("service.gui.MSG_RECEIVED",new String[]{evt.getSourceContact().getDisplayName()});
    fireChatNotification(evt.getSourceContact(),NotificationService.INCOMING_MESSAGE,title,evt.getSourceMessage().getContent());
  }
 catch (  Throwable t) {
    logger.error("Error notifying for message received",t);
  }
}
