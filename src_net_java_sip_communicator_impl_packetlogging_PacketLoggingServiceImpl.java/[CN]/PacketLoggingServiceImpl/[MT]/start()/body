{
  limit=PacketLoggingActivator.getConfigurationService().getLong(PacketLoggingActivator.PACKET_LOGGING_FILE_SIZE_PROPERTY_NAME,limit);
  logfileCount=PacketLoggingActivator.getConfigurationService().getInt(PacketLoggingActivator.PACKET_LOGGING_FILE_COUNT_PROPERTY_NAME,logfileCount);
  saverThread.start();
}
