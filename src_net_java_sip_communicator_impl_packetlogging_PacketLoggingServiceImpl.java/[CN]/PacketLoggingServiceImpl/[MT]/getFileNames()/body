{
  files=new File[getConfiguration().getLogfileCount()];
  for (int i=0; i < getConfiguration().getLogfileCount(); i++) {
    files[i]=PacketLoggingActivator.getFileAccessService().getPrivatePersistentFile(PacketLoggingActivator.LOGGING_DIR_NAME + File.separator + "sip-communicator"+ i+ ".pcap");
  }
}
