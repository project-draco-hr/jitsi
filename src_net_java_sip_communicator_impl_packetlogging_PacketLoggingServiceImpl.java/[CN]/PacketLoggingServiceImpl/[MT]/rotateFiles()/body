{
  if (outputStream != null) {
    outputStream.flush();
    outputStream.close();
  }
  for (int i=getLogfileCount() - 2; i >= 0; i--) {
    File f1=files[i];
    File f2=files[i + 1];
    if (f1.exists()) {
      if (f2.exists()) {
        f2.delete();
      }
      f1.renameTo(f2);
    }
  }
  outputStream=new FileOutputStream(files[0]);
  written=0;
  createGlobalHeader();
}
