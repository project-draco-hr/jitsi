{
switch (protocol) {
case SIP:
    return PacketLoggingActivator.isGlobalLoggingEnabled() && PacketLoggingActivator.isSipLoggingEnabled();
case JABBER:
  return PacketLoggingActivator.isGlobalLoggingEnabled() && PacketLoggingActivator.isJabberLoggingEnabled();
case RTP:
return PacketLoggingActivator.isGlobalLoggingEnabled() && PacketLoggingActivator.isRTPLoggingEnabled();
default :
return false;
}
}
