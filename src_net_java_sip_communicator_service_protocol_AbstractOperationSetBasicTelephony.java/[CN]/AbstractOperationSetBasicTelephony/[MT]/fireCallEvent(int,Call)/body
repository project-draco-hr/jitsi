{
  CallEvent cEvent=new CallEvent(sourceCall,eventID);
  logger.debug("Dispatching a CallEvent to " + callListeners.size() + " listeners. event is: "+ cEvent.toString());
  Iterator listeners=null;
synchronized (callListeners) {
    listeners=new ArrayList(callListeners).iterator();
  }
  while (listeners.hasNext()) {
    CallListener listener=(CallListener)listeners.next();
    if (eventID == CallEvent.CALL_INITIATED)     listener.outgoingCallCreated(cEvent);
 else     if (eventID == CallEvent.CALL_RECEIVED)     listener.incomingCallReceived(cEvent);
 else     if (eventID == CallEvent.CALL_ENDED)     listener.callEnded(cEvent);
  }
}
