{
  GroupNode groupNode;
synchronized (uiGroup) {
    groupNode=new GroupNode(treeModel,uiGroup);
    uiGroup.setGroupNode(groupNode);
  }
  if (children == null) {
    add(groupNode);
    fireNodeInserted(0);
  }
 else {
    final int insertionPoint=Collections.binarySearch(children,groupNode,nodeComparator);
    if (insertionPoint < 0) {
      final int index=(insertionPoint + 1) * -1;
      insert(groupNode,index);
      fireNodeInserted(index);
    }
  }
  return groupNode;
}
