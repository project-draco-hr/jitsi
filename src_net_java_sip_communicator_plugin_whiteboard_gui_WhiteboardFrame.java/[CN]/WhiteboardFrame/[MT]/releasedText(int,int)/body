{
  doneDrawing=true;
  Graphics g=drawCanvas.getGraphics();
  g.setColor(Color.BLACK);
  g.drawLine(x,y - 10,x,y + 10);
  String t=(String)JOptionPane.showInputDialog(this,"Please enter your text","Text",JOptionPane.QUESTION_MESSAGE,null,null,"text");
  if (t != null && t.length() > 0) {
    appendAndSend(new WhiteboardShapeText(id(),currentColor,new WhiteboardPoint(x,y),defaultFontSize,t,s2w));
  }
}
