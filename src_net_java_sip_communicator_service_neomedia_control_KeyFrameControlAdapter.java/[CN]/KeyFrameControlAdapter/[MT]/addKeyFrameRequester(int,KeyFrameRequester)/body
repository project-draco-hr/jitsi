    if (!keyFrameRequesters.contains(keyFrameRequester)) {
      List<KeyFrameRequester> newKeyFrameRequesters=new ArrayList<KeyFrameRequester>(keyFrameRequesters.size() + 1);
      newKeyFrameRequesters.addAll(keyFrameRequesters);
