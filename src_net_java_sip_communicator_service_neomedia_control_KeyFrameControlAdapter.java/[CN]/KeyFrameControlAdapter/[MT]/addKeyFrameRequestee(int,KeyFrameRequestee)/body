{
  if (keyFrameRequestee == null)   throw new NullPointerException("keyFrameRequestee");
synchronized (this) {
    if (!keyFrameRequestees.contains(keyFrameRequestee)) {
      List<KeyFrameRequestee> newKeyFrameRequestees=new ArrayList<KeyFrameRequestee>(keyFrameRequestees.size() + 1);
      newKeyFrameRequestees.addAll(keyFrameRequestees);
      if (-1 == index) {
        if (keyFrameRequestee.getClass().getName().contains(".neomedia."))         index=newKeyFrameRequestees.size();
 else         index=0;
      }
      newKeyFrameRequestees.add(index,keyFrameRequestee);
      keyFrameRequestees=newKeyFrameRequestees;
      unmodifiableKeyFrameRequestees=null;
    }
  }
}
