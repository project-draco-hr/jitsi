{
  try {
    IncomingFileTransferRequest req=event.getRequest();
    History history=getHistory(null,req.getSender());
    HistoryWriter historyWriter=history.getWriter();
    historyWriter.addRecord(new String[]{req.getFileName(),getDirection(FileTransfer.IN),String.valueOf(event.getTimestamp().getTime()),FILE_TRANSFER_ACTIVE,req.getID()});
  }
 catch (  IOException e) {
    logger.error("Could not add file transfer log to history",e);
  }
}
