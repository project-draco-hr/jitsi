{
  Boolean isWarningEnabled=GuiActivator.getConfigurationService().getBoolean(desktopSharingWarningProperty,true);
  if (isWarningEnabled.booleanValue()) {
    MessageDialog warningDialog=new MessageDialog(null,GuiActivator.getResources().getI18NString("service.gui.WARNING"),GuiActivator.getResources().getI18NString("service.gui.DESKTOP_SHARING_WARNING"),true);
    int result=warningDialog.showDialog();
switch (result) {
case 0:
      return true;
case 1:
    return false;
case 2:
  GuiActivator.getConfigurationService().setProperty(desktopSharingWarningProperty,false);
return true;
}
}
return true;
}
