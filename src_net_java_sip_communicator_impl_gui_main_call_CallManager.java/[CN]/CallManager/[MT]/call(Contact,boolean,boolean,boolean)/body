{
  if (isDesktop) {
    if (shareRegion) {
      createRegionDesktopSharing(contact.getProtocolProvider(),contact.getAddress());
    }
 else     createDesktopSharing(contact.getProtocolProvider(),contact.getAddress());
  }
 else {
    new CreateCallThread(contact.getProtocolProvider(),contact,null,isVideo).start();
  }
}
