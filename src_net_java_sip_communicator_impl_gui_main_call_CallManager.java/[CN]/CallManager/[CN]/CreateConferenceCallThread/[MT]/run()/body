{
  OperationSetTelephonyConferencing confOpSet=protocolProvider.getOperationSet(OperationSetTelephonyConferencing.class);
  if (confOpSet == null)   return;
  try {
    confOpSet.createConfCall(callees);
  }
 catch (  OperationNotSupportedException e) {
    logger.error("Failed to create conference call. " + e);
    new ErrorDialog(null,GuiActivator.getResources().getI18NString("service.gui.ERROR"),e.getMessage(),ErrorDialog.ERROR).showDialog();
  }
}
