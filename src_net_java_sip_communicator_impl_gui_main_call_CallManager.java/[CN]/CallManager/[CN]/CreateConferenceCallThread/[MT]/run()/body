{
  OperationSetTelephonyConferencing confOpSet=protocolProvider.getOperationSet(OperationSetTelephonyConferencing.class);
  if (confOpSet == null)   return;
  Throwable exception=null;
  if (ConfigurationManager.isNormalizePhoneNumber()) {
    normalizePhoneNumbers(callees);
  }
  try {
    confOpSet.createConfCall(callees);
  }
 catch (  OperationFailedException ofe) {
    exception=ofe;
  }
catch (  OperationNotSupportedException onse) {
    exception=onse;
  }
catch (  IllegalArgumentException iae) {
    exception=iae;
  }
  if (exception != null) {
    logger.error("Failed to create conference call. " + exception);
    new ErrorDialog(null,GuiActivator.getResources().getI18NString("service.gui.ERROR"),exception.getMessage(),ErrorDialog.ERROR).showDialog();
  }
}
