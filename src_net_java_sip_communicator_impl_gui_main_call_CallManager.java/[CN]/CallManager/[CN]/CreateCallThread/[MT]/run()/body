{
  OperationSetBasicTelephony telephonyOpSet=(OperationSetBasicTelephony)protocolProvider.getOperationSet(OperationSetBasicTelephony.class);
  if (telephonyOpSet == null)   return;
  try {
    if (contacts != null) {
      Contact contact=(Contact)contacts.get(0);
      telephonyOpSet.createCall(contact);
    }
 else {
      telephonyOpSet.createCall(stringContact);
    }
  }
 catch (  OperationFailedException e) {
    logger.error("The call could not be created: " + e);
    new ErrorDialog(null,GuiActivator.getResources().getI18NString("error"),e.getMessage(),ErrorDialog.ERROR).showDialog();
  }
catch (  ParseException e) {
    logger.error("The call could not be created: " + e);
    new ErrorDialog(null,GuiActivator.getResources().getI18NString("error"),e.getMessage(),ErrorDialog.ERROR).showDialog();
  }
}
