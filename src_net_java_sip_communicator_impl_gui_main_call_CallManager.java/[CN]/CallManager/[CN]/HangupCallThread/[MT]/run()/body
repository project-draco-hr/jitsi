{
  for (  Call conferenceCall : CallConference.getCalls(call)) {
    Iterator<? extends CallPeer> peerIter=conferenceCall.getCallPeers();
    OperationSetBasicTelephony<?> basicTelephony=conferenceCall.getProtocolProvider().getOperationSet(OperationSetBasicTelephony.class);
    while (peerIter.hasNext()) {
      CallPeer peer=peerIter.next();
      try {
        basicTelephony.hangupCallPeer(peer);
      }
 catch (      OperationFailedException ofe) {
        logger.error("Could not hang up: " + peer,ofe);
      }
    }
  }
}
