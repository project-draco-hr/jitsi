{
  for (  Call call : conference.getCalls())   putOffHold(call);
  if (!calls.isEmpty()) {
    for (    Call call : calls) {
      if (conference.containsCall(call))       continue;
      putOffHold(call);
      closeCallContainerIfNotNecessary(call,false);
      call.setConference(conference);
    }
  }
}
