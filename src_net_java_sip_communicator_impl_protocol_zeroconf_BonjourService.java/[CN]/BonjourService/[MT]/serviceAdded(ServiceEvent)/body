{
  String name=event.getName();
  String type=event.getType();
  if (name.equals(id))   return;
  if (logger.isDebugEnabled())   logger.debug("BONJOUR: " + name + "["+ type+ "] detected! Trying to get information...");
  try {
    sleep(2);
  }
 catch (  InterruptedException ex) {
    logger.error(ex);
  }
  jmdns.printServices();
  new AddThread(type,name);
}
