{
  String name=event.getName();
  if (name.equals(id))   return;
  ContactZeroconfImpl contact=getContact(name,null);
  if (contact == null)   return;
  opSetPersPresence.changePresenceStatusForContact(contact,ZeroconfStatusEnum.OFFLINE);
  if (logger.isDebugEnabled())   logger.debug("BONJOUR: Received announcement that " + name + " went offline!");
}
