{
  Object sourceService=RssActivator.bundleContext.getService(event.getServiceReference());
  if (!(sourceService instanceof ProtocolProviderFactoryRssImpl))   return;
switch (event.getType()) {
case ServiceEvent.REGISTERED:
    registerHandlerService();
  break;
case ServiceEvent.UNREGISTERING:
unregisterHandlerService();
break;
case ServiceEvent.MODIFIED:
default :
break;
}
}
