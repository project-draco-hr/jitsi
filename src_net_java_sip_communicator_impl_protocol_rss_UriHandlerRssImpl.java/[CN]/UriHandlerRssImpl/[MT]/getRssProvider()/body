{
  ServiceReference[] serRefs=null;
  String osgiFilter="(" + ProtocolProviderFactory.PROTOCOL + "="+ ProtocolNames.RSS+ ")";
  try {
    serRefs=RssActivator.bundleContext.getServiceReferences(ProtocolProviderService.class.getName(),osgiFilter);
  }
 catch (  InvalidSyntaxException ise) {
    return null;
  }
  if (serRefs == null || serRefs.length == 0)   return null;
  return (ProtocolProviderService)RssActivator.getBundleContext().getService(serRefs[0]);
}
