{
  okButton.setEnabled(false);
  try {
    Integer.parseInt(portField.getText());
  }
 catch (  NumberFormatException ex) {
    okButton.setEnabled(false);
    return;
  }
  if (serverField.getText().equals("") || userIDField.getText().equals("") || (new String(passField.getPassword())).equals("")|| (new String(pass2Field.getPassword())).equals("")) {
    okButton.setEnabled(false);
  }
 else {
    okButton.setEnabled(true);
  }
}
