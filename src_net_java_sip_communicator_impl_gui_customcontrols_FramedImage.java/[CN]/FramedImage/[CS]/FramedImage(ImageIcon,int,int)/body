{
  super();
  this.width=width;
  this.height=height;
  this.setPreferredSize(new Dimension(width,height));
  Image photoFrameImage=ImageLoader.getImage(ImageLoader.USER_PHOTO_FRAME);
  this.frameIcon=ImageUtils.scaleIconWithinBounds(photoFrameImage,width,height);
  this.shadowIcon=ImageUtils.scaleIconWithinBounds(ImageLoader.getImage(ImageLoader.USER_PHOTO_SHADOW),width,height);
  if (imageIcon != null) {
    this.setImageIcon(imageIcon);
  }
}
