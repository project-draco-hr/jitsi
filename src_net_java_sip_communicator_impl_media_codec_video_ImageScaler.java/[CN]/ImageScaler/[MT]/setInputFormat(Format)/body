{
  final VideoFormat videoFormat=(VideoFormat)format;
  if (videoFormat.getSize() == null)   return null;
  if (outputFormat != null) {
    VideoFormat outVFormat=(VideoFormat)outputFormat;
    if (outVFormat.getSize() != null)     toProcess=!outVFormat.getSize().equals(videoFormat.getSize());
  }
  bufferToImage=new BufferToImage((VideoFormat)format);
  return super.setInputFormat(format);
}
