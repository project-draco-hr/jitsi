{
  InetSocketAddress srvrAddress;
  try {
    srvrAddress=NetworkUtils.getSRVRecord(TURN_SRV_NAME,Transport.UDP.toString(),domainName);
    if (srvrAddress != null) {
      return new TurnCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP),new LongTermCredential(userName,password));
    }
    srvrAddress=NetworkUtils.getSRVRecord(STUN_SRV_NAME,Transport.UDP.toString(),domainName);
  }
 catch (  ParseException e) {
    logger.info(domainName + " seems to be causing parse problems",e);
    srvrAddress=null;
  }
  if (srvrAddress != null) {
    return new StunCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP));
  }
  return null;
}
