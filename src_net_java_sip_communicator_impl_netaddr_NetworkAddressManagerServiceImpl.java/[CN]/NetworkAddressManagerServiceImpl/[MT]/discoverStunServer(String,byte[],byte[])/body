{
  InetSocketAddress srvrAddress=null;
  try {
    SRVRecord srvRecord=NetworkUtils.getSRVRecord(TURN_SRV_NAME,Transport.UDP.toString(),domainName);
    if (srvRecord != null) {
      srvrAddress=srvRecord.getInetSocketAddress();
    }
    if (srvrAddress != null) {
      return new TurnCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP),new LongTermCredential(userName,password));
    }
    srvRecord=NetworkUtils.getSRVRecord(STUN_SRV_NAME,Transport.UDP.toString(),domainName);
    if (srvRecord != null) {
      srvrAddress=srvRecord.getInetSocketAddress();
    }
  }
 catch (  ParseException e) {
    logger.info(domainName + " seems to be causing parse problems",e);
    srvrAddress=null;
  }
  if (srvrAddress != null) {
    return new StunCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP));
  }
  return null;
}
