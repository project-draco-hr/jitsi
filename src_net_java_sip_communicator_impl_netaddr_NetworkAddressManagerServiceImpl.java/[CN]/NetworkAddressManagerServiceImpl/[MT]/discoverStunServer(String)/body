{
  InetSocketAddress srvrAddress=NetworkUtils.getSRVRecord(TURN_SRV_NAME,Transport.UDP.toString(),domainName);
  if (srvrAddress != null) {
    return new TurnCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP));
  }
  srvrAddress=NetworkUtils.getSRVRecord(STUN_SRV_NAME,Transport.UDP.toString(),domainName);
  if (srvrAddress != null) {
    return new StunCandidateHarvester(new TransportAddress(srvrAddress,Transport.UDP));
  }
  return null;
}
