{
  bundleContext=bc;
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  uiService=(UIService)bundleContext.getService(uiServiceRef);
  AccountRegistrationWizardContainer wizardContainer=uiService.getAccountRegWizardContainer();
  aimWizard=new AimAccountRegistrationWizard(wizardContainer);
  wizardContainer.addAccountRegistrationWizard(aimWizard);
}
