{
  try {
    if (packetLogging.isLoggingEnabled(PacketLoggingService.ProtocolName.JABBER)) {
      packetLogging.logPacket(PacketLoggingService.ProtocolName.JABBER,remoteAddress,connection.getPort(),localAddress,localPort,PacketLoggingService.TransportName.TCP,false,packet.toXML().getBytes("UTF-8"));
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
