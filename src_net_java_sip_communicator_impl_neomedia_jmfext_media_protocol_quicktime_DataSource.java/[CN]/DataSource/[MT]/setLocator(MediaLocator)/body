{
  super.setLocator(locator);
  locator=getLocator();
  QTCaptureDevice device;
  if ((locator != null) && QuickTimeSystem.LOCATOR_PROTOCOL.equalsIgnoreCase(locator.getProtocol())) {
    String deviceUID=locator.getRemainder();
    device=QTCaptureDevice.deviceWithUniqueID(deviceUID);
  }
 else   device=null;
  setDevice(device);
}
