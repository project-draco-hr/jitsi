{
  byte data[]=p.getData();
  int dataLen=p.getLength();
  int dataOffset=p.getOffset();
  DatagramPacket packet=new DatagramPacket(data,dataOffset,dataLen,new InetSocketAddress(localEndPoint.getAddress(),localEndPoint.getPort()));
  super.send(packet);
  if (StunStack.isPacketLoggerEnabled()) {
    StunStack.getPacketLogger().logPacket(super.getLocalAddress().getAddress(),super.getLocalPort(),packet.getAddress().getAddress(),packet.getPort(),packet.getData(),true);
  }
}
