{
  int i;
  for (i=0; i < timerImages.length; i++) {
    timerImages[i].setElapsed();
    try {
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
  playSound();
  grabSnapshot();
  WebcamDialog.this.setVisible(false);
  WebcamDialog.this.dispose();
}
