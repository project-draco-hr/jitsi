{
synchronized (waitingForResponseLock) {
    if (ran)     return;
    ran=true;
    if (evt.getSnacCommand() instanceof FullInfoAck) {
      FullInfoAck cmd=(FullInfoAck)evt.getSnacCommand();
      if (cmd.isCommandSuccesful()) {
        success=true;
      }
    }
    waitingForResponseLock.notifyAll();
  }
}
