{
synchronized (this) {
    if (ran)     return;
    ran=true;
  }
  if (e.getSnacCommand() instanceof FullInfoAck) {
    FullInfoAck cmd=(FullInfoAck)e.getSnacCommand();
    ;
    if (cmd.isCommandSuccesful()) {
      success=true;
    }
  }
synchronized (waitingForResponseLock) {
    waitingForResponseLock.notifyAll();
  }
}
