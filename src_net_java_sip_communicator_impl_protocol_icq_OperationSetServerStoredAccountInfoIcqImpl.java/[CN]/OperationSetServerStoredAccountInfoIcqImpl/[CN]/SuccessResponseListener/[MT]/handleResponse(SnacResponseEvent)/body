{
synchronized (this) {
    if (ran)     return;
    ran=true;
    FromIcqCmd cmd=new FromIcqCmd(e.getSnacPacket());
    byte[] data=cmd.getIcqData().toByteArray();
    if (cmd.getType().equals(RESPONSE_TYPE) && data.length == 1 && data[0] == SUCCESS_BYTE) {
      success=true;
      notifyAll();
    }
  }
}
