{
  if (getPeer().isJitsiVideoBridge())   return false;
  if (SDesControl.PROTO_NAME.equals(protoName)) {
    addSDesAdvertisedEncryptions(false,remoteDescription,mediaType);
    if (setSDesEncryptionToDescription(mediaType,localDescription,remoteDescription)) {
      return true;
    }
  }
 else   if (ZrtpControl.PROTO_NAME.equals(protoName)) {
    if (setZrtpEncryptionToDescription(mediaType,localDescription,remoteDescription)) {
      addZrtpAdvertisedEncryptions(false,remoteDescription,mediaType);
      return true;
    }
  }
  return false;
}
