{
  List<CryptoPacketExtension> cryptoPacketExtensions=encryptionPacketExtension.getCryptoList();
  Vector<SrtpCryptoAttribute> peerAttributes=new Vector<SrtpCryptoAttribute>(cryptoPacketExtensions.size());
  for (int i=0; i < cryptoPacketExtensions.size(); ++i) {
    peerAttributes.add(cryptoPacketExtensions.get(i).toSrtpCryptoAttribute());
  }
  if (peerAttributes == null) {
    return null;
  }
  if (isInitiator) {
    return sDesControl.initiatorSelectAttribute(peerAttributes);
  }
 else {
    return sDesControl.responderSelectAttribute(peerAttributes);
  }
}
