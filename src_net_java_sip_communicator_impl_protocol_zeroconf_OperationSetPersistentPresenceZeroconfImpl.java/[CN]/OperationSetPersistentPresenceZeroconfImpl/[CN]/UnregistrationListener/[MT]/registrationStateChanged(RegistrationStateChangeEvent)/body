{
  logger.debug("ZEROCONF : The Zeroconf provider changed state from: " + evt.getOldState() + " to: "+ evt.getNewState());
  Iterator groupsIter=getServerStoredContactListRoot().subgroups();
  while (groupsIter.hasNext()) {
    ContactGroupZeroconfImpl group=(ContactGroupZeroconfImpl)groupsIter.next();
    Iterator contactsIter=group.contacts();
    while (contactsIter.hasNext()) {
      ContactZeroconfImpl contact=(ContactZeroconfImpl)contactsIter.next();
      PresenceStatus oldContactStatus=contact.getPresenceStatus();
      if (!oldContactStatus.isOnline()) {
        fireContactPresenceStatusChangeEvent(contact,contact.getParentContactGroup(),oldContactStatus);
      }
    }
  }
}
