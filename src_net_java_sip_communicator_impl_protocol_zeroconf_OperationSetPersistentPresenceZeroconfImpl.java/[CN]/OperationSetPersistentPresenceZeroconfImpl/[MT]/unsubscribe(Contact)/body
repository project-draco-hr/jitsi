{
  String name=contact.getAddress();
  ContactGroupZeroconfImpl parentGroup=(ContactGroupZeroconfImpl)((ContactZeroconfImpl)contact).getParentContactGroup();
  BonjourService service=((ProtocolProviderServiceZeroconfImpl)contact.getProtocolProvider()).getBonjourService();
  service.removeContact(name,null);
  fireSubscriptionEvent(contact,((ContactZeroconfImpl)contact).getParentContactGroup(),SubscriptionEvent.SUBSCRIPTION_REMOVED);
}
