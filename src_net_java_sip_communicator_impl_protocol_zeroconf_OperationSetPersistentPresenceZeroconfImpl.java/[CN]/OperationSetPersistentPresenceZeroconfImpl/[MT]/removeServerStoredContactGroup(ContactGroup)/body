{
  ContactGroupZeroconfImpl zeroconfGroup=(ContactGroupZeroconfImpl)group;
  ContactGroupZeroconfImpl parent=findGroupParent(zeroconfGroup);
  if (parent == null) {
    throw new IllegalArgumentException("group " + group + " does not seem to belong to this protocol's contact list.");
  }
  parent.removeSubGroup(zeroconfGroup);
  this.fireServerStoredGroupEvent(zeroconfGroup,ServerStoredGroupEvent.GROUP_REMOVED_EVENT);
}
