{
  File[] list=tmpDir.listFiles();
  if (list == null) {
    return null;
  }
  boolean test=false;
  for (  File f : list) {
    if (f.getName().equals("info.properties")) {
      if (f.isFile()) {
        test=true;
      }
    }
  }
  if (!test) {
    if (list.length == 1) {
      return list[0];
    }
 else {
      return null;
    }
  }
  return tmpDir;
}
