{
switch (mode) {
case 'O':
    return new ModeEntry(add,Mode.bySymbol(mode),this.params[this.index++]);
case 'o':
  return new ModeEntry(add,Mode.bySymbol(mode),this.params[this.index++]);
case 'v':
return new ModeEntry(add,Mode.bySymbol(mode),this.params[this.index++]);
default :
throw new IllegalArgumentException("" + mode);
}
}
