{
  Map protocolStatusMap=Constants.getProtocolStatusIcons(protocolProvider.getProtocolName());
  StatusSelectorBox protocolStatusCombo=new StatusSelectorBox(this.mainFrame,protocolProvider,protocolStatusMap,(Image)protocolStatusMap.get(Constants.OFFLINE_STATUS));
  this.protocolStatusCombos.put(protocolProvider.getProtocolName(),protocolStatusCombo);
  this.add(protocolStatusCombo);
  this.getParent().validate();
}
