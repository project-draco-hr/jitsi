{
  Map protocolStatusMap=Constants.getProtocolStatusIcons(account.getProtocolName());
  StatusSelectorBox protocolStatusCombo=new StatusSelectorBox(this.mainFrame,account,protocolStatusMap,(Image)protocolStatusMap.get(Constants.OFFLINE_STATUS));
  this.protocolStatusCombos.put(account.getProtocolName(),protocolStatusCombo);
  this.add(protocolStatusCombo);
  this.getParent().validate();
}
