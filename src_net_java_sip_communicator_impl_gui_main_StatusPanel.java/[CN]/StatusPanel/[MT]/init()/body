{
  for (int i=0; i < userProtocols.length; i++) {
    ArrayList protocolStatusList=Constants.getProtocolStatusIcons(userProtocols[i]);
    SelectorBox protocolStatusCombo=new SelectorBox(protocolStatusList.toArray(),(SelectorBoxItem)protocolStatusList.get(protocolStatusList.size() - 2));
    this.add(protocolStatusCombo);
  }
}
