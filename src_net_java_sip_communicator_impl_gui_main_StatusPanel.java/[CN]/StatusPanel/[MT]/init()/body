{
  for (int i=0; i < userProtocols.length; i++) {
    Map protocolStatusMap=Constants.getProtocolStatusIcons(userProtocols[i]);
    StatusSelectorBox protocolStatusCombo=new StatusSelectorBox(protocolStatusMap,(Image)protocolStatusMap.get(Constants.OFFLINE_STATUS));
    this.protocolStatusCombos.put(userProtocols[i],protocolStatusCombo);
    this.add(protocolStatusCombo);
  }
}
