{
  if (evt.getEventType().equals(CallChangeEvent.CALL_STATE_CHANGE) && evt.getNewValue().equals(CallState.CALL_ENDED)) {
    T sourceCall=this.activeCalls.remove(evt.getSourceCall().getCallID());
    if (logger.isTraceEnabled())     logger.trace("Removing call " + sourceCall + " from the list of "+ "active calls because it entered an ENDED state");
    this.parentOperationSet.fireCallEvent(CallEvent.CALL_ENDED,sourceCall);
  }
}
