{
  Iterator<Map.Entry<MediaFormat,Byte>> entriesIter=payloadMappings.entrySet().iterator();
  while (entriesIter.hasNext()) {
    Map.Entry<MediaFormat,Byte> entry=entriesIter.next();
    byte fmtPayloadType=entry.getValue();
    if (fmtPayloadType == payloadType)     return entry.getKey();
  }
  return null;
}
