{
  confFile=new File(System.getProperty("user.home") + File.separator + ".sip-communicator"+ File.separator+ ourConfFileName);
  configurationService.purgeStoredConfiguration();
  originalConfFileName=System.getProperty("net.java.sip.communicator.CONFIGURATION_FILE_NAME");
  System.setProperty("net.java.sip.communicator.CONFIGURATION_FILE_NAME",ourConfFileName);
  confFile.createNewFile();
  FileOutputStream out=new FileOutputStream(confFile);
  out.write(confFileContent.getBytes());
  out.flush();
  out.close();
  configurationService.reloadConfiguration();
  super.setUp();
}
