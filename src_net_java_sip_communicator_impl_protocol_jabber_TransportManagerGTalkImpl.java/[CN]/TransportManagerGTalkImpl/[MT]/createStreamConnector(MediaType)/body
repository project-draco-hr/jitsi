{
  DatagramSocket[] streamConnectorSockets=getStreamConnectorSockets(mediaType);
  Socket[] tcpStreamConnectorSockets=getStreamConnectorTCPSockets(mediaType);
  if (tcpStreamConnectorSockets == null) {
    return (streamConnectorSockets == null) ? super.createStreamConnector(mediaType) : new DefaultStreamConnector(streamConnectorSockets[0],streamConnectorSockets[1]);
  }
 else {
    return (tcpStreamConnectorSockets == null) ? super.createStreamConnector(mediaType) : new DefaultTCPStreamConnector(tcpStreamConnectorSockets[0],tcpStreamConnectorSockets[1]);
  }
}
