{
  String osgiFilter="(" + ProtocolProviderFactory.PROTOCOL + "="+ protocolName+ ")";
  ProtocolProviderFactory protocolProviderFactory=null;
  try {
    ServiceReference[] refs=UtilActivator.bundleContext.getServiceReferences(ProtocolProviderFactory.class.getName(),osgiFilter);
    if ((refs != null) && (refs.length > 0)) {
      protocolProviderFactory=(ProtocolProviderFactory)UtilActivator.bundleContext.getService(refs[0]);
    }
  }
 catch (  InvalidSyntaxException ex) {
    logger.error("AccountUtils : " + ex);
  }
  return protocolProviderFactory;
}
