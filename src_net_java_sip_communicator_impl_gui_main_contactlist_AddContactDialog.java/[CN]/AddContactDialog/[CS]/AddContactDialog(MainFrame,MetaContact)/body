{
  this(parentWindow);
  this.metaContact=metaContact;
  this.setSelectedGroup(metaContact.getParentMetaContactGroup());
  this.setTitle(GuiActivator.getResources().getI18NString("service.gui.ADD_CONTACT") + " " + GuiActivator.getResources().getI18NString("service.gui.TO")+ " "+ metaContact.getDisplayName());
}
