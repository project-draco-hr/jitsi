{
  StringBuffer xml=new StringBuffer();
  xml.append("<?xml version=\"1.0\" encoding=\"UTF-8\"?>\r\n");
  append(xml,"<",ELEMENT_REMOTE_CONTROL,">");
  append(xml,"<",ELEMENT_KEY_PRESS);
