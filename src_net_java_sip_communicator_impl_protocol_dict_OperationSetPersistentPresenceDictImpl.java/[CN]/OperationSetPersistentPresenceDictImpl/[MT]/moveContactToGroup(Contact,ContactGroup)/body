{
  ContactDictImpl dictContact=(ContactDictImpl)contactToMove;
  ContactGroupDictImpl parentDictGroup=findContactParent(dictContact);
  parentDictGroup.removeContact(dictContact);
  if (!dictContact.isPersistent()) {
    fireSubscriptionEvent(dictContact,parentDictGroup,SubscriptionEvent.SUBSCRIPTION_REMOVED);
    try {
      this.subscribe(newParent,contactToMove.getAddress());
      fireSubscriptionEvent(dictContact,newParent,SubscriptionEvent.SUBSCRIPTION_CREATED);
    }
 catch (    Exception ex) {
      logger.error("Failed to move contact " + dictContact.getAddress(),ex);
    }
  }
 else {
    ((ContactGroupDictImpl)newParent).addContact(dictContact);
    fireSubscriptionMovedEvent(contactToMove,parentDictGroup,newParent);
  }
}
