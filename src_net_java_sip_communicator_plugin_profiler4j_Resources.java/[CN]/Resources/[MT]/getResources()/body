{
  if (resourcesService == null) {
    ServiceReference serviceReference=ProfilerActivator.bundleContext.getServiceReference(ResourceManagementService.class.getName());
    if (serviceReference == null) {
      return null;
    }
    resourcesService=(ResourceManagementService)ProfilerActivator.bundleContext.getService(serviceReference);
  }
  return resourcesService;
}
