{
  int pos=file.indexOf(':');
  if (pos >= 0 && Main.isPosBeforeChar(file,pos,File.separatorChar) && Main.isPosBeforeChar(file,pos,'\\') && Main.isPosBeforeChar(file,pos,'/')) {
    file=file.substring(pos + 1);
  }
  InputStream in=new FileInputStream(file);
  Properties props=null;
  try {
    props=new Properties();
    props.load(in);
  }
  finally {
    try {
      in.close();
    }
 catch (    Exception e) {
    }
  }
  return props;
}
