{
  if (dictName == null) {
    try {
      dictName=getDictAdapter().getDictionaryName(contactID);
      if (dictName == null) {
        dictName=contactID;
      }
    }
 catch (    Exception e) {
      logger.error("Error while getting dictionary long name",e);
      dictName=contactID;
    }
  }
  return dictName;
}
