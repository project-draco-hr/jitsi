{
  logger.info("Looking for Audio capturer");
  try {
    DirectSoundAuto directSoundAuto=new DirectSoundAuto();
  }
 catch (  Throwable exc) {
    logger.debug("No direct sound detected: " + exc.getMessage());
  }
  try {
    JavaSoundAuto javaSoundAuto=new JavaSoundAuto();
  }
 catch (  Throwable exc) {
    logger.debug("No JMF javasound detected: " + exc.getMessage());
  }
  try {
    new FMJJavaSoundAuto();
  }
 catch (  Throwable exc) {
    logger.debug("No FMJ javasound detected: " + exc.getMessage());
  }
  String disableVideoObj=(String)MediaActivator.getConfigurationService().getProperty(MediaServiceImpl.DISABLE_VIDEO_SUPPORT_PROPERTY_NAME);
  boolean disableVideo=false;
  if (disableVideoObj != null) {
    disableVideo=Boolean.valueOf(disableVideoObj).booleanValue();
  }
  if (disableVideo)   return;
  logger.info("Looking for video capture devices");
  int nDevices=0;
  try {
    new FMJCivilVideoAuto();
  }
 catch (  Throwable exc) {
    logger.debug("No FMJ CIVIL video detected: " + exc.getMessage(),exc);
  }
}
