{
  String provUri=getConfigurationService().getString(PROPERTY_PROVISIONING_URL);
  if (provUri == null || provUri.length() <= 0) {
    provUri=getResourceService().getSettingsString("plugin.provisioning.DEFAULT_PROVISIONING_URI");
    if (provUri != null && provUri.length() > 0)     setProvisioningUri(provUri);
  }
  return provUri;
}
