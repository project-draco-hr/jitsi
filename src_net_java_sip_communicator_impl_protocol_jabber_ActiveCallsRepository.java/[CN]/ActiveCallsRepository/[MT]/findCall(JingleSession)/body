{
  Iterator<CallJabberImpl> activeCalls=getActiveCalls();
  if (session == null) {
    if (logger.isDebugEnabled())     logger.debug("Cannot find a peer with a null session. " + "Returning null");
    return null;
  }
  if (logger.isTraceEnabled()) {
    if (logger.isTraceEnabled())     logger.trace("Looking for peer with session: " + session + " among "+ this.activeCalls.size()+ " calls");
  }
  while (activeCalls.hasNext()) {
    CallJabberImpl call=activeCalls.next();
    if (call.contains(session))     return call;
  }
  return null;
}
