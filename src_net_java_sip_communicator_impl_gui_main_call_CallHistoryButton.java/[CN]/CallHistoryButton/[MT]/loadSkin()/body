{
  historyImage=ImageLoader.getImage(ImageLoader.CALL_HISTORY_BUTTON);
  pressedImage=ImageLoader.getImage(ImageLoader.CALL_HISTORY_BUTTON_PRESSED);
  notificationImage=ImageLoader.getImage(ImageLoader.CALL_HISTORY_BUTTON_NOTIFICATION);
  this.setPreferredSize(new Dimension(historyImage.getWidth(this),historyImage.getHeight(this)));
  setHistoryView();
}
