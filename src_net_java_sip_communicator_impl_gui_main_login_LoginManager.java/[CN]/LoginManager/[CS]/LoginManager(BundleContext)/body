{
  this.bc=bc;
  ServiceReference[] serRefs=null;
  try {
    serRefs=this.bc.getServiceReferences(AccountManager.class.getName(),null);
  }
 catch (  InvalidSyntaxException e) {
    logger.error("LoginManager : " + e);
  }
  for (int i=0; i < serRefs.length; i++) {
    AccountManager accountManager=(AccountManager)this.bc.getService(serRefs[i]);
    this.accountManagersMap.put(serRefs[i].getProperty(AccountManager.PROTOCOL_PROPERTY_NAME),accountManager);
  }
}
