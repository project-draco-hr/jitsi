{
  Hashtable accountProperties=new Hashtable();
  accountProperties.put(AccountProperties.PASSWORD,passwd);
  AccountID accountID=providerFactory.installAccount(GuiActivator.bundleContext,user,accountProperties);
  ServiceReference serRef=providerFactory.getProviderForAccount(accountID);
  ProtocolProviderService protocolProvider=(ProtocolProviderService)GuiActivator.bundleContext.getService(serRef);
  return protocolProvider;
}
