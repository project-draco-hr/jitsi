{
  AccountRegWizardContainerImpl wizard=(AccountRegWizardContainerImpl)GuiActivator.getUIService().getAccountRegWizardContainer();
  NoAccountFoundPage noAccountFoundPage=new NoAccountFoundPage();
  wizard.registerWizardPage(noAccountFoundPage.getIdentifier(),noAccountFoundPage);
  wizard.setTitle(Messages.getI18NString("accountRegistrationWizard").getText());
  wizard.newAccount(noAccountFoundPage.getIdentifier());
  wizard.showDialog(true);
}
