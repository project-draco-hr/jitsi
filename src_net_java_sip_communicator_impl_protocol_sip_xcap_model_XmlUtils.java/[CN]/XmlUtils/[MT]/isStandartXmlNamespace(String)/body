{
  namespace=normalizeNamespace(namespace);
  return normalizeNamespace(XML_NS_URI).equals(namespace) || normalizeNamespace(XMLNS_ATTRIBUTE_NS_URI).equals(namespace) || normalizeNamespace(W3C_XML_SCHEMA_NS_URI).equals(namespace)|| normalizeNamespace(W3C_XML_SCHEMA_INSTANCE_NS_URI).equals(namespace);
}
