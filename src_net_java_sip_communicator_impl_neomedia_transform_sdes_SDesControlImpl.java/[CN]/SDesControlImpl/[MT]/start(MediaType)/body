{
  srtpListener.securityStarted(type.equals(MediaType.AUDIO) ? SecurityEventManager.AUDIO_SESSION : SecurityEventManager.VIDEO_SESSION);
  srtpListener.securityTurnedOn(type.equals(MediaType.AUDIO) ? SecurityEventManager.AUDIO_SESSION : SecurityEventManager.VIDEO_SESSION,selectedInAttribute.getCryptoSuite().encode(),this);
}
