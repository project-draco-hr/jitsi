{
  super();
  this.setBorder(null);
  this.setOpaque(false);
  this.setViewport(new SCViewport());
  this.getVerticalScrollBar().setUnitIncrement(30);
  String windowImageBackgroundProperty="net.java.sip.communicator.impl.gui.isWindowImageBackgroundEnabled";
  isWindowImageBgEnabled=new Boolean(GuiActivator.getResources().getSettingsString(windowImageBackgroundProperty)).booleanValue();
  if (isWindowImageBgEnabled)   this.initBackgroundImage();
}
