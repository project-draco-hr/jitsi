{
  super(owner);
  this.getContentPane().setLayout(new BorderLayout(5,5));
  this.messageArea.setLineWrap(true);
  this.messageArea.setWrapStyleWord(true);
  this.messagePanel.setBorder(BorderFactory.createEmptyBorder(10,10,0,10));
