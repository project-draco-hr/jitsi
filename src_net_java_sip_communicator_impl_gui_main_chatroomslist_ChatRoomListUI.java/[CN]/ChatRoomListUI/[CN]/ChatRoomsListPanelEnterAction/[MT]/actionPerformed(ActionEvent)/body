{
  Object selectedValue=chatRoomList.getSelectedValue();
  ChatWindowManager chatWindowManager=GuiActivator.getUIService().getChatWindowManager();
  if (selectedValue instanceof ChatRoomProviderWrapper) {
    ChatRoomProviderWrapper serverWrapper=(ChatRoomProviderWrapper)selectedValue;
    ChatPanel chatPanel=chatWindowManager.getMultiChat(serverWrapper.getSystemRoomWrapper());
    chatWindowManager.openChat(chatPanel,true);
  }
 else   if (selectedValue instanceof ChatRoomWrapper) {
    ChatRoomWrapper chatRoomWrapper=(ChatRoomWrapper)selectedValue;
    ChatPanel chatPanel=chatWindowManager.getMultiChat(chatRoomWrapper);
    chatWindowManager.openChat(chatPanel,true);
  }
}
