{
  uriStr=uriStr.trim();
  if (uriStr.indexOf('@') == -1 && !uriStr.trim().startsWith("tel:")) {
    uriStr=uriStr + "@" + ((SipURI)getOurSipAddress().getURI()).getHost();
  }
  if (uriStr.toLowerCase().indexOf("sip:") == -1 && uriStr.indexOf('@') != -1) {
    uriStr="sip:" + uriStr;
  }
  Address uri=getAddressFactory().createAddress(uriStr);
  return uri;
}
