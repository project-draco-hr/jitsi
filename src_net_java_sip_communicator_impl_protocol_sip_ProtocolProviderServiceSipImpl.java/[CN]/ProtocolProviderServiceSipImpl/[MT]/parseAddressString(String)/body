{
  uriStr=uriStr.trim();
  if (uriStr.toLowerCase().startsWith("tel:"))   uriStr="sip:" + uriStr.substring("tel:".length());
  if (uriStr.indexOf('@') == -1) {
    SipRegistrarConnection src=sipRegistrarConnection;
    if (src != null && !src.isRegistrarless()) {
      uriStr=uriStr + "@" + ((SipURI)src.getAddressOfRecord().getURI()).getHost();
    }
  }
  if (!uriStr.toLowerCase().startsWith("sip:")) {
    uriStr="sip:" + uriStr;
  }
  Address toAddress=getAddressFactory().createAddress(uriStr);
  return toAddress;
}
