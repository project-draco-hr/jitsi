{
  if (!isInitialized) {
    throw new OperationFailedException("Provided must be initialized before being able to register.",OperationFailedException.GENERAL_ERROR);
  }
  if (isRegistered()) {
    return;
  }
  sipStackSharing.addSipListener(this);
  authority.setUserNameEditable(true);
  sipSecurityManager.setSecurityAuthority(authority);
  if (sipRegistrarConnection == null)   initRegistrarConnection((SipAccountID)accountID);
  if (outboundProxySocketAddress == null)   initOutboundProxy((SipAccountID)accountID);
  if (sipRegistrarConnection != null)   sipRegistrarConnection.register();
}
