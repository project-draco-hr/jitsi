{
  String exMsg=ex.getMessage().toLowerCase();
  if (exMsg.indexOf("authentication failed") != -1 || (exMsg.indexOf("authentication") != -1 && exMsg.indexOf("failed") != -1) || exMsg.indexOf("login failed") != -1 || exMsg.indexOf("unable to determine password") != -1) {
    return true;
  }
 else   return false;
}
