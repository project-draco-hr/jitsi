{
  if (accountID.getAccountPropertyBoolean("GOOGLE_CONTACTS_ENABLED",false)) {
    logger.info("Unregister Google Contacts service as contact source");
    GoogleContactsService googleService=JabberActivator.getGoogleService();
    if (googleService != null) {
      googleService.removeContactSource(org.jivesoftware.smack.util.StringUtils.parseName(getOurJID()) + "@" + StringUtils.parseServer(getAccountID().getUserID()));
    }
  }
}
