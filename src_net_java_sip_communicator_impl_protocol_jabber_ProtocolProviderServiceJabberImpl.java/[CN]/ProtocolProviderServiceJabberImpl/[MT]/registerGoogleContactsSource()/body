{
  if (accountID.getAccountPropertyBoolean("GOOGLE_CONTACTS_ENABLED",true)) {
    logger.info("Register Google Contacts service as contact source");
    GoogleContactsService googleService=JabberActivator.getGoogleService();
    if (googleService != null) {
      googleService.addContactSource(org.jivesoftware.smack.util.StringUtils.parseName(getOurJID()) + "@" + StringUtils.parseServer(getAccountID().getUserID()),JabberActivator.getProtocolProviderFactory().loadPassword(getAccountID()));
    }
  }
}
