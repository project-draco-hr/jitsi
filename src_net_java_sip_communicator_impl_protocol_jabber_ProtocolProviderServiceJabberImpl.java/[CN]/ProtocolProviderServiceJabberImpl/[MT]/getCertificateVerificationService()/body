{
  if (guiVerification == null) {
    ServiceReference guiVerifyReference=JabberActivator.getBundleContext().getServiceReference(CertificateService.class.getName());
    if (guiVerifyReference != null)     guiVerification=(CertificateService)JabberActivator.getBundleContext().getService(guiVerifyReference);
  }
  return guiVerification;
}
