{
  if (guiVerification == null) {
    ServiceReference guiVerifyReference=JabberActivator.getBundleContext().getServiceReference(CertificateVerificationService.class.getName());
    if (guiVerifyReference != null)     guiVerification=(CertificateVerificationService)JabberActivator.getBundleContext().getService(guiVerifyReference);
  }
  return guiVerification;
}
