{
switch (state) {
case Bundle.INSTALLED:
    return Resources.getResources().getImage("plugin.pluginmanager.INSTALLED_STATE");
case Bundle.RESOLVED:
  return Resources.getResources().getImage("plugin.pluginmanager.DEACTIVATED_STATE");
case Bundle.STARTING:
return Resources.getResources().getImage("plugin.pluginmanager.STARTING_STATE");
case Bundle.ACTIVE:
return Resources.getResources().getImage("plugin.pluginmanager.ACTIVATE_STATE");
case Bundle.STOPPING:
return Resources.getResources().getImage("plugin.pluginmanager.STOPPING_STATE");
}
return null;
}
