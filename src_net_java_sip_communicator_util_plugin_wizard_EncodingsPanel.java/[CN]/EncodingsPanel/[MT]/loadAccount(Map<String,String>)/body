{
  String overrideEncodings=accountProperties.get(ProtocolProviderFactory.OVERRIDE_ENCODINGS);
  boolean isOverrideEncodings=Boolean.parseBoolean(overrideEncodings);
  overrideCheckBox.setSelected(isOverrideEncodings);
  encodingConfiguration=mediaConfiguration.getMediaService().createEmptyEncodingConfiguration();
  encodingConfiguration.loadProperties(accountProperties,ProtocolProviderFactory.ENCODING_PROP_PREFIX);
  encodingConfiguration.storeProperties(encodingProperties,ProtocolProviderFactory.ENCODING_PROP_PREFIX + ".");
  resetTables();
}
