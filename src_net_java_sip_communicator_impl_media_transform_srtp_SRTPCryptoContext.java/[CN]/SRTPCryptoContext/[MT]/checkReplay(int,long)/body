{
  long localIndex=(((long)this.roc) << 16) | this.seqNum;
  long delta=guessedIndex - localIndex;
  if (delta > 0) {
    return true;
  }
 else {
    if (-delta > REPLAY_WINDOW_SIZE) {
      return false;
    }
 else {
      if (((this.replayWindow >> (-delta)) & 0x1) != 0) {
        return false;
      }
 else {
        return true;
      }
    }
  }
}
