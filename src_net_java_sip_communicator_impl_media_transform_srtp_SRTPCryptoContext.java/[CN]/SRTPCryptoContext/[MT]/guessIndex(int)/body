{
  if (!this.seqNumSet) {
    this.seqNumSet=true;
    this.seqNum=seqNum;
  }
  if (this.seqNum < 32768) {
    if (seqNum - this.seqNum > 32768) {
      this.guessedROC=this.roc - 1;
    }
 else {
      this.guessedROC=this.roc;
    }
  }
 else {
    if (this.seqNum - 32768 > seqNum) {
      this.guessedROC=this.roc + 1;
    }
 else {
      this.guessedROC=this.roc;
    }
  }
  return ((long)this.guessedROC) << 16 | seqNum;
}
