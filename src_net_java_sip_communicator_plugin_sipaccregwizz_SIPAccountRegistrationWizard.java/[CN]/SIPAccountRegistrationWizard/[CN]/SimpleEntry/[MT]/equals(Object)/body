{
  if (!(o instanceof Map.Entry))   return false;
  if (o instanceof SimpleEntry) {
    SimpleEntry<K,V> e=(SimpleEntry<K,V>)o;
    return (SimpleEntry.equals(key,e.key) && SimpleEntry.equals(value,e.value));
  }
  Map.Entry<K,V> e=(Map.Entry<K,V>)o;
  return (SimpleEntry.equals(key,e.getKey()) && SimpleEntry.equals(value,e.getValue()));
}
