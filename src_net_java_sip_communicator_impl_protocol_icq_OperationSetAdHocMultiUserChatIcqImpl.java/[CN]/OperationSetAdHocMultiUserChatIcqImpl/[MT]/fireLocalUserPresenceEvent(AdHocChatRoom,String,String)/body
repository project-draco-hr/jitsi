{
  LocalUserAdHocChatRoomPresenceChangeEvent evt=new LocalUserAdHocChatRoomPresenceChangeEvent(this,chatRoom,eventType,reason);
  Iterator<LocalUserAdHocChatRoomPresenceListener> listeners=null;
synchronized (presenceListeners) {
    listeners=new ArrayList<LocalUserAdHocChatRoomPresenceListener>(presenceListeners).iterator();
  }
  while (listeners.hasNext()) {
    LocalUserAdHocChatRoomPresenceListener listener=(LocalUserAdHocChatRoomPresenceListener)listeners.next();
    listener.localUserAdHocPresenceChanged(evt);
  }
}
