{
  AdHocChatRoom chatRoom=null;
  ChatRoomManager chatRoomManager=icqProvider.getAimConnection().getChatRoomManager();
  ChatRoomSession chatRoomSession=chatRoomManager.joinRoom(adHocRoomName);
  if (chatRoomSession != null) {
    chatRoom=new AdHocChatRoomIcqImpl(adHocRoomName,chatRoomSession,icqProvider);
  }
  return chatRoom;
}
