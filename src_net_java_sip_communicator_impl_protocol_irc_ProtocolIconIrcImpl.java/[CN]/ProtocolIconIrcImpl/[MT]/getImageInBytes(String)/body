{
  InputStream in=IrcActivator.getResources().getImageInputStream(imageID);
  byte[] image=null;
  if (in != null) {
    try {
      image=new byte[in.available()];
      in.read(image);
    }
 catch (    IOException e) {
      logger.error("Failed to load image:" + imageID,e);
    }
  }
  return image;
}
