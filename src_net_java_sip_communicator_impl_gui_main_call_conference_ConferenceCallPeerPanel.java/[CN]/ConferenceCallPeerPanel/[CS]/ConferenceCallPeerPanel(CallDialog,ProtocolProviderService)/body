{
  super(callDialog,null);
  this.peerName=protocolProvider.getAccountID().getDisplayName();
  this.peerId=protocolProvider.getAccountID().getUserID();
  soundLevelsIndicator=createSoundLevelIndicator(protocolProvider);
  callTitlePanel=new CallTitlePanel(new Color(GuiActivator.getResources().getColor("service.gui.CALL_LOCAL_USER_BACKGROUND")));
  this.init();
}
