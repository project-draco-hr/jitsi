{
  super.paint(g);
  g=g.create();
  try {
    AntialiasingManager.activateAntialiasing(g);
    Graphics2D g2=(Graphics2D)g;
    g2.setColor(Constants.BORDER_COLOR);
    g2.setStroke(new BasicStroke(1f));
    g2.drawRoundRect(0,0,this.getWidth() - 1,this.getHeight() - 1,8,8);
  }
  finally {
    g.dispose();
  }
}
