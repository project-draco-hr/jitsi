{
  List<String> jmfEncodings=new ArrayList<String>();
  if (rtpPayloadTypeStrings != null)   for (  String rtpPayloadTypeStr : rtpPayloadTypeStrings) {
    String jmfEncoding=rtpPayloadTypeToJmfEncoding(rtpPayloadTypeStr);
    if (jmfEncoding != null)     jmfEncodings.add(jmfEncoding);
  }
  return jmfEncodings;
}
