{
  List<MediaFormat> mediaFormats=new ArrayList<MediaFormat>();
  for (  Map.Entry<String,String> rtpPayloadTypeStrToEncoding : rtpPayloadTypeStrToEncodings.entrySet())   if (rtpPayloadTypeStrToEncoding.getValue().equals(encoding))   for (  MediaFormat mediaFormat : rtpPayloadTypeToMediaFormats(rtpPayloadTypeStrToEncoding.getKey()))   mediaFormats.add(mediaFormat);
  return mediaFormats;
}
