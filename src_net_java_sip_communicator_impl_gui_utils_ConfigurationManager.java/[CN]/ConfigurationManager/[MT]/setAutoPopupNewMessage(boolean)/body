{
  ConfigurationManager.autoPopupNewMessage=autoPopupNewMessage;
  ConfigurationService configService=GuiActivator.getConfigurationService();
  if (autoPopupNewMessage)   configService.setProperty("net.java.sip.communicator.impl.gui.autoPopupNewMessage","yes");
 else   configService.setProperty("net.java.sip.communicator.impl.gui.autoPopupNewMessage","no");
}
