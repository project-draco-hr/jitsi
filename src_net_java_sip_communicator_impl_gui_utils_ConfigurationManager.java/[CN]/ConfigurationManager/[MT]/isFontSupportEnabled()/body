{
  String fontDisabledProp="net.java.sip.communicator.impl.gui.FONT_SUPPORT_ENABLED";
  boolean defaultValue=false;
  String defaultSettingStr=GuiActivator.getResources().getSettingsString(fontDisabledProp);
  if (defaultSettingStr != null)   defaultValue=Boolean.parseBoolean(defaultSettingStr);
  return configService.getBoolean(fontDisabledProp,defaultValue);
}
