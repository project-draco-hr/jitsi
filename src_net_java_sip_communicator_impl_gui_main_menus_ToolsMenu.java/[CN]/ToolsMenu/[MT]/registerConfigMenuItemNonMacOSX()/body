{
  ResourceManagementService r=GuiActivator.getResources();
  configMenuItem=new JMenuItem(r.getI18NString("service.gui.SETTINGS"),r.getImage("service.gui.icons.CONFIGURE_ICON"));
  add(configMenuItem);
  configMenuItem.setMnemonic(r.getI18nMnemonic("service.gui.SETTINGS"));
  configMenuItem.setName("config");
  configMenuItem.addActionListener(this);
}
