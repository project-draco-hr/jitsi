{
  configMenuItem=new JMenuItem(GuiActivator.getResources().getI18NString("service.gui.SETTINGS"),GuiActivator.getResources().getImage("service.gui.icons.CONFIGURE_ICON"));
  this.add(configMenuItem);
  configMenuItem.setMnemonic(GuiActivator.getResources().getI18nMnemonic("service.gui.SETTINGS"));
  configMenuItem.setName("config");
  configMenuItem.addActionListener(this);
}
