{
  MediaLocator locator=null;
  try {
    if (streams == null) {
      locator=getLocator();
      streams=new DSAudioStream[]{new DSAudioStream(locator)};
    }
  }
 catch (  Exception e) {
    streams=new DSAudioStream[0];
    logger.error("Failed to create DSAudioStream from locator " + locator,e);
  }
  return streams;
}
