{
  bundleContext=bc;
  bundleContext.registerService(ConfigurationForm.class.getName(),new LazyConfigurationForm("net.java.sip.communicator.plugin.autoaway.AutoAwayConfigurationPanel",getClass().getClassLoader(),"plugin.autoaway.PLUGIN_ICON","plugin.autoaway.AUTO_STATUS"),null);
  new Thread(new Runnable(){
    public void run(){
      try {
        Thread.sleep(5000);
      }
 catch (      InterruptedException e) {
      }
      startThread();
    }
  }
).start();
}
