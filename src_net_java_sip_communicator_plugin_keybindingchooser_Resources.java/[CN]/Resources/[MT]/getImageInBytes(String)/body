{
  InputStream in=KeybindingChooserActivator.getResources().getImageInputStream(imageID);
  if (in == null)   return null;
  byte[] image=null;
  try {
    image=new byte[in.available()];
    in.read(image);
  }
 catch (  IOException e) {
    log.error("Failed to load image:" + imageID,e);
  }
  return image;
}
