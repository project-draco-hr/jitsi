{
  image=GuiActivator.getResources().getImage("service.gui.buttons.CALL_NUMBER_BUTTON").getImage();
  pressedImage=GuiActivator.getResources().getImage("service.gui.buttons.CALL_NUMBER_BUTTON_PRESSED").getImage();
  rolloverImage=GuiActivator.getResources().getImage("service.gui.buttons.CALL_NUMBER_BUTTON_ROLLOVER").getImage();
