{
  image=GuiActivator.getResources().getImage(ImageLoader.CALL_BUTTON_SMALL.getId()).getImage();
  pressedImage=GuiActivator.getResources().getImage(ImageLoader.CALL_BUTTON_SMALL_PRESSED.getId()).getImage();
  rolloverImage=GuiActivator.getResources().getImage(ImageLoader.CALL_BUTTON_SMALL_ROLLOVER.getId()).getImage();
  setBackgroundImage(image);
  setPressedImage(pressedImage);
  setRolloverImage(rolloverImage);
}
