{
  logger.debug("Trying to dispatch geolocation contact update for " + sourceContact);
  Contact source=opsetprez.findContactByID(sourceContact);
  GeolocationEvent evt=new GeolocationEvent(source,jabberProvider,newGeolocation,OperationSetGeolocationJabberImpl.this);
  logger.debug("Dispatching  geolocation contact update. Listeners=" + geolocationContactsListeners.size() + " evt="+ evt);
  Iterator listeners=null;
synchronized (geolocationContactsListeners) {
    listeners=new ArrayList(geolocationContactsListeners).iterator();
  }
  while (listeners.hasNext()) {
    GeolocationListener listener=(GeolocationListener)listeners.next();
    listener.contactGeolocationChanged(evt);
  }
}
