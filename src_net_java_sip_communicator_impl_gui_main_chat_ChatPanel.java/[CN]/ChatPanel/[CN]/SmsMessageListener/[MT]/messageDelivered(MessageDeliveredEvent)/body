{
  Message msg=evt.getSourceMessage();
  Contact contact=evt.getDestinationContact();
  processMessage(contact.getDisplayName(),new Date(System.currentTimeMillis()),Constants.OUTGOING_MESSAGE,msg.getContent(),msg.getContentType());
  processMessage(contact.getDisplayName(),new Date(System.currentTimeMillis()),Constants.ACTION_MESSAGE,GuiActivator.getResources().getI18NString("service.gui.SMS_SUCCESSFULLY_SENT"),"text");
}
