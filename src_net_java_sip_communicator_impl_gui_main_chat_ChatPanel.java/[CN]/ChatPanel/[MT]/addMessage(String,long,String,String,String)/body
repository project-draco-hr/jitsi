{
  ChatMessage chatMessage=new ChatMessage(contactName,date,messageType,message,contentType);
  if (ConfigurationManager.isHistoryShown() && !isHistoryLoaded) {
synchronized (incomingEventBuffer) {
      incomingEventBuffer.add(chatMessage);
    }
  }
 else {
    appendChatMessage(chatMessage);
  }
  this.lastHistoryMsgTimestamp=date;
}
