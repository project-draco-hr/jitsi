{
  this.conversationPanel.appendMessageToEnd("<DIV identifier=\"message\" style=\"color:#707070;\">" + GuiActivator.getResources().getI18NString("service.gui.IS_NOW",new String[]{evt.getSourceMember().getName(),getRoleDescription(evt.getNewRole())}) + "</DIV>",ChatConversationPanel.HTML_CONTENT_TYPE);
}
