{
  if (subjectPanel != null) {
    subjectPanel.setSubject(subject);
    this.processMessage(chatSession.getChatName(),new Date(System.currentTimeMillis()),Constants.STATUS_MESSAGE,Messages.getI18NString("chatRoomSubjectChanged",new String[]{chatSession.getChatName(),subject}).getText(),ChatConversationPanel.TEXT_CONTENT_TYPE);
  }
}
