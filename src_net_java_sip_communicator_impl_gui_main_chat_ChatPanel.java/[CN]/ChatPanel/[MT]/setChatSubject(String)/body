{
  if (subjectPanel != null) {
    subjectPanel.setSubject(subject);
    this.addMessage(chatSession.getChatName(),System.currentTimeMillis(),Chat.STATUS_MESSAGE,GuiActivator.getResources().getI18NString("service.gui.CHAT_ROOM_SUBJECT_CHANGED",new String[]{chatSession.getChatName(),subject}),ChatConversationPanel.TEXT_CONTENT_TYPE);
  }
}
