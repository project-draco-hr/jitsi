{
  ChatConversationPanel chatConversationPanel=getChatConversationPanel();
  HTMLDocument doc=(HTMLDocument)chatConversationPanel.getChatEditorPane().getDocument();
  Element root=doc.getDefaultRootElement();
  try {
    doc.insertAfterEnd(root.getElement(root.getElementCount() - 1),"<br>");
  }
 catch (  BadLocationException e) {
    logger.error("Insert in the HTMLDocument failed.",e);
  }
catch (  IOException e) {
    logger.error("Insert in the HTMLDocument failed.",e);
  }
  chatConversationPanel.setCarretToEnd();
}
