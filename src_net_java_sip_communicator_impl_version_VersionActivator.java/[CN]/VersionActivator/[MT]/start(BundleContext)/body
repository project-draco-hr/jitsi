{
  if (logger.isDebugEnabled())   logger.debug("Started.");
  VersionActivator.bundleContext=context;
  VersionServiceImpl versionServiceImpl=new VersionServiceImpl();
  versionServReg=context.registerService(VersionService.class.getName(),versionServiceImpl,null);
  if (logger.isDebugEnabled())   logger.debug("SIP Protocol Provider Factory ... [REGISTERED]");
  if (logger.isDebugEnabled())   logger.debug("SIP Communicator Version: sip-communicator-" + VersionImpl.currentVersion().toString());
  getConfigurationService().setProperty("sip-communicator.version",VersionImpl.currentVersion().toString(),true);
  getConfigurationService().setProperty("sip-communicator.application.name",VersionImpl.currentVersion().getApplicationName(),true);
}
