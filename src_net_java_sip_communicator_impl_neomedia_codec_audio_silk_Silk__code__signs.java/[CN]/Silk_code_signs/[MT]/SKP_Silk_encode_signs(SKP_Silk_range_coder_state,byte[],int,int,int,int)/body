{
  int i;
  int inData;
  int[] cdf=new int[3];
  i=Silk_macros.SKP_SMULBB(Silk_define.N_RATE_LEVELS - 1,(sigtype << 1) + QuantOffsetType) + RateLevelIndex;
  cdf[0]=0;
  cdf[1]=Silk_tables_sign.SKP_Silk_sign_CDF[i];
  cdf[2]=65535;
  for (i=0; i < length; i++) {
    if (q[i] != 0) {
      inData=(q[i] >> 15) + 1;
      Silk_range_coder.SKP_Silk_range_encoder(sRC,inData,cdf,0);
    }
  }
}
