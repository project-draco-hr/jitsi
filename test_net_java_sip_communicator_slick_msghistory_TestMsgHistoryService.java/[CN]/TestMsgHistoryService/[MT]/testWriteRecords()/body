{
  ServiceReference ref=MsgHistoryServiceLick.bc.getServiceReference(MetaContactListService.class.getName());
  MetaContactListService metaClService=(MetaContactListService)MsgHistoryServiceLick.bc.getService(ref);
  Calendar c=Calendar.getInstance();
  c.set(Calendar.YEAR,c.get(Calendar.YEAR) - 10);
  Date date=c.getTime();
  Iterator iter=metaClService.getRoot().getChildContacts();
  while (iter.hasNext()) {
    MetaContact item=(MetaContact)iter.next();
    logger.debug("item --------- " + item);
    QueryResultSet rs=msgHistoryService.findByStartDate(item,date);
    Iterator iter1=rs;
    while (iter1.hasNext()) {
      logger.debug("222222222222222222222 " + iter1.next());
    }
  }
}
