{
  if (parentOperationSet == null) {
    logger.debug("No presence op. set available. Bailing out.");
    return;
  }
  ServerStoredGroupEvent evt=new ServerStoredGroupEvent(group,eventID,parentOperationSet.getServerStoredContactListRoot(),icqProvider,parentOperationSet);
  logger.trace("Will dispatch the following grp event: " + evt);
synchronized (serverStoredGroupListeners) {
    Iterator listeners=this.serverStoredGroupListeners.iterator();
    while (listeners.hasNext()) {
      ServerStoredGroupListener l=(ServerStoredGroupListener)listeners.next();
      if (eventID == ServerStoredGroupEvent.GROUP_REMOVED_EVENT)       l.groupRemoved(evt);
 else       if (eventID == ServerStoredGroupEvent.GROUP_RENAMED_EVENT)       l.groupNameChanged(evt);
 else       if (eventID == ServerStoredGroupEvent.GROUP_CREATED_EVENT)       l.groupCreated(evt);
 else       if (eventID == ServerStoredGroupEvent.GROUP_RESOLVED_EVENT)       l.groupResolved(evt);
    }
  }
}
