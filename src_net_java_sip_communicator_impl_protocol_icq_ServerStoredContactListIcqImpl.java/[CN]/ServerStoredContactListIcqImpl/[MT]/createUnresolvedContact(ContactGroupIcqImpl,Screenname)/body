{
  Buddy volatileBuddy=new VolatileBuddy(screenname);
  ContactIcqImpl newUnresolvedContact=new ContactIcqImpl(volatileBuddy,this,false,false);
  parentGroup.addContact(newUnresolvedContact);
  fireContactAdded(parentGroup,newUnresolvedContact,parentGroup.findContactIndex(volatileBuddy));
  return newUnresolvedContact;
}
