{
  logger.trace("Group name for " + group.getName() + "changed from="+ oldName+ " to="+ newName);
  ContactGroupIcqImpl contactGroup=findContactGroup(group);
  if (contactGroup == null) {
    logger.debug("group name changed event received for unknown group" + group);
  }
  if (contactGroup.getGroupName().equals(contactGroup.getNameCopy())) {
    logger.trace("Group name hasn't really changed(" + contactGroup.getGroupName() + "). Ignoring");
    return;
  }
  logger.trace("Dispatching group change event.");
  contactGroup.initNameCopy();
  fireGroupEvent(contactGroup,ServerStoredGroupEvent.GROUP_RENAMED_EVENT);
}
