{
  ContentPacketExtension content=JingleUtils.createDescription(CreatorEnum.initiator,supportedFormats.get(0).getMediaType().toString(),JingleUtils.getSenders(direction,!getPeer().isInitiator()),supportedFormats,supportedExtensions,getDynamicPayloadTypes(),getRtpExtensionsRegistry());
  this.localContentMap.put(content.getName(),content);
  return content;
}
