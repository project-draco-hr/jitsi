{
  doFormat();
  mydispose();
  String locatorPrefix=LOCATOR_PREFIX + id;
  Vector devices=new Vector();
  if (anyVideo) {
    for (Enumeration ve=sortedFormats(videoFormats); ve.hasMoreElements(); ) {
      String vKey=(String)ve.nextElement();
      Format vForm=(VideoFormat)videoFormats.get(vKey);
      Format[] farray=null;
      farray=new Format[1];
      farray[0]=vForm;
      String name=locatorPrefix + "/" + vKey;
      CaptureDeviceInfo cdi=new CaptureDeviceInfo(name,new MediaLocator(name),farray);
      CaptureDeviceManager.addDevice(cdi);
      devices.addElement(cdi);
    }
  }
  return devices;
}
