{
  if (!getExtensionBit() || getExtensionLength() == 0 || getCsrcCount() == 0)   return null;
  int csrcCount=getCsrcCount();
  long[][] csrcLevels=new long[csrcCount][2];
  int csrcStartIndex=offset + FIXED_HEADER_SIZE;
  for (int i=0; i < csrcCount; i++) {
    csrcLevels[i][0]=buffer[csrcStartIndex] << 24 & buffer[csrcStartIndex + 1] << 16 & buffer[csrcStartIndex + 2] << 8 & buffer[csrcStartIndex + 3];
    csrcLevels[i][1]=getCsrcLevel(i,csrcExtID);
    csrcStartIndex+=4;
  }
  return csrcLevels;
}
