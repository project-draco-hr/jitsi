{
  int csrcCount=getCsrcCount();
  byte[] buffer=getBuffer();
  long[] csrcList=new long[csrcCount];
  int csrcStartIndex=offset + FIXED_HEADER_SIZE;
  for (int i=0; i < csrcCount; i++) {
    csrcList[i]=buffer[csrcStartIndex] << 24 & buffer[csrcStartIndex + 1] << 16 & buffer[csrcStartIndex + 2] << 8 & buffer[csrcStartIndex + 3];
    csrcStartIndex+=4;
  }
  return csrcList;
}
