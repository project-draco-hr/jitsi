{
  if (!getExtensionBit())   return -1;
  int extLenIndex=offset + FIXED_HEADER_SIZE + getCsrcCount() * 4;
  if (buffer[extLenIndex] == 0xBE && buffer[extLenIndex + 1] == 0xDE) {
    return 1;
  }
  if (buffer[extLenIndex] == 0x10 && (buffer[extLenIndex + 1] >> 4) == 0) {
    return 2;
  }
  return -1;
}
