{
  RTPManager rtpMgr;
  for (int i=0; i < rtpMgrs.length; i++) {
    rtpMgr=rtpMgrs[i];
    rtpMgr.removeReceiveStreamListener(this);
    rtpMgr.removeSessionListener(this);
    rtpMgr.removeTargets("Session ended.");
    rtpMgr.dispose();
  }
  sendStreams.clear();
  mediaControl.stopProcessingMedia(this);
}
