    OperationSetBasicInstantMessagingMsnImpl basicInstantMessaging=new OperationSetBasicInstantMessagingMsnImpl(this);
    supportedOperationSets.put(OperationSetBasicInstantMessaging.class.getName(),basicInstantMessaging);
    typingNotifications=new OperationSetTypingNotificationsMsnImpl(this);
    supportedOperationSets.put(OperationSetTypingNotifications.class.getName(),typingNotifications);
    OperationSetFileTransferMsnImpl fileTransferOpSet=new OperationSetFileTransferMsnImpl(this);
