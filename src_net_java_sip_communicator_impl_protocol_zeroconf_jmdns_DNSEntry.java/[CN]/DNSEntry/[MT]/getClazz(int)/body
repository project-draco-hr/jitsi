{
switch (clazz & DNSConstants.CLASS_MASK) {
case DNSConstants.CLASS_IN:
    return "in";
case DNSConstants.CLASS_CS:
  return "cs";
case DNSConstants.CLASS_CH:
return "ch";
case DNSConstants.CLASS_HS:
return "hs";
case DNSConstants.CLASS_NONE:
return "none";
case DNSConstants.CLASS_ANY:
return "any";
default :
return "?";
}
}
