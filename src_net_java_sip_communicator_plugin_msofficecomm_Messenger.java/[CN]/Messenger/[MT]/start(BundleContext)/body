{
  Messenger.bundleContext=bundleContext;
  metaContactListService=ServiceUtils.getService(bundleContext,MetaContactListService.class);
  bundleContext.addServiceListener(serviceListener);
  for (  ServiceReference reference : bundleContext.getServiceReferences(ProtocolProviderService.class.getName(),null)) {
    serviceListener.serviceChanged(new ServiceEvent(ServiceEvent.REGISTERED,reference));
  }
}
