{
  int mistatus;
  if (presenceStatus == Integer.MIN_VALUE)   mistatus=MISTATUS_UNKNOWN;
 else {
    if (presenceStatus < PresenceStatus.ONLINE_THRESHOLD)     mistatus=MISTATUS_OFFLINE;
 else     if (presenceStatus < PresenceStatus.AWAY_THRESHOLD)     mistatus=MISTATUS_MAY_BE_AVAILABLE;
 else     if (presenceStatus < PresenceStatus.AVAILABLE_THRESHOLD)     mistatus=MISTATUS_AWAY;
 else     mistatus=MISTATUS_ONLINE;
  }
  return mistatus;
}
