{
  String signinName=getSigninName(event.getSourceContact(),event.getSourceProvider());
  if (signinName != null) {
    PresenceStatus oldStatus=event.getOldStatus();
    Messenger.onContactStatusChange(signinName,presenceStatusToMISTATUS((oldStatus == null) ? Integer.MIN_VALUE : oldStatus.getStatus()));
  }
}
