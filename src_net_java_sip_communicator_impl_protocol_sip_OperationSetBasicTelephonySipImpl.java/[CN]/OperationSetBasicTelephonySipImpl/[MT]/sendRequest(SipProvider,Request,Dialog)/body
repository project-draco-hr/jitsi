{
  ClientTransaction clientTransaction=null;
  try {
    clientTransaction=sipProvider.getNewClientTransaction(request);
  }
 catch (  TransactionUnavailableException ex) {
    throwOperationFailedException("Failed to create a client transaction for request:\n" + request,OperationFailedException.INTERNAL_ERROR,ex);
  }
  try {
    dialog.sendRequest(clientTransaction);
  }
 catch (  SipException ex) {
    throwOperationFailedException("Failed to send request:\n" + request,OperationFailedException.NETWORK_FAILURE,ex);
  }
  logger.debug("Sent request:\n" + request);
}
