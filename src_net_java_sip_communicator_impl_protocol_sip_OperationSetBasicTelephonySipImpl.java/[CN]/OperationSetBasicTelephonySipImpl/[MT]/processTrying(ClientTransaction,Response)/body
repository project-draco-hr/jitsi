{
  Dialog dialog=clientTransaction.getDialog();
  CallPeerSipImpl callParticipant=activeCallsRepository.findCallParticipant(dialog);
  if (callParticipant == null) {
    logger.debug("Received a stray trying response.");
    return;
  }
  CallParticipantState callParticipantState=callParticipant.getState();
  if (!CallParticipantState.CONNECTED.equals(callParticipantState) && !CallParticipantState.isOnHold(callParticipantState))   callParticipant.setState(CallParticipantState.CONNECTING);
}
