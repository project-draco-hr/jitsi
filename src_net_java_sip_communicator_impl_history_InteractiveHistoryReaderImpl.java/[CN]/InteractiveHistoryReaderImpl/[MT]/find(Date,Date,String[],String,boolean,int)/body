{
  String queryString="";
  for (  String s : keywords)   queryString+=" " + s;
  final HistoryQueryImpl query=new HistoryQueryImpl(queryString);
  new Thread(){
    public void run(){
      find(startDate,endDate,keywords,field,caseSensitive,resultCount,query);
    }
  }
.start();
  return query;
}
