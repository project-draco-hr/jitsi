{
  ServiceReference[] serRefs=null;
  String osgiFilter="(" + ResourcePack.RESOURCE_NAME + "="+ typeName+ ")";
  try {
    serRefs=ResourceManagementActivator.bundleContext.getServiceReferences(className,osgiFilter);
  }
 catch (  InvalidSyntaxException exc) {
    logger.error("Could not obtain resource packs reference.",exc);
  }
  if (serRefs != null) {
    for (int i=0; i < serRefs.length; i++) {
      ResourcePack rp=(ResourcePack)ResourceManagementActivator.bundleContext.getService(serRefs[i]);
      return rp;
    }
  }
  return null;
}
