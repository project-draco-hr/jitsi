{
  Vector localePacks=new Vector();
  Enumeration packsEnum=languagePacks.keys();
  while (packsEnum.hasMoreElements()) {
    LanguagePack langPack=(LanguagePack)packsEnum.nextElement();
    ResourceBundle rBundle=ResourceBundle.getBundle(langPack.getResourcePackBaseName(),locale,langPack.getClass().getClassLoader());
    if (rBundle != null)     localePacks.add(rBundle);
  }
  return localePacks;
}
