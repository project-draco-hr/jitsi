{
  ResourceBundle resourceBundle;
  while (resourceBundles.hasMoreElements()) {
    resourceBundle=resourceBundles.nextElement();
    try {
      String value=resourceBundle.getString(key);
      if (value != null) {
        return value;
      }
    }
 catch (    MissingResourceException e) {
      continue;
    }
  }
  return null;
}
