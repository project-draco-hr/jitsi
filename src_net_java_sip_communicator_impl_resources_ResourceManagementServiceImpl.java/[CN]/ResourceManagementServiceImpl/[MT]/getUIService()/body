{
  if (uiService == null) {
    ServiceReference uiReference=ResourceManagementActivator.bundleContext.getServiceReference(UIService.class.getName());
    if (uiReference == null) {
      return null;
    }
    uiService=(UIService)ResourceManagementActivator.bundleContext.getService(uiReference);
  }
  return uiService;
}
