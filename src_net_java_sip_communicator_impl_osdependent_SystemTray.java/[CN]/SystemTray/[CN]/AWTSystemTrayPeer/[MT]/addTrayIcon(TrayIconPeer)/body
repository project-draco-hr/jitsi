{
  try {
    addTrayIcon.invoke(impl,new Object[]{((AWTTrayIconPeer)trayIconPeer).getImpl()});
  }
 catch (  IllegalAccessException ex) {
    throw new UndeclaredThrowableException(ex);
  }
catch (  InvocationTargetException ex) {
    Throwable cause=ex.getCause();
    if (cause == null)     throw new UndeclaredThrowableException(ex);
    if (cause instanceof NullPointerException)     throw (NullPointerException)cause;
    if (cause instanceof IllegalArgumentException)     throw (IllegalArgumentException)cause;
    throw new UndeclaredThrowableException(cause);
  }
}
