{
  if (Constants.TABBED_CHAT_WINDOW) {
    if (chatWindow.getTabCount() > 0)     this.chatWindow.removeChatTab(chatPanel);
 else {
      chatWindow.dispose();
      chatWindow=null;
    }
  }
 else {
    chatPanel.getChatWindow().setVisible(false);
  }
synchronized (chats) {
    chats.remove(chatPanel.getMetaContact());
  }
}
