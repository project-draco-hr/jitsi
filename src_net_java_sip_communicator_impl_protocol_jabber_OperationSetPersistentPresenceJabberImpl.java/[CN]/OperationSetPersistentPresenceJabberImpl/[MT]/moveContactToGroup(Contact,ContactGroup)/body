{
  assertConnected();
  if (!(contactToMove instanceof ContactJabberImpl))   throw new IllegalArgumentException("The specified contact is not an jabber contact." + contactToMove);
  if (!(newParent instanceof ContactGroupJabberImpl))   throw new IllegalArgumentException("The specified group is not an jabber contact group." + newParent);
  ssContactList.moveContact((ContactJabberImpl)contactToMove,(ContactGroupJabberImpl)newParent);
}
