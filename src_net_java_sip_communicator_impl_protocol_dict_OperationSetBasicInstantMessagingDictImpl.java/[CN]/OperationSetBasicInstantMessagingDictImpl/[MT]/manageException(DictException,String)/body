{
  int errorCode=dix.getErrorCode();
  if (errorCode == DictReturnCode.INVALID_DATABASE) {
    return DictActivator.getResources().getI18NString("dict.invalidDatabase",new String[]{database});
  }
 else   if (errorCode == DictReturnCode.INVALID_STRATEGY) {
    return DictActivator.getResources().getI18NString("dict.invalidStrategy");
  }
 else   if (errorCode == DictReturnCode.NO_MATCH) {
    return DictActivator.getResources().getI18NString("dict.noMatch");
  }
  return dix.getMessage();
}
