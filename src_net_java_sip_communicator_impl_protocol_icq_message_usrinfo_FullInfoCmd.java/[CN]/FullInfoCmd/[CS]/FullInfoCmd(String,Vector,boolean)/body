{
  super(21,2);
  this.senderUin=senderUin;
  Iterator iter=changedData.iterator();
  while (iter.hasNext()) {
    ServerStoredDetails.GenericDetail item=(ServerStoredDetails.GenericDetail)iter.next();
    if (isToBeCleared)     changeDataTlvs.add(getClearTlv(item));
 else     changeDataTlvs.add(getTlvForChange(item));
  }
}
