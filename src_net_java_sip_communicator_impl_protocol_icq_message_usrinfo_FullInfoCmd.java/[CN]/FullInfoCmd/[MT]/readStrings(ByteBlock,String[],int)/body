{
  for (int i=0; i < result.length; i++) {
    final int textlen=LEBinaryTools.getUShort(block,offset) - 1;
    offset+=2;
    if (textlen > 0) {
      ByteBlock field=block.subBlock(offset,textlen);
      result[i]=OscarTools.getString(field,"US-ASCII");
      offset+=textlen;
    }
    offset++;
  }
  return offset;
}
