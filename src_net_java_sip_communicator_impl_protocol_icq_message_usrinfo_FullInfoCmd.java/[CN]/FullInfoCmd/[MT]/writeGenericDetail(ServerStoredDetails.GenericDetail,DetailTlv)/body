{
  if (detail instanceof ServerStoredDetails.StringDetail) {
    tlv.writeString(((ServerStoredDetails.StringDetail)detail).getString());
  }
 else   if (detail instanceof ServerStoredDetails.TimeZoneDetail) {
    int offset=((ServerStoredDetails.TimeZoneDetail)detail).getTimeZone().getRawOffset() / (60 * 60 * 1000);
    tlv.writeUByte(offset);
  }
}
