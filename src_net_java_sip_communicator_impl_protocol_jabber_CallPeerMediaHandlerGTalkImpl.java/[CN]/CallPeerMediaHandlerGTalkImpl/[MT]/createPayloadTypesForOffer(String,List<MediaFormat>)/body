{
  List<PayloadTypePacketExtension> peList=new ArrayList<PayloadTypePacketExtension>();
  for (  MediaFormat fmt : supportedFormats) {
    PayloadTypePacketExtension ext=JingleUtils.formatToPayloadType(fmt,getDynamicPayloadTypes());
    ext.setNamespace(name.equals(AUDIO_RTP) ? SessionIQProvider.GTALK_AUDIO_NAMESPACE : SessionIQProvider.GTALK_VIDEO_NAMESPACE);
    peList.add(ext);
  }
  return peList;
}
