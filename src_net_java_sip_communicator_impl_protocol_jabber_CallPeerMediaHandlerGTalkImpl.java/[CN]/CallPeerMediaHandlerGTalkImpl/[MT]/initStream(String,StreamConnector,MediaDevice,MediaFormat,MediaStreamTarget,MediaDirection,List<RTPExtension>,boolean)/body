{
  if (format instanceof VideoMediaFormat) {
    Map<String,String> settings=new HashMap<String,String>();
    settings.put("h264.intrarefresh","false");
    settings.put("h264.profile","baseline");
    format.setAdditionalCodecSettings(settings);
  }
  MediaStream stream=super.initStream(connector,device,format,target,direction,rtpExtensions,masterStream);
  if (stream != null)   stream.setName(streamName);
  return stream;
}
