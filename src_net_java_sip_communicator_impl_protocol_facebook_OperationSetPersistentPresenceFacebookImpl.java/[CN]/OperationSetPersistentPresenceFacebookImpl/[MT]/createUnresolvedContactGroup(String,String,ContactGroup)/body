{
  ContactGroupFacebookImpl newGroup=new ContactGroupFacebookImpl(ContactGroupFacebookImpl.createNameFromUID(groupUID),parentProvider);
  newGroup.setResolved(false);
  if (parentGroup == null)   parentGroup=getServerStoredContactListRoot();
  ((ContactGroupFacebookImpl)parentGroup).addSubgroup(newGroup);
  this.fireServerStoredGroupEvent(newGroup,ServerStoredGroupEvent.GROUP_CREATED_EVENT);
  return newGroup;
}
