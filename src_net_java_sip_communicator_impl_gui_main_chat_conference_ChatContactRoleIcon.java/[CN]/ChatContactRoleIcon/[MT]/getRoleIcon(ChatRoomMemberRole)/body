{
  Icon i=null;
switch (role) {
case OWNER:
    i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_OWNER));
  break;
case ADMINISTRATOR:
i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_ADMIN));
break;
case MODERATOR:
i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_MODERATOR));
break;
case MEMBER:
i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_STANDARD));
break;
case GUEST:
i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_GUEST));
break;
default :
i=new ImageIcon(ImageLoader.getImage(ImageLoader.CHATROOM_MEMBER_SILENT));
}
return i;
}
