{
  OutputPortAudioStream out=new OutputPortAudioStream(deviceIndex,sampleRate,channels);
  outputStreams.add(out);
  if (isEnabledEchoCancel() && inputStreams.size() > 0) {
    MasterPortAudioStream st=inputStreams.values().iterator().next();
    st.setParams(out,isEnabledEchoCancel(),isEnabledDeNoise(),getFrameSize(),getFilterLength());
  }
  return out;
}
