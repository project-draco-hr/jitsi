{
  if (maxPacketsPerMillisPolicy == null) {
    if (maxPackets > 0) {
      if (perMillis < 1)       throw new IllegalArgumentException("perMillis");
      maxPacketsPerMillisPolicy=new MaxPacketsPerMillisPolicy(maxPackets,perMillis);
    }
  }
 else {
    maxPacketsPerMillisPolicy.setMaxPacketsPerMillis(maxPackets,perMillis);
  }
}
