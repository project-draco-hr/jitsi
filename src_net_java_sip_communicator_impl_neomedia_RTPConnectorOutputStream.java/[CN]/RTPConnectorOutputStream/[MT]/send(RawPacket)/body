{
  if (!isSocketValid()) {
    return false;
  }
  numberOfPackets++;
  for (  InetSocketAddress target : targets) {
    try {
      sendToTarget(packet,target);
      if (logPacket(numberOfPackets)) {
        PacketLoggingService packetLogging=NeomediaActivator.getPacketLogging();
        if ((packetLogging != null) && packetLogging.isLoggingEnabled(PacketLoggingService.ProtocolName.RTP))         doLogPacket(packet,target);
      }
    }
 catch (    IOException ex) {
      availRawPackets.offer(packet);
      return false;
    }
  }
  availRawPackets.offer(packet);
  return true;
}
