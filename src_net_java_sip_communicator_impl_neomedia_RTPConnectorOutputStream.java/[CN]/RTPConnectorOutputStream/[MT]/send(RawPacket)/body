{
  for (  InetSocketAddress target : targets) {
    try {
      socket.send(new DatagramPacket(packet.getBuffer(),packet.getOffset(),packet.getLength(),target.getAddress(),target.getPort()));
    }
 catch (    IOException ex) {
      availRawPackets.offer(packet);
      return false;
    }
  }
  availRawPackets.offer(packet);
  return true;
}
