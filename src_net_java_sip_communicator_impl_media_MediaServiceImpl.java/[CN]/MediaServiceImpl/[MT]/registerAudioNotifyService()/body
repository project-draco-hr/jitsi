{
  AudioNotifierServiceImpl audioNotifier=new AudioNotifierServiceImpl(deviceConfiguration);
  audioNotifier.setMute(!MediaActivator.getConfigurationService().getBoolean("net.java.sip.communicator.impl.sound.isSoundEnabled",true));
  MediaActivator.getBundleContext().registerService(AudioNotifierService.class.getName(),audioNotifier,null);
  if (logger.isInfoEnabled())   logger.info("Audio Notifier Service ...[REGISTERED]");
}
