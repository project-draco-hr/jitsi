{
  MediaServiceImpl msimpl=new MediaServiceImpl();
  msimpl.start();
  String className="classname";
  Class pic=Class.forName(className);
  Object instance=pic.newInstance();
  PlugInManager.addPlugIn(className,((Codec)instance).getSupportedInputFormats(),((Codec)instance).getSupportedOutputFormats(null),PlugInManager.CODEC);
  System.out.println("done");
}
