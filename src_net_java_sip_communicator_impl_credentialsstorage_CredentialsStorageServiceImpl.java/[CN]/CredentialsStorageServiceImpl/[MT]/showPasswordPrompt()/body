{
  String master;
  UIService uiService=CredentialsStorageActivator.getUIService();
  boolean correct=true;
  do {
    master=uiService.getMasterPassword(correct);
    if (master == null)     return null;
    correct=((master.length() != 0) && verifyMasterPassword(master));
  }
 while (!correct);
  return master;
}
