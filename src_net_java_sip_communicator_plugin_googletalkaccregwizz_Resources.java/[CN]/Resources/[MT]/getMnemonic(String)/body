{
  String resourceString;
  try {
    resourceString=STRING_RESOURCE_BUNDLE.getString(key);
    int mnemonicIndex=resourceString.indexOf('&');
    if (mnemonicIndex > -1) {
      return resourceString.charAt(mnemonicIndex + 1);
    }
  }
 catch (  MissingResourceException e) {
    return '!';
  }
  return '!';
}
