{
  float MEAN_ENER=Ld8k.MEAN_ENER;
  float[] pred=TabLd8k.pred;
  float ener_code, pred_code;
  int i;
  float gcode0;
  pred_code=MEAN_ENER;
  ener_code=0.01f;
  for (i=0; i < l_subfr; i++)   ener_code+=code[i] * code[i];
  ener_code=10.0f * (float)Math.log10(ener_code / (float)l_subfr);
  pred_code-=ener_code;
  for (i=0; i < 4; i++)   pred_code+=pred[i] * past_qua_en[i];
  gcode0=pred_code;
  gcode0=(float)Math.pow((double)10.0,(double)(gcode0 / 20.0));
  return gcode0;
}
