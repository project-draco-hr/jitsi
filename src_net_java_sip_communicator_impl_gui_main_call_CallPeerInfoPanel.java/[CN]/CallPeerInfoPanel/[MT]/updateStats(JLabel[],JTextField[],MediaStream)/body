{
  boolean setVisible=false;
  if (mediaStream != null) {
    MediaStreamStats mediaStreamStats=mediaStream.getMediaStreamStats();
    if (mediaStreamStats != null) {
      mediaStreamStats.updateStats();
      CallPeerInfoPanel.updateLabel(label,textField,mediaStreamStats);
      setVisible=true;
    }
  }
  for (int i=0; i < label.length; ++i) {
    label[i].setVisible(setVisible);
    textField[i].setVisible(setVisible);
  }
}
