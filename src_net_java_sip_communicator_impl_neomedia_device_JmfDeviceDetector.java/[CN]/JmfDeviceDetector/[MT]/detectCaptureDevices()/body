{
  if (!NeomediaActivator.getConfigurationService().getBoolean(MediaServiceImpl.DISABLE_AUDIO_SUPPORT_PNAME,false)) {
    initializeAudio();
  }
  DeviceConfiguration.addAudioSystem(DeviceConfiguration.AUDIO_NONE);
  if (!NeomediaActivator.getConfigurationService().getBoolean(MediaServiceImpl.DISABLE_VIDEO_SUPPORT_PNAME,false)) {
    initializeVideo();
  }
}
