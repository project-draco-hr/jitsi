{
  Hashtable<String,String> hashtable=new Hashtable<String,String>();
  hashtable.put(ReplacementService.SOURCE_NAME,ReplacementServiceVbox7Impl.VBOX7_CONFIG_LABEL);
  vbox7Source=new ReplacementServiceVbox7Impl();
  vbox7ServReg=context.registerService(ReplacementService.class.getName(),vbox7Source,hashtable);
  logger.info("Vbox7 source implementation [STARTED].");
}
