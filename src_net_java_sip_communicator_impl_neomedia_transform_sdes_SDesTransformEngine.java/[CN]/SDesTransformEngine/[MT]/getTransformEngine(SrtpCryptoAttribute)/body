{
  if (attribute.getSessionParams() != null && attribute.getSessionParams().length > 0) {
    throw new IllegalArgumentException("session parameters are not supported");
  }
  SrtpCryptoSuite cs=attribute.getCryptoSuite();
  return new SRTPTransformEngine(getKey(attribute),getSalt(attribute),new SRTPPolicy(getEncryptionCipher(cs),cs.getEncKeyLength() / 8,getHashAlgorithm(cs),cs.getSrtpAuthKeyLength() / 8,cs.getSrtpAuthTagLength() / 8,cs.getSaltKeyLength() / 8),new SRTPPolicy(getEncryptionCipher(cs),cs.getEncKeyLength() / 8,getHashAlgorithm(cs),cs.getSrtcpAuthKeyLength() / 8,cs.getSrtcpAuthTagLength() / 8,cs.getSaltKeyLength() / 8));
}
