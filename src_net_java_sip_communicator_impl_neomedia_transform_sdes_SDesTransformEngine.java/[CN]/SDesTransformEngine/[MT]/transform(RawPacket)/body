{
  long ssrc=pkt.getSSRC();
  SRTPCryptoContext context=contexts.get(ssrc);
  if (context == null) {
    logger.debug("OutContext created for SSRC=" + Long.toHexString(ssrc) + " and key "+ outAttribute.getKeyParams()[0].encode());
    context=createContext(ssrc,outAttribute);
    context.deriveSrtpKeys(0);
    contexts.put(ssrc,context);
  }
  context.transformPacket(pkt);
  return pkt;
}
