{
  SyndFeedInput input=new SyndFeedInput();
  try {
    this.feed=input.build(new XmlReader(rssURL));
  }
 catch (  FileNotFoundException ex) {
    throw ex;
  }
catch (  IOException ex) {
    throw new OperationFailedException("Failed to create and XmlReader for url: " + rssURL,OperationFailedException.GENERAL_ERROR,ex);
  }
catch (  FeedException fex) {
    throw new OperationFailedException("Failed to create and XmlReader for url: " + rssURL,OperationFailedException.GENERAL_ERROR,fex);
  }
  feed.getEntries();
  this.title=this.feed.getTitle();
  items=(SyndEntry[])this.feed.getEntries().toArray(new SyndEntry[0]);
  if (items.length == 0) {
    lastItemKey=new RssItemKey(new Date(0));
    return;
  }
  if (items[items.length - 1].getPublishedDate() != null) {
    Arrays.sort(items,syndEntryComparator);
    lastItemKey=new RssItemKey(items[items.length - 1].getPublishedDate());
  }
 else {
    lastItemKey=new RssItemKey(items[0].getLink());
  }
}
