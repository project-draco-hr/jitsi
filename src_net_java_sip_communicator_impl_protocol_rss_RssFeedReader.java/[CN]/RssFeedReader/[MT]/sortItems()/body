{
  int i;
  int size=items.length;
  SyndEntry temp;
  boolean inversion;
  do {
    inversion=false;
    for (i=0; i < size - 1; i++) {
      if ((items[i].getPublishedDate() != null) && (items[i + 1].getPublishedDate() != null))       if (items[i].getPublishedDate().compareTo(items[i + 1].getPublishedDate()) < 0) {
        temp=items[i];
        items[i]=items[i + 1];
        items[i + 1]=temp;
        inversion=true;
      }
    }
    size--;
  }
 while (inversion);
}
