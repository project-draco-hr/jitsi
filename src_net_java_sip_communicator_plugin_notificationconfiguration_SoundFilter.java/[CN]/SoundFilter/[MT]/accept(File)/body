{
  if (f.isDirectory()) {
    return true;
  }
  String extension=Utils.getExtension(f);
  if (extension != null) {
    if (extension.equals(Utils.wav) || extension.equals(Utils.mid) || extension.equals(Utils.mp2)|| extension.equals(Utils.mp3)|| extension.equals(Utils.mod)|| extension.equals(Utils.ogg)|| extension.equals(Utils.wma)|| extension.equals(Utils.au)|| extension.equals(Utils.ram)) {
      return true;
    }
 else {
      return false;
    }
  }
  return false;
}
