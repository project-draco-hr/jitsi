{
  String desc="Sound File (";
  if (this.soundFormats != null) {
    for (int i=0; i < this.soundFormats.length; ++i) {
      if (i != 0) {
        desc+=", ";
      }
      desc+="*." + this.soundFormats[i];
    }
  }
 else {
    desc+="*.au, *.mid, *.mod, *.mp2, *.mp3, *.ogg, *.ram, *.wav, " + "*.wma";
  }
  desc+=")";
  return desc;
}
