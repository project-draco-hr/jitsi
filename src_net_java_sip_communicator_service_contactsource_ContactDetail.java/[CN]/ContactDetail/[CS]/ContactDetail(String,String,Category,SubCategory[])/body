{
  this.contactDetailValue=contactDetailValue;
  if (!StringUtils.isNullOrEmpty(detailDisplayName))   this.detailDisplayName=detailDisplayName;
 else   this.detailDisplayName=contactDetailValue;
  this.category=category;
  if (subCategories != null) {
    for (    SubCategory subCategory : subCategories) {
      if ((subCategory != null) && !this.subCategories.contains(subCategory)) {
        this.subCategories.add(subCategory);
      }
    }
  }
}
