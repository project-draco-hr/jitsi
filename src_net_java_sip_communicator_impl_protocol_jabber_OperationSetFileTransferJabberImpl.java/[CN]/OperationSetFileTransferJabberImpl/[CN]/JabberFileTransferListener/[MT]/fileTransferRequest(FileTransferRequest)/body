{
  logger.debug("Incoming Jabber file transfer request.");
  IncomingFileTransferRequest incomingFileTransferRequest=new IncomingFileTransferRequestJabberImpl(jabberProvider,request);
  FileTransferRequestEvent fileTransferRequestEvent=new FileTransferRequestEvent(incomingFileTransferRequest,new Date());
  fireFileTransferRequest(fileTransferRequestEvent);
}
