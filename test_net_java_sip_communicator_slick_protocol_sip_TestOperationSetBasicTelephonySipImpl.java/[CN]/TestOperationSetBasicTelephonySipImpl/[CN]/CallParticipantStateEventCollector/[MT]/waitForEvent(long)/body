{
  logger.trace("Waiting for a CallParticpantEvent");
synchronized (this) {
    if (listenedCallParticipant.getState() == awaitedState) {
      logger.trace("Event already received. " + collectedEvents);
      return;
    }
    try {
      wait(waitFor);
      if (collectedEvents.size() > 0)       logger.trace("Received a CallParticpantEvent.");
 else       logger.trace("No CallParticpantEvent received for " + waitFor + "ms.");
      listenedCallParticipant.removeCallParticipantListener(this);
    }
 catch (    InterruptedException ex) {
      logger.debug("Interrupted while waiting for a " + "CallParticpantEvent",ex);
    }
  }
}
