{
  for (Iterator<? extends Call> callIter=telephony.getActiveCalls(); callIter.hasNext(); ) {
    Call call=callIter.next();
    for (Iterator<CallPeer> peerIter=call.getCallPeers(); peerIter.hasNext(); ) {
      CallPeer peer=peerIter.next();
      if (address.equals(peer.getAddress()))       return peer;
    }
  }
  return null;
}
