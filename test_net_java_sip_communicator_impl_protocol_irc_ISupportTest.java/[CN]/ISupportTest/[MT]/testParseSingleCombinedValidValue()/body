{
  Map<Character,Integer> destination=new HashMap<Character,Integer>();
  ISupport.parseChanLimit(destination,"#&+:25");
  Assert.assertEquals(3,destination.size());
  Assert.assertEquals(25,destination.get('#').intValue());
  Assert.assertEquals(25,destination.get('&').intValue());
  Assert.assertEquals(25,destination.get('+').intValue());
}
