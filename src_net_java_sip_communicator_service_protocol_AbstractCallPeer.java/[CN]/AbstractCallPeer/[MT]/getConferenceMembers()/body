{
  ConferenceMember[] conferenceMembers;
synchronized (this.conferenceMembers) {
    int conferenceMemberCount=getConferenceMemberCount();
    if (conferenceMemberCount <= 0)     conferenceMembers=NO_CONFERENCE_MEMBERS;
 else     conferenceMembers=this.conferenceMembers.toArray(new ConferenceMember[conferenceMemberCount]);
  }
  return conferenceMembers;
}
