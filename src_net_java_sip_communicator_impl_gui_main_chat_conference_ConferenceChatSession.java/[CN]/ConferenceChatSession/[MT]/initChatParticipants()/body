{
  if (chatRoomWrapper.getChatRoom() != null && chatRoomWrapper.getChatRoom().isJoined()) {
    List membersList=chatRoomWrapper.getChatRoom().getMembers();
    for (int i=0; i < membersList.size(); i++) {
      ChatContact chatContact=new ConferenceChatContact((ChatRoomMember)membersList.get(i));
      chatParticipants.add(chatContact);
    }
  }
}
