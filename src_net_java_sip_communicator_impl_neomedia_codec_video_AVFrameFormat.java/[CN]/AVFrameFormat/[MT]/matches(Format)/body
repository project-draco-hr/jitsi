{
  boolean matches;
  if (super.matches(format)) {
    if (format instanceof AVFrameFormat) {
      AVFrameFormat avFrameFormat=(AVFrameFormat)format;
      matches=((pixFmt == NOT_SPECIFIED) || (avFrameFormat.pixFmt == NOT_SPECIFIED) || (pixFmt == avFrameFormat.pixFmt));
    }
 else     matches=true;
  }
 else   matches=false;
  return matches;
}
