{
  if (evt.getPropertyName().equals(WizardModel.CANCEL_BUTTON_ENABLED_PROPERTY)) {
    if (evt.getNewValue() instanceof Boolean)     cancelButton.setEnabled((Boolean)evt.getNewValue());
  }
 else   if (evt.getPropertyName().equals(WizardModel.NEXT_FINISH_BUTTON_ENABLED_PROPERTY)) {
    if (evt.getNewValue() instanceof Boolean)     addAccountButton.setEnabled((Boolean)evt.getNewValue());
  }
}
