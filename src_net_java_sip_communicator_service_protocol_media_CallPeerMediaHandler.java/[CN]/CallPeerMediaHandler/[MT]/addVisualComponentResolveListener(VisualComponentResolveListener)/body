{
  if (listener == null)   throw new NullPointerException("listener");
synchronized (visualComponentResolveSyncRoot) {
    if ((visualComponentResolveListeners == null) || visualComponentResolveListeners.isEmpty()) {
      visualComponentResolveListeners=new ArrayList<VisualComponentResolveListener>();
    }
    visualComponentResolveListeners.add(listener);
  }
}
