{
  if (logger.isInfoEnabled())   logger.info("Try to open port on NAT if any");
  try {
    videoStreamConnector.getDataSocket().send(new DatagramPacket(new byte[0],0,target.getDataAddress().getAddress(),target.getDataAddress().getPort()));
    videoStreamConnector.getControlSocket().send(new DatagramPacket(new byte[0],0,target.getControlAddress().getAddress(),target.getControlAddress().getPort()));
  }
 catch (  Exception e) {
    logger.error("Error cannot send to remote peer",e);
  }
}
