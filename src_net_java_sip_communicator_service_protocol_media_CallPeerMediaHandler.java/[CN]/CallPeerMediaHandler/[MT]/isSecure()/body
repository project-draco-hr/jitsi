{
  boolean isAudioSecured=false;
  if (audioStream != null)   isAudioSecured=audioStream.getZrtpControl().getSecureCommunicationStatus();
 else   isAudioSecured=true;
  boolean isVideoSecured=false;
  if (videoStream != null)   isVideoSecured=videoStream.getZrtpControl().getSecureCommunicationStatus();
 else   isVideoSecured=true;
  return isAudioSecured && isVideoSecured;
}
