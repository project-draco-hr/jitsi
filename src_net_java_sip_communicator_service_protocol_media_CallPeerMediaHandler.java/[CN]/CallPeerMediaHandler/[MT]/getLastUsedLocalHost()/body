{
  if (audioStreamConnector != null)   return audioStreamConnector.getDataSocket().getLocalAddress();
  if (videoStreamConnector != null)   return videoStreamConnector.getDataSocket().getLocalAddress();
  NetworkAddressManagerService nam=ProtocolMediaActivator.getNetworkAddressManagerService();
  InetAddress intendedDestination=getIntendedDestination(peer);
  return nam.getLocalHost(intendedDestination);
}
