{
  boolean mediaHandlerCloseStream=false;
switch (mediaType) {
case AUDIO:
    if (audioStream != null) {
      audioStream=null;
      mediaHandlerCloseStream=true;
    }
  break;
case VIDEO:
if (videoStream != null) {
  videoStream=null;
  mediaHandlerCloseStream=true;
}
break;
}
if (mediaHandlerCloseStream) mediaHandler.closeStream(this,mediaType);
getTransportManager().closeStreamConnector(mediaType);
}
