{
  if (isStarted())   return;
  MediaStream stream=getStream(MediaType.AUDIO);
  if (stream != null && !stream.isStarted() && isLocalAudioTransmissionEnabled()) {
    stream.start();
  }
  stream=getStream(MediaType.VIDEO);
  if (stream != null && !stream.isStarted()) {
    stream.start();
    if (stream instanceof VideoMediaStream && !isLocalVideoTransmissionEnabled()) {
      sendHolePunchPacket(stream.getTarget());
    }
  }
}
