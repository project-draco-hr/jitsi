{
  ProviderPresenceStatusChangeEvent evt=new ProviderPresenceStatusChangeEvent(this.parentProvider,oldValue,this.getPresenceStatus());
  for (int i=0; i < providerPresenceStatusListeners.size(); i++) {
    ProviderPresenceStatusListener listener=(ProviderPresenceStatusListener)providerPresenceStatusListeners.get(i);
    listener.providerStatusChanged(evt);
  }
}
