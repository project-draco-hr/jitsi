{
  if (row < 0)   return null;
  AccountID accountID=savedPasswordAccounts.get(row);
switch (column) {
case ACCOUNT_TYPE_INDEX:
    String protocol=accountID.getAccountPropertyString(ProtocolProviderFactory.PROTOCOL);
  return (protocol == null) ? resources.getI18NString("plugin.securityconfig.masterpassword.PROTOCOL_UNKNOWN") : protocol;
case ACCOUNT_NAME_INDEX:
return accountID.getUserID();
case PASSWORD_INDEX:
String pass=credentialsStorageService.loadPassword(accountIdPrefixes.get(accountID));
return (pass == null) ? resources.getI18NString("plugin.securityconfig.masterpassword.CANNOT_DECRYPT") : pass;
default :
return null;
}
}
