{
  logger.trace("Loading default icon at " + defaultIconPath);
  InputStream is=ContactRssImpl.class.getClassLoader().getResourceAsStream(defaultIconPath);
  byte[] result=null;
  if (is == null)   return result;
  try {
    logger.trace("Icon is " + is.available() + " bytes long");
    result=new byte[is.available()];
    is.read(result);
  }
 catch (  IOException ioex) {
    logger.error("Error loading default icon at" + defaultIconPath,ioex);
  }
  return result;
}
