{
  super(ImageLoader.getImage(ImageLoader.PARK_CALL_BUTTON),GuiActivator.getResources().getI18NString("service.gui.PARK_BUTTON_TOOL_TIP"));
  this.call=c;
  addActionListener(new ActionListener(){
    /** 
 * Invoked when an action occurs.
 * @param evt the <tt>ActionEvent</tt> instance containing the
 * data associated with the action and the act of its performing
 */
    public void actionPerformed(    ActionEvent evt){
      parkCall();
    }
  }
);
}
