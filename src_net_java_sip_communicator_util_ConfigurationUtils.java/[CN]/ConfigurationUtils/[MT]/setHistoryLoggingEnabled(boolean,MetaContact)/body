{
  if (isEnabled)   configService.setProperty(MessageHistoryService.PNAME_IS_MESSAGE_HISTORY_PER_CONTACT_ENABLED_PREFIX + "." + contact.getMetaUID(),null);
 else   configService.setProperty(MessageHistoryService.PNAME_IS_MESSAGE_HISTORY_PER_CONTACT_ENABLED_PREFIX + "." + contact.getMetaUID(),isEnabled);
}
