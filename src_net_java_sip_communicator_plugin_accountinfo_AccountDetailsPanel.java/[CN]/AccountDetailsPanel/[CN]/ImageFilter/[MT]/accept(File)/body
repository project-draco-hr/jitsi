{
  if (f.isDirectory()) {
    return true;
  }
  String extension=getExtension(f);
  if (extension != null) {
    if (extension.equals("tiff") || extension.equals("tif") || extension.equals("gif")|| extension.equals("jpeg")|| extension.equals("jpg")|| extension.equals("png")) {
      return true;
    }
 else {
      return false;
    }
  }
  return false;
}
