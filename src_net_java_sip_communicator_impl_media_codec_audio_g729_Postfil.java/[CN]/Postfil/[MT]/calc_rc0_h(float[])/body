{
  float acf0, acf1;
  float temp, temp2;
  int ptrs;
  int i;
  temp=0.f;
  for (i=0; i < LONG_H_ST; i++) {
    temp+=h[i] * h[i];
  }
  acf0=temp;
  temp=0.f;
  ptrs=0;
  for (i=0; i < LONG_H_ST - 1; i++) {
    temp2=h[ptrs];
    ptrs++;
    temp+=temp2 * h[ptrs];
  }
  acf1=temp;
  if (acf0 == 0.f) {
    return 0.f;
  }
  if (acf0 < Math.abs(acf1)) {
    return 0.0f;
  }
  return -acf1 / acf0;
}
