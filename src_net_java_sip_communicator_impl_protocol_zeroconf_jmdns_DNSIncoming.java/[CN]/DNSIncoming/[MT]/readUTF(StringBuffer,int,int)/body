{
  for (int end=off + len; off < end; ) {
    int ch=get(off++);
switch (ch >> 4) {
case 0:
case 1:
case 2:
case 3:
case 4:
case 5:
case 6:
case 7:
      break;
case 12:
case 13:
    ch=((ch & 0x1F) << 6) | (get(off++) & 0x3F);
  break;
case 14:
ch=((ch & 0x0f) << 12) | ((get(off++) & 0x3F) << 6) | (get(off++) & 0x3F);
break;
default :
ch=((ch & 0x3F) << 4) | (get(off++) & 0x0f);
break;
}
buf.append((char)ch);
}
}
