{
  if (isNameStartChar(c))   return true;
 else   if ((c == '-') || (c == '.'))   return true;
 else   if (('0' <= c) && (c <= '9'))   return true;
 else   if (c == 0xB7)   return true;
 else   if (c < 0x0300)   return false;
 else   if (c <= 0x036F)   return true;
 else   if (c < 0x203F)   return false;
 else   if (c <= 0x2040)   return true;
 else   return false;
}
