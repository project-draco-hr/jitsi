{
  if (accountRegsTable.getSelectedRow() > -1)   this.wizardContainer.setNextFinishButtonEnabled(true);
 else   this.wizardContainer.setNextFinishButtonEnabled(false);
  if (wizardContainer.getCurrentWizard() != null)   this.wizardContainer.unregisterWizardPages();
  this.wizardContainer.removeWizzardIcon();
}
