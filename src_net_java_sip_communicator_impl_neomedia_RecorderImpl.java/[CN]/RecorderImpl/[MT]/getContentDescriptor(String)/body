{
  String type;
  if (SoundFileUtils.wav.equalsIgnoreCase(format))   type=FileTypeDescriptor.WAVE;
 else   if (SoundFileUtils.mp2.equalsIgnoreCase(format))   type=FileTypeDescriptor.MPEG_AUDIO;
 else   if (SoundFileUtils.gsm.equalsIgnoreCase(format))   type=FileTypeDescriptor.GSM;
 else   if (SoundFileUtils.au.equalsIgnoreCase(format))   type=FileTypeDescriptor.BASIC_AUDIO;
 else   if (SoundFileUtils.aif.equalsIgnoreCase(format))   type=FileTypeDescriptor.AIFF;
 else   throw new IllegalArgumentException("format");
  return new ContentDescriptor(type);
}
