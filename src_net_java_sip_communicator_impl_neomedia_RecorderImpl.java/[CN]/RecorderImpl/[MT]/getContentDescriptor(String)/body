{
  String type=FileTypeDescriptor.MPEG_AUDIO;
  if (SoundFileUtils.wav.equals(format))   type=FileTypeDescriptor.WAVE;
 else   if (SoundFileUtils.gsm.equals(format))   type=FileTypeDescriptor.GSM;
 else   if (SoundFileUtils.au.equals(format))   type=FileTypeDescriptor.BASIC_AUDIO;
 else   if (SoundFileUtils.aif.equals(format))   type=FileTypeDescriptor.AIFF;
  return new ContentDescriptor(type);
}
