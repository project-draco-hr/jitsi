{
  int L_INTER4=Ld8k.L_INTER4;
  int UP_SAMP=Ld8k.UP_SAMP;
  float[] inter_3=TabLd8k.inter_3;
  int i;
  float s;
  int x1, x2, c1, c2;
  if (frac < 0) {
    frac+=UP_SAMP;
    x_offset--;
  }
  x1=x_offset;
  x2=x_offset + 1;
  c1=frac;
  c2=UP_SAMP - frac;
  s=0.0f;
  for (i=0; i < L_INTER4; i++, c1+=UP_SAMP, c2+=UP_SAMP) {
    s+=x[x1] * inter_3[c1] + x[x2] * inter_3[c2];
    x1--;
    x2++;
  }
  return s;
}
