{
  if (evt.getActionCommand().equals(Wizard.CANCEL_BUTTON_ACTION_COMMAND))   cancelButtonPressed();
 else   if (evt.getActionCommand().equals(Wizard.BACK_BUTTON_ACTION_COMMAND))   backButtonPressed();
 else   if (evt.getActionCommand().equals(Wizard.NEXT_BUTTON_ACTION_COMMAND))   nextButtonPressed();
}
