{
  WizardModel model=wizard.getModel();
  WizardPage page=model.getCurrentWizardPage();
  try {
    page.commitPage();
  }
 catch (  Exception ex) {
    new ErrorDialog(null,Messages.getI18NString("error").getText(),ex.getMessage(),ex).showDialog();
    return;
  }
  Object nextPageIdentifier=page.getNextPageIdentifier();
  if (nextPageIdentifier.equals(WizardPage.FINISH_PAGE_IDENTIFIER)) {
    wizard.close(Wizard.FINISH_RETURN_CODE);
  }
 else {
    wizard.setCurrentPage(nextPageIdentifier);
  }
}
