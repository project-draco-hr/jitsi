{
  Format[] supportedFormats=captureDeviceInfo.getFormats();
  MediaType mediaType=getMediaType();
  List<MediaFormat> supportedMediaFormats=new ArrayList<MediaFormat>(supportedFormats.length);
  for (  Format format : supportedFormats) {
    MediaFormat mediaFormat=MediaFormatImpl.createInstance(format);
    if ((mediaFormat != null) && mediaFormat.getMediaType().equals(mediaType))     supportedMediaFormats.add(mediaFormat);
  }
  return supportedMediaFormats;
}
