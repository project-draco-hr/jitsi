{
synchronized (privateRoomCache) {
    if (privateRoomCache.containsKey(nickIdentifier))     return (ChatRoomIrcImpl)privateRoomCache.get(nickIdentifier);
    ChatRoomIrcImpl chatRoom=new ChatRoomIrcImpl(nickIdentifier,ircProvider,true);
    privateRoomCache.put(nickIdentifier,chatRoom);
    fireLocalUserPresenceEvent(chatRoom,LocalUserChatRoomPresenceChangeEvent.LOCAL_USER_JOINED,"Private conversation initiated.");
    return chatRoom;
  }
}
