{
  boolean showSystem;
synchronized (showSystemBundlesSync) {
    showSystem=showSystemBundles;
  }
  for (int i=0; i < bundles.length; i++) {
    Bundle b=bundles[i];
    if (b.equals(bundle)) {
      if (showSystem)       return true;
 else {
        return (!PluginManagerActivator.isSystemBundle(bundle));
      }
    }
  }
  return false;
}
