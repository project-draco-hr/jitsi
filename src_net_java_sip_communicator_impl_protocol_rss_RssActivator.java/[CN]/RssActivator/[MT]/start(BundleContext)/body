{
  this.bundleContext=context;
  Hashtable hashtable=new Hashtable();
  hashtable.put(ProtocolProviderFactory.PROTOCOL,"RSS");
  rssProviderFactory=new ProtocolProviderFactoryRssImpl();
  rssProviderFactory.loadStoredAccounts();
  rssPpFactoryServReg=context.registerService(ProtocolProviderFactory.class.getName(),rssProviderFactory,hashtable);
  logger.info("RSS protocol implementation [STARTED].");
  System.setProperty("http.agent",System.getProperty("sip-communicator.application.name") + "/" + System.getProperty("sip-communicator.version"));
  logger.debug("User-Agent set to " + System.getProperty("http.agent"));
  installCustomSSLTrustManager();
  new UriHandlerRssImpl();
}
