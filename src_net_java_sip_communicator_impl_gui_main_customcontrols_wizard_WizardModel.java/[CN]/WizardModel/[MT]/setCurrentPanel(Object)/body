{
  WizardPanelDescriptor nextPanel=(WizardPanelDescriptor)panelHashmap.get(id);
  if (nextPanel == null)   throw new WizardPanelNotFoundException();
  WizardPanelDescriptor oldPanel=currentPanel;
  currentPanel=nextPanel;
  if (oldPanel != currentPanel)   firePropertyChange(CURRENT_PANEL_DESCRIPTOR_PROPERTY,oldPanel,currentPanel);
  return true;
}
