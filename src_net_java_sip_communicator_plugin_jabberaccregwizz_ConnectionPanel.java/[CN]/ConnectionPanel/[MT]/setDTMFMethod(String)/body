{
  if (dtmfMethod == null) {
    dtmfMethodBox.setSelectedItem(0);
  }
 else {
    String selString;
    if (dtmfMethod.equals("RTP_DTMF")) {
      selString=Resources.getString("plugin.sipaccregwizz.DTMF_RTP");
    }
 else     if (dtmfMethod.equals("INBAND_DTMF")) {
      selString=Resources.getString("plugin.sipaccregwizz.DTMF_INBAND");
    }
 else {
      selString=Resources.getString("plugin.jabberaccregwizz.DTMF_AUTO");
    }
    dtmfMethodBox.setSelectedItem(selString);
  }
}
