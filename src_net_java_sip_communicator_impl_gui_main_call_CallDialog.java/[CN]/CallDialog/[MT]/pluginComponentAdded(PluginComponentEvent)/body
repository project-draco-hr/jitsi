{
  PluginComponent c=event.getPluginComponent();
  if (c.getContainer().equals(net.java.sip.communicator.service.gui.Container.CONTAINER_CALL_DIALOG) && c.getComponent() instanceof Component) {
    settingsPanel.add((Component)c.getComponent());
    settingsPanel.revalidate();
    settingsPanel.repaint();
  }
}
