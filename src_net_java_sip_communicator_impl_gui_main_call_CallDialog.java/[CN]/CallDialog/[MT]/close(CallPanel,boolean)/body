{
  if (this.callPanel.equals(callPanel)) {
    if (delay) {
      Timer timer=new Timer(5000,new ActionListener(){
        public void actionPerformed(        ActionEvent ev){
          dispose();
        }
      }
);
      timer.setRepeats(false);
      timer.start();
    }
 else {
      dispose();
    }
  }
}
