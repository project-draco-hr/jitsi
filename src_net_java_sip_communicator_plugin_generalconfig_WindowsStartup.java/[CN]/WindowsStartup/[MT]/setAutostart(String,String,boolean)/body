{
  if (isAutoStart) {
    Advapi32Util.registrySetStringValue(WinReg.HKEY_CURRENT_USER,REGISTRY_STARTUP_KEY,appName,workingDirectory + File.separator + "run.exe");
  }
 else {
    try {
      Advapi32Util.registryDeleteValue(WinReg.HKEY_CURRENT_USER,REGISTRY_STARTUP_KEY,appName);
    }
 catch (    Throwable t) {
      logger.warn("Cannot remove startup key or don't exist",t);
    }
  }
}
