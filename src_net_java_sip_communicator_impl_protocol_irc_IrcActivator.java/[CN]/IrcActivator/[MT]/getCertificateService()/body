{
  if (certiticateService == null) {
    ServiceReference guiVerifyReference=IrcActivator.getBundleContext().getServiceReference(CertificateService.class.getName());
    if (guiVerifyReference != null)     certiticateService=(CertificateService)IrcActivator.getBundleContext().getService(guiVerifyReference);
  }
  return certiticateService;
}
