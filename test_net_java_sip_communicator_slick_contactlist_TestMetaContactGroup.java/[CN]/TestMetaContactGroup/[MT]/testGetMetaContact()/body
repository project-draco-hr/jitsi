{
  MetaContact firstContact=metaGroup.getMetaContact(0);
  MetaContact lastContact=metaGroup.getMetaContact(metaGroup.countChildContacts() - 1);
  assertNotNull("getMetaContact(int) returned null for contact 0",firstContact);
  assertNotNull("getMetaContact(int) returned null for its last contact " + (metaGroup.countChildContacts() - 1),lastContact);
}
