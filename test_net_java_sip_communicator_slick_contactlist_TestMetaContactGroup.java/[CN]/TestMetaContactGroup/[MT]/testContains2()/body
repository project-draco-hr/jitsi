{
  MetaContactGroup metaContactGroup1=fixture.metaClService.findMetaContactGroupByContactGroup(MetaContactListServiceLick.topLevelMockGroup);
  MetaContactGroup metaContactGroup2=metaContactGroup1.getMetaContactSubgroup(MetaContactListServiceLick.subLevelGroup.getGroupName());
  assertTrue(metaContactGroup1.contains(metaContactGroup2));
}
