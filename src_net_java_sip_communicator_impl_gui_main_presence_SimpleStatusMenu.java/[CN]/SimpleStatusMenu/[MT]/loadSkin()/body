{
  super.loadSkin();
  setIcon(new ImageIcon(ImageLoader.getBytesInImage(protocolProvider.getProtocolIcon().getIcon(ProtocolIcon.ICON_SIZE_16x16))));
  onlineItem.setIcon(getIcon());
  offlineItem.setIcon(new ImageIcon(LightGrayFilter.createDisabledImage(ImageLoader.getBytesInImage(protocolProvider.getProtocolIcon().getIcon(ProtocolIcon.ICON_SIZE_16x16)))));
}
