{
  JMenuItem menuItem=(JMenuItem)e.getSource();
  String itemName=menuItem.getName();
  if (itemName.equals(GlobalStatusEnum.ONLINE_STATUS)) {
    GuiActivator.getGlobalStatusService().publishStatus(protocolProvider,GlobalStatusEnum.ONLINE,true);
  }
 else {
    GuiActivator.getGlobalStatusService().publishStatus(protocolProvider,GlobalStatusEnum.OFFLINE,true);
  }
}
