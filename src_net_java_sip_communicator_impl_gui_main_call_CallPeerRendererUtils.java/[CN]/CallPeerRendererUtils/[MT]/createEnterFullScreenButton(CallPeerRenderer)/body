{
  SIPCommButton button=new SIPCommButton(ImageLoader.getImage(ImageLoader.ENTER_FULL_SCREEN_BUTTON));
  button.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.ENTER_FULL_SCREEN_TOOL_TIP"));
  button.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      renderer.enterFullScreen();
    }
  }
);
  return button;
}
