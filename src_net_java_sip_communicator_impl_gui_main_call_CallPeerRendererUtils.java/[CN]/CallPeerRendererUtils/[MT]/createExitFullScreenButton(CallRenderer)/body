{
  JButton button=new SIPCommButton(ImageLoader.getImage(ImageLoader.FULL_SCREEN_BUTTON_BG),ImageLoader.getImage(ImageLoader.EXIT_FULL_SCREEN_BUTTON));
  button.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.EXIT_FULL_SCREEN_TOOL_TIP"));
  button.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent event){
      renderer.exitFullScreen();
    }
  }
);
  return button;
}
