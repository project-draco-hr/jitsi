{
  PropertyChangeListener[] listeners;
synchronized (this.listeners) {
    listeners=this.listeners.toArray(new PropertyChangeListener[this.listeners.size()]);
  }
  PropertyChangeEvent event=new PropertyChangeEvent(getPropertyChangeSource(property,oldValue,newValue),property,oldValue,newValue);
  for (  PropertyChangeListener listener : listeners)   listener.propertyChange(event);
}
