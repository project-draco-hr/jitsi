{
  ChatRoom testChatRoom=opSetMultiChat1.findRoom(fixture.chatRoomName);
  assertNotNull("Could not find the test chat room on the server",testChatRoom);
  String roomName=testChatRoom.getName();
  assertEquals("Name of the test chat room did not match the name of the " + "room we created",fixture.chatRoomName,roomName);
}
