{
  logger.debug("Adding protocol provider " + provider.getProtocolName());
  OperationSetBasicTelephony opSetTelephony=(OperationSetBasicTelephony)provider.getSupportedOperationSets().get(OperationSetBasicTelephony.class.getName());
  if (opSetTelephony != null) {
    opSetTelephony.addCallListener(this);
  }
 else {
    logger.trace("Service did not have a basic telephony op. set.");
  }
}
