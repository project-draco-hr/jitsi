{
  final char prefix=name.charAt(0);
  if (!this.parentProvider.getIrcStack().getChannelTypes().contains(prefix)) {
    throw new IllegalArgumentException("invalid channel prefix: " + prefix);
  }
  for (  char c : IrcStack.SPECIAL_CHARACTERS) {
    if (name.contains("" + c)) {
      throw new IllegalArgumentException("chat room identifier contains illegal character: " + c);
    }
  }
  return name;
}
