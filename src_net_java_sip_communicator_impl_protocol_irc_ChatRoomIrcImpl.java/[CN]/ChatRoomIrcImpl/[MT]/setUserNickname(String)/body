{
  try {
    parentProvider.getIrcStack().setUserNickname(nickName);
  }
 catch (  RuntimeException e) {
    if (e.getCause() instanceof IOException) {
      throw new OperationFailedException("Failed to change nick name.",OperationFailedException.NETWORK_FAILURE,e.getCause());
    }
    throw new OperationFailedException("Failed to change nick name.",OperationFailedException.GENERAL_ERROR,e);
  }
}
