{
  ConfigurationService configService=GuiActivator.getConfigurationService();
  String width=configService.getString("net.java.sip.communicator.impl.ui.chatWindowWidth");
  String height=configService.getString("net.java.sip.communicator.impl.ui.chatWindowHeight");
  String x=configService.getString("net.java.sip.communicator.impl.ui.chatWindowX");
  String y=configService.getString("net.java.sip.communicator.impl.ui.chatWindowY");
  if (width != null && height != null)   this.setSize(new Integer(width).intValue(),new Integer(height).intValue());
  if (x != null && y != null)   this.setLocation(new Integer(x).intValue(),new Integer(y).intValue());
 else   this.setCenterLocation();
}
