{
  ConfigurationService configService=GuiActivator.getConfigurationService();
  String width=configService.getString(CHAT_WINDOW_WIDTH_PROPERTY);
  String height=configService.getString(CHAT_WINDOW_HEIGHT_PROPERTY);
  String x=configService.getString(CHAT_WINDOW_X_PROPERTY);
  String y=configService.getString(CHAT_WINDOW_Y_PROPERTY);
  if (width != null && height != null)   this.setSize(new Integer(width).intValue(),new Integer(height).intValue());
  if (x != null && y != null)   this.setLocation(new Integer(x).intValue(),new Integer(y).intValue());
 else   this.setCenterLocation();
}
