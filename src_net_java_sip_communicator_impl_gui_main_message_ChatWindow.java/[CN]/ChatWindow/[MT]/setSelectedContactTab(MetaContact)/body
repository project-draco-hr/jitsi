{
  if (this.contactTabsTable != null && !this.contactTabsTable.isEmpty()) {
    ChatPanel chatPanel=((ChatPanel)this.contactTabsTable.get(contact.getMetaUID()));
    this.chatTabbedPane.setSelectedIndex(chatPanel.getTabIndex());
    chatPanel.getWriteMessagePanel().getEditorPane().requestFocus();
  }
}
