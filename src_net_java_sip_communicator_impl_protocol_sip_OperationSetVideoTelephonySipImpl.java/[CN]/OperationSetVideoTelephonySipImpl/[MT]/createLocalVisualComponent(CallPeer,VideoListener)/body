{
  CallSession callSession=((CallPeerSipImpl)peer).getMediaCallSession();
  if (callSession != null) {
    try {
      return callSession.createLocalVisualComponent(listener);
    }
 catch (    MediaException ex) {
      throw new OperationFailedException("Failed to create visual Component for local " + "video (capture).",OperationFailedException.INTERNAL_ERROR,ex);
    }
  }
  return null;
}
