{
  if (!chatRoom.isSystem() && chatRoom.isJoined() && (chatRoom.getMembersCount() == 1)) {
    String nickname=chatRoom.getUserNickname();
    if (nickname != null) {
      for (      ChatRoomMember member : chatRoom.getMembers())       if (nickname.equals(member.getName()))       return true;
      return false;
    }
  }
  return false;
}
