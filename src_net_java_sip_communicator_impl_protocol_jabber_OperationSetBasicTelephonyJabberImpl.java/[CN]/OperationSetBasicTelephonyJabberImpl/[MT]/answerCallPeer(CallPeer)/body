{
  CallPeerJabberImpl callParticipant=(CallPeerJabberImpl)participant;
  try {
    ((IncomingJingleSession)callParticipant.getJingleSession()).start();
  }
 catch (  XMPPException ex) {
    throw new OperationFailedException("Failed to answer an incoming call",OperationFailedException.INTERNAL_ERROR);
  }
}
