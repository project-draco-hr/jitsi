{
  if (!(packet instanceof JingleIQ))   return false;
  JingleIQ jingleIQ=(JingleIQ)packet;
  if (jingleIQ.getAction() == JingleAction.SESSION_INITIATE) {
    if (jingleIQ.containsContentChildOfType(RtpDescriptionPacketExtension.class))     return true;
 else     return false;
  }
  if (activeCallsRepository.findJingleSID(jingleIQ.getSID()) != null)   return true;
 else   return false;
}
