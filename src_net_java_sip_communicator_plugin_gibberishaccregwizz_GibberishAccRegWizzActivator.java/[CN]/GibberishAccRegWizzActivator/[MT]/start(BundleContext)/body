{
  logger.info("Loading gibberish account wizard.");
  bundleContext=bc;
  ServiceReference uiServiceRef=bundleContext.getServiceReference(UIService.class.getName());
  uiService=(UIService)bundleContext.getService(uiServiceRef);
  wizardContainer=uiService.getAccountRegWizardContainer();
  gibberishWizard=new GibberishAccountRegistrationWizard(wizardContainer);
  wizardContainer.addAccountRegistrationWizard(gibberishWizard);
  logger.info("Gibberish account registration wizard [STARTED].");
}
