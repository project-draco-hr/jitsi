{
  super(query,query.getContactSource(),chatRoom.getName(),generateDefaultContactDetails(chatRoom.getName()));
  this.chatRoomName=chatRoom.getName();
  this.chatRoomID=chatRoom.getIdentifier();
  this.provider=chatRoom.getParentProvider();
  this.parentQuery=query;
  initContactProperties((chatRoom.isJoined() ? ChatRoomPresenceStatus.CHAT_ROOM_ONLINE : ChatRoomPresenceStatus.CHAT_ROOM_OFFLINE));
}
