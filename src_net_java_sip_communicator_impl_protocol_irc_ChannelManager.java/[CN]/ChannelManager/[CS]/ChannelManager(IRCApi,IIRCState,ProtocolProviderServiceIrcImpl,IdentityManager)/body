{
  if (irc == null) {
    throw new IllegalArgumentException("irc instance cannot be null");
  }
  irc.addListener(new ManagerListener(irc));
  this.irc=irc;
  if (connectionState == null) {
    throw new IllegalArgumentException("connectionState cannot be null");
  }
  this.connectionState=connectionState;
  if (provider == null) {
    throw new IllegalArgumentException("provider cannot be null");
  }
  this.provider=provider;
  if (identity == null) {
    throw new IllegalArgumentException("IdentityManager instance cannot be null");
  }
  this.identity=identity;
}
