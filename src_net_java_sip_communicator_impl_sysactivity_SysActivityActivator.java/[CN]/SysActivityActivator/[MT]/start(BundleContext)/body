{
  if (logger.isDebugEnabled())   logger.debug("Started.");
  SysActivityActivator.bundleContext=context;
  sysActivitiesServiceImpl=new SystemActivityNotificationsServiceImpl();
  sysActivitiesServiceImpl.start();
  sysActivitiesServReg=context.registerService(SystemActivityNotificationsService.class.getName(),sysActivitiesServiceImpl,null);
}
