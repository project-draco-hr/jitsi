{
  TranscodingDataSource transcodingDataSource;
  if (inputDataSource instanceof TranscodingDataSource)   transcodingDataSource=null;
 else {
    transcodingDataSource=new TranscodingDataSource(inputDataSource,outputFormat);
    if (connected > 0)     transcodingDataSource.connect();
    if (started > 0)     transcodingDataSource.start();
  }
  return transcodingDataSource;
}
