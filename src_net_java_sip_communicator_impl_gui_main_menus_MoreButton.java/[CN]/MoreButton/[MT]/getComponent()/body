{
  if (label == null) {
    label=new JLabel(new ImageIcon(ImageLoader.getImage(ImageLoader.MORE_BUTTON)),JLabel.CENTER);
    label.setVerticalTextPosition(SwingConstants.BOTTOM);
    label.setHorizontalTextPosition(SwingConstants.CENTER);
    label.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.MORE"));
    label.addFocusListener(this);
    label.addMouseListener(this);
  }
  return label;
}
