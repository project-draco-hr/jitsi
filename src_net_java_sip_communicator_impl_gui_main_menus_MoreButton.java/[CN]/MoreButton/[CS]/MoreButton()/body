{
  super(new ImageIcon(ImageLoader.getImage(ImageLoader.MORE_BUTTON)),JLabel.CENTER);
  this.setVerticalTextPosition(SwingConstants.BOTTOM);
  this.setHorizontalTextPosition(SwingConstants.CENTER);
  this.setToolTipText(GuiActivator.getResources().getI18NString("service.gui.MORE"));
