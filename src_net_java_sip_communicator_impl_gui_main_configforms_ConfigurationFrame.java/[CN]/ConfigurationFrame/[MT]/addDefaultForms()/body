{
  Boolean showAccountConfigProp=GuiActivator.getConfigurationService().getBoolean(SHOW_ACCOUNT_CONFIG_PROPERTY,true);
  if (showAccountConfigProp.booleanValue())   addConfigurationForm(new LazyConfigurationForm("net.java.sip.communicator.impl.gui.main." + "account.AccountsConfigurationPanel",getClass().getClassLoader(),"service.gui.icons.ACCOUNT_ICON","service.gui.ACCOUNTS",0));
}
