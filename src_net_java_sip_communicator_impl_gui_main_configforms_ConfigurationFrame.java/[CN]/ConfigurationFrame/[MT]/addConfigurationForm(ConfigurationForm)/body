{
  if (configForm.getForm() instanceof Component) {
    this.configContainer.add(configForm);
    this.recalculateSize();
    Icon image=null;
    try {
      image=new ImageIcon(ImageIO.read(new ByteArrayInputStream(configForm.getIcon())));
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    ConfigMenuItemPanel configItem=new ConfigMenuItemPanel(configForm.getTitle(),image);
    configItem.addMouseListener(this);
    this.configList.addCell(configItem);
  }
 else {
    throw new ClassCastException("ConfigurationFrame :" + configForm.getForm().getClass() + " is not a class supported by this ui implementation");
  }
}
