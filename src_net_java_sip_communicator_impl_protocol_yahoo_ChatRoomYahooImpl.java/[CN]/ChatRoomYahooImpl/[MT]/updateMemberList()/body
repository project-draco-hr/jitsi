{
  Vector<YahooUser> memberList=yahooConference.getMembers();
  for (  YahooUser user : memberList) {
    ChatRoomMemberYahooImpl member=new ChatRoomMemberYahooImpl(this,user.getId(),user.getId(),ChatRoomMemberRole.MEMBER);
    if (!members.containsKey(member.getName())) {
      members.put(member.getName(),member);
    }
  }
}
