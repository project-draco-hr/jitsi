{
  try {
    provider.getYahooSession().leaveConference(yahooConference);
    for (    ChatRoomMember member : members.values()) {
      fireMemberPresenceEvent(member,ChatRoomMemberPresenceChangeEvent.MEMBER_LEFT,"Local user has left the chat room.");
    }
  }
 catch (  IOException ioe) {
    logger.debug("Failed to leave the chat room: " + yahooConference.getName() + " Error: "+ ioe);
  }
  members.clear();
}
