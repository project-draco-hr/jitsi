{
  MetaContact metaContact=mainFrame.getContactList().findMetaContactByContact(contact);
  if (!Constants.TABBED_CHAT_WINDOW) {
    return contactMsgWindows.containsKey(metaContact);
  }
 else {
    if (tabbedChatWindow != null) {
      return tabbedChatWindow.getContactChatsTable().contains(metaContact.getMetaUID());
    }
 else {
      return false;
    }
  }
}
