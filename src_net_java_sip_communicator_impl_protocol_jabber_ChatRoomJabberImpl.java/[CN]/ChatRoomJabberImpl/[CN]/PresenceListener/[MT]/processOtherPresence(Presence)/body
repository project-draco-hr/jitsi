{
  PacketExtension ext=presence.getExtension(ConferenceDescriptionPacketExtension.NAMESPACE);
  if (presence.isAvailable() && ext != null) {
    if (logger.isDebugEnabled())     logger.debug("Looks like " + presence.getFrom() + " has a"+ " little conference going on. I guess we should"+ " notify the GUI, or something. "+ ext.toXML());
  }
}
