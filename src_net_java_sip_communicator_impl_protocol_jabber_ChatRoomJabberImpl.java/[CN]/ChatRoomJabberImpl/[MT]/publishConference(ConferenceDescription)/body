{
  if (cd != publishedConference) {
    publishedConference=cd;
    publishedConferenceExt=(cd == null) ? null : new ConferenceDescriptionPacketExtension(publishedConference);
    if (lastPresenceSent != null) {
      setConferenceDescriptionPacketExtension(lastPresenceSent,publishedConferenceExt);
      provider.getConnection().sendPacket(lastPresenceSent);
    }
 else {
      logger.warn("Could not publish conference," + " lastPresenceSent is null.");
      return null;
    }
  }
  return publishedConference;
}
