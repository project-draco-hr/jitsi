{
  firePropertyChangeEvent(new ChatRoomPropertyChangeFailedEvent(this,ChatRoomPropertyChangeEvent.CHAT_ROOM_IS_ALLOW_MESSAGE_FORMATTING,null,new Boolean(isAllowMessageFormatting),ChatRoomPropertyChangeFailedEvent.PROPERTY_NOT_SUPPORTED,"The isAllowMessageFormatting property is not supported by the jabber" + "multi user chat implementation."));
}
