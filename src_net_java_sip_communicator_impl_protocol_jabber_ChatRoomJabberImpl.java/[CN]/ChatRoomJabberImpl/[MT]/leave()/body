{
  multiUserChat.leave();
  Iterator<Map.Entry<String,ChatRoomMember>> membersSet=members.entrySet().iterator();
  while (membersSet.hasNext()) {
    Map.Entry<String,ChatRoomMember> memberEntry=membersSet.next();
    ChatRoomMember member=memberEntry.getValue();
    fireMemberPresenceEvent(member,ChatRoomMemberPresenceChangeEvent.MEMBER_LEFT,"Local user has left the chat room.");
  }
  members.clear();
}
