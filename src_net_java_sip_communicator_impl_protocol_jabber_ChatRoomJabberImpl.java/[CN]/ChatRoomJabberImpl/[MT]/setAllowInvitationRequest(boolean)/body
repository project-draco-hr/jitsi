{
  firePropertyChangeEvent(new ChatRoomPropertyChangeFailedEvent(this,ChatRoomPropertyChangeEvent.CHAT_ROOM_IS_ALLOW_INVITATION_REQUEST,null,new Boolean(isAllowInvitationRequest),ChatRoomPropertyChangeFailedEvent.PROPERTY_NOT_SUPPORTED,"The isAllowInvitationRequest property is not supported by the jabber" + "multi user chat implementation."));
}
