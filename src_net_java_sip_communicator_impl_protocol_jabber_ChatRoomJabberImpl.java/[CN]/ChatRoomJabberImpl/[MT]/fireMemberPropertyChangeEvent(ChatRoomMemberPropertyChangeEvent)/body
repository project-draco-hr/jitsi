{
  Iterator<ChatRoomMemberPropertyChangeListener> listeners=null;
synchronized (memberPropChangeListeners) {
    listeners=new ArrayList<ChatRoomMemberPropertyChangeListener>(memberPropChangeListeners).iterator();
  }
  while (listeners.hasNext()) {
    ChatRoomMemberPropertyChangeListener listener=(ChatRoomMemberPropertyChangeListener)listeners.next();
    listener.chatRoomPropertyChanged(evt);
  }
}
