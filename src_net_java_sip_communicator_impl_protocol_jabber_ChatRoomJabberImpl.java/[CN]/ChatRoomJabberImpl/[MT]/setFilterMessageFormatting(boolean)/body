{
  firePropertyChangeEvent(new ChatRoomPropertyChangeFailedEvent(this,ChatRoomPropertyChangeEvent.CHAT_ROOM_IS_FILTER_MESSAGE_FORMATTING,null,new Boolean(isFilterMessageFormatting),ChatRoomPropertyChangeFailedEvent.PROPERTY_NOT_SUPPORTED,"The isFilterMessageFormatting property is not supported by the jabber" + "multi user chat implementation."));
}
