{
  firePropertyChangeEvent(new ChatRoomPropertyChangeFailedEvent(this,ChatRoomPropertyChangeEvent.CHAT_ROOM_IS_ALLOW_SPECIAL_MESSAGE,null,new Boolean(isAllowSpecialMessage),ChatRoomPropertyChangeFailedEvent.PROPERTY_NOT_SUPPORTED,"The isAllowSpecialMessage property is not supported by the jabber" + "multi user chat implementation."));
}
