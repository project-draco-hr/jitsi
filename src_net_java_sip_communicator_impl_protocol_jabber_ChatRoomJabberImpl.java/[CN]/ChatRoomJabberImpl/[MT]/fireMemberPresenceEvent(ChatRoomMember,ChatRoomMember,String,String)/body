{
  ChatRoomMemberPresenceChangeEvent evt=new ChatRoomMemberPresenceChangeEvent(this,member,actor,eventID,eventReason);
  logger.trace("Will dispatch the following ChatRoom event: " + evt);
  Iterator listeners=null;
synchronized (memberListeners) {
    listeners=new ArrayList(memberListeners).iterator();
  }
  while (listeners.hasNext()) {
    ChatRoomMemberPresenceListener listener=(ChatRoomMemberPresenceListener)listeners.next();
    listener.memberPresenceChanged(evt);
  }
}
