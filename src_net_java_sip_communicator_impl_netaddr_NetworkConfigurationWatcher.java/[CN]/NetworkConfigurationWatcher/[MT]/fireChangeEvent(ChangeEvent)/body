{
synchronized (listeners) {
    for (int i=0; i < listeners.size(); i++) {
      NetworkConfigurationChangeListener nCChangeListener=listeners.get(i);
      try {
        nCChangeListener.configurationChanged(evt);
      }
 catch (      Throwable e) {
        logger.warn("Error delivering event:" + evt + ", to:"+ nCChangeListener);
      }
    }
  }
}
