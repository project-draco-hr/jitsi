{
  while (isPLIThreadRunning) {
    long time=System.currentTimeMillis();
    if (missFrame) {
      if (time > (lastReceivedKeyframeTime + PLI_KEYFRAME_INTERVAL)) {
        if (lastPLIRequestTime == -1 || time > lastPLIRequestTime + PLI_INTERVAL) {
          lastPLIRequestTime=time;
          sendRTCPFeedbackPLI();
        }
      }
    }
    try {
      Thread.sleep(100);
    }
 catch (    Exception e) {
    }
  }
}
