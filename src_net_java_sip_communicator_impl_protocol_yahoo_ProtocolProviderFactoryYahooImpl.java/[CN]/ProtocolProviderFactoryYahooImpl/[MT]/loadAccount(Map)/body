{
  BundleContext context=YahooActivator.getBundleContext();
  if (context == null)   throw new NullPointerException("The specified BundleContext was null");
  String userIDStr=(String)accountProperties.get(USER_ID);
  AccountID accountID=new YahooAccountID(userIDStr,accountProperties);
  Hashtable properties=new Hashtable();
  properties.put(PROTOCOL,ProtocolNames.YAHOO);
  properties.put(USER_ID,userIDStr);
  ProtocolProviderServiceYahooImpl yahooProtocolProvider=new ProtocolProviderServiceYahooImpl();
  yahooProtocolProvider.initialize(userIDStr,accountID);
  ServiceRegistration registration=context.registerService(ProtocolProviderService.class.getName(),yahooProtocolProvider,properties);
  registeredAccounts.put(accountID,registration);
  return accountID;
}
