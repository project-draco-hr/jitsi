{
  this.currentProtoContact=protoContact;
  SelectedObject selectedObject=new SelectedObject(icon,protoContact);
  this.menu.setSelected(selectedObject);
  String tooltipText;
  if (!protoContact.getDisplayName().equals(protoContact.getAddress()))   tooltipText=protoContact.getDisplayName() + " (" + protoContact.getAddress()+ ")";
 else   tooltipText=protoContact.getDisplayName();
  this.menu.setToolTipText(tooltipText);
}
