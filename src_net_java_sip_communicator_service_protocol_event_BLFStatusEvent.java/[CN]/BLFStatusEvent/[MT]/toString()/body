{
  String statusName=null;
switch (type) {
case STATUS_OFFLINE:
    statusName="Offline";
  break;
case STATUS_RINGING:
statusName="Ringing";
break;
case STATUS_BUSY:
statusName="Busy";
break;
case STATUS_FREE:
statusName="Free";
break;
}
return "BLFStatusEvent{" + "type=" + type + ", name="+ statusName+ '}';
}
