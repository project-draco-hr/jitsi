{
  String align=(String)fElement.getAttributes().getAttribute(HTML.Attribute.ALIGN);
  if (align != null) {
    align=align.toLowerCase();
    if (align.equals(TOP) || align.equals(TEXTTOP))     return 0.0f;
 else     if (align.equals(this.CENTER) || align.equals(MIDDLE) || align.equals(ABSMIDDLE))     return 0.5f;
  }
  return 1.0f;
}
