{
  if (isSecured) {
    zrtpButton.setIcon(new ImageIcon(SCCallbackResources.getImage(SCCallbackResources.SECURE_ON_ICON)));
    zrtpButton.setToolTipText(SCCallbackResources.TOGGLE_OFF_SECURITY);
  }
 else {
    zrtpButton.setIcon(new ImageIcon(SCCallbackResources.getImage(SCCallbackResources.SECURE_OFF_ICON)));
    zrtpButton.setToolTipText(SCCallbackResources.TOGGLE_ON_SECURITY);
  }
}
