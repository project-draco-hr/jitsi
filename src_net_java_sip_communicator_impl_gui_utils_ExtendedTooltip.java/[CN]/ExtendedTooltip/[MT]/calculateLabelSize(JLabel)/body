{
  Icon icon=label.getIcon();
  String text=label.getText();
  int iconWidth=0;
  if (icon != null)   iconWidth=icon.getIconWidth();
  int stringWidth=GuiUtils.getStringWidth(label,text) + iconWidth + label.getIconTextGap();
  int stringHeight=GuiUtils.getStringSize(label,text).height;
  return new Dimension(stringWidth,stringHeight);
}
