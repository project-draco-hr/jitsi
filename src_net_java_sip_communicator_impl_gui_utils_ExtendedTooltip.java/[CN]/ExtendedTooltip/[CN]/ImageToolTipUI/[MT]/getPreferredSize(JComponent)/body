{
  Icon icon=imageLabel.getIcon();
  int width=0;
  if (icon != null)   width+=icon.getIconWidth();
  width+=textWidth;
  int imageHeight=0;
  if (icon != null)   imageHeight=icon.getIconHeight();
  int height=imageHeight > textHeight ? imageHeight : textHeight;
  return new Dimension(width + 15,height);
}
