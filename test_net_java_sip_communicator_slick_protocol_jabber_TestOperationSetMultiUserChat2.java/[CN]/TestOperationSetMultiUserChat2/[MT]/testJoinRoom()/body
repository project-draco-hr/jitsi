{
  String testRoomName=testRoomBaseName + roomID++;
  ChatRoom opSet1Room=opSetMUC1.createChatRoom(testRoomName,null);
  opSet1Room.join();
  assertTrue("we are not in the room we just joined",opSet1Room.isJoined());
  List<ChatRoomMember> members=opSet1Room.getMembers();
  String memberAddress=members.iterator().next().getContactAddress();
  assertEquals("the member addres is not the good one",memberAddress,fixture.userID1);
}
