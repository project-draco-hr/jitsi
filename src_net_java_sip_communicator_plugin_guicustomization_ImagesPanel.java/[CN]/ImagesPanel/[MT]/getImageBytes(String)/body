{
  InputStream in=GuiCustomizationActivator.getResources().getImageInputStream(imageID);
  if (in == null)   return null;
  try {
    byte[] bs=new byte[in.available()];
    in.read(bs);
    return bs;
  }
 catch (  IOException e) {
    logger.error("Failed to load image:" + imageID,e);
  }
  return null;
}
