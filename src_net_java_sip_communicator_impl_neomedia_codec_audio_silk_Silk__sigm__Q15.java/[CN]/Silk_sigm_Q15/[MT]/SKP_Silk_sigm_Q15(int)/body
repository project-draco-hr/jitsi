{
  int ind;
  if (in_Q5 < 0) {
    in_Q5=-in_Q5;
    if (in_Q5 >= 6 * 32) {
      return 0;
    }
 else {
      ind=in_Q5 >> 5;
      return (sigm_LUT_neg_Q15[ind] - Silk_macros.SKP_SMULBB(sigm_LUT_slope_Q10[ind],in_Q5 & 0x1F));
    }
  }
 else {
    if (in_Q5 >= 6 * 32) {
      return 32767;
    }
 else {
      ind=in_Q5 >> 5;
      return (sigm_LUT_pos_Q15[ind] + Silk_macros.SKP_SMULBB(sigm_LUT_slope_Q10[ind],in_Q5 & 0x1F));
    }
  }
}
