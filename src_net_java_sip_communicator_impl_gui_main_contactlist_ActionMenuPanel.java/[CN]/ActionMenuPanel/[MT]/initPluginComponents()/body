{
  ServiceReference[] serRefs=null;
  try {
    serRefs=GuiActivator.bundleContext.getServiceReferences(FavoritesButton.class.getName(),null);
  }
 catch (  InvalidSyntaxException exc) {
    logger.trace("Could not obtain plugin reference.",exc);
  }
  if (serRefs != null) {
    for (    ServiceReference serRef : serRefs) {
      FavoritesButton c=(FavoritesButton)GuiActivator.bundleContext.getService(serRef);
      addPluginComponent(c);
    }
  }
}
