{
  SIPCommButton cButton=new SIPCommButton(ImageLoader.getBytesInImage(c.getImage()));
  JPanel cPanel=createMoreActionsButtonPanel(cButton,c.getText());
  cButton.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
      c.actionPerformed();
    }
  }
);
  this.add(cPanel);
}
