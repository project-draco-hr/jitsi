{
  InputStream in=GuiActivator.getResources().getImageInputStream(imageID.getId());
  if (in == null)   return null;
  byte[] image=null;
  try {
    image=new byte[in.available()];
    in.read(image);
  }
 catch (  IOException e) {
    log.error("Failed to load image:" + imageID.getId(),e);
  }
  return image;
}
