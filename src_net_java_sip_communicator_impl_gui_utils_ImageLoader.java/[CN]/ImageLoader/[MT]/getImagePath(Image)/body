{
  String path=null;
  Iterator i=ImageLoader.loadedImages.entrySet().iterator();
  while (i.hasNext()) {
    Map.Entry entry=(Map.Entry)i.next();
    if (entry.getValue().equals(image)) {
      String imageID=((ImageID)entry.getKey()).getId();
      path=Images.getString(imageID);
    }
  }
  return path;
}
