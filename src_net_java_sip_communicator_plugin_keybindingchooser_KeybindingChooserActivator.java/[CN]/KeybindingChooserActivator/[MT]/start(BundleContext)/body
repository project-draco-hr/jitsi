{
  bundleContext=context;
  logger.debug("Service Impl: " + getClass().getName() + " [  STARTED ]");
  ServiceReference keybindingRef=context.getServiceReference(KeybindingsService.class.getName());
  KeybindingsService keybingingsService=(KeybindingsService)context.getService(keybindingRef);
  KeybindingsConfigForm keybindingsManager=new KeybindingsConfigForm(keybingingsService);
  context.registerService(ConfigurationForm.class.getName(),keybindingsManager,null);
}
