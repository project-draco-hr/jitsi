{
  if (!(answerConditional || answerUnconditional))   return false;
  if (answerConditional) {
    SIPHeader callAnswerHeader=(SIPHeader)invite.getHeader(headerName);
    if (callAnswerHeader == null)     return false;
    if (!StringUtils.isNullOrEmpty(headerValue)) {
      String value=callAnswerHeader.getHeaderValue();
      if (value == null || !headerValue.equals(value))       return false;
    }
  }
  Iterator<? extends CallPeer> peers=call.getCallPeers();
  while (peers.hasNext()) {
    final CallPeer peer=peers.next();
    answerPeer(peer);
  }
  return true;
}
