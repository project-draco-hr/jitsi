{
  if (userIDField.getText() == null || userIDField.getText().equals("") || serverField.getText() == null || serverField.getText().equals("") || (!passwordNotRequired.isSelected() && isEmpty(passField))) {
    wizard.getWizardContainer().setNextFinishButtonEnabled(false);
  }
 else {
    wizard.getWizardContainer().setNextFinishButtonEnabled(true);
  }
}
