{
  if (nickField.getText() == null || nickField.getText().equals("") || serverField.getText() == null || serverField.getText().equals("") || (!passwordNotRequired.isSelected() && passField.getText().equals(""))) {
    wizard.getWizardContainer().setNextFinishButtonEnabled(false);
  }
 else {
    wizard.getWizardContainer().setNextFinishButtonEnabled(true);
  }
}
