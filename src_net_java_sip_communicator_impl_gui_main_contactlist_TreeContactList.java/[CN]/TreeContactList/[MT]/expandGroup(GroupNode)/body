{
  final TreePath path=new TreePath(treeModel.getPathToRoot(groupNode));
  if (!isExpanded(path))   if (!SwingUtilities.isEventDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        expandPath(path);
      }
    }
);
  }
 else {
    expandPath(path);
  }
}
