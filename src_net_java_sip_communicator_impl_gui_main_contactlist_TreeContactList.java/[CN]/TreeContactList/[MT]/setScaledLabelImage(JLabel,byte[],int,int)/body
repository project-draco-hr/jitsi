{
  if (!SwingUtilities.isEventDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        setScaledLabelImage(label,image,width,height);
      }
    }
);
    return;
  }
  label.setIcon(ImageUtils.getScaledRoundedIcon(image,width,height));
  label.repaint();
}
