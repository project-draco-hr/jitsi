{
  TreePath selectionPath=getSelectionPath();
  if (selectionPath == null)   return;
  ContactListTreeCellRenderer renderer=(ContactListTreeCellRenderer)getCellRenderer().getTreeCellRendererComponent(this,selectionPath.getLastPathComponent(),true,true,true,this.getRowForPath(selectionPath),true);
  renderer.getCallButton().doClick();
}
