{
  MetaContactGroup parentGroup=metaGroup.getParentMetaContactGroup();
  GroupNode parentGroupNode=treeModel.findGroupNodeByMetaGroup(parentGroup);
  if (parentGroupNode == null)   return;
  parentGroupNode.removeMetaContactGroup(metaGroup);
  if (parentGroupNode.getChildCount() < 1)   treeModel.removeNodeFromParent(parentGroupNode);
}
