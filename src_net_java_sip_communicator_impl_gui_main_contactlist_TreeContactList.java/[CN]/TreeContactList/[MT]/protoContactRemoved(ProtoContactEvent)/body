{
  final MetaContact oldParent=evt.getOldParent();
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
synchronized (tempTreeModel) {
        UIContact oldUIContact=MetaContactListSource.getUIContact(oldParent);
        if (oldUIContact != null)         removeContact(oldUIContact);
      }
    }
  }
);
}
