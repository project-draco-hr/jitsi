{
  MetaContact oldParent=evt.getOldParent();
synchronized (tempTreeModel) {
    ContactNode contactNode=treeModel.findContactNodeByMetaContact(oldParent);
    if (contactNode != null && !currentFilter.isMatching(oldParent)) {
      removeContact(oldParent,oldParent.getParentMetaContactGroup());
    }
  }
}
