{
  GroupNode parentGroupNode=treeModel.findGroupNodeByMetaGroup(parentMetaGroup);
  if (parentGroupNode == null)   return;
  parentGroupNode.removeMetaContact(metaContact);
  if (parentGroupNode.getChildCount() < 1 && parentGroupNode.getParent() != null) {
    treeModel.removeNodeFromParent(parentGroupNode);
  }
}
