{
  filterQuery=new FilterQuery();
  treeModel.clear();
  if (filter.hasDefaultSource())   applyFilter(filter,null);
  Iterator<ExternalContactSource> filterSources=filter.getContactSources().iterator();
  while (filterSources.hasNext()) {
    final ExternalContactSource filterSource=filterSources.next();
    if (filterQuery.isCanceled())     return filterQuery;
    applyFilter(filter,filterSource);
  }
  return filterQuery;
}
