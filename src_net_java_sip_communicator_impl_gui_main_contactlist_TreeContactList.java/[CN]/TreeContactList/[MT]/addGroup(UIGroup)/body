{
  if (!SwingUtilities.isEventDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        addGroup(group);
      }
    }
);
    return;
  }
  GroupNode parentNode=treeModel.getRoot();
  parentNode.sortedAddContactGroup(group);
  expandGroup(treeModel.getRoot());
}
