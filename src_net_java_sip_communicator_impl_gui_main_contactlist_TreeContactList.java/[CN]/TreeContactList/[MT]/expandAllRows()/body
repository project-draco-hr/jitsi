{
  for (int i=0; i < getRowCount(); i++) {
    TreePath treePath=getPathForRow(i);
    Object c=treePath.getLastPathComponent();
    GroupNode groupNode=null;
    if (!(c instanceof GroupNode))     continue;
 else     groupNode=(GroupNode)c;
    if (groupNode != null && (currentFilter.equals(searchFilter) || !groupNode.isCollapsed()))     expandRow(i);
  }
}
