{
  UIContact uiContact=MetaContactListSource.getUIContact(evt.getSourceContact());
  if (uiContact != null && evt.getEventID() == MetaContactCapabilitiesEvent.SUPPORTED_OPERATION_SETS_CHANGED) {
    ContactNode contactNode=uiContact.getContactNode();
    if (contactNode != null)     treeModel.nodeChanged(contactNode);
  }
}
