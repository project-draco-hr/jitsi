{
  String clearPortStr=SipActivator.getConfigurationService().getString(PREFERRED_CLEAR_PORT_PROPERTY_NAME);
  int clearPort=ListeningPoint.PORT_5060;
  if (clearPortStr != null) {
    try {
      clearPort=Integer.parseInt(clearPortStr);
    }
 catch (    NumberFormatException ex) {
      logger.error(clearPortStr + " does not appear to be an integer. " + "Defaulting port bind retries to "+ clearPort+ ".",ex);
    }
  }
  return clearPort;
}
