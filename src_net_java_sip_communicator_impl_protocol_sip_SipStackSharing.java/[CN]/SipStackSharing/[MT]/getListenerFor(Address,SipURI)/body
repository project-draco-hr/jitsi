{
  Set<ProtocolProviderServiceSipImpl> currentListeners=this.getSipListeners();
  for (  ProtocolProviderServiceSipImpl listener : currentListeners)   if (listener.getOurSipAddress(remoteURI).equals(localParty)) {
    logger.trace("found listener for local party: " + localParty);
    return listener;
  }
  logger.error("no listener found for local party: " + localParty);
  return null;
}
