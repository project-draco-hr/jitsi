{
  logger.trace(timeoutEvent);
  ProtocolProviderServiceSipImpl recipient;
  if (timeoutEvent.isServerTransaction()) {
    recipient=findTargetFor(timeoutEvent.getServerTransaction().getRequest());
    if (recipient != null)     recipient.processTimeout(timeoutEvent);
  }
 else {
    recipient=findTargetFor(timeoutEvent.getClientTransaction());
    if (recipient != null)     recipient.processTimeout(timeoutEvent);
  }
}
