{
  String securePortStr=SipActivator.getConfigurationService().getString(PREFERRED_SECURE_PORT_PROPERTY_NAME);
  int securePort=ListeningPoint.PORT_5061;
  if (securePortStr != null) {
    try {
      securePort=Integer.parseInt(securePortStr);
    }
 catch (    NumberFormatException ex) {
      logger.error(securePortStr + " does not appear to be an integer. " + "Defaulting port bind retries to "+ securePort+ ".",ex);
    }
  }
  return securePort;
}
