{
  Transaction transaction;
  if (event.isServerTransaction())   transaction=event.getServerTransaction();
 else   transaction=event.getClientTransaction();
  ProtocolProviderServiceSipImpl recipient=getServiceData(transaction);
  if (recipient == null)   logger.error("We received a transaction terminated which wasn't " + "marked, please report this to " + "dev@sip-communicator.dev.java.net");
 else   recipient.processTransactionTerminated(event);
}
