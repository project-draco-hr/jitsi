{
  StatusSelectorBox selectorBox=(StatusSelectorBox)protocolStatusCombos.get(pps);
  if (selectorBox == null)   return;
  if (selectorBox instanceof PresenceStatusSelectorBox) {
    PresenceStatusSelectorBox presenceSelectorBox=(PresenceStatusSelectorBox)selectorBox;
    presenceSelectorBox.updateStatus(status);
  }
  if (globalStatusBox != null) {
    globalStatusBox.updateStatus();
  }
}
