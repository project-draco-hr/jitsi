{
  attributes=new SrtpCryptoAttribute[supportedCryptoSuites.length];
  for (int i=0; i < attributes.length; i++) {
    attributes[i]=sdesFactory.createCryptoAttribute(i + 1,supportedCryptoSuites[i]);
  }
  String[] result=new String[attributes.length];
  for (int i=0; i < attributes.length; i++)   result[i]=attributes[i].encode();
  return result;
}
