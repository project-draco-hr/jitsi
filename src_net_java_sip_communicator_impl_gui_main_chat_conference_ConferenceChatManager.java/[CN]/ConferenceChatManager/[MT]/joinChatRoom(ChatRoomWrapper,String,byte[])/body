{
  ChatRoom chatRoom=chatRoomWrapper.getChatRoom();
  if (chatRoom == null) {
    new ErrorDialog(GuiActivator.getUIService().getMainFrame(),Messages.getI18NString("warning").getText(),Messages.getI18NString("haveToBeConnectedToJoin").getText()).showDialog();
    return;
  }
  new JoinChatRoomTask(chatRoomWrapper,nickName,password).execute();
}
