{
  ChatRoom chatRoom=chatRoomWrapper.getChatRoom();
  if (chatRoom == null) {
    new ErrorDialog(GuiActivator.getUIService().getMainFrame(),GuiActivator.getResources().getI18NString("service.gui.WARNING"),GuiActivator.getResources().getI18NString("service.gui.CHAT_ROOM_NOT_CONNECTED",new String[]{chatRoomWrapper.getChatRoomName()})).showDialog();
    return;
  }
  new JoinChatRoomTask(chatRoomWrapper,nickName,password,rememberPassword,isFirstAttempt).execute();
}
