{
  ChatRoom chatRoom=chatRoomWrapper.getChatRoom();
  if (chatRoom == null) {
    new ErrorDialog(GuiActivator.getUIService().getMainFrame(),GuiActivator.getResources().getI18NString("service.gui.WARNING"),GuiActivator.getResources().getI18NString("haveToBeConnectedToJoin")).showDialog();
    return;
  }
  new JoinChatRoomTask(chatRoomWrapper,null,null).execute();
}
