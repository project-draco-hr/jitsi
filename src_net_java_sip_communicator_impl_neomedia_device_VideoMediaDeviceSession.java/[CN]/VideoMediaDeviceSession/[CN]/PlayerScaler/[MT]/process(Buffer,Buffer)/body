{
  int result=super.process(input,output);
  if (result == BUFFER_PROCESSED_OK) {
    Format inputFormat=getInputFormat();
    if (inputFormat != null) {
      Dimension size=((VideoFormat)inputFormat).getSize();
      if ((size != null) && ((lastSize == null) || !lastSize.equals(size))) {
        lastSize=size;
        playerSizeChange(player,lastSize.width,lastSize.height);
      }
    }
  }
  return result;
}
