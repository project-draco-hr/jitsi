{
  DataSource captureDevice=getCaptureDevice();
  if (captureDevice != null) {
    MediaLocator locator=captureDevice.getLocator();
    if ((locator != null) && ImageStreamingAuto.LOCATOR_PROTOCOL.equals(locator.getProtocol()))     return createLocalVisualComponentForDesktopStreaming();
  }
  getLocalPlayer();
  return null;
}
