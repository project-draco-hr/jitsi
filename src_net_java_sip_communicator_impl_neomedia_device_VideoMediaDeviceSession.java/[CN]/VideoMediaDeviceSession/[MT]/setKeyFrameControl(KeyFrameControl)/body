{
  if (this.keyFrameControl != keyFrameControl) {
    if ((this.keyFrameControl != null) && (keyFrameRequester != null))     this.keyFrameControl.removeKeyFrameRequester(keyFrameRequester);
    this.keyFrameControl=keyFrameControl;
    if ((this.keyFrameControl != null) && (keyFrameRequester != null))     this.keyFrameControl.addKeyFrameRequester(-1,keyFrameRequester);
  }
}
