{
  this.add(kickItem);
  this.add(banItem);
  this.kickItem.setName("kick");
  this.banItem.setName("ban");
  this.kickItem.setMnemonic(kickString.getMnemonic());
  this.banItem.setMnemonic(banString.getMnemonic());
  this.kickItem.addActionListener(this);
  this.banItem.addActionListener(this);
}
