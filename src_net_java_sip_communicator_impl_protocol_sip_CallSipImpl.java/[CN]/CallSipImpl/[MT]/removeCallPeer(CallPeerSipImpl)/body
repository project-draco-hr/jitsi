{
  if (!callPeers.contains(callPeer))   return;
  this.callPeers.remove(callPeer);
  callPeer.removeCallPeerListener(this);
  try {
    fireCallPeerEvent(callPeer,CallPeerEvent.CALL_PEER_REMOVED);
  }
  finally {
    callPeer.setCall(null);
  }
  if (callPeers.size() == 0)   setCallState(CallState.CALL_ENDED);
}
