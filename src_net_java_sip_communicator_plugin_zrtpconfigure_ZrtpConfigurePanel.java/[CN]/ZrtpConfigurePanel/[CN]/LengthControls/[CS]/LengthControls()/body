{
  String id=ZrtpConfigureUtils.getPropertyID(ZrtpConstants.SupportedAuthLengths.HS32);
  String savedConf=ZrtpConfigureActivator.configService.getString(id);
  if (savedConf == null)   savedConf="";
  dataModel=new ZrtpConfigureTableModel<ZrtpConstants.SupportedAuthLengths>(ZrtpConstants.SupportedAuthLengths.HS32,active,inActive,savedConf);
  createControls(this,dataModel,ZrtpConfigureActivator.resourceService.getI18NString("impl.media.security.zrtp.SRTP_LENGTHS"));
}
