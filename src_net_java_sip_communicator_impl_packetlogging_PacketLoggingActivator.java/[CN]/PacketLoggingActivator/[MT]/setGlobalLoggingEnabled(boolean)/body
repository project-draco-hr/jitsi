{
  if (enabled) {
    getConfigurationService().setProperty(PACKET_LOGGING_ENABLED_PROPERTY_NAME,Boolean.TRUE);
  }
 else {
    getConfigurationService().removeProperty(PACKET_LOGGING_ENABLED_PROPERTY_NAME);
    sipLoggingEnabled=false;
    jabberLoggingEnabled=false;
    rtpLoggingEnabled=false;
    ice4jLoggingEnabled=false;
  }
  globalLoggingEnabled=enabled;
}
