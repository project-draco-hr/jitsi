{
  globalLoggingEnabled=getConfigurationService().getBoolean(PACKET_LOGGING_ENABLED_PROPERTY_NAME,globalLoggingEnabled);
  sipLoggingEnabled=getConfigurationService().getBoolean(PACKET_LOGGING_SIP_ENABLED_PROPERTY_NAME,sipLoggingEnabled);
  jabberLoggingEnabled=getConfigurationService().getBoolean(PACKET_LOGGING_JABBER_ENABLED_PROPERTY_NAME,jabberLoggingEnabled);
  rtpLoggingEnabled=getConfigurationService().getBoolean(PACKET_LOGGING_RTP_ENABLED_PROPERTY_NAME,rtpLoggingEnabled);
  ice4jLoggingEnabled=getConfigurationService().getBoolean(PACKET_LOGGING_ICE4J_ENABLED_PROPERTY_NAME,ice4jLoggingEnabled);
}
