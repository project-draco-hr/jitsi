{
  logger.debug("Started.");
  MediaActivator.bundleContext=context;
  mediaServiceImpl=new MediaServiceImpl();
  mediaServiceImpl.start();
  mediaServiceRegistration=context.registerService(MediaService.class.getName(),mediaServiceImpl,null);
  logger.debug("Media Service ... [REGISTERED]");
  context.registerService(ConfigurationForm.class.getName(),new MediaConfigurationForm(),null);
}
