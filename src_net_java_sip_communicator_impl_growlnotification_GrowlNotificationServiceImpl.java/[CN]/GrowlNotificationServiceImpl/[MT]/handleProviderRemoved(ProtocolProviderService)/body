{
  OperationSetBasicInstantMessaging opSetIm=(OperationSetBasicInstantMessaging)provider.getSupportedOperationSets().get(OperationSetBasicInstantMessaging.class.getName());
  if (opSetIm != null) {
    opSetIm.removeMessageListener(this);
    try {
      notifyGrowlOf("Protocol events","Protocol deregistered",provider.getProtocolName() + " deregistered");
    }
 catch (    Exception ex) {
      logger.error("Could not notify the message to Growl",ex);
    }
  }
}
