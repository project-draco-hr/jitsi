{
  OperationSetBasicInstantMessaging opSetIm=(OperationSetBasicInstantMessaging)provider.getOperationSet(OperationSetBasicInstantMessaging.class);
  if (opSetIm != null) {
    opSetIm.removeMessageListener(this);
    try {
      notifyGrowlOf("Protocol events",sipIconPath,"Protocol deregistered",provider.getProtocolName() + " deregistered");
    }
 catch (    Exception ex) {
      logger.error("Could not notify the message to Growl",ex);
    }
  }
}
