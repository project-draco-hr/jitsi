{
  if (logger.isDebugEnabled())   logger.debug("Starting the Growl Notification implementation.");
  if (Growl.isGrowlRunning()) {
    String[] dict={"Default","Welcome message"};
    byte[] sipIcon=GrowlNotificationActivator.getResources().getImageInBytes("service.gui.SIP_COMMUNICATOR_LOGO_45x45");
    growl=new Growl("SIP Communicator","net.sip-communicator",sipIcon,dict,dict);
    growl.addClickedNotificationsListener(this);
    growl.notifyGrowlOf("SIP Communicator","http://www.sip-communicator.org/","Welcome message",null,null);
  }
}
