{
  BundleContext context=GibberishActivator.getBundleContext();
  if (context == null)   throw new NullPointerException("The specified BundleContext was null");
  String userIDStr=(String)accountProperties.get(USER_ID);
  AccountID accountID=new GibberishAccountID(userIDStr,accountProperties);
  Hashtable properties=new Hashtable();
  properties.put(PROTOCOL,"Gibberish");
  properties.put(USER_ID,userIDStr);
  ProtocolProviderServiceGibberishImpl gibberishProtocolProvider=new ProtocolProviderServiceGibberishImpl();
  gibberishProtocolProvider.initialize(userIDStr,accountID);
  ServiceRegistration registration=context.registerService(ProtocolProviderService.class.getName(),gibberishProtocolProvider,properties);
  registeredAccounts.put(accountID,registration);
  return accountID;
}
