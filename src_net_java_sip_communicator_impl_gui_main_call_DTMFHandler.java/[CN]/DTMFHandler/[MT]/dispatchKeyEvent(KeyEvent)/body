{
  if (e.getID() == KeyEvent.KEY_TYPED)   return false;
  boolean singleWindowRule=GuiActivator.getUIService().getSingleWindowContainer() == null || callContainer.isFocusOwner();
  boolean dispatch=false;
  for (int i=0; i < parents.size(); i++) {
    if (parents.get(i).isFocused() && singleWindowRule) {
      dispatch=true;
      break;
    }
  }
  if (!dispatch)   return false;
  for (int i=0; i < availableTones.length; i++) {
    DTMFToneInfo info=availableTones[i];
    if (info.keyChar == e.getKeyChar()) {
switch (e.getID()) {
case KeyEvent.KEY_PRESSED:
        startSendingDtmfTone(info);
      break;
case KeyEvent.KEY_RELEASED:
    stopSendingDtmfTone();
  break;
}
return false;
}
}
return false;
}
