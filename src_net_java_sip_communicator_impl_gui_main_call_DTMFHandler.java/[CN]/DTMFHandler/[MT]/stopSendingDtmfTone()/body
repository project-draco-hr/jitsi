{
  if (currentlyPlayingTone != null)   GuiActivator.getNotificationService().stopNotification(currentlyPlayingTone);
  if (callContainer != null) {
    stopSendingDtmfTone(callContainer.getCurrentCallRenderer().getCall());
  }
 else {
    Collection<Call> activeCalls=CallManager.getInProgressCalls();
    if (activeCalls != null) {
      for (      Call activeCall : activeCalls)       stopSendingDtmfTone(activeCall);
    }
  }
}
