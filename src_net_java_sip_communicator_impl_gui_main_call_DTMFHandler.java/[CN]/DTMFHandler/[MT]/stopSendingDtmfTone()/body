{
  if (currentlyPlayingAudio != null)   currentlyPlayingAudio.stop();
  currentlyPlayingAudio=null;
  if (callContainer != null)   stopSendingDtmfTone(callContainer.getCurrentCallRenderer().getCall());
 else {
    Collection<Call> activeCalls=CallManager.getActiveCalls();
    if (activeCalls != null) {
      Iterator<Call> callsIter=activeCalls.iterator();
      while (callsIter.hasNext()) {
        stopSendingDtmfTone(callsIter.next());
      }
    }
  }
}
