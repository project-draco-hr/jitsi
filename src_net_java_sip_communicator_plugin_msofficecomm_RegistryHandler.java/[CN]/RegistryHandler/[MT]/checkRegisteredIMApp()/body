{
  if (!Advapi32Util.registryKeyExists(WinReg.HKEY_LOCAL_MACHINE,REGISTRY_IM_APPLICATION_KEY + "\\" + getApplicationName())) {
    logger.error(REGISTRY_IM_APPLICATION_KEY + "\\" + getApplicationName()+ " doesn;t exsts");
    return;
  }
  logger.info("Registered IM App friendly name: " + Advapi32Util.registryGetStringValue(WinReg.HKEY_LOCAL_MACHINE,REGISTRY_IM_APPLICATION_KEY + "\\" + getApplicationName(),"FriendlyName"));
}
