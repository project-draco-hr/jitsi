{
  if (logType.equals(LogMessageNotificationHandler.ERROR_LOG_TYPE))   logger.error(message);
 else   if (logType.equals(LogMessageNotificationHandler.INFO_LOG_TYPE))   if (logger.isInfoEnabled())   logger.info(message);
 else   if (logType.equals(LogMessageNotificationHandler.TRACE_LOG_TYPE))   if (logger.isTraceEnabled())   logger.trace(message);
}
