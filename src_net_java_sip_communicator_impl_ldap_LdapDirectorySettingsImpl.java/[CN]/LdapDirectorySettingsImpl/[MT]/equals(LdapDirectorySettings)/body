{
  return this.getName().equals(other.getName()) && this.getHostname().equals(other.getHostname()) && this.getEncryption().equals(other.getEncryption())&& this.getPort() == other.getPort() && this.getAuth().equals(other.getAuth()) && this.getBindDN().equals(other.getBindDN()) && this.getPassword().equals(other.getPassword()) && this.getBaseDN().equals(other.getBaseDN()) && this.getScope().equals(other.getScope());
}
