{
  ChatRoom chatroom=EasyMock.createMock(ChatRoom.class);
  ProtocolProviderService provider=EasyMock.createMock(ProtocolProviderService.class);
  ChatRoomMemberIrcImpl member=new ChatRoomMemberIrcImpl(provider,chatroom,"user",ChatRoomMemberRole.SILENT_MEMBER);
  Assert.assertSame(ChatRoomMemberRole.SILENT_MEMBER,member.getRole());
  try {
    member.setRole(null);
    Assert.fail("expected IAE because of null role");
  }
 catch (  IllegalArgumentException e) {
  }
}
