{
  String s="";
  if (getAction().equals(WhiteboardObjectPacketExtensionImpl.ACTION_DELETE)) {
    s="<delete id=\"#i\"/>";
    s=s.replaceAll("#i",getWhiteboardObjectID());
  }
 else   s=getWhiteboardObject().toXML();
  return "<" + WhiteboardObjectPacketExtensionImpl.ELEMENT_NAME + " xmlns=\""+ WhiteboardObjectPacketExtensionImpl.NAMESPACE+ "\">"+ s+ "</"+ WhiteboardObjectPacketExtensionImpl.ELEMENT_NAME+ ">";
}
