{
  String imageID;
switch (getSessionStatus()) {
case ENCRYPTED:
    imageID=OtrActivator.scOtrKeyManager.isVerified(contact) ? "plugin.otr.ENCRYPTED_ICON_16x16" : "plugin.otr.ENCRYPTED_UNVERIFIED_ICON_16x16";
  break;
case FINISHED:
imageID="plugin.otr.FINISHED_ICON_16x16";
break;
case PLAINTEXT:
imageID="plugin.otr.PLAINTEXT_ICON_16x16";
break;
default :
return;
}
setIcon(OtrActivator.resourceService.getImage(imageID));
}
