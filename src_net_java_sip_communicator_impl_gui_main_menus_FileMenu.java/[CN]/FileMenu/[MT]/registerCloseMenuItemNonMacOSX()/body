{
  JMenuItem closeMenuItem=new JMenuItem(GuiActivator.getResources().getI18NString("service.gui.QUIT"),new ImageIcon(ImageLoader.getImage(ImageLoader.QUIT_16x16_ICON)));
  this.addSeparator();
  this.add(closeMenuItem);
  closeMenuItem.setName("close");
  closeMenuItem.addActionListener(this);
  closeMenuItem.setMnemonic(GuiActivator.getResources().getI18nMnemonic("service.gui.QUIT"));
}
