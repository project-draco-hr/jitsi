{
  newAccountMenuItem.setIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.ADD_ACCOUNT_MENU_ICON)));
  addContactItem.setIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.ADD_CONTACT_16x16_ICON)));
  createGroupItem.setIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.GROUPS_16x16_ICON)));
  myChatRoomsItem.setIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.CHAT_ROOM_16x16_ICON)));
  if (closeMenuItem != null) {
    closeMenuItem.setIcon(new ImageIcon(ImageLoader.getImage(ImageLoader.QUIT_16x16_ICON)));
  }
}
