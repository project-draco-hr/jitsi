{
  this.launchArgHandler=launchArgHandler;
  setAEGetURLListener(new IAEGetURLListener(){
    public void handleAEGetURLEvent(    final String url){
      new Thread(){
        public void run(){
          AEGetURLEventHandler.this.launchArgHandler.handleArgs(new String[]{url});
        }
      }
.start();
    }
  }
);
}
