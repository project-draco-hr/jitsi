{
  boolean fullScreen=this.callContainer.isFullScreen();
  setIconImage(ImageLoader.getImage(fullScreen ? ImageLoader.EXIT_FULL_SCREEN_BUTTON : ImageLoader.ENTER_FULL_SCREEN_BUTTON));
  setPreferredSize(new Dimension(44,38));
  setToolTipText(GuiActivator.getResources().getI18NString(fullScreen ? "service.gui.EXIT_FULL_SCREEN_TOOL_TIP" : "service.gui.ENTER_FULL_SCREEN_TOOL_TIP"));
}
