{
  JMenuItem menuItem=(JMenuItem)e.getSource();
  Enumeration i=accountsTable.keys();
  while (i.hasMoreElements()) {
    ProtocolProviderService pps=(ProtocolProviderService)i.nextElement();
    if (accountsTable.get(pps).equals(menuItem)) {
      this.setSelected(pps,(ImageIcon)menuItem.getIcon());
      return;
    }
  }
  logger.debug("Could not find account for menu item " + menuItem.getText() + ". accountsTable("+ accountsTable.size()+ ") is : "+ accountsTable);
}
