{
  JMenuItem menuItem=(JMenuItem)e.getSource();
  for (  Map.Entry<ProtocolProviderService,JMenuItem> entry : accountsTable.entrySet()) {
    if (entry.getValue().equals(menuItem)) {
      this.setSelected(entry.getKey(),(ImageIcon)menuItem.getIcon());
      return;
    }
  }
  logger.debug("Could not find account for menu item " + menuItem.getText() + ". accountsTable("+ accountsTable.size()+ ") is : "+ accountsTable);
}
