{
  if (!failedOnce) {
    failedOnce=true;
    return true;
  }
  Object[] ob={passwordField};
  int result=JOptionPane.showConfirmDialog(null,ob,"Auth Failed: " + message,JOptionPane.OK_CANCEL_OPTION);
  if (result == JOptionPane.OK_OPTION) {
    sshContact.setPassword(passwordField.getText());
    return true;
  }
  return false;
}
