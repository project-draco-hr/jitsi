{
  if (getRequestingProtocol().startsWith(type) && getRequestingHost().equals(host) && port == getRequestingPort() && getRequestorType().equals(Authenticator.RequestorType.SERVER)) {
    final char[] pass;
    if (password == null) {
      pass=new char[0];
    }
 else {
      pass=password.toCharArray();
    }
    return new PasswordAuthentication(username,pass);
  }
 else   return super.getPasswordAuthentication();
}
