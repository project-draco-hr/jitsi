{
  final Pattern p=Pattern.compile("\\/play\\:([a-zA-Z0-9_\\-]+)([?&]\\w+=[\\w-]*)*",Pattern.CASE_INSENSITIVE | Pattern.DOTALL);
  Matcher m=p.matcher(sourceString);
  String thumbUrl=sourceString;
  while (m.find()) {
    thumbUrl="http://i.vbox7.com/p/" + m.group(1) + "3.jpg";
  }
  return thumbUrl;
}
