{
  if (!(sourceContact instanceof MetaContact) && (sourceContact.getPreferredContactDetail(OperationSetMultiUserChat.class) != null)) {
    return new ChatRoomRightButtonMenu(GuiActivator.getMUCService().findChatRoomWrapperFromSourceContact(sourceContact));
  }
 else {
    return new SourceContactRightButtonMenu(this);
  }
}
