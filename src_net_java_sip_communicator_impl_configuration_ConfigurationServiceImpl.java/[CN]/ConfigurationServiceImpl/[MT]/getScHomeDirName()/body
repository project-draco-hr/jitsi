{
  String scHomeDirName=null;
  if (store != null)   scHomeDirName=getString(PNAME_SC_HOME_DIR_NAME);
  if (scHomeDirName == null) {
    scHomeDirName=getSystemProperty(PNAME_SC_HOME_DIR_NAME);
    if (scHomeDirName == null)     scHomeDirName=".sip-communicator";
    if (store != null)     store.setNonSystemProperty(PNAME_SC_HOME_DIR_NAME,scHomeDirName);
  }
  return scHomeDirName;
}
