{
  this.configurationFile=null;
  File file=getConfigurationFile();
  if (file != null) {
    FailSafeTransaction trans=this.faService.createFailSafeTransaction(file);
    try {
      trans.restoreFile();
    }
 catch (    Exception e) {
      logger.error("Failed to restore configuration file " + file,e);
    }
  }
  store.reloadConfiguration(file);
}
