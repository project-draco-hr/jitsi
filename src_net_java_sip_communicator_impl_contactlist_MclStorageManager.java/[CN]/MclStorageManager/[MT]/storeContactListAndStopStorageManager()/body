{
  Object contactLstRWLock=getContactListRWLock();
synchronized (contactLstRWLock) {
    if (!isStarted())     return;
    started=false;
    contactLstRWLock.notifyAll();
    try {
      storeContactList0();
    }
 catch (    IOException ex) {
      logger.debug("Failed to store contact list before stopping",ex);
    }
  }
}
