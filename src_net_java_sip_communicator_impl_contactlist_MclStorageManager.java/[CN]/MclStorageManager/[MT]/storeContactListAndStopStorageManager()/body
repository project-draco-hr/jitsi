{
  Object contactListRWLock=getContactListRWLock();
synchronized (contactListRWLock) {
    if (!isStarted())     return;
    started=false;
    contactListRWLock.notifyAll();
    try {
      storeContactList0();
    }
 catch (    IOException ex) {
      logger.debug("Failed to store contact list before stopping",ex);
    }
  }
}
