{
  if (!isStarted())   return;
  Node root=contactListDocument.getFirstChild();
  Element metaContactListRoot=XMLUtils.findChild((Element)root,META_CONTACT_LIST_ROOT_NAME);
  NodeList children=metaContactListRoot.getChildNodes();
  for (int i=0; i < children.getLength(); i++) {
    Node currentNode=children.item(i);
    if (currentNode.getNodeType() != Node.ELEMENT_NODE || !currentNode.getNodeName().equals(GROUP_NODE_NAME))     continue;
    processGroupXmlNode(mclServiceImpl,accountID,(Element)currentNode,mclServiceImpl.getRoot(),null);
  }
}
