{
  data.clear();
  if (defaultCiphers == null)   return;
  if (ciphers == null)   ciphers=defaultCiphers;
  data.add(new Entry(SrtpCryptoSuite.AES_CM_128_HMAC_SHA1_80,ciphers.contains(SrtpCryptoSuite.AES_CM_128_HMAC_SHA1_80)));
  data.add(new Entry(SrtpCryptoSuite.AES_CM_128_HMAC_SHA1_32,ciphers.contains(SrtpCryptoSuite.AES_CM_128_HMAC_SHA1_32)));
  data.add(new Entry(SrtpCryptoSuite.F8_128_HMAC_SHA1_80,ciphers.contains(SrtpCryptoSuite.F8_128_HMAC_SHA1_80)));
  fireTableDataChanged();
}
