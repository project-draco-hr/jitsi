{
  assertConnected();
  if (!(contactToMove instanceof ContactMsnImpl))   throw new IllegalArgumentException("The specified contact is not an msn contact." + contactToMove);
  if (!(newParent instanceof ContactGroupMsnImpl))   throw new IllegalArgumentException("The specified group is not an msn contact group." + newParent);
  ssContactList.moveContact((ContactMsnImpl)contactToMove,(ContactGroupMsnImpl)newParent);
}
