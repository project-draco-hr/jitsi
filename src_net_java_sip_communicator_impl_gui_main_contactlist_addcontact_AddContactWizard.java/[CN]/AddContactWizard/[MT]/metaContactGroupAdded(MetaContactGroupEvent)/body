{
  ArrayList ppList=newContact.getProtocolProviders();
  for (int i=0; i < ppList.size(); i++) {
    ProtocolProviderService pps=(ProtocolProviderService)ppList.get(i);
    MetaContactGroup group=(MetaContactGroup)evt.getSourceMetaContactGroup();
    new CreateContact(pps,group,newContact).start();
  }
}
