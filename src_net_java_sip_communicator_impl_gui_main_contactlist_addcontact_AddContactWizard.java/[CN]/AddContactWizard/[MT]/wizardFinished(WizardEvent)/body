{
  if (e.getEventCode() == WizardEvent.SUCCESS) {
    ArrayList<ProtocolProviderService> ppList=newContact.getProtocolProviders();
    for (int i=0; i < ppList.size(); i++) {
      ProtocolProviderService pps=(ProtocolProviderService)ppList.get(i);
      new CreateContact(pps,newContact).start();
    }
  }
}
