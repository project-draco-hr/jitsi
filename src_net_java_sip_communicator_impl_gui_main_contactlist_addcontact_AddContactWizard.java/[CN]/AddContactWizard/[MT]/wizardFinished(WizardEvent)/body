{
  if (e.getEventCode() == WizardEvent.SUCCESS) {
    if (newContact.getNewGroup() != null && !newContact.getNewGroup().equals("")) {
      new CreateGroup(mainFrame.getContactList(),newContact).start();
    }
    ArrayList ppList=newContact.getProtocolProviders();
    MetaContactGroup group=newContact.getGroup();
    for (int i=0; i < ppList.size(); i++) {
      ProtocolProviderService pps=(ProtocolProviderService)ppList.get(i);
      new CreateContact(pps,group,newContact).start();
    }
  }
}
