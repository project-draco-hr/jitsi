{
  if (setting)   return;
  filtering=true;
  Filter filter=null;
  if (text.getText().length() > 0) {
    filter=new StartsWithFilter(text.getText());
  }
  ((FilterableComboBoxModel)getModel()).setFilter(filter);
  setPopupVisible(false);
  if (getModel().getSize() > 0)   setPopupVisible(true);
  filtering=false;
}
