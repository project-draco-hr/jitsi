{
  super.registerCustomCodecFormats(rtpManager);
  if (!FMJConditionals.REGISTER_FORMATS_WITH_EVERY_RTP_MANAGER && formatsRegisteredOnce)   return;
  Format format=new VideoFormat(Constants.H264_RTP);
  logger.debug("registering format " + format + " with RTP manager");
  rtpManager.addFormat(format,MediaUtils.getRTPPayloadType(format.getEncoding(),VideoMediaFormatImpl.DEFAULT_CLOCK_RATE));
  formatsRegisteredOnce=true;
}
