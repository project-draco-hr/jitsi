{
  VideoListener[] listeners;
synchronized (videoListeners) {
    listeners=videoListeners.toArray(new VideoListener[videoListeners.size()]);
  }
  if (listeners.length > 0) {
    VideoEvent event=new VideoEvent(this,type,visualComponent,origin);
    for (    VideoListener listener : listeners) switch (type) {
case VideoEvent.VIDEO_ADDED:
      listener.videoAdded(event);
    break;
case VideoEvent.VIDEO_REMOVED:
  listener.videoRemoved(event);
break;
}
}
}
