{
  int i, dataSize4;
  dataSize4=dataSize & 0xFFFC;
  for (i=0; i < dataSize4; i+=4) {
    data1[data1_offset + i + 0]*=gain;
    data1[data1_offset + i + 1]*=gain;
    data1[data1_offset + i + 2]*=gain;
    data1[data1_offset + i + 3]*=gain;
  }
  for (; i < dataSize; i++) {
    data1[data1_offset + i]*=gain;
  }
}
