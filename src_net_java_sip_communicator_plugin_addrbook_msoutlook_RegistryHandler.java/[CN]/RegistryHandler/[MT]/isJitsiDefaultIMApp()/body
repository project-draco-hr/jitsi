{
  if (!Advapi32Util.registryValueExists(WinReg.HKEY_CURRENT_USER,REGISTRY_DEFAULT_IM_APPLICATION_KEY,REGISTRY_DEFAULT_IM_APPLICATION_VALUE))   return false;
  return Advapi32Util.registryGetStringValue(WinReg.HKEY_CURRENT_USER,REGISTRY_DEFAULT_IM_APPLICATION_KEY,REGISTRY_DEFAULT_IM_APPLICATION_VALUE).equals(getApplicationName());
}
