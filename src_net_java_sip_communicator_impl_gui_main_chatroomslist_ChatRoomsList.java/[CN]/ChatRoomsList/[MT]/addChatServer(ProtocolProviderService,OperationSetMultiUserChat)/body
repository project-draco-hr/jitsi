{
  listModel.addElement(pps);
  try {
    List existingChatRooms=multiUserChatOperationSet.getExistingChatRooms();
    if (existingChatRooms == null)     return;
    Iterator i=existingChatRooms.iterator();
    while (i.hasNext()) {
      ChatRoom chatRoom=(ChatRoom)i.next();
      listModel.addElement(chatRoom);
    }
  }
 catch (  OperationFailedException ex) {
    logger.error("Failed to obtain existing chat rooms for the following server: " + pps.getAccountID().getService(),ex);
  }
}
