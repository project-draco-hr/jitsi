{
  try {
    setIcon.invoke(getImpl(),new Object[]{(icon == null) ? null : icon.getImage()});
  }
 catch (  IllegalAccessException ex) {
    throw new UndeclaredThrowableException(ex);
  }
catch (  InvocationTargetException ex) {
    Throwable cause=ex.getCause();
    if (cause instanceof NullPointerException)     throw (NullPointerException)cause;
    throw new UndeclaredThrowableException((cause == null) ? ex : cause);
  }
}
