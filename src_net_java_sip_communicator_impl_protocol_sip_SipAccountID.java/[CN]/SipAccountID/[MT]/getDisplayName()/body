{
  String returnValue=getUserID();
  String protocolName=(String)getAccountProperties().get(ProtocolProviderFactory.PROTOCOL);
  String service=getService();
  if (service == null || service.trim().length() == 0) {
    protocolName="RegistrarLess " + protocolName;
  }
 else {
    returnValue+="@" + service;
  }
  if (protocolName != null && protocolName.trim().length() > 0)   returnValue+=" (" + protocolName + ")";
  return returnValue;
}
