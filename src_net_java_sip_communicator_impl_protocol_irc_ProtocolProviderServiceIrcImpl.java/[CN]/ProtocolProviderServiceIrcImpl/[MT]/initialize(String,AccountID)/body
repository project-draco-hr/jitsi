{
synchronized (initializationLock) {
    this.accountID=accountID;
    OperationSetMultiUserChatIrcImpl multiUserChat=new OperationSetMultiUserChatIrcImpl(this);
    supportedOperationSets.put(OperationSetMultiUserChat.class.getName(),multiUserChat);
    this.ircStack=new IrcStack(this,getAccountID().getUserID(),getAccountID().getUserID(),"SIP Communicator 1.0","");
    isInitialized=true;
  }
}
