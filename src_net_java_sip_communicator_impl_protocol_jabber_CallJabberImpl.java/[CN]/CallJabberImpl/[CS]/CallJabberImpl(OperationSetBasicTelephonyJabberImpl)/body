{
  super(parentOpSet);
  int mediaTypeValueCount=MediaType.values().length;
  cobriStreamConnectors=new ArrayList<WeakReference<CobriStreamConnector>>(mediaTypeValueCount);
  for (int i=0; i < mediaTypeValueCount; i++)   cobriStreamConnectors.add(null);
  parentOpSet.getActiveCallsRepository().addCall(this);
}
