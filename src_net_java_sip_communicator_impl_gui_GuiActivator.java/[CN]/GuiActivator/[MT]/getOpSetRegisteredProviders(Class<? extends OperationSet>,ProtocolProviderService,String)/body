{
  List<ProtocolProviderService> providers=new ArrayList<ProtocolProviderService>();
  if (preferredProvider != null) {
    if (preferredProvider.isRegistered())     providers.add(preferredProvider);
 else {
      providers=GuiActivator.getRegisteredProviders(preferredProvider.getProtocolName(),opSet);
    }
  }
 else {
    if (preferredProtocolName != null)     providers=GuiActivator.getRegisteredProviders(preferredProtocolName,opSet);
 else     providers=GuiActivator.getRegisteredProviders(opSet);
  }
  return providers;
}
