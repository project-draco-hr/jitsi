{
  ServiceReference[] serRefs=null;
  String osgiFilter="(" + ProtocolProviderFactory.PROTOCOL + "="+ protocolProvider.getProtocolName()+ ")";
  try {
    serRefs=GuiActivator.bundleContext.getServiceReferences(ProtocolProviderFactory.class.getName(),osgiFilter);
  }
 catch (  InvalidSyntaxException ex) {
    logger.error("GuiActivator : " + ex);
  }
  return (ProtocolProviderFactory)GuiActivator.bundleContext.getService(serRefs[0]);
}
