{
  for (int i=0; i < 256; i++) {
    int input=~i;
    int mantissa=((input & 0xf) << 3) + 0x84;
    int segment=(input & 0x70) >> 4;
    int value=mantissa << segment;
    value-=0x84;
    if ((input & 0x80) != 0)     value=-value;
    lutTableL[i]=(byte)value;
    lutTableH[i]=(byte)(value >> 8);
  }
}
