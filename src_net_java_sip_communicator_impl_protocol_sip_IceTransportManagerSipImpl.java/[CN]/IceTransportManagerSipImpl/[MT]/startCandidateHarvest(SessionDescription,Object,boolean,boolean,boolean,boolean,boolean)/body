{
  iceAgent=createIceAgent();
  iceAgent.setControlling(true);
  IceSdpUtils.setIceCredentials(ourOffer,iceAgent.getLocalUfrag(),iceAgent.getLocalPassword());
  iceAgent.setControlling(true);
  for (  MediaDescription mLine : SdpUtils.extractMediaDescriptions(ourOffer)) {
    IceMediaStream iceStream=createIceStream(SdpUtils.getMediaType(mLine).toString(),iceAgent);
    IceSdpUtils.initMediaDescription(mLine,iceStream);
  }
}
