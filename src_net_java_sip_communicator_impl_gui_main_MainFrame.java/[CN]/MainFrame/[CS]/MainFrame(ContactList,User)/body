{
  this.clist=clist;
  this.user=user;
  callPanel=new CallPanel(this);
  tabbedPane=new MainTabbedPane(this);
  statusPanel=new StatusPanel(user.getProtocols());
  this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
  this.setInitialBounds();
