{
  ConfigurationService configService=GuiActivator.getConfigurationService();
  try {
    configService.setProperty("net.java.sip.communicator.impl.gui.showCallPanel",new Boolean(callManager.isShown()));
    configService.setProperty("net.java.sip.communicator.impl.gui.showOffline",new Boolean(getContactListPanel().getContactList().isShowOffline()));
  }
 catch (  PropertyVetoException e1) {
    logger.error("The proposed property change " + "represents an unacceptable value");
  }
}
