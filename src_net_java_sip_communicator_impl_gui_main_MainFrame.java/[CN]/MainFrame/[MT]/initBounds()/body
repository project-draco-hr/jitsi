{
  int width=GuiActivator.getResources().getSettingsInt("mainWindowWidth");
  int height=GuiActivator.getResources().getSettingsInt("mainWindowHeight");
  int minWidth=GuiActivator.getResources().getSettingsInt("minMainWindowWidth");
  int minHeight=GuiActivator.getResources().getSettingsInt("minMainWindowHeight");
  this.setMinimumSize(new Dimension(minWidth,minHeight));
  this.setSize(width,height);
  this.setLocation(Toolkit.getDefaultToolkit().getScreenSize().width - this.getWidth(),50);
}
