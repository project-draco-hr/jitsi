{
  ConfigurationService configService=GuiActivator.getConfigurationService();
  String isShown=configService.getString("net.java.sip.communicator.impl.ui.showCallPanel");
  if (isShown != null && isShown != "") {
    callManager.setShown(new Boolean(isShown).booleanValue());
  }
 else {
    callManager.setShown(true);
  }
}
