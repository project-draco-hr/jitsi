{
  ConfigurationService configService=GuiActivator.getConfigurationService();
  String isCallPanelShown=configService.getString("net.java.sip.communicator.impl.gui.showCallPanel");
  String isShowOffline=configService.getString("net.java.sip.communicator.impl.gui.showOffline");
  if (isCallPanelShown != null && isCallPanelShown != "") {
    callManager.setShown(new Boolean(isCallPanelShown).booleanValue());
  }
 else {
    callManager.setShown(true);
  }
  if (isShowOffline != null && isShowOffline != "") {
    getContactListPanel().getContactList().setShowOffline(new Boolean(isShowOffline).booleanValue());
  }
}
