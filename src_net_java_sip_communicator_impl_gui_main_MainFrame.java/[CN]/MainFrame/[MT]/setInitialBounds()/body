{
  this.setLocation(Toolkit.getDefaultToolkit().getScreenSize().width - MainFrame.WIDTH,50);
  this.getContentPane().setSize(LookAndFeelConstants.MAINFRAME_WIDTH,LookAndFeelConstants.MAINFRAME_HEIGHT);
  this.tabbedPane.setPreferredSize(new Dimension(LookAndFeelConstants.MAINFRAME_WIDTH,LookAndFeelConstants.MAINFRAME_HEIGHT));
}
