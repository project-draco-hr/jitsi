{
  if (!getStatusPanel().containsAccount(protocolProvider)) {
    this.getStatusPanel().addAccount(protocolProvider);
    this.tabbedPane.getContactListPanel().getContactList().requestFocus();
  }
  if (!callManager.containsCallAccount(protocolProvider) && getTelephony(protocolProvider) != null) {
    callManager.addCallAccount(protocolProvider);
  }
}
