{
  ServiceReference[] serRefs=null;
  String osgiFilter="(" + ProtocolProviderFactory.PROTOCOL + "="+ protocolProvider.getProtocolName()+ ")";
  try {
    serRefs=GuiActivator.bundleContext.getServiceReferences(ContactEventHandler.class.getName(),osgiFilter);
  }
 catch (  InvalidSyntaxException ex) {
    logger.error("GuiActivator : " + ex);
  }
  if (serRefs == null)   return null;
  return (ContactEventHandler)GuiActivator.bundleContext.getService(serRefs[0]);
}
