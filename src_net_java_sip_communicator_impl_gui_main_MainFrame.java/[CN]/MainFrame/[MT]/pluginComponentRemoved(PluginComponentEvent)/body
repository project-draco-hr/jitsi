{
  Component c=(Component)event.getSource();
  ContainerID containerID=event.getContainerID();
  if (containerID.equals(UIService.CONTAINER_CONTACT_LIST_SOUTH) || containerID.equals(UIService.CONTAINER_CONTACT_LIST_NORTH) || containerID.equals(UIService.CONTAINER_CONTACT_LIST_EAST)|| containerID.equals(UIService.CONTAINER_CONTACT_LIST_WEST)) {
    this.getContentPane().remove(c);
  }
}
