{
  PluginComponent c=event.getPluginComponent();
  Container containerID=c.getContainer();
  if (containerID.equals(Container.CONTAINER_CONTACT_LIST) || containerID.equals(UIService.CONTAINER_CONTACT_LIST_SOUTH) || containerID.equals(UIService.CONTAINER_CONTACT_LIST_NORTH)|| containerID.equals(UIService.CONTAINER_CONTACT_LIST_EAST)|| containerID.equals(UIService.CONTAINER_CONTACT_LIST_WEST)) {
    this.getContentPane().remove((Component)c.getComponent());
  }
}
