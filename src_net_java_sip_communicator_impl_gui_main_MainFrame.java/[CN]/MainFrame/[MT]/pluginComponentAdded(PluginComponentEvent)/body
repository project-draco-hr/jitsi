{
  Component c=(Component)event.getSource();
  if (event.getContainerID().equals(UIService.CONTAINER_CONTACT_LIST_SOUTH)) {
    this.getContentPane().add(c,BorderLayout.SOUTH);
  }
 else   if (event.getContainerID().equals(UIService.CONTAINER_CONTACT_LIST_NORTH)) {
    this.getContentPane().add(c,BorderLayout.NORTH);
  }
 else   if (event.getContainerID().equals(UIService.CONTAINER_CONTACT_LIST_EAST)) {
    this.getContentPane().add(c,BorderLayout.EAST);
  }
 else   if (event.getContainerID().equals(UIService.CONTAINER_CONTACT_LIST_WEST)) {
    this.getContentPane().add(c,BorderLayout.WEST);
  }
  this.pack();
}
