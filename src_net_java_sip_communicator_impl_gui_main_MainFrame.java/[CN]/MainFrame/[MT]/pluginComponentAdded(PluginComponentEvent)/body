{
  PluginComponent c=event.getPluginComponent();
  if (c.getContainer().equals(Container.CONTAINER_CONTACT_LIST)) {
    Object constraints=null;
    if (c.getConstraints() != null)     constraints=UIServiceImpl.getBorderLayoutConstraintsFromContainer(c.getConstraints());
 else     constraints=BorderLayout.SOUTH;
    this.getContentPane().add((Component)c.getComponent(),constraints);
  }
  this.pack();
}
