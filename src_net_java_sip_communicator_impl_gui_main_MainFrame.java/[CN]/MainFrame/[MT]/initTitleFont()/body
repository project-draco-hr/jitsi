{
  JComponent layeredPane=this.getLayeredPane();
  Font font=new Font(ApplicationProperties.getProperty("fontName"),Font.BOLD,new Integer(ApplicationProperties.getProperty("titleFontSize")).intValue());
  for (int i=0; i < layeredPane.getComponentCount(); i++) {
    layeredPane.getComponent(i).setFont(font);
  }
}
