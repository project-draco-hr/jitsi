{
  InputStream is=Constants.class.getClassLoader().getResourceAsStream("net/java/sip/communicator/impl/gui/resources/styles/defaultStyle.css");
  Reader r=new BufferedReader(new InputStreamReader(is));
  try {
    style.loadRules(r,null);
    r.close();
  }
 catch (  IOException e) {
    logger.error("Failed to load css style.",e);
  }
}
