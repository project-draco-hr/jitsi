{
  if (status.equals(ONLINE_STATUS))   return ImageLoader.getImage(ImageLoader.USER_ONLINE_ICON);
 else   if (status.equals(OFFLINE_STATUS))   return ImageLoader.getImage(ImageLoader.USER_OFFLINE_ICON);
 else   if (status.equals(AWAY_STATUS))   return ImageLoader.getImage(ImageLoader.USER_AWAY_ICON);
 else   if (status.equals(FREE_FOR_CHAT_STATUS))   return ImageLoader.getImage(ImageLoader.USER_FFC_ICON);
 else   return ImageLoader.getImage(ImageLoader.USER_OFFLINE_ICON);
}
