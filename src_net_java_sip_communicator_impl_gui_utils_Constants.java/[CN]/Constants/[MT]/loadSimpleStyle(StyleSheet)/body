{
  InputStream is=Constants.class.getClassLoader().getResourceAsStream(DEFAULT_STYLE_PATH + "/defaultStyle.css");
  Reader r=new BufferedReader(new InputStreamReader(is));
  try {
    style.loadRules(r,null);
    r.close();
  }
 catch (  IOException e) {
    logger.error("Failed to load css style.",e);
  }
}
