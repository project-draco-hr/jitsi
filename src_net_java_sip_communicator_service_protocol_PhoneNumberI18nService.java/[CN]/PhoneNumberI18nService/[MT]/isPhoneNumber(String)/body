{
  if (possibleNumber.indexOf('@') == -1) {
    if (!possibleNumber.matches(".*[a-zA-Z].*")) {
      return true;
    }
 else {
      String tmpPossibleNumber=possibleNumber.replaceAll(" \\(\\)","");
      if (!configService.getBoolean(DISABLED_PROP,false) && tmpPossibleNumber.startsWith("+")) {
        return true;
      }
    }
  }
  return false;
}
