{
  JCheckBoxMenuItem menuItem=new JCheckBoxMenuItem(GlobalStatusEnum.getI18NStatusName(status),new ImageIcon(status.getStatusIcon()));
  menuItem.setName(status.getStatusName());
  menuItem.addActionListener(this);
  if (index == -1)   add(menuItem);
 else   add(menuItem,index);
  return menuItem;
}
