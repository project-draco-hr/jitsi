{
  if (status < PresenceStatus.ONLINE_THRESHOLD) {
    return offlineItem;
  }
 else   if (status < PresenceStatus.AVAILABLE_THRESHOLD) {
    return awayItem;
  }
 else   if (status < PresenceStatus.EAGER_TO_COMMUNICATE_THRESHOLD) {
    return onlineItem;
  }
 else   if (status < PresenceStatus.MAX_STATUS_VALUE) {
    return ffcItem;
  }
 else {
    return offlineItem;
  }
}
