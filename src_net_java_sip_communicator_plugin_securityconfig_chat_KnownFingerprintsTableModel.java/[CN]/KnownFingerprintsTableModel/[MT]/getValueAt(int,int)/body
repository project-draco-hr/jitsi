{
  if (row < 0)   return null;
  Contact contact=allContacts.get(row);
switch (column) {
case CONTACTNAME_INDEX:
    return contact.getDisplayName();
case VERIFIED_INDEX:
  return (SecurityConfigActivator.getOtrKeyManagerService().isVerified(contact)) ? SecurityConfigActivator.getResources().getI18NString("plugin.otr.configform.COLUMN_VALUE_VERIFIED_TRUE") : SecurityConfigActivator.getResources().getI18NString("plugin.otr.configform.COLUMN_VALUE_VERIFIED_FALSE");
case FINGERPRINT_INDEX:
return SecurityConfigActivator.getOtrKeyManagerService().getRemoteFingerprint(contact);
default :
return null;
}
}
