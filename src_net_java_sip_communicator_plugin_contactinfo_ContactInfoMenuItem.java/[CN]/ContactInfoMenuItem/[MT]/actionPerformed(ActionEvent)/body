{
  ContactInfoDialog cinfoDialog=new ContactInfoDialog(metaContact);
  cinfoDialog.setLocation(Toolkit.getDefaultToolkit().getScreenSize().width / 2 - cinfoDialog.getWidth() / 2,Toolkit.getDefaultToolkit().getScreenSize().height / 2 - cinfoDialog.getHeight() / 2);
  cinfoDialog.setVisible(true);
}
