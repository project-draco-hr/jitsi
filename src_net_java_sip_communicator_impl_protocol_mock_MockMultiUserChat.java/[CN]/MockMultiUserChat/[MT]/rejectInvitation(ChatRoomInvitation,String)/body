{
  ChatRoomInvitationRejectedEvent evt=new ChatRoomInvitationRejectedEvent(this,invitation.getTargetChatRoom(),provider.getAccountID().getUserID(),invitation.getReason(),new Date());
  Iterator iter=invitationRejectListeners.iterator();
  while (iter.hasNext()) {
    ChatRoomInvitationRejectionListener elem=(ChatRoomInvitationRejectionListener)iter.next();
    elem.invitationRejected(evt);
  }
}
