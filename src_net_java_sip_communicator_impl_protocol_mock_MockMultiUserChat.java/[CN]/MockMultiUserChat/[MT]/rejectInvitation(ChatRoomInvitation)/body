{
  ChatRoomInvitationRejectedEvent evt=new ChatRoomInvitationRejectedEvent(invitation,null,null,invitation.getReason(),new Date());
  Iterator iter=invitationRejectListeners.iterator();
  while (iter.hasNext()) {
    ChatRoomInvitationRejectionListener elem=(ChatRoomInvitationRejectionListener)iter.next();
    elem.invitationRejected(evt);
  }
}
