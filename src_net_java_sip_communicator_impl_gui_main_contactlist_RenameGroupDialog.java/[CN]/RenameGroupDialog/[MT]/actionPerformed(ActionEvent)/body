{
  JButton button=(JButton)e.getSource();
  String name=button.getName();
  if (name.equals("rename")) {
    if (metaGroup != null) {
      new Thread(){
        public void run(){
          clist.renameMetaContactGroup(metaGroup,renameGroupPanel.getNewName());
        }
      }
.start();
    }
    this.dispose();
  }
 else {
    this.dispose();
  }
}
