{
  List<RemoteControlListener> listeners=getListeners();
  if (!listeners.isEmpty()) {
    RemoteControlRevokedEvent event=new RemoteControlRevokedEvent(peer);
    for (    RemoteControlListener l : listeners)     l.remoteControlRevoked(event);
  }
  this.removesNullAndRevokedControlPeer(peer.getPeerID());
}
