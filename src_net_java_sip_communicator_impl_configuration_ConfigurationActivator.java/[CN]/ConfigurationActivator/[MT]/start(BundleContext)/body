{
  try {
    logger.logEntry();
    logger.debug("Service Impl: " + getClass().getName() + " [  STARTED ]");
    bundleContext.registerService(ConfigurationService.class.getName(),impl,new java.util.Hashtable());
    logger.debug("Service Impl: " + getClass().getName() + " [REGISTERED]");
  }
  finally {
    logger.logExit();
  }
}
