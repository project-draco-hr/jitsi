{
  Iterator<CallPeer> callParticipants=this.callPeersList.iterator();
  try {
    while (callParticipants.hasNext()) {
      CallPeer participant=callParticipants.next();
      if (participant.getProtocolProvider().getOperationSet(OperationSetDTMF.class) != null) {
        OperationSetDTMF dtmfOpSet=(OperationSetDTMF)participant.getProtocolProvider().getOperationSet(OperationSetDTMF.class);
        dtmfOpSet.sendDTMF(participant,dtmfTone);
      }
    }
  }
 catch (  NullPointerException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
catch (  ClassCastException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
catch (  OperationFailedException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
}
