{
  Iterator<? extends CallPeer> callPeers=this.callPeersList.iterator();
  try {
    while (callPeers.hasNext()) {
      CallPeer peer=callPeers.next();
      OperationSetDTMF dtmfOpSet=peer.getProtocolProvider().getOperationSet(OperationSetDTMF.class);
      if (dtmfOpSet != null)       dtmfOpSet.sendDTMF(peer,dtmfTone);
    }
  }
 catch (  NullPointerException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
catch (  ClassCastException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
catch (  OperationFailedException e1) {
    logger.error("Failed to send a DTMF tone.",e1);
  }
}
