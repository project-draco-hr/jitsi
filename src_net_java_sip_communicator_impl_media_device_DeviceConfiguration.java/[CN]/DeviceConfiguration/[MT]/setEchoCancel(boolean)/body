{
  try {
    PortAudioManager.getInstance().setEchoCancel(enabled,PortAudioManager.getInstance().getFrameSize(),PortAudioManager.getInstance().getFilterLength());
    MediaActivator.getConfigurationService().setProperty(PROP_AUDIO_ECHOCANCEL_ENABLED,enabled);
  }
 catch (  PortAudioException ex) {
    logger.error("Error setting echocancel config",ex);
  }
}
