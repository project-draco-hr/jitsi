{
  if (participant.getState().equals(CallPeerState.DISCONNECTED)) {
    logger.debug("Ignoring a request to hangup a call participant " + "that is already DISCONNECTED");
    return;
  }
  MockCallParticipant callParticipant=(MockCallParticipant)participant;
  logger.info("hangupCallParticipant");
  callParticipant.setState(CallPeerState.DISCONNECTED,null);
}
