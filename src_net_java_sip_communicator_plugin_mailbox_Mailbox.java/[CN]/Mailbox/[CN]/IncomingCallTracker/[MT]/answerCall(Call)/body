{
  OperationSetBasicTelephony telephony=(OperationSetBasicTelephony)call.getProtocolProvider().getOperationSet(OperationSetBasicTelephony.class);
  Iterator participants=call.getCallParticipants();
  while (participants.hasNext()) {
    CallPeer participant=(CallPeer)participants.next();
    try {
      telephony.answerCallParticipant(participant);
    }
 catch (    OperationFailedException exc) {
      logger.error("Could not answer to : " + participant + " caused by the following exception: "+ exc.getMessage(),exc);
    }
  }
}
