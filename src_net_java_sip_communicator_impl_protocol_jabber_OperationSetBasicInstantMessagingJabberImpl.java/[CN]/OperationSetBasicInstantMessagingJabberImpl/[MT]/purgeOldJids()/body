{
  long currentTime=System.currentTimeMillis();
  Iterator<Map.Entry<String,TargetAddress>> entries=jids.entrySet().iterator();
  while (entries.hasNext()) {
    Map.Entry<String,TargetAddress> entry=entries.next();
    TargetAddress target=entry.getValue();
    if (currentTime - target.lastUpdatedTime > JID_INACTIVITY_TIMEOUT)     entries.remove();
  }
}
