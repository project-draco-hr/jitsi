{
  jabberProvider.fireRegistrationStateChanged(jabberProvider.getRegistrationState(),RegistrationState.CONNECTION_FAILED,RegistrationStateChangeEvent.REASON_INTERNAL_ERROR,null);
  opSetPersPresence.fireProviderStatusChangeEvent(opSetPersPresence.getPresenceStatus(),jabberProvider.getJabberStatusEnum().getStatus(JabberStatusEnum.OFFLINE));
}
