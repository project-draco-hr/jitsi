{
  if (this.keyFrameControl != keyFrameControl) {
    if ((this.keyFrameControl != null) && (keyFrameRequestee != null))     this.keyFrameControl.removeKeyFrameRequestee(keyFrameRequestee);
    this.keyFrameControl=keyFrameControl;
    if ((this.keyFrameControl != null) && (keyFrameRequestee != null))     this.keyFrameControl.addKeyFrameRequestee(-1,keyFrameRequestee);
  }
}
