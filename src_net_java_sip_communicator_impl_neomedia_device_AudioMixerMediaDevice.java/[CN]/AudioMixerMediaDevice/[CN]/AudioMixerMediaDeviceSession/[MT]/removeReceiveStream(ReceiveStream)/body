{
  removeInputDataSources(new DataSourceFilter(){
    public boolean accept(    DataSource dataSource){
      if (dataSource instanceof ReceiveStreamPushBufferDataSource)       return receiveStream.equals(((ReceiveStreamPushBufferDataSource)dataSource).getReceiveStream());
 else       return dataSource.equals(receiveStream.getDataSource());
    }
  }
);
}
