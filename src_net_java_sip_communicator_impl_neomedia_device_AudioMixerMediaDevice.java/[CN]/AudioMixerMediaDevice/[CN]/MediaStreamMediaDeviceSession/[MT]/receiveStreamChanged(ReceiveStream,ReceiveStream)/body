{
  super.receiveStreamChanged(oldValue,newValue);
  if (oldValue != null)   audioMixerMediaDeviceSession.removeReceiveStream(oldValue);
  if (newValue != null) {
synchronized (streamAudioLevelListenerLock) {
      if (this.streamAudioLevelListener != null)       audioMixerMediaDeviceSession.putStreamAudioLevelListener(newValue,streamAudioLevelListener);
    }
    audioMixerMediaDeviceSession.addReceiveStream(newValue);
  }
}
