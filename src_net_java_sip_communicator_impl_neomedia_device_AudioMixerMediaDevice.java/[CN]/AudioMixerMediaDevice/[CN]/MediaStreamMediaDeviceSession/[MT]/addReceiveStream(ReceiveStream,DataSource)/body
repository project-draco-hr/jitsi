{
  audioMixerMediaDeviceSession.addReceiveStream(receiveStream,receiveStreamDataSource);
  DataSource captureDevice=getCaptureDevice();
  if (captureDevice instanceof PushBufferDataSourceDelegate<?>)   captureDevice=((PushBufferDataSourceDelegate<?>)captureDevice).getDataSource();
  if (captureDevice instanceof AudioMixingPushBufferDataSource)   ((AudioMixingPushBufferDataSource)captureDevice).addInputDataSource(receiveStreamDataSource);
synchronized (streamAudioLevelListenerLock) {
    if (this.streamAudioLevelListener != null) {
      audioMixerMediaDeviceSession.putStreamAudioLevelListener(receiveStream,streamAudioLevelListener);
    }
  }
}
