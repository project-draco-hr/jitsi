{
synchronized (streamAudioLevelListenerLock) {
    this.streamAudioLevelListener=listener;
    if (listener != null) {
      if (getReceiveStream() != null)       audioMixerMediaDeviceSession.putStreamAudioLevelListener(getReceiveStream(),listener);
    }
 else     if (getReceiveStream() != null)     audioMixerMediaDeviceSession.removeStreamAudioLevelListener(getReceiveStream());
  }
}
