{
synchronized (streamAudioLevelListenerLock) {
    this.streamAudioLevelListener=listener;
    ReceiveStream receiveStream=getReceiveStream();
    if (listener != null) {
      if (receiveStream != null)       audioMixerMediaDeviceSession.putStreamAudioLevelListener(receiveStream,listener);
    }
 else     if (receiveStream != null)     audioMixerMediaDeviceSession.removeStreamAudioLevelListener(receiveStream);
  }
}
