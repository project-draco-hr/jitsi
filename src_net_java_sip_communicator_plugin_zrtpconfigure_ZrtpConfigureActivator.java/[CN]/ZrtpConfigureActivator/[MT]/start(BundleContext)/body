{
  bundleContext=bc;
  bundleContext.registerService(ConfigurationForm.class.getName(),new LazyConfigurationForm("net.java.sip.communicator.plugin.zrtpconfigure.ZrtpConfigurePanel",getClass().getClassLoader(),"impl.media.security.zrtp.CONF_ICON","impl.media.security.zrtp.TITLE",1100),null);
  resourceService=ResourceManagementServiceUtils.getService(bundleContext);
  if (resourceService == null)   return;
  ServiceReference refConfigService=bundleContext.getServiceReference(ConfigurationService.class.getName());
  if (refConfigService == null)   return;
  configService=(ConfigurationService)bundleContext.getService(refConfigService);
}
