{
  if (ce instanceof ControllerClosedEvent) {
    if (ce instanceof ControllerErrorEvent)     logger.warn("ControllerErrorEvent: " + ce);
 else     logger.debug("ControllerClosedEvent: " + ce);
    setFailed(true);
  }
  Object stateLock=getStateLock();
synchronized (stateLock) {
    stateLock.notifyAll();
  }
}
