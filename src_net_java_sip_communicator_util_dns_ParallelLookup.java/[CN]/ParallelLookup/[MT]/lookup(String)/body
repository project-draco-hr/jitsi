{
  Record[] records=null;
  Lookup lookup;
  try {
    lookup=new Lookup(domain,Type.SRV);
    new LookupThread(lookup).start();
  }
 catch (  TextParseException tpe) {
    logger.error("Failed to parse domain=" + domain,tpe);
    throw new ParseException(tpe.getMessage(),0);
  }
  return records;
}
