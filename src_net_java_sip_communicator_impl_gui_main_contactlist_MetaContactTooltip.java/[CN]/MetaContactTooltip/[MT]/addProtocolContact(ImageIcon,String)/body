{
  JLabel protocolContactLabel=new JLabel(protocolContactName,protocolContactIcon,JLabel.CENTER);
  protocolContactsPanel.add(protocolContactLabel);
  int stringWidth=GuiUtils.getStringWidth(protocolContactLabel,protocolContactName) + protocolContactIcon.getIconWidth() + protocolContactLabel.getIconTextGap();
  if (textWidth < stringWidth)   textWidth=stringWidth;
  textHeight+=textRowHeight;
}
