      Desktop.getDesktop().browse(new URI(OtrActivator.resourceService.getI18NString("plugin.otr.authbuddydialog.HELP_URI")));
    }
 catch (    Exception ex) {
      fallback=true;
    }
  }
  if (fallback) {
  }
}
