{
  SessionID sessionID=new SessionID(contact.getProtocolProvider().getAccountID().getAccountUniqueID(),contact.getAddress(),contact.getProtocolProvider().getProtocolName());
  if (contactsMap.containsKey(sessionID))   return sessionID;
  ScSessionID scSessionID=new ScSessionID(sessionID);
synchronized (contactsMap) {
    contactsMap.put(scSessionID,contact);
  }
  return sessionID;
}
