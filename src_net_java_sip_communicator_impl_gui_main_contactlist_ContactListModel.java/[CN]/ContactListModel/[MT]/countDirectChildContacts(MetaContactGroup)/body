{
  int count=0;
  if (parentGroup != null && !this.isGroupClosed(parentGroup)) {
    if (showOffline) {
      count=parentGroup.countChildContacts();
    }
 else {
      Iterator i=parentGroup.getChildContacts();
      while (i.hasNext()) {
        MetaContact contact=(MetaContact)i.next();
        if (isContactOnline(contact))         count++;
      }
    }
  }
  return count;
}
