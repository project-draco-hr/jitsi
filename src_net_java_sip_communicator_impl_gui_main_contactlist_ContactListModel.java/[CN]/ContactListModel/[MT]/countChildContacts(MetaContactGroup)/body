{
  if (showOffline)   return group.countChildContacts();
 else {
    int count=0;
    Iterator i=group.getChildContacts();
    while (i.hasNext()) {
      MetaContact metaContact=(MetaContact)i.next();
      if (isContactOnline(metaContact)) {
        count++;
      }
 else {
        break;
      }
    }
    return count;
  }
}
