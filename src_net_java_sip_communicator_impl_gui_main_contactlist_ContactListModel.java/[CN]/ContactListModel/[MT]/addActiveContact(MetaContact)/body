{
synchronized (activeContacts) {
    SystrayService stray=GuiActivator.getSystrayService();
    if (activeContacts.size() == 0 && stray != null)     stray.setSystrayIcon(SystrayService.ENVELOPE_IMG_TYPE);
    if (!activeContacts.contains(metaContact))     this.activeContacts.add(metaContact);
  }
}
