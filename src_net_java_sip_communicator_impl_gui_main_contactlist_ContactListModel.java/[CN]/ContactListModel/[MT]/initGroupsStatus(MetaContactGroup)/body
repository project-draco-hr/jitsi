{
  boolean isClosed=ConfigurationManager.getContactListGroupStatus(group.getMetaUID());
  System.out.println("GROUPATAAAA========" + group.getGroupName() + "=========IS CLOSED =============="+ isClosed);
  if (isClosed) {
    closedGroups.add(group);
  }
  Iterator subgroups=group.getSubgroups();
  while (subgroups.hasNext()) {
    MetaContactGroup subgroup=(MetaContactGroup)subgroups.next();
    this.initGroupsStatus(subgroup);
  }
}
