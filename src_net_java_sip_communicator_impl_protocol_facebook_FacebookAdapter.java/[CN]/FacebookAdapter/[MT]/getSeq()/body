{
  int tempSeq=-1;
  while (tempSeq == -1) {
    String seqResponseBody;
    try {
      seqResponseBody=facebookGetMethod(getMessageRequestingUrl(-1));
      tempSeq=parseSeq(seqResponseBody);
      logger.trace("getSeq(): SEQ: " + tempSeq);
      if (tempSeq >= 0) {
        return tempSeq;
      }
    }
 catch (    JSONException e) {
      logger.warn(e.getMessage());
    }
    try {
      logger.trace("retrying to fetch the seq code after 1 second...");
      Thread.sleep(1000);
    }
 catch (    InterruptedException e) {
      logger.warn(e.getMessage());
    }
  }
  return tempSeq;
}
