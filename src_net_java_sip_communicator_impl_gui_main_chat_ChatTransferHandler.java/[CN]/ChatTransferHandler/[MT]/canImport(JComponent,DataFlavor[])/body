{
  JTextComponent c=(JTextComponent)comp;
  for (int i=0, n=flavor.length; i < n; i++) {
    if (flavor[i].equals(DataFlavor.javaFileListFlavor)) {
      return true;
    }
 else     if (flavor[i].equals(DataFlavor.stringFlavor)) {
      if (c.isEditable() && c.isEnabled()) {
        return true;
      }
      return false;
    }
  }
  return false;
}
