{
  try {
    return this.googleService.query(query,ContactFeed.class);
  }
 catch (  Exception e) {
    logger.info("Failed to execute query. Going to refresh token" + " and try again.",e);
    refreshToken();
  }
  try {
    return this.googleService.query(query,ContactFeed.class);
  }
 catch (  Exception e) {
    throw new FailedContactQueryException(e);
  }
}
