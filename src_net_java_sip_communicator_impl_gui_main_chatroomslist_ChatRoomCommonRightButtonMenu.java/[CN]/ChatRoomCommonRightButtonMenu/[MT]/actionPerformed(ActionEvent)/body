{
  JMenuItem menuItem=(JMenuItem)e.getSource();
  String itemName=menuItem.getName();
  if (itemName.equals("createChatRoom")) {
    AddChatRoomDialog addChatRoomDialog=new AddChatRoomDialog(mainFrame,protocolProvider);
    addChatRoomDialog.setVisible(true);
  }
 else   if (itemName.equals("searchForChatRooms")) {
    JoinChatRoomDialog joinChatRoomDialog=new JoinChatRoomDialog(mainFrame,protocolProvider);
    joinChatRoomDialog.showDialog();
  }
}
