{
  String eventName=event.getSourceEventType();
  NotificationEntry entry=getNotificationEntry(eventName);
  if (entry == null) {
    PopupMessageNotificationHandler popupHandler=(PopupMessageNotificationHandler)notificationService.getEventNotificationActionHandler(eventName,NotificationService.ACTION_POPUP_MESSAGE);
    CommandNotificationHandler programHandler=(CommandNotificationHandler)notificationService.getEventNotificationActionHandler(eventName,NotificationService.ACTION_COMMAND);
    SoundNotificationHandler soundHandler=(SoundNotificationHandler)notificationService.getEventNotificationActionHandler(eventName,NotificationService.ACTION_SOUND);
    entry=new NotificationEntry(notificationService.isActive(event.getSourceEventType()),programHandler != null && programHandler.isEnabled(),(programHandler != null) ? programHandler.getDescriptor() : null,popupHandler != null && popupHandler.isEnabled(),soundHandler != null && soundHandler.isEnabled(),(soundHandler != null) ? soundHandler.getDescriptor() : null,eventName);
    addEntry(entry);
    notifTable.repaint();
  }
}
