{
  VideoFormat ivf=(VideoFormat)in;
  Dimension inSize=ivf.getSize();
  Dimension outSize;
  if (inSize == null) {
    VideoFormat ovf=outputFormats[0];
    if (ovf == null)     return null;
 else     outSize=ovf.getSize();
  }
 else   outSize=inSize;
  return new Format[]{new AVFrameFormat(outSize,ensureFrameRate(ivf.getFrameRate()),FFmpeg.PIX_FMT_YUV420P,Format.NOT_SPECIFIED)};
}
