{
  for (  VideoFormat vf : outputFormats) {
    Dimension size=vf.getSize();
    if (size.getWidth() == width) {
      return new RGBFormat(size,-1,Format.intArray,ensureFrameRate(frameRate),32,RED_MASK,GREEN_MASK,BLUE_MASK,1,size.width,Format.FALSE,Format.NOT_SPECIFIED);
    }
  }
  return null;
}
