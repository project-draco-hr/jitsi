{
  try {
    File imageFile=GuiActivator.getFileAccessService().getPrivatePersistentFile(STORE_DIR + index + ".png");
    if (imageFile.exists() && !imageFile.delete())     logger.error("Can't delete stored image at index " + index);
  }
 catch (  Exception e) {
    logger.info("Can't access to file: " + STORE_DIR + index+ ".png");
  }
}
