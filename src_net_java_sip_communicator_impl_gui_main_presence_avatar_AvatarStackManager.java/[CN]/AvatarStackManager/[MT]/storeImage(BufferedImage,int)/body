{
  String imagePath=STORE_DIR + index + ".png";
  try {
    File storeDir=GuiActivator.getFileAccessService().getPrivatePersistentDirectory(STORE_DIR);
    storeDir.mkdirs();
    File file=GuiActivator.getFileAccessService().getPrivatePersistentFile(imagePath);
    ImageIO.write(image,"png",file);
  }
 catch (  IOException e) {
