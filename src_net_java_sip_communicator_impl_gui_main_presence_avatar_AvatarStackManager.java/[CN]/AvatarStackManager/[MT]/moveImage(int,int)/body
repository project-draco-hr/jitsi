{
  String oldImagePath=STORE_DIR + oldIndex + ".png";
  String newImagePath=STORE_DIR + newIndex + ".png";
  try {
    File oldFile=GuiActivator.getFileAccessService().getPrivatePersistentFile(oldImagePath);
    File newFile=GuiActivator.getFileAccessService().getPrivatePersistentFile(newImagePath);
    if (oldFile.exists())     oldFile.renameTo(newFile);
  }
 catch (  Exception e) {
  }
}
