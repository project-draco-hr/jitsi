{
  int nextDelim;
  char stringChar=0;
  int textLen=this.text.length();
  for (nextDelim=start; nextDelim < textLen; nextDelim++) {
    char c=this.text.charAt(nextDelim);
    if (stringChar == 0) {
      if (this.delims.indexOf(c) >= 0) {
        break;
      }
 else       if (DELIMS.indexOf(c) >= 0) {
        stringChar=c;
      }
    }
 else {
      if (c == stringChar) {
        stringChar=0;
      }
    }
  }
  return nextDelim;
}
