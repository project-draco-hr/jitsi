{
  InputStream is=MsnStatusEnum.class.getClassLoader().getResourceAsStream(imagePath);
  if (is == null)   return null;
  byte[] icon=null;
  try {
    icon=new byte[is.available()];
    is.read(icon);
  }
 catch (  IOException exc) {
    logger.error("Failed to load icon: " + imagePath,exc);
  }
  return icon;
}
